    function colorsw(){
  setTimeout(function()              
  {
  $(".swatch-element-color").on("click", function(){
   var varnt = $(this).find("a").attr("hreff")
  // var hipl = $(this).parents(".swatch-standard").siblings('.item-image').attr("href");
  // var splithipl = hipl.split('=')[0];
   var comli = varnt;
   // alert(varnt);
   $(this).parents(".swatch-standard").siblings('.item-image').attr("href",comli);
   /* swatch image */
   var varntimg = $(this).attr("swatchv-img");
   var varntsrcset = $(this).attr("swatchv-srcset");
   $(this).parents(".swatch-standard").siblings('.item-image').find('img.fir_img').attr("src",varntimg);
   $(this).parents(".swatch-standard").siblings('.item-image').find('img.fir_img').attr("srcset",varntsrcset);
   $(".swatch-value-checked").removeAttr("style");
    $(this).find('.swatch-value-checked').attr("style","display:block");
    
  });
          },1600);
    }
    function swchov(){
      
       setTimeout(function()              
  {
      $(".swatch-element-color").hover(
  
   function () {
    var orgimg = $(this).parent().parent().parent().siblings('.item-image').find('img.fir_img').attr("src");
    var varntimg = $(this).attr("swatchv-img")
   $(this).parent().parent().parent().siblings('.item-image').find('img.fir_img').attr("src",varntimg);
               }, 
   function () {
   $(this).parent().parent().parent().siblings('.item-image').find('img.fir_img').attr("src",orgimg);
               }
   ); 
        },600); 
    } 
    
    
