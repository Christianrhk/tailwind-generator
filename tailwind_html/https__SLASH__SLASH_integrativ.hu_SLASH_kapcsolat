<script>
function contactSubmit(token){

  var text='';
  var haserr = false;
  $('input[type=text],input[type=email],textarea', '#contactform').each(function() {

    var rrrattr = $(this).attr('required');
    if (typeof rrrattr !== 'undefined' && rrrattr !== false && $(this).val().trim()=='') {
      haserr = true;
    }

    var label = $(this).attr("data-pluss");
    if(label){
      text=text+label+": "+$(this).val()+"\n";
    }
  });
  $("#contactdata").val(text);
  console.log(text);
  if(!haserr){
    $("#GCV3").val(token);
    document.getElementById("contactform").submit();
    return true;
  }
  else{
    alert("Kérjük adja meg adatait!");
  }
}
</script>

