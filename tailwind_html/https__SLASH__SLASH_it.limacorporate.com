<script>
    function toggleLogin () {
  const loginContainer = document.getElementById('login-container')
  const loginButton = document.getElementById('user-button')

  if (loginContainer.classList.contains('open')) {
    loginContainer.classList.remove('open')
    loginContainer.classList.add('hidden')
    if (loginButton.classList.contains('selected')) {
      loginButton.classList.remove('selected')
    }
  } else {
    loginContainer.classList.add('open')
    loginContainer.classList.remove('hidden')
    if (!loginButton.classList.contains('selected')) {
      loginButton.classList.add('selected')
    }
  }
}

function toggleRecover () {
  const recoverContainer = document.getElementById('recover-container')
  const loginButton = document.getElementById('user-button')

  if (recoverContainer.classList.contains('open')) {
    recoverContainer.classList.remove('open')
    recoverContainer.classList.add('hidden')
    if (loginButton.classList.contains('selected')) {
      loginButton.classList.remove('selected')
    }
  } else {
    recoverContainer.classList.add('open')
    recoverContainer.classList.remove('hidden')
    if (!loginButton.classList.contains('selected')) {
      loginButton.classList.add('selected')
    }
  }
}

function toggleBoth (modalToClose) {
  switch (modalToClose) {
    case 'recover':
      toggleRecover()
      toggleLogin()
      break

    default:
      toggleLogin()
      toggleRecover()
      break
  }
}

if ($('#login-container form#login-form').length > 0) {
  $('#login-container form#login-form').on('submit', function () {
    if ($('#login-container form#login-form input#login-disclaimer').length <= 0 || ($('#login-container form#login-form input#login-disclaimer').length > 0 && $('#login-container form#login-form input#login-disclaimer').is(':checked'))) {
      var username = $('#login-container form#login-form input#login-email').val()
      var password = $('#login-container form#login-form input#login-password').val()
      const payload = { username: username, password: password }
      var redirectURL = $('#login-container form#login-form input#login_redirect_url').val()

      $('#login-container form#login-form span.alert').removeClass('alert-danger').html('')

      fetch(RUN_URI + 'meded-account?action=login&ajax_call=1&lang=' + $('html').attr('lang'), {
        method: 'post',
        headers: {
          Accept: 'application/json, text/plain, */*',
          'Content-Type': 'application/json'
        },
        body: JSON.stringify(payload)
      })
        .then(res => res.text())
        .then(res => {
          // Login success
          if (res === 'loginOk') {
            window.location.href = redirectURL
          } else {
            $('#login-container form#login-form span.alert').addClass('alert-danger').html(res)
          }
        })
    }
    return false
  })
}

if ($('#recover-container form#recover-form').length > 0) {
  $('#recover-container form#recover-form').on('submit', function () {
    var email = $('#recover-container form#recover-form input#recover-email').val()
    var recoveryOkText = $('#recover-container form#recover-form input#recover-ok-text').val()
    const payload = { email: email }

    $('#recover-container form#recover-form span.alert').removeClass('alert-danger').removeClass('alert-success').html('')

    $('#recover-container form#recover-form  button#recover-submit').attr('disabled', 'disabled')

    fetch(RUN_URI + 'meded-account?action=recovery&ajax_call=1&lang=' + $('html').attr('lang'), {
      method: 'post',
      headers: {
        Accept: 'application/json, text/plain, */*',
        'Content-Type': 'application/json'
      },
      body: JSON.stringify(payload)
    })
      .then(res => res.text())
      .then(res => {
        // Login success
        if (res === 'recoveryOk') {
          $('#recover-container form#recover-form span.alert').addClass('alert-success').html(recoveryOkText)
        } else {
          $('#recover-container form#recover-form  button#recover-submit').removeAttr('disabled')
          $('#recover-container form#recover-form span.alert').addClass('alert-danger').html(res)
        }
      })
    return false
  })
}
</script>

