        <script>
  function isMedical () {
    fetch(RUN_URI + 'register?action=accept')
    .then(data => {
      const surgeonsDisclaimers = document.getElementsByClassName('megamenu-disclaimer-box')
      const menulist = document.getElementById('menu-list-surgeons')
      Array.from(surgeonsDisclaimers).forEach(disclaimer => {
        disclaimer.classList.add('accepted')
      });
      menulist.classList.remove('pointer-events-none')
      return false
    });
  }
