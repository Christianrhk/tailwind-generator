            '                                    <div class="image-edit">\n' +
            '                                        <input type=\'file\'\n' +
            '                                               id="imageCommentUpload-' + action + '" class="image-upload-comment"\n' +
            '                                               hidden\n' +
            '                                               accept=".png, .jpg, .jpeg" data-action="' + action + '"/>\n' +
            '                                    </div>\n' +
            '                                    <div wire:ignore\n' +
            '                                         class="image-preview-comment display-none image-input image-input-outline"\n' +
            '                                         data-kt-image-input="true" data-action="' + action + '">\n' +
            '                                        <!--begin::Preview existing avatar-->\n' +
            '                                        <div id="imageCommentPreview-' + action + '"\n' +
            '                                             class="image-input-wrapper w-125px h-125px"></div>\n' +
