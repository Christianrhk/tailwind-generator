            '                                            class="btn btn-icon btn-circle btn-comment-image-remove btn-active-color-primary w-25px h-25px bg-white shadow"\n' +
            '                                            data-kt-image-input-action="change" data-bs-toggle="tooltip"\n' +
            '                                            title="Xóa hình ảnh" data-action="' + action + '">\n' +
            '                                            <i class="bi bi-x fs-2"></i>\n' +
            '                                            <!--begin::Inputs-->\n' +
            '                                            <!--end::Inputs-->\n' +
            '                                        </label>\n' +
            '                                        <!--end::Label-->\n' +
            '                                    </div>\n' +
            '                                </div>\n' +
            '                            </form>\n' +
            '                        </div>')
    })


    function resetComment(action) {
        $('.comment-reply-container[data-action="' + action + '"]').addClass('display-none').html('');
        $('.comment-action[data-action="' + action + '"]').html('<span class="comment-like text-gray-700" data-action="' + action + '"><i class="fas fa-arrow-up text-700 "></i><span class="text-gray-700 text-700 ">  0  </span><i class="fas fa-arrow-down text-700"></i></span> <span class="comment-reply text-gray-700 text-700 ml-2" data-action="' + action + '">Trả lời</span>');

    }

