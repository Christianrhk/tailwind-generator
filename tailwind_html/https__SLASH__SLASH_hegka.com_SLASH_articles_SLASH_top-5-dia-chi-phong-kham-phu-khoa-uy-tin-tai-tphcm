<script>
    var action;
    var imageReplyDelete = [];

    function readURLReply(input) {
        action = $(input).data("action");
        comment = $(input).data("comment");
        if (input.files && input.files[0]) {
            var reader = new FileReader();
            reader.onload = function (e) {
                $('#imagePreviewReply-' + action).css('background-image', "url(" + e.target.result + ")");
                $('#imagePreviewReply-' + action).attr('data-delete') == 0;
                $('#imagePreviewReply-' + action).hide();
                $('#imagePreviewReply-' + action).fadeIn(650);
            }

            reader.readAsDataURL(input.files[0]);
            $('.image-preview-commit-reply[data-action="' + action + '"]').removeClass('display-none')
        }
    }

    $(document).on('change', '.image-upload-reply', function () {
        readURLReply(this);
    })

    $(document).on('keypress', '.post-comment-text-reply', function (e) {
        comment = $(this).data("comment");
        articleId = $(this).data("article");
        if (!e.shiftKey && (e.keyCode == 13 || e.keyCode == 10)) {
            action = $(this).data("action");
            var $this = $(this)
            let html = $(this).html();
            html.replace('<div>', '')
            html.replace('<span>', '')
            // html.replace('<span class="emoji">', '')
            html.replace('</span>', '')
            html.replace('</div>', '')
            html.replace('<div><br></div>', '')
            // let result = html.split('<br>');
            let data = html;

            Livewire.emitTo('user.articles.comment',"setContentReply", data)

            var element = $('.comments[data-article="' + articleId +'"] .button-submit[data-article="' + articleId +'"]');
            var inputFile = $('#imageUploadReply-' + action + '-' + comment)[0];
            if (inputFile.files && inputFile.files[0]) {
                var reader = new FileReader();
                reader.readAsDataURL(inputFile.files[0]);

                reader.onload = function () {
                    Livewire.emitTo('user.articles.comment', "setImageReply", reader.result)
                };
            } else {
                Livewire.emitTo('user.articles.comment', "setImageReply", null)
            }
            Livewire.emitTo('user.articles.comment', "reply", comment)
            Livewire.emitTo('user.articles.comment', "setReplyId", action)
            if(  $('#imagePreviewReply-' + action).attr('data-delete') == 1) {
                Livewire.emitTo('user.articles.comment', "setDeleteImageReply", 1)
            } else {
                Livewire.emitTo('user.articles.comment', "setDeleteImageReply", 0)
            }
            setTimeout(function () {
                element.click()
                    if (action == '0') {
                        $(this).html('')
                        $('#imageUploadReply-' + action +'-' +comment).removeAttr('style')
                        $('.image-preview-commit-reply[data-action="' + action + '"]').addClass('display-none')
                    } else {
                        $this.html(html)
                        resetReply(action, comment)
                    }
                // resetReply(action, comment)
                // }
            }, 1000);
        }
        // if (!e.shiftKey && (e.keyCode == 13 || e.keyCode == 10)) {
        //     let html = $(this).html();
        //     html.replace('<span>', '')
        //     html.replace('</span>', '')
        //     let result = html.split('<br>');
        //     let data = '';
        //     $.each(result, function (key, value) {
        //         if (key != 0) {
        //             data += '<br>'
        //         }
        //         data = data + '<span>' + value + '</span>'
        //     });
        //     var inputFile = $('#imageUploadReply-' + action + '-' + comment)[0];
        //
        //     if (inputFile.files && inputFile.files[0]) {
        //         var reader = new FileReader();
        //         reader.readAsDataURL(inputFile.files[0]);
        //
        //         reader.onload = function () {
        //             Livewire.emitTo('user.articles.reply', "reply", data, comment, action, reader.result,imageReplyDelete)
        //         };
        //     } else {
        //         Livewire.emitTo('user.articles.reply', "reply", data, comment, action,null,imageReplyDelete)
        //     }
        //     if (action == '0') {
        //         $(this).html('')
        //         $('#imageUploadReply-' + action +'-' +comment).removeAttr('style')
        //         $('.image-preview-commit-reply[data-action="' + action + '"]').addClass('display-none')
        //     } else {
        //         resetReply(action, comment)
        //     }
        //
        //     imageReplyDelete = imageReplyDelete.filter(item => item !== action)
        //
        // }
    })

    $(document).on('mouseover', '.comment-post-reply', function () {
        action = $(this).data("action");
        $('.icon-comment-edit-reply[data-action="' + action + '"]').removeClass("display-none")
    })
    $(document).on('mouseout', '.comment-post-reply', function () {
        action = $(this).data("action");
        $('.icon-comment-edit-reply[data-action="' + action + '"]').addClass("display-none")
    })

    $(document).on('click', '.reply-cancel', function () {
        action = $(this).data("action");
        comment = $(this).data("comment");
        resetReply(action, comment)
    })
    $(document).on('click', '.btn-reply-image-remove', function () {
        action = $(this).data("action");
        comment = $(this).data("comment");
        $('#imagePreviewReply-' + action).removeAttr('style')
        $('#imagePreviewReply-' + action).attr('data-delete', "1")
        $('#imageUploadReply-' + action+ '-' +comment).val('')
        $('.image-preview-commit-reply[data-action="' + action + '"]').addClass('display-none')
        imageReplyDelete.push(action)
    })
    $(document).on('click', '.icon-comment-edit-reply', function () {
        action = $(this).data("action");
        comment = $(this).data("comment");
        $('.post-comment-text-reply[data-action="' + action + '"]').attr("contenteditable", "true").attr("spellcheck", "false").css('width', '100%');
        $(this).remove();
        $('.comment-post-reply[data-action="' + action + '"]').css('width', '100%');
        $('.fonts-edit-reply[data-action="' + action + '"]').removeClass("display-none")
        $('.btn-reply-image-remove[data-action="' + action + '"]').removeClass("display-none")
        $('.image-upload-reply-main[data-action="' + action + '"]').append('<div class="d-flex flex-row align-items-center reply-cancel" data-action="' + action + '" data-comment="' + comment + '">\n' +
            '                                <small>Hủy</small>\n' +
            '                            </div>')
        $('.reply-action[data-action="' + action + '"]').html('')
    })

    function resetReply(action, comment) {
        $('.post-comment-text-reply[data-action="' + action + '"]').removeAttr("contenteditable").removeAttr("spellcheck").removeAttr('style');
        $('.comment-post-reply[data-action="' + action + '"]').removeAttr('style').append('<button\n' +
            '                                    class="btn btn-icon display-none btn-circle btn-active-color-primary w-25px h-25px bg-white shadow icon-comment-edit-reply ml-2"\n' +
            '                                    data-kt-image-input-action="change" data-action="' + action + '" data-comment="' + comment + '"\n' +
            '                                    data-bs-toggle="tooltip" title="">\n' +
            '                                    <i class="bi bi-pencil-fill fs-7"></i>\n' +
            '                                </button>');
        $('.fonts-edit-reply[data-action="' + action + '"]').addClass("display-none")
        $('.btn-reply-image-remove[data-action="' + action + '"]').addClass("display-none")
        $('.reply-cancel[data-action="' + action + '"]').remove()
        $('.reply-action[data-action="' + action + '"]').html('<span class="comment-like text-gray-700" data-action="' + action + '" data-comment="' + comment + '"><i class="fas fa-arrow-up text-700 "></i><span class="text-gray-700 text-700 ">  0  </span><i class="fas fa-arrow-down text-700"></i></span> <span class="reply-comment text-gray-700 text-700 ml-2" data-action="' + action + '" data-comment="' + comment + '">Trả lời</span>');

        if( $('#imageUploadReply-' + action).attr('data-delete') == 1) {
            $('.image-preview-commit-reply[data-action="' + action + '"]').addClass("display-none")
        }
    }
    $(document).on('click','#article .card .card-body .content-right .rightbar-top .share', function () {
        $.getScript("https://sp.zalo.me/plugins/sdk.js", function(){});
    })
</script>



<script>
    var action;
    var comment;
    var imageDelete = [];

    function readURLReplyComment(input) {
        action = $(input).data("action");
        if (input.files && input.files[0]) {
            var reader = new FileReader();
            reader.onload = function (e) {
                $('#imageReplyPreview-' + action).css('background-image', "url(" + e.target.result + ")");
                $('#imageReplyPreview-' + action).attr('data-delete') == 0;
                $('#imageReplyPreview-' + action).hide();
                $('#imageReplyPreview-' + action).fadeIn(650);
            }

            reader.readAsDataURL(input.files[0]);
            $('.image-preview-reply[data-action="' + action + '"]').removeClass('display-none')
        }
    }

    $(document).on('change', '.image-upload-comment', function () {
        readURLReplyComment(this);
    })


    $(document).on('keypress', '.post-reply-text', function (e) {
        action = $(this).data("action");
        comment = $(this).data("comment");
        articleId = $(this).data("article");


        if (!e.shiftKey && (e.keyCode == 13 || e.keyCode == 10)) {
            action = $(this).data("action");
            var $this = $(this)
            let html = $(this).html();
            html.replace('<div>', '')
            html.replace('<span>', '')
            // html.replace('<span class="emoji">', '')
            html.replace('</span>', '')
            html.replace('</div>', '')
            html.replace('<div><br></div>', '')
            // let result = html.split('<br>');
            let data = html;
            Livewire.emitTo('user.articles.comment',"setContentReply", data)

            var element = $('.comments[data-article="' + articleId +'"] .button-submit[data-article="' + articleId +'"]');
            var inputFile = $('#imageReplyUpload-' + action + '-' + comment)[0];
            if (inputFile.files && inputFile.files[0]) {
                var reader = new FileReader();
                reader.readAsDataURL(inputFile.files[0]);

                reader.onload = function () {
                    Livewire.emitTo('user.articles.comment', "setImageReply", reader.result)
                };
            } else {
                Livewire.emitTo('user.articles.comment', "setImageReply", null)
            }
            Livewire.emitTo('user.articles.comment', "reply", comment)
            if( $('#imageReplyPreview-' + action).attr('data-delete') == 1) {
                Livewire.emitTo('user.articles.comment', "setDeleteImageReply", 1)
            } else {
                Livewire.emitTo('user.articles.comment', "setDeleteImageReply", 0)
            }
            setTimeout(function () {
                element.click()
                resetCommentReply(action, comment)
            }, 1000);
        }



        // var imagePreview = $('.image-preview-reply[data-action="' + action + '"]').html();
        //
        // if (!e.shiftKey && (e.keyCode == 13 || e.keyCode == 10)) {
        //     let html = $(this).html();
        //     html.replace('<span>', '')
        //     html.replace('</span>', '')
        //     let result = html.split('<br>');
        //     let data = '';
        //     $.each(result, function (key, value) {
        //         if (key != 0) {
        //             data += '<br>'
        //         }
        //         data = data + '<span>' + value + '</span>'
        //     });
        //     var inputFile = $('#imageReplyUpload-' + action + '-' + comment)[0];
        //     if (inputFile.files && inputFile.files[0]) {
        //         var reader = new FileReader();
        //         reader.readAsDataURL(inputFile.files[0]);
        //
        //         reader.onload = function () {
        //             Livewire.emitTo('user.articles.reply', "reply", data, comment, null, reader.result)
        //         };
        //     } else {
        //         Livewire.emitTo('user.articles.reply', "reply", data, comment)
        //     }
        //     resetCommentReply(action, comment)
        //
        // }
    })

    $(document).on('click', '.reply-comment-cancel', function () {
        action = $(this).data("action");
        comment = $(this).data("comment");
        resetCommentReply(action, comment)
    })
    $(document).on('click', '.btn-reply-comment-image-remove', function () {
        action = $(this).data("action");
        comment = $(this).data("comment");
        $('#imageReplyUpload-' + action + '-' + comment).val('')
        $('#imageReplyPreview-' + action).removeAttr('style')
        $('#imageReplyPreview-' + action).attr('data-delete', "1")
        $('.image-preview-reply[data-action="' + action + '"]').addClass('display-none')
    })
    $(document).on('click', '.reply-comment', function () {
        action = $(this).data("action");
        comment = $(this).data("comment");
        articleId = $(this).data("article");

        $('.reply-action[data-action="' + action + '"]').html('')
        $('.reply-container[data-action="' + action + '"]').removeClass('display-none')
        $('.reply-container[data-action="' + action + '"]').html('<img src="https://ui-avatars.com/api/?name=IM&amp;color=7F9CF5&amp;background=EBF4FF" class="rounded-image">\n' +
            '                        <div class="d-flex flex-column ml-2 comment-item">\n' +
            '                            <form class="form form-' + action + '-' + comment + '">\n' +
            '                                <div wire:ignore class="reply-comment-post">\n' +
            '                                    <div class="post-reply-text" contenteditable="true" spellcheck="false"\n' +
            '                                         data-action="' + action + '" data-comment="' + comment + '" data-article="' + articleId + '"></div>\n' +
            '                                </div>\n' +
            '                                <label class="fonts" for="imageReplyUpload-' + action + '-' + comment + '">\n' +
            '                                        <img width="20px" height="20px" src="https://hegka.com/assets/icon/picture.png"></label>\n' +
            '                                <label class="comment-emoji">\n' +
            '                                        <img width="20px" height="20px" src="https://hegka.com/assets/icon/smile.png"></label>\n' +
            '\n' +
            '                                <div class="image-upload-container">\n' +
            '                                    <div wire:ignore class="image-edit">\n' +
            '                                        <input wire:model="imageReply.' + action + '" type=\'file\'\n' +
            '                                               id="imageReplyUpload-' + action + '-' + comment + '" class="image-upload-comment"\n' +
            '                                               hidden\n' +
            '                                               accept=".png, .jpg, .jpeg" data-action="' + action + '" data-comment="' + comment + '"/>\n' +
            '                                    </div>\n' +
            '                                    <div wire:ignore\n' +
            '                                         class="image-preview-reply display-none image-input image-input-outline"\n' +
            '                                         data-kt-image-input="true" data-action="' + action + '" data-comment="' + comment + '">\n' +
            '                                        <!--begin::Preview existing avatar-->\n' +
            '                                        <div id="imageReplyPreview-' + action + '"\n' +
            '                                             class="image-input-wrapper w-125px h-125px" data-delete="0"></div>\n' +
            '                                        <!--end::Preview existing avatar-->\n' +
            '                                        <!--begin::Label-->\n' +
            '                                        <label\n' +
            '                                            class="btn btn-icon btn-circle btn-reply-comment-image-remove btn-active-color-primary w-25px h-25px bg-white shadow"\n' +
            '                                            data-kt-image-input-action="change" data-bs-toggle="tooltip"\n' +
            '                                            title="Xóa hình ảnh" data-action="' + action + '" data-comment="' + comment + '">\n' +
            '                                            <i class="bi bi-x fs-2"></i>\n' +
            '                                            <!--begin::Inputs-->\n' +
            '                                            <!--end::Inputs-->\n' +
            '                                        </label>\n' +
            '                                        <!--end::Label-->\n' +
            '                                    </div>\n' +
            '                                </div>\n' +
            '                            </form>\n' +
            '                        </div>')
    })


    function resetCommentReply(action, comment) {
        $('#imageReplyUpload-' + action + '-' + comment).val('')
        $('.reply-container[data-action="' + action + '"]').addClass('display-none').html('');
        $('.reply-action[data-action="' + action + '"]').html('<span class="comment-like text-gray-700" data-action="' + action + '" data-comment="' + comment + '"><i class="fas fa-arrow-up text-700 "></i><span class="text-gray-700 text-700 ">  0  </span><i class="fas fa-arrow-down text-700"></i></span> <span class="reply-comment text-gray-700 text-700 ml-2" data-action="' + action + '" data-comment="' + comment + '">Trả lời</span>');

    }

    $(window).scroll(function() {
        var height = $(window).scrollTop();
        var heightScreen = $(window).height();
        var topBanner = heightScreen - height
        if(topBanner <= 135 ){
            topBanner = 135
        }
        var heightScroll = $(window).scrollTop();
        if(heightScroll >= 350 ){
            heightScroll = 350
        }
        heightScroll = heightScroll *3;
        $('.banner-right').css('top', topBanner);
        $('.banner-left').css('top', topBanner);
        $('.banner-scroll').css('background-position-y',-heightScroll);
        if(height > 50) {
            $('#my-progress').addClass('progress-fixed');
            $('#my-progress-footer').addClass('progress-fixed-footer');
        } else {
            $('#my-progress').removeClass('progress-fixed');
            $('#my-progress-footer').removeClass('progress-fixed-footer');
        }

        var footerHeight = $('.detail-content').innerHeight();

        var elem = document.getElementById("my-bar");
        var elemFooter = document.getElementById("my-bar-footer");

        var width = (height / footerHeight) * 100;
        elem.style.width = width + "%";
        elemFooter.style.width = width + "%";
    });
</script>
<script>
    $(document).ready(function () {
        $(document).on('click','.container-banner #rightbar,.container-banner #leftbar,.container-banner .event-notification-box,.container-banner #leftbar,.container-banner .toolbar,.container-banner .modal', function (e) {
            e.stopPropagation();
        })
        window.onscroll = function (ev) {
            if($(window).width() <= 992) {
                var top = $('.ads-show').offset().top - $(window).height() + 50;
                var bottom = $('.ads-show').offset().top + $(window).height();
                let heightScroll = window.scrollY;
                if (heightScroll >= top && heightScroll <= bottom) {
                    $('.ads-container').addClass('position-fixed').removeClass('d-none')
                } else {
                    $('.ads-container').removeClass('position-fixed').addClass('d-none')
                }
            }

            if(heightScroll >= 230) {
                $('.container-banner').addClass('banner-scroll').addClass('background-size-cover')
            } else {
                $('.container-banner').removeClass('banner-scroll').removeClass('background-size-cover')
            }
            if ((window.innerHeight + window.scrollY) >= ($('.main-body').height() - 300)) {
                Livewire.emitTo('user.articles.detail', 'loadMore')
                
                
                
            }
        };
    })
</script>
                            <script src="https://hegka.com/home/js/ads-detail-mobile.js"></script>
                <script>
                    var background = '/storage/article_groups/4mKvfW7vrtN4mZCXk1cqlzilVKYCEiCJXA8WWLL2.gif';
                    $('.article-content .content-left .ar-content').addClass('mobile');
                    var childrenLength = parseInt($('.article-content .content-left .ar-content p').length / 2);

                    if ($('.article-content .content-left .ar-content').has('.ads-show').length <= 0) {
                        $('.article-content .content-left .ar-content p:eq(' + childrenLength + ')').after('<div class="ads-show"></div><a class="ads-container"' +
                            ' rel="sponsored" href="' + linkBackground +'" target="' + targetBackground +'">' +
                            '<img src="' + background + '">' +
                            '</a><div class="ads-container-scroll"><div class="scroll-content text-uppercase">' + adsTitle + '</div></div>\n')
                    }
                </script>
                            <script>
        $(document).ready(function () {
            window.livewire.on('ConfirmCloseModal', (e) => {
                $('#' + e.id).modal('hide');
            })
        })
    </script>

<script>
    // click to Question button and Back to Top For New Right bar
    const questionBtns = document.querySelectorAll('.right-control .item')
    const questionBtn = document.querySelectorAll('.right-control .item')[2];
    let newBackToTop = document.querySelectorAll('.right-control .item')[0];
    if(questionBtns.length) {
        newBackToTop.onclick = () => {
            window.scrollTo({
                top: 0,
                behavior: 'smooth',
            })
        }
        if(questionBtn) {
            questionBtn.onclick = (e) => {
                e.stopPropagation();
                const img = questionBtn.querySelectorAll('img');
                img[0].style.opacity = '0';
                img[1].style.opacity = '1';
                const tagBox = document.querySelector('.right-control .tags-box');
                if (!tagBox.classList.contains('active')) {
                    tagBox.classList.add('active');
                    img[0].style.opacity = '0';
                    img[1].style.opacity = '1';
                } else {
                    tagBox.classList.remove('active');
                    img[0].style.opacity = '1';
                    img[1].style.opacity = '0';
                }
            }

            // make a hover after clicking question button
            questionBtn.addEventListener('mouseover', () => {
                const img = questionBtn.querySelectorAll('img');
                img[0].style.opacity = '0';
                img[1].style.opacity = '1';
            })
            questionBtn.addEventListener('mouseout', () => {
                const tagBox = document.querySelector('.right-control .tags-box');
                const img = questionBtn.querySelectorAll('img');
                if (!tagBox.classList.contains('active')) {
                    img[0].style.opacity = '1';
                    img[1].style.opacity = '0';
                } else {
                    img[0].style.opacity = '0';
                    img[1].style.opacity = '1';
                }
            })
        }
        // Turn off Tags Box When clicking to the child of TagsBox
        const tagsInBox = document.querySelectorAll('.copyright a');
        tagsInBox.forEach(ele => {
            ele.onclick = () => {
                const itemLength = document.querySelectorAll('.right-control .item').length;
                const tagBox = document.querySelector('.right-control .tags-box');
                tagBox.classList.remove('active');
                const img = document.querySelectorAll('.right-control .item')[itemLength-1].querySelectorAll('img');
                img[0].style.opacity = '1';
                img[1].style.opacity = '0';
            }
        })

        // StopPropagation when clicking the button inside the tagsInBox
        const tagsBox = document.querySelector('.tags-box .copyright');
        const tagsInBoxBtn = document.querySelector('.copyright .btn-group .create-company');
        tagsBox.onclick = (e) => {
            e.stopPropagation();
        }
        tagsInBoxBtn.onclick = () => {
            const itemLength = document.querySelectorAll('.right-control .item').length;
            const tagBox = document.querySelector('.right-control .tags-box');
            tagBox.classList.remove('active');
            const img = document.querySelectorAll('.right-control .item')[itemLength-1].querySelectorAll('img');
            img[0].style.opacity = '1';
            img[1].style.opacity = '0';
        }
        // turn off Tags Box when clicking to the body
        document.body.onclick = () => {
            const itemLength = document.querySelectorAll('.right-control .item').length;
            const tagBox = document.querySelector('.right-control .tags-box');
            tagBox.classList.remove('active');
            const img = document.querySelectorAll('.right-control .item')[itemLength-1].querySelectorAll('img');
            img[0].style.opacity = '1';
            img[1].style.opacity = '0';
        }
    }
    $(document).on('click', '.dropdown .delete-deal', function (e) {
        $(this).parents('article').remove();
        $(this).parents('.swiper .swiper-slide').remove();
    })
    $(document).on('click', '.copy-button', function (e) {
        e.stopPropagation();
        e.preventDefault();
        var $input = $(this).parent().find('.copy-input');
        $input.select();
        document.execCommand("copy");
    })
    const isHover = (e) => e.parentElement.querySelector(":hover") === e;
    function showLikeBtn() {
        $(document).on('mouseenter','.newly-post', function () {
            $(this).find('.reaction-article-btn').removeClass('hidden');
        })
        $(document).on('mouseleave','.newly-post', function () {
            let $this = $(this);
            setTimeout(function () {
                $this.find('.reaction-article-btn').addClass('hidden');
            }, 1000)
        })
    }
    showLikeBtn()
</script>
</body>
</html>
