            <script>
    var action;
    var articleId;
    var imageDelete = [];

    function readURL(input) {
        action = $(input).data("action");
        if (input.files && input.files[0]) {
            var reader = new FileReader();
            reader.onload = function (e) {
                $('#imagePreview-' + action).css('background-image', "url(" + e.target.result + ")");
                $('#imagePreview-' + action).attr('data-delete') == 0;
                $('#imagePreview-' + action).hide();
                $('#imagePreview-' + action).fadeIn(650);
            }

            reader.readAsDataURL(input.files[0]);
            $('.image-preview[data-action="' + action + '"]').removeClass('display-none')
        }
    }

    $(document).on('change', '.image-upload', function () {
        readURL(this);
    })

    $(document).on('keypress', '.post-comment-text', function (e) {
        if (!e.shiftKey && (e.keyCode == 13 || e.keyCode == 10)) {
            action = $(this).data("action");
            articleId = $(this).data("article");
            var $this = $(this)
            let html = $(this).html();
            html.replace('<div>', '')
            html.replace('<span>', '')
            // html.replace('<span class="emoji">', '')
            html.replace('</span>', '')
            html.replace('</div>', '')
            html.replace('<div><br></div>', '')
            // let result = html.split('<br>');
            let data = html;
            Livewire.emitTo('user.articles.comment',"setDeleteImage", false)
            imageDelete = imageDelete.filter(function(value){
                if (value == action){
                    Livewire.emitTo('user.articles.comment',"setDeleteImage", true)
                    return false;
                }
                return true;
            });
            Livewire.emitTo('user.articles.comment',"setContent", data)
            Livewire.emitTo('user.articles.comment',"setAction", action)
            var element = $('.comments[data-article="' + articleId +'"] .button-submit[data-article="' + articleId +'"]');
            setTimeout(function () {
                element.click()
                if (action == '0') {
                    $this.empty();
                    $('#imagePreview-' + action).removeAttr('style')
                    $('.image-preview[data-action="' + action + '"]').addClass('display-none')
                } else {
                    $this.html(html)
                    reset(action)
                }
                // imageDelete = imageDelete.filter(item => item !== action)
            }, 1000);
        }
    })

    $(document).on('mouseover', '.comment-post', function () {
        action = $(this).data("action");
        $('.icon-comment-edit[data-action="' + action + '"]').removeClass("display-none")
    })
    $(document).on('mouseout', '.comment-post', function () {
        action = $(this).data("action");
        $('.icon-comment-edit[data-action="' + action + '"]').addClass("display-none")
    })

    $(document).on('click', '.comment-cancel', function () {
        action = $(this).data("action");
        reset(action)
    })
    $(document).on('click', '.btn-icon-image-remove', function () {
        action = $(this).data("action");
        $('#imagePreview-' + action).removeAttr('style')
        $('.image-preview[data-action="' + action + '"]').addClass('display-none')
        imageDelete.push(action)
    })
    $(document).on('click', '.icon-comment-edit', function () {
        action = $(this).data("action");
        $('.post-comment-text[data-action="' + action + '"]').attr("contenteditable", "true").attr("spellcheck", "false").css('width', '100%');
        $('.icon-comment-edit[data-action="' + action + '"]').remove();
        $('.comment-post[data-action="' + action + '"]').css('width', '100%');
        $('.fonts-edit[data-action="' + action + '"]').removeClass("display-none")
        $('.btn-icon-image-remove[data-action="' + action + '"]').removeClass("display-none")
        $('.image-upload-main[data-action="' + action + '"]').append('<div class="d-flex flex-row align-items-center comment-cancel" data-action="' + action + '">\n' +
            '                                <small>Hủy</small>\n' +
            '                            </div>')
        $('.comment-action[data-action="' + action + '"]').html('')
    })

    function reset(action) {
        $('.post-comment-text[data-action="' + action + '"]').removeAttr("contenteditable").removeAttr("spellcheck").removeAttr('style');
        $('.comment-post[data-action="' + action + '"]').removeAttr('style').append('<button\n' +
            '                                    class="btn btn-icon display-none btn-circle btn-active-color-primary w-25px h-25px bg-white shadow icon-comment-edit ml-2"\n' +
            '                                    data-kt-image-input-action="change" data-action="' + action + '"\n' +
            '                                    data-bs-toggle="tooltip" title="">\n' +
            '                                    <i class="bi bi-pencil-fill fs-7"></i>\n' +
            '                                </button>');
        $('.fonts-edit[data-action="' + action + '"]').addClass("display-none")
        // $('.image-preview[data-action="' + action + '"]').addClass("display-none")
        $('.btn-icon-image-remove[data-action="' + action + '"]').addClass("display-none")
        $('.comment-cancel[data-action="' + action + '"]').remove();
        $('.comment-action[data-action="' + action + '"]').html('<span class="comment-like text-gray-700" data-action="' + action + '"><i class="fas fa-arrow-up text-700 "></i><span class="text-gray-700 text-700 ">  0  </span><i class="fas fa-arrow-down text-700"></i></span> <span class="comment-reply text-gray-700 text-700 ml-2" data-action="' + action + '">Trả lời</span>');
    }

</script>



<script>
    var action;
    var imageDelete = [];

    function readURLComment(input) {
        action = $(input).data("action");
        if (input.files && input.files[0]) {
            var reader = new FileReader();
            reader.onload = function (e) {
                $('#imageCommentPreview-' + action).css('background-image', "url(" + e.target.result + ")");
                $('#imageCommentPreview-' + action).attr('data-delete') == 0;
                $('#imageCommentPreview-' + action).hide();
                $('#imageCommentPreview-' + action).fadeIn(650);
            }

            reader.readAsDataURL(input.files[0]);
            $('.image-preview-comment[data-action="' + action + '"]').removeClass('display-none')
        }
    }

    $(document).on('change', '.image-upload-comment', function () {
        readURLComment(this);
    })

    $(document).on('keypress', '.post-reply-comment-text', function (e) {
        if (!e.shiftKey && (e.keyCode == 13 || e.keyCode == 10)) {
            action = $(this).data("action");
            articleId = $(this).data("article");
            var $this = $(this)
            let html = $(this).html();
            html.replace('<div>', '')
            html.replace('<span>', '')
            // html.replace('<span class="emoji">', '')
            html.replace('</span>', '')
            html.replace('</div>', '')
            html.replace('<div><br></div>', '')
            // let result = html.split('<br>');
            let data = html;
            var image;

            Livewire.emitTo('user.articles.comment',"setContentReply", data)

            var element = $('.comments[data-article="' + articleId +'"] .button-submit[data-article="' + articleId +'"]');
            var inputFile = $('#imageCommentUpload-' + action)[0];
            if (inputFile.files && inputFile.files[0]) {
                image = inputFile.files[0];
                var reader = new FileReader();
                reader.readAsDataURL(inputFile.files[0]);

                reader.onload = function () {
                    // image = reader.result
                    Livewire.emitTo('user.articles.comment', "setImageReply", reader.result)
                };
            } else {
                Livewire.emitTo('user.articles.comment', "setImageReply", null)
            }
            Livewire.emitTo('user.articles.comment', "reply", action)
            if( $('#imageCommentPreview-' + action).attr('data-delete') == 1) {
                Livewire.emitTo('user.articles.comment', "setDeleteImageReply", 1)
            } else {
                Livewire.emitTo('user.articles.comment', "setDeleteImageReply", 0)
            }
            setTimeout(function () {
                element.click()
                // if (action == '0') {
                //     $this.empty();
                //     $('#imagePreview-' + action).removeAttr('style')
                //     $('.image-preview[data-action="' + action + '"]').addClass('display-none')
                //     element.click()
                // } else {
                    resetComment(action)
                // }
            }, 1000);
        }
        // action = $(this).data("action");
        // var imagePreview = $('.image-preview-comment[data-action="' + action + '"]').html();
        //
        // if (!e.shiftKey && (e.keyCode == 13 || e.keyCode == 10)) {
        //     let html = $(this).html();
        //     html.replace('<span>', '')
        //     html.replace('</span>', '')
        //     let result = html.split('<br>');
        //     let data = '';
        //     $.each(result, function (key, value) {
        //         if (key != 0) {
        //             data += '<br>'
        //         }
        //         data = data + '<span>' + value + '</span>'
        //     });
        //
        //     var inputFile = $('#imageCommentUpload-' + action)[0];
        //     if (inputFile.files && inputFile.files[0]) {
        //         var reader = new FileReader();
        //         reader.readAsDataURL(inputFile.files[0]);
        //
        //         reader.onload = function () {
        //             Livewire.emitTo('user.articles.reply', "reply", data, action, null, reader.result)
        //         };
        //     } else {
        //         Livewire.emitTo('user.articles.reply', "reply", data, action)
        //     }
        //     resetComment(action)
        // }
    })

    $(document).on('click', '.reply-comment-cancel', function () {
        action = $(this).data("action");
        resetComment(action)
    })
    $(document).on('click', '.btn-comment-image-remove', function () {
        action = $(this).data("action");
        $('#imageCommentPreview-' + action).removeAttr('style')
        $('.image-preview-comment[data-action="' + action + '"]').addClass('display-none')
        $('#imageCommentUpload-' + action).val('')
        $('#imageCommentPreview-' + action).attr('data-delete', "1");
    })
    $(document).on('click', '.comment-reply', function () {
        action = $(this).data("action");
        articleId = $(this).data("article");

        $('.comment-action[data-action="' + action + '"]').html('')
        $('.comment-reply-container[data-action="' + action + '"]').removeClass('display-none')
        $('.comment-reply-container[data-action="' + action + '"]').html('<img src="https://ui-avatars.com/api/?name=IM&amp;color=7F9CF5&amp;background=EBF4FF" class="rounded-image">\n' +
            '                        <div class="d-flex flex-column ml-2 comment-item">\n' +
            '                            <form class="form" wire:submit.prevent="comment">\n' +
            '                                   <div wire:ignore class="reply-comment-post">' +
