                                                                        });
                                                                }
                                                                if (pageURL == BasePath + "?v=" + (BLOG_VER + 1)||pageURL == baseURL + "?v=" + (BLOG_VER + 1)|| pageURL== baseURL || pageURL == BasePath || pathURL == BasePath + "index.html" && theme_slug == "" && (!post_preview)) {
                                                                    document.title = pageTitle;
                                                                    await fetch(BasePath + "templates/slider.html?v=" + BLOG_VER)
