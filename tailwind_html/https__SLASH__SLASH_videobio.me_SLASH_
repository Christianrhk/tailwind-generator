                console.log("chkforgotpassword:NOT CHECKED");
                 $("#forgotPassword").hide();
                $("#signinform").show();
                $("#signintitle").text("Sign into your account");
                $("#labelforgotpassword").text("I forgot my password");
            }
        });

        $("#resetPassword").click(function(){
            $.post('https://videobio.me/c5adminservices',
            {"mode": "resetpassword",'uEmail': $("#forgotuEmail").val(),'tid':""
            },
            function (response) {
                console.log("response",response);
                if(response.isuser=="yes"){
                    toastr.success("Password email sent");
                    $( "#chkforgotpassword" ).prop( "checked", false );

                    $("#forgotPassword").hide();
                    $("#signinform").show();
                    $("#signintitle").text("Sign into your account");
                    $("#labelforgotpassword").text("I forgot my password");

                }else{
                    toastr.error("Email does not exist");
                }

            },'json');
        });

  });
