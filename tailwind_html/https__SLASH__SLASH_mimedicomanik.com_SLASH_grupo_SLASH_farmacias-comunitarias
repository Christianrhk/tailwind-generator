    <script>
        var my_meds;
        window.onload = ()=>{document.getElementById('q').addEventListener("keyup",(event)=>{if(event.code==='Enter')search()})};
        function search(){
            var q=document.getElementById("q").value.trim();
            if(q.length>3){
                var e=document.getElementById('body');if(e)e.remove();
                my_meds.search(q);
            }
        }
    </script>
    <script src="https://unpkg.com/riot@4/riot.min.js"></script>
    <script type="module">
        import MyMeds from '/my-meds.js'
        riot.register('my-meds', MyMeds)
        my_meds=riot.mount('my-meds')[0];
    </script>
    
