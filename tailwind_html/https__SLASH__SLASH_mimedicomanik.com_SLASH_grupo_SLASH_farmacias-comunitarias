    <script>
        var g_mqaeu;
        var u=15843;
        var n="Llanito";
        var myPrivTok = null;
    function mqaeu(i,u,n){
        document.getElementById("n").innerHTML=n;
        var e=document.getElementById('meds');
        if(e)e.remove();
        this.u=u;this.n=n;
        g_mqaeu.setPRVT(myPrivTok);
        g_mqaeu.search(i,u);
    }
    var photo_viewer;
    var time=0;
    var imgs = null;
    function displayImg(index) {
        if (photo_viewer) {
            if (time == 0) {
                photo_viewer.update({items: imgs});
                time++;
            }

            photo_viewer.update({ index: index, visible: true });
        }
    }
    function displayAvatar(av) {
        if (av && av.length>0) {
            time=0;
            photo_viewer.update({ index: -1, items:[]});
            photo_viewer.update({ src: av, visible: true});
        }
    }
    var hr_cons;
    function agendar(i){
        hr_cons.open(u,i,n)
    }   
    </script>
    
