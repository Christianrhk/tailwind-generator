    <script>
        var photo_viewer;
        var time = 0;
        var imgs = [{"uuid":"184f2b3c-d9d1-4eb5-9b41-2cefed3fb0de","portada":0},{"uuid":"adb4cba0-c974-44c6-9913-6f7570f4bc41","portada":0},{"uuid":"405c99c2-ac65-4cb9-9217-a21bfa0dbf63","portada":0},{"uuid":"25035eef-c140-43a7-9661-0ff0594be64c","portada":0},{"uuid":"5ffdb5e5-a77e-4f0d-afff-6261ce11d95a","portada":0},{"uuid":"674f7554-557a-4848-87c0-8e99d25a46b3","portada":0},{"uuid":"59cd8104-085e-4621-90e0-2f934736c5c1","portada":0}];
        var myPrivTok = null;

        function displayImg(index) {
            if (photo_viewer) {
                if (time == 0) {
                    photo_viewer.update({ items: imgs });
                    time = 1;
                }
                photo_viewer.update({ index: index, visible: true });
            }
        }
        function displayAvatar(av) {
            if (av && av.length > 0) {
                time = 0;
                photo_viewer.update({ index: -1, items: [] });
                photo_viewer.update({ src: av, visible: true });
            }
        }                        
    </script>
