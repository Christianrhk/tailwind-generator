<script>

  jQuery('.currency-option').each(function(){
      jQuery(this).click(function(){
       
        jQuery.ajax('/change-currency/'+jQuery(this).data('value'),{
          
          success:function(data){
            var a='currency-'+jQuery('#site_currency').val();
            console.log(a);
            jQuery('body').removeClass(a).addClass('currency-'+data.currency);
            jQuery('#site_currency').val(data.currency);
            jQuery('#btnSiteCurrency').html(data.currency);
            jQuery('#btnSiteCurrency').html(data.currency);
            var c = data.currency;
            if (c == 'euro') c = 'eur';
            jQuery('input[type="radio"][value="'+c+'"').prop('checked',true);
          }
        });
      });
  });

</script>	
