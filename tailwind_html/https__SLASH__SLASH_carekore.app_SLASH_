    <script>

        document.querySelector('#select_prev_language').addEventListener('change', (e) => {
            localStorage.setItem('lang', e.target.value);
            changedLabelsDOM(e.target.value);
        });

        const changedLabelsDOM = (lang) => {
            switch (lang) {
                case 'es':
                    document.querySelector('#lang_join_carekore').innerHTML = 'Unirse Carekore';
                    document.querySelector('#lang_login').innerHTML = 'Iniciar sesion';
                    break;
                case 'en':
                    document.querySelector('#lang_join_carekore').innerHTML = 'Join Carekore';
                    document.querySelector('#lang_login').innerHTML = 'Login';
                    break;
            
                default:
                    break;
            }
        }

        document.querySelector('#select_prev_language').value = localStorage.getItem('lang') || 'en';
        changedLabelsDOM(localStorage.getItem('lang') || 'en');
    </script>
