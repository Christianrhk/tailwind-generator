<script>
    (function($) {
        jQuery(document).on('closing', '.modalVideos', function(e) {
            var $this = jQuery(this);
            //get iframe on click
            var vidsrc_frame = $this.find("iframe");
            var vidsrc_src = vidsrc_frame.attr("src");
            // Add autoplay 0 in iframe Youtube
            var searchParams = new URLSearchParams(vidsrc_src);
            searchParams.set('autoplay', '0');
            var newParams = searchParams.toString();
            $($this).attr('src', decodeURIComponent(newParams));
            // Silent Video close Video

            vidsrc_frame[0].contentWindow.postMessage('{"event":"command","func":"' + 'pauseVideo' + '","args":""}', '*');
        });

        jQuery(".modalVideos").iziModal({
            headerColor: '#f58025',
            width: 800,
            radius: 0,
            navigateArrows: true, // Boolean, 'closeToModal', 'closeScreenEdge'
            overlayColor: 'rgba(0,0,0, 0.8)',
        });
    })(jQuery);
</script>


