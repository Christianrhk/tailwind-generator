  <script>
    jQuery(document).ready(function() {
      const MenuToggle = document.getElementById('mobile-menu-toggle');
      const MobileMegamenu = document.getElementById('mobile-megamenu');
      const MobileNavMenu = document.getElementById('mobile-nav-menu');
      const MobileTopNav = document.getElementById('mobile-top-nav');
      const MobileBottomMenu = document.getElementById('mobile-bottom-menu');
      const MobileShowroomModal = document.getElementById('mobile-showroom-modal');

      jQuery(MenuToggle).click(function() {
        if (!MenuToggle.classList.contains('menu-is-open')) {
          MenuToggle.classList.add('menu-is-open');
          // MenuToggle.setAttribute('aria-expanded', true);
          // Header.classList.add("menu-is-open");
          MobileMegamenu.classList.add('menu-is-open');
          MobileTopNav.classList.add('menu-is-open');
          MobileBottomMenu.classList.add('menu-is-open');
          MobileShowroomModal.classList.add('menu-is-open');
          // console.log("menu opened");
        } else {
          MenuToggle.classList.remove('menu-is-open');
          // Header.classList.remove("menu-is-open");
          MobileMegamenu.classList.remove('menu-is-open');
          MobileTopNav.classList.remove('menu-is-open');
          MobileBottomMenu.classList.remove('menu-is-open');
          MobileShowroomModal.classList.remove('menu-is-open');
          // MenuToggle.setAttribute('aria-expanded', false);
          // console.log("menu closed");
        }
      });

      jQuery('.mobile-megamenu li').click(function() {
        const submenuSelector = '#is-mobile-' + jQuery(this).attr('id');
        // console.log(submenuSelector);

        // jQuery('.submenu').hide();
        jQuery(this).toggleClass('is-active');
        jQuery(submenuSelector).toggle();

        // jQuery(submenuSelector).mouseleave(function() {
        //   // console.log("mouseleave", submenuSelector);
        //   jQuery('.submenu').hide();
        // })
      });

      jQuery('.desktop-megamenu li').click(function() {
        const submenuSelector = '#is-desktop-' + jQuery(this).attr('id');
        // console.log(submenuSelector);
        
        jQuery('.desktop-megamenu li').removeClass('active-menu-li');
        jQuery(this).addClass('active-menu-li');

        // jQuery('.submenu').hide();
        // jQuery(submenuSelector).show();
        
        jQuery('.submenu').hide();
        jQuery(submenuSelector).toggle();

        jQuery(submenuSelector).mouseleave(function() {
          // console.log("mouseleave", submenuSelector);
          jQuery('.submenu').hide();
          jQuery('.desktop-megamenu li').removeClass('active-menu-li');
        })
      });
    });

  // const stateLocator = document.getElementById('test-location');
  const triggerCurrentLocationMobile = document.getElementById('trigger-current-location__mobile');
  const triggerCurrentLocation = document.getElementById('trigger-current-location');

  window.onload = function() {
    // console.log(localStorage.getItem(CurrentState));

    function getGeoData() {
      navigator.geolocation.getCurrentPosition(position => {
        // console.log(position);

        const latitude = position.coords.latitude;
        const longitude = position.coords.longitude;
        const geoAPI = `https://api.bigdatacloud.net/data/reverse-geocode-client?latitude=${latitude}&longitude=${longitude}&localityLanguage=en`;

        fetch(geoAPI)
        .then(res =>res.json())
        .then(data => {
          const foundState = data.principalSubdivision;
          // stateLocator.textContent = "State is now: " + foundState;

          localStorage.setItem('CurrentState', foundState);
          // console.log("local is now:", localStorage.getItem('CurrentState'));
          
          // once location is set, reload to show with new location based data
          location.reload();
          return false;
        });
      });
    };

    if (localStorage.getItem('CurrentState') !== null) {
        // console.log("State is already:", localStorage.getItem('CurrentState'));
        // stateLocator.textContent = "State is already: " + localStorage.getItem('CurrentState');
        triggerCurrentLocation.style.display = 'none';
        triggerCurrentLocationMobile.style.display = 'none';
    } else {
      // console.log(`state not found. let's find it`);
      getGeoData();
    }

  } // END window.onload = function()

    // https://developer.mozilla.org/en-US/docs/Web/API/MutationObserver
    function waitForElm(selector) {
      return new Promise(resolve => {
          if (document.querySelector(selector)) {
              return resolve(document.querySelector(selector));
          }

          const observer = new MutationObserver(mutations => {
              if (document.querySelector(selector)) {
                  resolve(document.querySelector(selector));
                  observer.disconnect();
              }
          });

          observer.observe(document.body, {
              childList: true,
              subtree: true
          });
      });
    }

    // Hide Siding Menu when user is in Arizona
    waitForElm('.submenu-desktop-Siding').then((el) => {
      // console.log("el is ready", el);

      if (localStorage.getItem('CurrentState') == 'Arizona') { 
        el.style.display = 'none';
      }
    });

    // .top-nav__phone-number
    waitForElm('.top-nav__phone-number').then((el) => {
      // console.log("el is ready", el);

      if (localStorage.getItem('CurrentState') == 'Arizona') 
      { 
        // el.style.backgroundColor = 'red';
        el.innerHTML = `<i class="icon ltw-phone"></i>480.550.9993`;
        el.setAttribute('href', 'tel:480.550.9993');

      }
      else if (localStorage.getItem('CurrentState') == 'Colorado') 
      {
        el.innerHTML = `<i class="icon ltw-phone"></i>303.536.8809`;
        el.setAttribute('href', 'tel:303.536.8809');
      }
      else if (localStorage.getItem('CurrentState') == 'Texas') 
      {
        el.innerHTML = `<i class="icon ltw-phone"></i>972.433.5339`;
        el.setAttribute('href', 'tel:972.433.5339');
      }
      else {
        const parentLi = el.closest('li');
        parentLi.remove();
      }
    });

    // .mobile-top-nav__phone
    waitForElm('.mobile-top-nav__phone').then((el) => {
      // console.log("el is ready", el);

      if (localStorage.getItem('CurrentState') == 'Arizona') 
      { 
        // el.style.backgroundColor = 'red';
        el.innerHTML = `<i class="icon ltw-phone"></i>480.550.9993`;
        el.setAttribute('href', 'tel:480.550.9993');

      }
      else if (localStorage.getItem('CurrentState') == 'Colorado') 
      {
        el.innerHTML = `<i class="icon ltw-phone"></i>303.536.8809`;
        el.setAttribute('href', 'tel:303.536.8809');
      }
      else if (localStorage.getItem('CurrentState') == 'Texas') 
      {
        el.innerHTML = `<i class="icon ltw-phone"></i>972.433.5339`;
        el.setAttribute('href', 'tel:972.433.5339');
      }
      else {
        const parentLi = el.closest('div');
        parentLi.remove();
      }
    });


    waitForElm('.simple-banner-text').then((el) => {
      // simple-banner-text
      // console.log("el is ready", el);

      if (
        localStorage.getItem('CurrentState') !== 'Arizona' &&
        localStorage.getItem('CurrentState') !== 'Colorado' &&
        localStorage.getItem('CurrentState') !== 'Texas' 
        ) {
          const bannerSpan = el.querySelector('span');
          // console.log("el is ready", bannerSpan);
          bannerSpan.innerHTML = 'Lifetime Windows provides siding and installation services in Colorado & Texas only.'
      }
    });


  function getState(zipString) {

    /* Ensure param is a string to prevent unpredictable parsing results */
    if (typeof zipString !== 'string') {
        // console.log('Must pass the zipcode as a string.');
        return;
    }

    /* Ensure we have exactly 5 characters to parse */
    if (zipString.length !== 5) {
        // console.log('Must pass a 5-digit zipcode.');
        return;
    }

    /* Ensure we don't parse strings starting with 0 as octal values */
    const zipcode = parseInt(zipString, 10);
    // console.log('zipcode', zipcode);

    let st;
    let state;

    /* Code cases alphabetized by state */
    if (zipcode >= 35000 && zipcode <= 36999) {
        st = 'AL';
        state = 'Alabama';
    } else if (zipcode >= 99500 && zipcode <= 99999) {
        st = 'AK';
        state = 'Alaska';
    } else if (zipcode >= 85000 && zipcode <= 86999) {
        st = 'AZ';
        state = 'Arizona';
    } else if (zipcode >= 71600 && zipcode <= 72999) {
        st = 'AR';
        state = 'Arkansas';
    } else if (zipcode >= 90000 && zipcode <= 96699) {
        st = 'CA';
        state = 'California';
    } else if (zipcode >= 80000 && zipcode <= 81999) {
        st = 'CO';
        state = 'Colorado';
    } else if ((zipcode >= 6000 && zipcode <= 6389) || (zipcode >= 6391 && zipcode <= 6999)) {
        st = 'CT';
        state = 'Connecticut';
    } else if (zipcode >= 19700 && zipcode <= 19999) {
        st = 'DE';
        state = 'Delaware';
    } else if (zipcode >= 32000 && zipcode <= 34999) {
        st = 'FL';
        state = 'Florida';
    } else if ( (zipcode >= 30000 && zipcode <= 31999) || (zipcode >= 39800 && zipcode <= 39999) ) {
        st = 'GA';
        state = 'Georgia';
    } else if (zipcode >= 96700 && zipcode <= 96999) {
        st = 'HI';
        state = 'Hawaii';
    } else if (zipcode >= 83200 && zipcode <= 83999) {
        st = 'ID';
        state = 'Idaho';
    } else if (zipcode >= 60000 && zipcode <= 62999) {
        st = 'IL';
        state = 'Illinois';
    } else if (zipcode >= 46000 && zipcode <= 47999) {
        st = 'IN';
        state = 'Indiana';
    } else if (zipcode >= 50000 && zipcode <= 52999) {
        st = 'IA';
        state = 'Iowa';
    } else if (zipcode >= 66000 && zipcode <= 67999) {
        st = 'KS';
        state = 'Kansas';
    } else if (zipcode >= 40000 && zipcode <= 42999) {
        st = 'KY';
        state = 'Kentucky';
    } else if (zipcode >= 70000 && zipcode <= 71599) {
        st = 'LA';
        state = 'Louisiana';
    } else if (zipcode >= 3900 && zipcode <= 4999) {
        st = 'ME';
        state = 'Maine';
    } else if (zipcode >= 20600 && zipcode <= 21999) {
        st = 'MD';
        state = 'Maryland';
    } else if ( (zipcode >= 1000 && zipcode <= 2799) || (zipcode == 5501) || (zipcode == 5544 ) ) {
        st = 'MA';
        state = 'Massachusetts';
    } else if (zipcode >= 48000 && zipcode <= 49999) {
        st = 'MI';
        state = 'Michigan';
    } else if (zipcode >= 55000 && zipcode <= 56899) {
        st = 'MN';
        state = 'Minnesota';
    } else if (zipcode >= 38600 && zipcode <= 39999) {
        st = 'MS';
        state = 'Mississippi';
    } else if (zipcode >= 63000 && zipcode <= 65999) {
        st = 'MO';
        state = 'Missouri';
    } else if (zipcode >= 59000 && zipcode <= 59999) {
        st = 'MT';
        state = 'Montana';
    } else if (zipcode >= 27000 && zipcode <= 28999) {
        st = 'NC';
        state = 'North Carolina';
    } else if (zipcode >= 58000 && zipcode <= 58999) {
        st = 'ND';
        state = 'North Dakota';
    } else if (zipcode >= 68000 && zipcode <= 69999) {
        st = 'NE';
        state = 'Nebraska';
    } else if (zipcode >= 88900 && zipcode <= 89999) {
        st = 'NV';
        state = 'Nevada';
    } else if (zipcode >= 3000 && zipcode <= 3899) {
        st = 'NH';
        state = 'New Hampshire';
    } else if (zipcode >= 7000 && zipcode <= 8999) {
        st = 'NJ';
        state = 'New Jersey';
    } else if (zipcode >= 87000 && zipcode <= 88499) {
        st = 'NM';
        state = 'New Mexico';
    } else if ( (zipcode >= 10000 && zipcode <= 14999) || (zipcode == 6390) || (zipcode == 501) || (zipcode == 544) ) {
        st = 'NY';
        state = 'New York';
    } else if (zipcode >= 43000 && zipcode <= 45999) {
        st = 'OH';
        state = 'Ohio';
    } else if ((zipcode >= 73000 && zipcode <= 73199) || (zipcode >= 73400 && zipcode <= 74999) ) {
        st = 'OK';
        state = 'Oklahoma';
    } else if (zipcode >= 97000 && zipcode <= 97999) {
        st = 'OR';
        state = 'Oregon';
    } else if (zipcode >= 15000 && zipcode <= 19699) {
        st = 'PA';
        state = 'Pennsylvania';
    } else if (zipcode >= 300 && zipcode <= 999) {
        st = 'PR';
        state = 'Puerto Rico';
    } else if (zipcode >= 2800 && zipcode <= 2999) {
        st = 'RI';
        state = 'Rhode Island';
    } else if (zipcode >= 29000 && zipcode <= 29999) {
        st = 'SC';
        state = 'South Carolina';
    } else if (zipcode >= 57000 && zipcode <= 57999) {
        st = 'SD';
        state = 'South Dakota';
    } else if (zipcode >= 37000 && zipcode <= 38599) {
        st = 'TN';
        state = 'Tennessee';
    } else if ( (zipcode >= 75000 && zipcode <= 79999) || (zipcode >= 73301 && zipcode <= 73399) ||  (zipcode >= 88500 && zipcode <= 88599) ) {
        st = 'TX';
        state = 'Texas';
    } else if (zipcode >= 84000 && zipcode <= 84999) {
        st = 'UT';
        state = 'Utah';
    } else if (zipcode >= 5000 && zipcode <= 5999) {
        st = 'VT';
        state = 'Vermont';
    } else if ( (zipcode >= 20100 && zipcode <= 20199) || (zipcode >= 22000 && zipcode <= 24699) || (zipcode == 20598) ) {
        st = 'VA';
        state = 'Virginia';
    } else if ( (zipcode >= 20000 && zipcode <= 20099) || (zipcode >= 20200 && zipcode <= 20599) || (zipcode >= 56900 && zipcode <= 56999) ) {
        st = 'DC';
        state = 'Washington DC';
    } else if (zipcode >= 98000 && zipcode <= 99499) {
        st = 'WA';
        state = 'Washington';
    } else if (zipcode >= 24700 && zipcode <= 26999) {
        st = 'WV';
        state = 'West Virginia';
    } else if (zipcode >= 53000 && zipcode <= 54999) {
        st = 'WI';
        state = 'Wisconsin';
    } else if (zipcode >= 82000 && zipcode <= 83199) {
        st = 'WY';
        state = 'Wyoming';
    } else {
        st = 'none';
        state = 'none';
        console.log('No state found matching', zipcode);
    }

    localStorage.setItem('CurrentState', state);
    // console.log("local is now:", localStorage.getItem('CurrentState'));
    return state;
  }


  // getState(zipString)
  const zipcodeUpdateForm = document.querySelector('form#location-modal-form');
  const zipcodeUpdateFormMobile = document.querySelector('form#location-modal-form__mobile');
  const zipcodeValue = document.getElementById('zipcodeInput');
  const zipcodeValueMobile = document.getElementById('zipcodeInputMobile');
  zipcodeUpdateForm.addEventListener('submit', e => {
    e.preventDefault();
    getState(zipcodeValue.value);
    location.reload();
    return false;
  });

  zipcodeUpdateFormMobile.addEventListener('submit', e => {
    e.preventDefault();
    getState(zipcodeValueMobile.value);
    location.reload();
    return false;
  });

  const findMyShowroomButton = document.getElementById('find-my-showroom');
  const findMyShowroomMobileButton = document.getElementById('find-my-showroom__mobile');
  const locationModalMobile = document.getElementById('location-modal__mobile');
  const locationModal = document.getElementById('location-modal');
  const closeLocationModal = document.getElementById('location-modal__close-modal');

  // open location modal by clicking "Find My Showroom"
  findMyShowroomMobileButton.addEventListener('click', e => {
    // console.log("click mobile", e);
    locationModalMobile.classList.toggle('hidden');
  });

  findMyShowroomButton.addEventListener('click', e => {
    // console.log("click", e);
    locationModal.style.display = 'block';
  });

  // open location modal
  closeLocationModal.addEventListener('click', e => {
    locationModal.style.display = 'none';
  });

  triggerCurrentLocationMobile.addEventListener('click', e => {
    // console.log("prompting Browser Location alert");
    getGeoData();
  });

  triggerCurrentLocation.addEventListener('click', e => {
    // console.log("prompting Browser Location alert");
    getGeoData();
  });



  // Location based header logo
  if (localStorage.getItem('CurrentState') == 'Arizona') 
  { 
    waitForElm('.main-logo--arizona').then((el) => {
      const logoALink = el.closest('a');
      logoALink.style.display = "inline-block";
    });
    waitForElm('.main-mobile-logo--arizona').then((el) => {
      const logoALink = el.closest('a');
      logoALink.style.display = "inline-block";
    });
  } else if (localStorage.getItem('CurrentState') == 'Colorado') 
  { 
    waitForElm('.main-logo--colorado').then((el) => {
      const logoALink = el.closest('a');
      logoALink.style.display = "inline-block";
    });
    waitForElm('.main-mobile-logo--colorado').then((el) => {
      const logoALink = el.closest('a');
      logoALink.style.display = "inline-block";
    });
  } else if (localStorage.getItem('CurrentState') == 'Texas') 
  { 
    waitForElm('.main-logo--texas').then((el) => {
      const logoALink = el.closest('a');
      logoALink.style.display = "inline-block";
    });
    waitForElm('.main-mobile-logo--texas').then((el) => {
      const logoALink = el.closest('a');
      logoALink.style.display = "inline-block";
    });
  } else {
    waitForElm('.main-logo--default').then((el) => {
      const logoALink = el.closest('a');
      logoALink.style.display = "inline-block";
    });
    waitForElm('.main-mobile-logo--default').then((el) => {
      const logoALink = el.closest('a');
      logoALink.style.display = "inline-block";
    });
  }

  if (localStorage.getItem('CurrentState') == null) {
    waitForElm('.main-logo--default').then((el) => {
      const logoALink = el.closest('a');
      logoALink.style.display = "inline-block";
    });
  }


  // Location based Header Showroom
  //<li><button class="find-my-showroom"><i class="icon ltw-pin"></i>Find My Showroom</button></li>
  waitForElm('.find-my-showroom__desktop').then((el) => {
    // console.log(".find-my-showroom is ready", el);
    if (localStorage.getItem('CurrentState') == 'Arizona') {
      el.innerHTML = `<i class="icon ltw-pin"></i><span class="sans-lb4-label">MY SHOWROOM: </span>Phoenix, AZ <span class="sans-b5-body">(Change)</span>`;
    } else if (localStorage.getItem('CurrentState') == 'Colorado') {
      el.innerHTML = `<i class="icon ltw-pin"></i><span class="sans-lb4-label">MY SHOWROOM: </span>Denver, CO <span class="sans-b5-body">(Change)</span>`;
    } else if (localStorage.getItem('CurrentState') == 'Texas') {
      el.innerHTML = `<i class="icon ltw-pin"></i><span class="sans-lb4-label">MY SHOWROOM: </span>Flower Mound, TX <span class="sans-b5-body">(Change)</span>`;
    }
  });

  waitForElm('.find-my-showroom__mobile').then((el) => {
    // console.log(".find-my-showroom is ready", el);
    if (localStorage.getItem('CurrentState') == 'Arizona') {
      el.innerHTML = `<i class="icon ltw-pin"></i><span class="sans-lb4-label">MY SHOWROOM: </span>Phoenix, AZ <span class="sans-b5-body">(Change)</span>`;
    } else if (localStorage.getItem('CurrentState') == 'Colorado') {
      el.innerHTML = `<i class="icon ltw-pin"></i><span class="sans-lb4-label">MY SHOWROOM: </span>Denver, CO <span class="sans-b5-body">(Change)</span>`;
