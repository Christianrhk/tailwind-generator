<script>
    toastr.options = {
  "debug": false,
  "positionClass": "toast-top-center",
  "preventDuplicates": true,
"preventOpenDuplicates": true
//  "onclick": null,
//  "fadeIn": 300,
//  "fadeOut": 1000,
//  "timeOut": 5000,
//  "extendedTimeOut": 1000
}

function BtnLoading(elem) {
        $(elem).attr("data-original-text", $(elem).html());
        var loadingtext = $(elem).attr("data-loading");
        $(elem).prop("disabled", true);
        $(elem).html(loadingtext);
    }

    function BtnReset(elem) {
        $(elem).prop("disabled", false);
        $(elem).html($(elem).attr("data-original-text"));
    }
</script>


<script type="text/javascript">
    $(function () {
      var isJobSeekerVerified = "";
      if(isJobSeekerVerified){
        $("#transferMsg").modal("show");
      }
    });
</script>

<script>
      $(function () {

        // remove alert after 2 second
        setTimeout(function(){
           $(".alert").hide();
        },15000);

      $("#chkforgotpassword").on('click',function(){
          if(this.checked) {
              $("#forgotPassword").show();
              $(".signinform").hide();
              $("#signintitle").text("Forgot your password?");
              $("#labelforgotpassword").text("Sign in");
          console.log("chkforgotpassword:CHECKED");
          }else{
              console.log("chkforgotpassword:NOT CHECKED");
               $("#forgotPassword").hide();
              $(".signinform").show();
              $("#signintitle").text("Signin");
              $("#labelforgotpassword").text("I forgot my password");
          }
      });

      $("#resetPassword").on('click',function(){
          $.post('https://videobio.me/c5service',
          {"mode": "resetpassword",'uEmail': $("#forgotuEmail").val(),'tid':""
          },
          function (response) {
              console.log("response",response);
              if(response.isuser=="yes"){
                $(".successtext").text("Password reset sent to your email.");
                $(".successAlert").show();
                $(".errorAlert").hide();

                  $( "#chkforgotpassword" ).prop( "checked", false );

                  $("#forgotPassword").hide();
                  $(".signinform").show();
                  $("#signintitle").text("Sign into your account");
                  $("#labelforgotpassword").text("I forgot my password");

              }else{
                $(".errortext").text("Email does not exist");
                $(".errorAlert").show();
                $(".successAlert").hide();
              }

          },'json');
      });
    });
</script>

<script src="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/toastr.min.js" ></script>
