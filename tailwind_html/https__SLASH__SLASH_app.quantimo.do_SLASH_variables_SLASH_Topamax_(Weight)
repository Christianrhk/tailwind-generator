        showLoader();
        window.location.href = p.linksTo[0].url
    } else if (p.linksFrom[0]) {
        showLoader();
        window.location.href = p.linksFrom[0].url
    } else {
        debugger
        console.error("Could not find url on event:", event);
    }
}
            },
            "visible": true
        }
    ],
    "subtitle": {
        "text": "These are the outcomes most likely influenced by Topamax based on our current data. The size is proportional to the change from baseline in the outcome following above average measurements for Topamax (Weight)."
    },
    "themeName": "white",
    "title": {
        "text": "Topamax Outcomes Network Graph",
        "enabled": true
    },
    "tooltip": {
        "nodeFormat": "{point.name}: <b>{point.sum}<\/b><br\/>",
        "formatter": function() {
            //debugger;
            var data = this.point.series.data;
            var i = this.point.index;
            var point = data[i - 1];
            if(!point){
                return this.point.name;
            }
            return point.tooltip;
        },
        "followPointer": false,
        "style": {
            "pointerEvents": "auto"
        },
        "useHtml": true,
        "hideDelay": 2000
    },
    "useHighStocks": false,
    "xAxis": null,
    "yAxis": {
        "title": {
            "enabled": true
        }
    }
});
});
        
</script>

</div>


