            var tooltips = series.options.tooltips;
            if(tooltips){
                var x = this.x || this.point.x
                var tooltip = tooltips[x] || null;
                if(tooltip){
                    //console.warn(this.point)
                    //debugger
                    return tooltip
                }
            }
            
            return this.y +' days where the <br/> average daily value is '+this.x+'mg';
        
        }
    },
    "useHighStocks": false,
