				onClick: {
					close: false
				},
            });

		var api = $('#mmenu').data('mmenu');

		var icon = document.getElementById("hamburgericon");
		var icon1 = document.getElementById("hamburgera");
		var icon2 = document.getElementById("hamburgerb");
		var icon3 = document.getElementById("hamburgerc");
																	
		api.bind('close:finish', function() {
		   //console.log('close');
		   //$( ".img-svg-burger" ).attr("src","image/catalog/svg2020/sc-burger-icon.svg");
			icon1.classList.remove('a');
			icon2.classList.remove('c');
			icon3.classList.remove('b');
		});

		api.bind('open:finish', function () {
			//$( ".img-svg-burger" ).attr("src","image/catalog/svg2020/sc-close.svg");
			icon1.classList.add('a');
			icon2.classList.add('c');
			icon3.classList.add('b');
		});
	});
</script>


