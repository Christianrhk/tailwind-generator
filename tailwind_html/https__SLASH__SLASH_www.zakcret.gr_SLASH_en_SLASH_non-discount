					],
					"bottom": [
						"<a href=\'https://www.facebook.com/pg/sneakercage\' target=\'_blank\'><img src=\"//zakcretcdn.r.worldssl.net/image/catalog/svg2020/sc-facebook.svg\" loading=\"lazy\" class=\"img-svg-mobile-menu\" title=\"facebook\" alt=\"facebook\"></a>",
						"<a href='https://www.instagram.com/zakcret_sports' target='_blank'><img src=\"//zakcretcdn.r.worldssl.net/image/catalog/svg2020/sc-instagram.svg\" loading=\"lazy\" class=\"img-svg-mobile-menu\" title=\"instagram\" alt=\"instagram\"></a>",
