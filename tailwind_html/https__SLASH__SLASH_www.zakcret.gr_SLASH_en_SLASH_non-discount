<script>
//show more descr
const ps = document.querySelectorAll('.categorydescription .descriptiontext');
const observer = new ResizeObserver(entries => {
	for (let entry of entries) {
		//console.log(entry.target.scrollHeight , entry.contentRect.height);
		entry.target.classList[entry.target.scrollHeight > entry.contentRect.height ? 'add' : 'remove']('truncated');
	}
});

ps.forEach(p => {
  observer.observe(p);
});

$(document).ready(function() {

	// MOBILE--filter on-off
	$('.mfilter-free-button').on( "click", handleFilters );
   
	$( "#overlay" ).click(function() {
		$( ".mfilter-free-button" ).trigger( "click" );
	});
   
   var overlaySrc = "url('catalog/view/theme/bootstrap4_basic/image/spin_arrows.gif')";
   
   function handleFilters(){

		if($("body > div").hasClass("mfilter-free-container-closed")){
			$("body").css("overflow", "hidden");
			$("body").css("width", "100%");
			$("body").css("position", "fixed");
			$("#overlay").css("display","block");
			$("#overlay").css("background-color","#484848");
			$("#overlay").css("background-image","none");
			$(".mfilter-widget-position-right .mfilter-free-button").css("display","block");
		   //console.log("filter now is open");
		}
		if($("body > div").hasClass("mfilter-free-container-opened")){
			$("body").css("overflow", "auto");
			$("body").css("position", "static");
			$("#overlay").css("display","none");
			$("#overlay").css("background-color","#ddd;");
			$("#overlay").css("background-image",overlaySrc);
			$(".mfilter-widget-position-right .mfilter-free-button").css("display","none");
		  // console.log("filter  now is closed");
	   }
			
   }
  
});
</script>
