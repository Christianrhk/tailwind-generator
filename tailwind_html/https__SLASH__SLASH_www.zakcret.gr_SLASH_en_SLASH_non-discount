<input type="hidden" id="iscroll-params" name="iscroll-params" value="{&quot;infScroll&quot;:{&quot;containerSelector&quot;:&quot;#mfilter-content-container div.row:last-of-type&quot;,&quot;paginatorSelector&quot;:&quot;.pagination&quot;,&quot;itemSelector&quot;:&quot;#mfilter-content-container .product-layout&quot;,&quot;minDistToBottom&quot;:&quot;100&quot;,&quot;loadingMsg&quot;:&quot;\u03a0\u03b1\u03c1\u03b1\u03ba\u03b1\u03bb\u03ce \u03c0\u03b5\u03c1\u03b9\u03bc\u03ad\u03bd\u03b5\u03c4\u03b5&quot;,&quot;finishMsg&quot;:&quot;\u03a4\u03ad\u03bb\u03bf\u03c2 \u03c3\u03b5\u03bb\u03af\u03b4\u03b1\u03c2&quot;,&quot;loadingMode&quot;:&quot;button&quot;,&quot;buttonLabel&quot;:&quot;Load More&quot;,&quot;statefulScroll&quot;:&quot;1&quot;,&quot;showSeparator&quot;:&quot;1&quot;,&quot;domObserverEnabled&quot;:&quot;1&quot;,&quot;enabled&quot;:&quot;1&quot;,&quot;noMoreLabel&quot;:&quot;No more pages to load&quot;,&quot;endofcontent&quot;:&quot;No more pages to load&quot;}}">
<script>
$(document).ready(function(){
	
	var totalProducts = $('#total_products').val();
	var ProductsPagination = $('#pagination_limit').val();
	var text_paggination_seen = $('#text_paggination_seen').val();
	var text_paggination_from = $('#text_paggination_from').val();
	var text_paggination_products = $('#text_paggination_products').val();	
	var ProductsSeen = ProductsPagination;
	
	//function for opencart to get pagination path
	var nextUrlSlugsold = $(".paginationmobile .page-item.number .pagination__next");
	var allUrlSlugs = $(".paginationmobile .page-item.number");
	var NonUrlSlugs = $(".paginationmobile .page-item").not('.number');
	var CurrentPage = $(".paginationmobile .page-item.number.active .page-link").html();
	
	if( CurrentPage > 2){
		var nextUrlSlugs = $(".paginationmobile .page-item.number .pagination__next").slice( CurrentPage - 2 );
	}else{ //cur page = 1
		var nextUrlSlugs = $(".paginationmobile .page-item.number .pagination__next");
	}
	
	if( CurrentPage > 1){
		ProductsSeen = (ProductsSeen * CurrentPage);
	}
	//fix for last page sum of products or first page with less products than paggination
	if( parseInt(ProductsSeen) > parseInt(totalProducts) ){
		ProductsSeen = totalProducts;
	}
	//console.log('ProductsSeen:', ProductsSeen, 'totalProducts:', totalProducts);
	
	var totalProductsText = text_paggination_seen+' '+ProductsSeen+' '+text_paggination_from+' '+totalProducts+' '+text_paggination_products+'.';
	//console.log('allUrlSlugs:', allUrlSlugs.length, 'nextUrlSlugs.length', nextUrlSlugs.length,'NonUrlSlugs:', NonUrlSlugs.length, 'CurrentPage:', CurrentPage );
	
	//create button
	$( 'div.row.iscroller-page-results' ).after( '<div class="page-load-status"><div class="loader"><div class="dot dot1"></div><div class="dot dot2"></div><div class="dot dot3"></div><div class="dot dot4"></div></div> <p class="infinite-scroll-last">End of content</p> <p class="infinite-scroll-error">No more pages to load</p></div><div class="text-center d-block d-md-none mb-3 totalproducts">'+totalProductsText+'</div><p class="text-center d-block d-md-none"><button class="view-more-button iscroller-btn btn btn-light">Load More <i class="fa fa-angle-right animateright p-1"></i></button></p>' );
	
	if (nextUrlSlugs.length == 0)
	{
		$('.view-more-button').hide();
		return;
	}

	function getPenPath() {
	  var slug = nextUrlSlugs[ this.loadCount ];
	  //console.log('iscroller', this);
	  if ( slug ) {
		return ''+ slug;
	  }
	}
	
	var $container = $('.iscroller-row').infiniteScroll({
	  // options
		//path: '.pagination__next',
		path: getPenPath,
		append: '.product-layout',
		history: 'replace',
		loadOnScroll: false,
		button: '.view-more-button',
		scrollThreshold: false,
		status: '.page-load-status',
		hideNav: '.iscroller-page-results',
	});

	$('.iscroller-row').on( 'append.infiniteScroll', function( event, body, path, items, response ) {
		//console.log('Appended ${'+items.length+'} items on ${'+path+'}');
		if (items.length < ProductsPagination )
		{
			$('.view-more-button').hide();
			$('.page-load-status').show();
			$('.infinite-scroll-last').show();
			$('.loader').hide();
		}
		
		//console.log('totals', ProductsSeen ,items.length);
		ProductsSeen = +ProductsSeen + +items.length;
		totalProductsText = text_paggination_seen+' '+ProductsSeen+' '+text_paggination_from+' '+totalProducts+' '+text_paggination_products+'.';
		$('.totalproducts').html( totalProductsText );

		// display available sizes
		$('[data-url]').click(function (event) {
			var e = $(this);
			e.off('click');

			// show this LOADING popover
			$.get(e.data('url'), function(d) {
				e.popover({
					content: d,
					html: true,
					delay: { "show": 500, "hide": 100 },
					placement: "top",
					container: 'body',
					trigger: 'click'
				}).popover('show');
			});
		});
	});
	
	var snapbackCache = SnapbackCache({
		bodySelector: "#mfilter-content-container",
		  finish: function () {
			//optional method of something that needs to finish on your page before caching the page
		},
		removeAutofocus: function () {
			//optional method to kill autofocusing which screws with scrolling the page 
		},
		refreshItems: function (dirtyThings) {
			//optional method to fetch fresh bits from your server you want to replace in the cache
		},  
		nextPageOffset: function () {
		   //optional method to fetch the current page your scrolled to. this is so you can track what page you should scroll next. see the page-cache:loaded event. 
		}
	});
	
});
</script>
</footer>
