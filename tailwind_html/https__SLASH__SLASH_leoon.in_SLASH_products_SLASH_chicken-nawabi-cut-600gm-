<script>

    $(document).ready(function(){

        get_cart();

        get_fav_data();

        getLocation();

    });

    $(document).on('click','.loda_data',function(){

        get_cart_details();

        get_cart();

        get_cart_final_data();

        get_fav_data();

    });

    $(document).on('click','.add_to_cart1',function(){

        var pro_id = $(this).attr('data-id');

        var product_id = $(this).attr('data-product');

        $.ajax({

            type: "GET",

            data: {pro_id:pro_id,product_id:product_id},

            url: 'https://leoon.in/home/get_items',

            success: function(response) {

                // console.log(response);

            var ab =[];

            var ab1 =[];

            var obj = jQuery.parseJSON(response);

            var j =0;

            for (var i =0; i<obj.length; i++) { j++;

                if (!(obj[i].pack)) {
            
             
                ab +="<div  class='clearfix' style='margin-top:10px;'><span class='float-start' style='width: 50%'>"+obj[i].unit+" "+obj[i].unit_in+"</span><span class='price'>&#8377; "+obj[i].current_price+"</span> <span class='discount'>&nbsp;<s>&#8377;"+obj[i].sell_price+"</s></span></div>";

                }else{

                    ab +="<div  class='clearfix' style='margin-top:10px;'><span class='float-start' style='width: 30%'>"+obj[i].unit+" "+obj[i].unit_in+"</span><span class='price'>&#8377; "+obj[i].current_price+"</span> <span class='discount'>&nbsp;<s>&#8377;"+obj[i].sell_price+"</s>&nbsp;"+obj[i].pack+"</span></div>";

                }

                if (obj[i].mes == 'Y') {

                ab1 +="<div class='a"+obj[i].id+"'><div class='clearfix' style='text-align:center;border: 1px solid #ddd;'><button class='btn btn-plus float-start btn-minus11' rounded-0' data-value="+pro_id+" data-info="+obj[i].id+" data-pro_id="+product_id+" style='padding: 0px 5px;outline: none;box-shadow: none;'><i class='far fa-minus text-danger'></i></button><b style='padding:0px 10px;' class="+obj[i].id+">"+obj[i].qty+"</b><button class='btn btn-plus btn-plus11 float-end rounded-0' data-value="+pro_id+"  data-info="+obj[i].id+" data-pro_id="+product_id+" style='padding: 0px 5px;outline: none;box-shadow: none;'><i class='far fa-plus text-success'></i></button></div></div>";

                  }else{

                    ab1 +="<div style='margin-bottom:7px;' class='a"+obj[i].id+"'><div class='form-check-input check_data' data-pro_id="+obj[i].product_id+" data-id="+obj[i].id+" data-value="+pro_id+" type='checkbox' name='gram' value="+obj[i].id+">Add</div></div>";

                  }

                  if (obj[i].sum == "0") {

                    abcd ="<button class='btn form-control add_to_cart1' data-bs-toggle='offcanvas' data-bs-target='#addToCart' style='background: #710302;color: #fff;' data-info="+obj[i].id+" data-product="+product_id+"   data-id="+pro_id+">Add To Cart</button>";

                  }else{

                    abcd ="<div style='border: 1px solid #ddd;' class='add_to_cart1' data-bs-toggle='offcanvas' data-bs-target='#addToCart'  data-id="+pro_id+" data-info="+obj[i].id+" data-product="+product_id+"><button class='btn btn-plus' rounded-0'  style='padding: 0px 5px;outline: none;box-shadow: none;'><i class='far fa-minus text-danger'></i></button><b style='padding:0px 10px;' >"+obj[i].sum+"</b><button class='btn btn-plus  rounded-0'  style='padding: 0px 5px;outline: none;box-shadow: none;'><i class='far fa-plus text-success'></i></button></div>";

                  }

                  abc ="<div><button type='button' class='btn addCart count_value cd"+product_id+"' data-bs-dismiss='offcanvas'>Add to Cart</button></div>";

            }

                $(".a").html(ab);

                $(".b").html(ab1);

                $(".c").html(abc);

            },

            error:function(){

                alert("Something went wrong.");

            }                    

        });

         

    });



</script>

<script>

     $(document).on('click','.add_to_cart',function(){

        var product_id = $(this).attr('data-id');

        var pro_id = $(this).attr('data-product');

        var info_id= $("."+product_id).val();

        if (!info_id) {

        var info_id = $(this).attr('data-info');

        }

        $.ajax({

            type: "GET",

            data: {id:pro_id,info_id:info_id},

            url: 'https://leoon.in/home/add_cart',

            success: function(response) {

            var obj = jQuery.parseJSON(response);

            var cur_price = (obj.cur_price);

            var sell_price =(obj.sel_price);

            var unit_in =(obj.unit_in);

            var unit =(obj.unit);

            get_cart();

            get_cart_final_data();

            get_cart_details();

            $(".t"+product_id).hide();

            	var va = "<button  class='btn btn-plus btn-minus1' data-id="+pro_id+" ><i class='far fa-minus text-danger'></i></button><b class="+pro_id+">1</b><button class='btn btn-plus btn-plus1' data-id="+pro_id+" ><i class='far fa-plus text-success'></i></button>";

                var va1 = "<span style='color: #d0181e;font-weight: 600;font-size: 18px;padding: 2px;' class="+product_id+" >&#8377; "+cur_price+"  </span><span><s>&#8377;"+sell_price+"</s></span></small>";

                var va2 = "<span style='color: #d0181e;font-weight: 600;font-size: 18px;padding: 2px;' class="+product_id+" >&#8377; "+cur_price+"  </span><span><s>&#8377;"+sell_price+"</s></span></small>";

           $(".am"+pro_id).html(va);

           $("#a"+product_id).html(va1);

           $("#kg"+product_id).html(unit+unit_in);

            },

            error:function(){

                alert("Something went wrong.");

            }                    

        });

    });

 

$(document).on('click','.check_data',function(){

        var jsonString = $(this).attr('data-id');

        var pro_id = $(this).attr('data-pro_id');

        var value_id = $(this).attr('data-value');

        var value_new = $(this).attr('data-new');

        if (!value_new) {

            var new_define = 0;

        }else{

            var new_define = value_new;

        }

        // alert(new_define);

        $.ajax({

            type: "POST",

            data: {data:jsonString,pro_id:pro_id},

            url: 'https://leoon.in/home/add_cart2',

            success: function(response) {

                 // console.log(response);
                 
                if (new_define == 0) {

                    ab1 ="<div class='clearfix' style='text-align:center;border: 1px solid #ddd;'><button class='btn btn-plus btn-minus11 float-start rounded-0' data-info="+jsonString+" data-pro_id="+pro_id+" data-value="+value_id+" style='padding: 0px 5px;outline: none;box-shadow: none;background: #eee;'><i class='far fa-minus text-danger'></i></button><b class="+jsonString+">1</b><button class='btn btn-plus float-end btn-plus11 rounded-0'  data-info="+jsonString+"  data-pro_id="+pro_id+" data-value="+value_id+" style='padding: 0px 5px;outline: none;box-shadow: none;background: #eee;'><i class='far fa-plus text-success'></i></button></div>";

                    ab2 ="<div class='clearfix' style='text-align:center;border: 1px solid #ddd;'><button class='btn btn-plus btn-minus11 float-start rounded-0' data-info="+jsonString+" data-value="+value_id+" data-pro_id="+pro_id+" style='padding: 0px 5px;outline: none;box-shadow: none;background: #eee;'><i class='far fa-minus text-danger'></i></button><b class="+jsonString+">1</b><button class='btn btn-plus btn-plus11 float-end rounded-0'  data-info="+jsonString+" data-pro_id="+pro_id+" data-value="+value_id+" style='padding: 0px 5px;outline: none;box-shadow: none;background: #eee;'><i class='far fa-plus text-success'></i></button></div>";

                    ab3 ="<div class='clearfix' style='text-align:center;border: 1px solid #ddd;' class='clickBtn"+value_id+"'><button class='btn btn-plus float-start rounded-0'  style='padding: 0px 5px;outline: none;box-shadow: none;background: #eee;'><i class='far fa-minus text-danger'></i></button><b style='padding: 0px 10px;' >"+response+"</b><button class='btn btn-plus float-end rounded-0'   style='padding: 0px 5px;outline: none;box-shadow: none;background: #eee;'><i class='far fa-plus text-success'></i></button></div>";

                    ab4 ="<div class='clearfix' style='text-align:center;border: 1px solid #ddd;' class='add_to_cart1' data-bs-toggle='offcanvas' data-bs-target='#addToCart' data-info="+jsonString+"  data-id="+value_id+" data-product="+pro_id+"><button class=''  style='padding: 0px 5px;outline: none;box-shadow: none;background: #eee;'></button><b style='padding: 0px 10px;font-size:12px;'>"+response+" ITEM ADDED</b><button class=''  style='padding: 0px 5px;outline: none;box-shadow: none;background: #eee;'></button></div>";

                    $(".a"+jsonString).html(ab1);

                    $(".a"+jsonString).html(ab2);

                    $(".am"+pro_id).html(ab3);

                    $(".amn"+pro_id).html(ab4);

                }else{

                     ab4 ="<div class='clearfix' style='text-align:center;border: 1px solid #ddd;'><button class='btn float-start btn-plus btn-minus11' rounded-0' data-info="+jsonString+" data-new='unit' data-pro_id="+pro_id+" data-value="+value_id+" style='padding: 0px 5px;outline: none;box-shadow: none;'><i class='far fa-minus text-danger'></i></button><b style='padding: 0px 3px;font-size:12px;' class="+jsonString+">1 Item</b><button class='btn btn-plus float-end btn-plus11 rounded-0' data-new='unit'  data-info="+jsonString+"  data-pro_id="+pro_id+" data-value="+value_id+" style='padding: 0px 5px;outline: none;box-shadow: none;'><i class='far fa-plus text-success'></i></button></div>";

                    $(".am"+pro_id).html(ab4);

                    $(".amn"+pro_id).html(ab4);

                }

                get_cart();

                get_cart_final_data();

                get_cart_details();

            },

            error:function(){

                alert("Something went wrong.");

            }                    

        });

})

 

 

    	function get_cart_details(){

        $.ajax({

          url:"https://leoon.in/home/get_cart_details",

          type:"get",

          success:function(data){

            // console.log(data);

            var obj = jQuery.parseJSON(data);

            opt1=[];

            opt2=[];

            opt=[];

            for (var i =0; i<obj.length; i++) {

            	if (obj[i].coupon_avail == 1) {

                    if (obj[i].applied == 0) {

            		opt ="<a>Coupon Available</a>";

            		opt3 ="<span style='font-weight: 500;font-size: 13px;color: darkgreen;' class='mx-1'>"+obj[i].coupon_amt+"."+obj[i].coupon_type+" OFF</span><span style='font-weight: 500;font-size: 13px;color: #710302;cursor: pointer;' class='' data-coupon="+obj[i].coupon_id+" data-order="+obj[i].pro_id+" data-amt="+obj[i].coupon_amt+">Apply</span>";

                    }else{

                        opt3 ="<span style='font-weight: 500;font-size: 13px;color: #710302;cursor: pointer;' class='' data-coupon="+obj[i].coupon_id+" data-order="+obj[i].order_id+" data-amt="+obj[i].coupon_amt+">Coupon Applied</span>";

                    }

            	}else{

            		opt ="";

            		opt3 ="";

            	}

            	opt1+="<div class='row mt-1 rounded-0'><div class='card'><div class='row g-0'><div class='col-2 col-md-2'><img src='https://leoon.in/assets/img/product_img/"+obj[i].thumbnail+"' class='img-fluid rounded-start' alt='...'></div><div class='col-10 col-md-10'><div class='card-body pt-1'><div style='float: right;' class='text-danger'><i class='fa fa-times remove_product' style='cursor:pointer;' data-info="+obj[i].info_id+" data-newid="+obj[i].id+" data-id="+obj[i].pro_id+"></i></div><h6 class='card-title'>"+obj[i].pro_name+"</h6><div class='incres-bt cart' ><button class='btn btn-plus btn-minus1' data-id="+obj[i].pro_id+" data-info="+obj[i].info_id+"><i class='far fa-minus text-danger'></i></button>&nbsp; "+obj[i].qty+"&nbsp; <a   class='btn btn-plus btn-plus1' data-value="+obj[i].id+" data-id="+obj[i].pro_id+" data-info="+obj[i].info_id+"><i class='far fa-plus text-success'></i></a></div><p class='card-text'><small class='text-muted'><span style='border: 1px solid #212529;color: #212529;padding: 2px;border-radius: 5px;'>"+obj[i].unit+" "+obj[i].unit_in+"</span><span style='color: #d0181e;font-weight: 600;font-size: 16px;padding: 2px;'>&#8377; "+obj[i].current_price+"</span><span><s>&#8377;"+obj[i].sell_price+"</s></span> <span style='color: #d0181e;font-weight: 600;font-size: 12px;padding: 2px;'> </span></small></p></div></div></div></div></div>";



            	opt2+="<div class='card mt-1 rounded-0'><div class='card-body'><div class='row g-0'><div class='col-2 col-md-2'><img src='https://leoon.in/assets/img/product_img/"+obj[i].thumbnail+"' class='img-fluid rounded-start'></div><div class='col-10 col-md-10'><div class='card-body p-0'><div class='text-danger cross'><i class='fa fa-times remove_product' style='cursor:pointer;' data-info="+obj[i].info_id+" data-id="+obj[i].pro_id+"></i></div><h6 class='card-title'>"+obj[i].pro_name+"</h6><div style='float: right;border: 1px solid #ccc;'><button class='btn btn-plus btn-minus1' data-id="+obj[i].pro_id+" data-info="+obj[i].info_id+"><i class='far fa-minus text-danger'></i></button>&nbsp; "+obj[i].qty+" &nbsp; <a  class='btn btn-plus btn-plus1' data-value="+obj[i].id+" data-id="+obj[i].pro_id+" data-info="+obj[i].info_id+"><i class='far fa-plus text-success'></i></a></div><div class='card-text'><small class='text-muted'><span style='border: 1px solid #212529;color: #212529;padding: 2px;border-radius: 5px;'>"+obj[i].unit+" "+obj[i].unit_in+"</span><span style='color: #d0181e;font-weight: 600;font-size: 16px;padding: 2px;'>&#8377; "+obj[i].current_price+"</span><span><s>&#8377;"+obj[i].sell_price+"</s></span> </small><div style='font-size: 11px;float: right;margin-right: 100px;'><i>"+opt+"</i><br/>"+opt3+"</div></div></div></div></div></div></div>";

            }

               $("#final_show").html(opt1);

               $(".final_show").html(opt2);

          }

        });

      }

     

      function get_cart_final_data(){

        $.ajax({

          url:"https://leoon.in/home/get_cart_final_data",

          type:"get",

          success:function(data){

            // console.log(data);

            var obj = jQuery.parseJSON(data);

               $(".subtotal").html(obj.sub_total);

               $(".delivery").html(obj.delivery);

               $(".coupon_value").html(obj.coupon_value);

               $(".final_amt").html(obj.final_amt);

               $(".old_final_amt").val(obj.final_amt);



               $(".subtotal1").val(obj.sub_total);

               $(".delivery1").val(obj.delivery);

               $(".coupon_value1").val(obj.coupon_value);

               $(".final_amt1").val(obj.final_amt);

          }

        });

      }



      function get_cart(){

        $.ajax({

          url:"https://leoon.in/home/get_cart",

          type:"get",

          success:function(data){

            if (data == 0) {

                var new_data = 0;   

                $("#close_notification").hide(0.1);

                $("#empty-cart-containers").show(0.1);

                $(".cart_data").html(new_data);

            }else{

                // alert("teste");

                var new_data = data;

              	// console.log(data);

                $(".cart_data").html(new_data);

                $("#empty-cart-containers").hide();

                $("#close_notification").show();

            }

          }

        });

      }



      function get_fav_data(){

        $.ajax({

          url:"https://leoon.in/home/get_fav_data",

          type:"get",

          success:function(data){

            if (!data) {

                var new_data = 0;   

            }else{

                var new_data = data;

            }

            // console.log(data);

           $(".cart_fav").html(new_data);

          }

        });

      }



	 $(document).on('click','.btn-plus1',function(){

        var pid = $(this).attr('data-id');

        var info_id = $(this).attr('data-info');

        var value_id = $(this).attr('data-value');

        // alert(pid);

      	$.ajax({

            type: "GET",

            data: {id:info_id,pro_id:pid},

            url: 'https://leoon.in/home/update_qty',

            success: function(response) {

            var obj = jQuery.parseJSON(response);

            	// console.log(response);

            $("."+info_id).html(obj.qty);

   			$("."+info_id).val(response);

            ab3 ="<p><div class='clearfix' style='border: 1px solid #ddd;' class='clickBtn"+value_id+"'><button class='btn btn-plus float-start' rounded-0'  style='padding: 0px 5px;outline: none;box-shadow: none;'><i class='far fa-minus text-danger'></i></button><b>"+obj.sum+"</b><button class='btn btn-plus float-end rounded-0'   style='padding: 0px 5px;outline: none;box-shadow: none;'><i class='far fa-plus text-success'></i></button></div></p>";

                $(".am"+pid).html(ab3);

            ab4 ="<div class='clearfix' style='border: 1px solid #ddd;' class='add_to_cart1' data-id="+value_id+" data-product="+pid+" data-bs-toggle='offcanvas' data-bs-target='#addToCart'><button class='btn btn-plus float-start' rounded-0'  style='padding: 0px 5px;outline: none;box-shadow: none;'><i class='far fa-minus text-danger'></i></button><b style='font-size:12px;'>"+obj.sum+" Item</b><button class='btn btn-plus float-end rounded-0'  style='padding: 0px 5px;outline: none;box-shadow: none;'><i class='far fa-plus text-success'></i></button></div>";

                $(".amn"+pid).html(ab4);

                get_cart();

                get_cart_final_data();

                get_cart_details();

                 

            },

            error:function(){

                alert("Something went wrong.");

            }                    

        });		

        		

       })

     $(document).on('click','.btn-plus11',function(){

        var pid = $(this).attr('data-info');

        var pro_id = $(this).attr('data-pro_id');

        var value_id = $(this).attr('data-value');

        var value_new = $(this).attr('data-new');

        if (!value_new) {

            var new_define = 0;

        }else{

            var new_define = value_new;

        }

        // alert(new_define);

        $.ajax({

            type: "GET",

            data: {id:pid,pro_id:pro_id},

            url: 'https://leoon.in/home/update_qty',

            success: function(response) {

            var obj = jQuery.parseJSON(response);

                get_cart();

                get_cart_final_data();

                get_cart_details();

                // console.log(obj.qty);

                if (new_define == 0) {

                $("."+pid).html(obj.qty);

                // $(".cd"+pro_id).html(obj.sum);

                ab3 ="<p><div class='clearfix' style='border: 1px solid #ddd;' class='clickBtn"+value_id+"'><button class='btn btn-plus float-start' rounded-0'  style='padding: 0px 5px;outline: none;box-shadow: none;'><i class='far fa-minus text-danger'></i></button><b style='padding: 0px 10px;'>"+obj.sum+"</b><button class='btn float-end btn-plus  rounded-0'   style='padding: 0px 5px;outline: none;box-shadow: none;'><i class='far fa-plus text-success'></i></button></div></p>";

                    $(".am"+pro_id).html(ab3);

                ab4 ="<div class='clearfix' style='border: 1px solid #ddd;' class='add_to_cart1' data-id="+value_id+" data-product="+pro_id+" data-bs-toggle='offcanvas' data-bs-target='#addToCart'><button class='btn float-start btn-plus' rounded-0'  style='padding: 0px 5px;outline: none;box-shadow: none;'><i class='far fa-minus text-danger'></i></button><b style='padding: 0px 10px;font-size:12px'>"+obj.sum+" Item</b><button class='btn btn-plus float-end rounded-0'  style='padding: 0px 5px;outline: none;box-shadow: none;'><i class='far fa-plus text-success'></i></button></div>";

                    $(".amn"+pro_id).html(ab4);

                 }else{

                    $("."+pid).html(obj.qty);

                 }

            },

            error:function(){

                alert("Something went wrong.");

            }                    

        });     

                

       })

	 $(document).on('click','.btn-minus1',function(){

      	var pid = $(this).attr('data-id');

        var info_id = $(this).attr('data-info');

        var value_id = $(this).attr('data-value');

        // alert(pid);

      	$.ajax({

            type: "GET",

            data: {id:info_id,pro_id:pid},

            url: 'https://leoon.in/home/update_qty1',

            success: function(response) {

            var obj = jQuery.parseJSON(response);

            	// console.log(response);

            if (obj.qty == "0") {

            ab ="<div class='form-check-input check_data' data-id="+info_id+" data-pro_id="+pid+"  type='checkbox' name='gram' value="+info_id+">Add</div>";

            $(".a"+info_id).html(ab);

            $("."+pid).html(obj.qty);

            get_cart_details();

            get_cart();

            get_cart_final_data();

            }else{

                 ab3 ="<div style='border: 1px solid #ddd;width:100%' class='clearfix clickBtn"+value_id+"'><button class='btn btn-plus float-start rounded-0'  style='padding: 0px 5px;outline: none;box-shadow: none;'><i class='far fa-minus text-danger'></i></button><b >"+obj.sum+"</b><button class='btn btn-plus float-end rounded-0'   style='padding: 0px 5px;outline: none;box-shadow: none;'><i class='far fa-plus text-success'></i></button></div>";

                $(".am"+pid).html(ab3);

            ab4 ="<div style=';border: 1px solid #ddd;width:100%' class='clearfix add_to_cart1' data-id="+value_id+" data-product="+pid+" data-bs-toggle='offcanvas' data-bs-target='#addToCart'><button class='btn btn-plus float-start rounded-0'  style='padding: 0px 5px;outline: none;box-shadow: none;'><i class='far fa-minus text-danger'></i></button><b>"+obj.sum+"</b><button class='btn btn-plus float-end rounded-0'  style='padding: 0px 5px;outline: none;box-shadow: none;'><i class='far fa-plus text-success'></i></button></div>";

                $(".amn"+pid).html(ab4);

                get_cart();

                get_cart_final_data();

                get_cart_details();

                $("."+info_id).html(obj.qty);

                $("."+pid).html(obj.qty);

            }

       			 

            },

            error:function(){

                // alert("Something went wrong.");

            }                    

        });

        		



       })

     $(document).on('click','.btn-minus11',function(){

        var pid = $(this).attr('data-info');

        var pro_id = $(this).attr('data-pro_id');

        var value_id = $(this).attr('data-value');

        var value_new = $(this).attr('data-new');

        if (!value_new) {

            var new_define = 0;

        }else{

            var new_define = value_new;

        }

        // alert(new_define);

        $.ajax({

            type: "GET",

            data: {id:pid,pro_id:pro_id},

            url: 'https://leoon.in/home/update_qty1',

            success: function(response) {

            var obj = jQuery.parseJSON(response);

                // console.log(response);

            if (new_define == 0) {

              if (obj.qty == "0") {

                ab ="<div class='form-check-input check_data' data-value="+value_id+" data-id="+pid+" data-pro_id="+pro_id+" type='checkbox' name='gram' value="+pid+">Add</div>";

                $(".a"+pid).html(ab);

                $(".amn"+pid).html(ab);

                if (obj.sum == "0") {

                abc ="<a style='background: #710302;color: white;' class='btn btn-add-cart clickBtn"+value_id+"' data-info="+pid+" data-product="+pro_id+"><b class='cd"+pro_id+"'>Add To Cart</b></a>";

                abc1 ="<button class='btn form-control add_to_cart1 rounded-0' data-bs-toggle='offcanvas' data-bs-target='#addToCart' style='background: #fff;color: #710302;border: 1px solid  #710302;' data-info="+pid+" data-product="+pro_id+"   data-id="+value_id+">Add To Cart</button>";

                $(".am"+pro_id).html(abc);

                $(".amn"+pro_id).html(abc1);

                }else{

                    ab3 ="<p><div style='border: 1px solid #ddd;' class='clickBtn"+value_id+"'><button class='btn btn-plus' rounded-0'  style='padding: 0px 5px;outline: none;box-shadow: none;background: #eee;'><i class='far fa-minus text-danger'></i></button>&nbsp;<b style='padding: 0px 10px;'>"+obj.sum+"</b> &nbsp;<button class='btn btn-plus  rounded-0'   style='padding: 0px 5px;outline: none;box-shadow: none;background: #eee;'><i class='far fa-plus text-success'></i></button></div></p>";

                    ab4 ="<div class='clearfix' style='border: 1px solid #ddd;' class='add_to_cart1' data-bs-toggle='offcanvas' data-bs-target='#addToCart'  data-id="+value_id+" data-product="+pro_id+"><button class='btn btn-plus float-start rounded-0'  style='padding: 0px 5px;outline: none;box-shadow: none;background: #eee;'><i class='far fa-minus text-danger'></i></button><b style='padding: 0px 10px;font-size:12px;'>"+obj.sum+" Item</b><button class='btn btn-plus float-end rounded-0'  style='padding: 0px 5px;outline: none;box-shadow: none;background: #eee;'><i class='far fa-plus text-success'></i></button></div>";

                    $(".am"+pro_id).html(ab3);

                    $(".amn"+pro_id).html(ab4);

                }

                get_cart_details();

                get_cart();

                get_cart_final_data();

            }else{

                get_cart();

                get_cart_final_data();

                get_cart_details();

                $("."+pid).html(obj.qty);

                // $(".cd"+pro_id).html(obj.sum);

                 ab3 ="<div style='border: 1px solid #ddd;' class='clickBtn"+value_id+"'><button class='btn btn-plus' rounded-0'  style='padding: 0px 5px;outline: none;box-shadow: none;'><i class='far fa-minus text-danger'></i></button><b style='padding: 0px 10px;'>"+obj.sum+"</b><button class='btn btn-plus  rounded-0'   style='padding: 0px 5px;outline: none;box-shadow: none;'><i class='far fa-plus text-success'></i></button></div>";

                 ab4 ="<div class='clearfix' style='border: 1px solid #ddd;' class='add_to_cart1' data-bs-toggle='offcanvas' data-bs-target='#addToCart'  data-id="+value_id+" data-product="+pro_id+"><button class='btn btn-plus float-start rounded-0'  style='padding: 0px 5px;outline: none;box-shadow: none;'><i class='far fa-minus text-danger'></i></button>&nbsp;<b style='padding: 0px 10px;font-size:12px;'>"+obj.sum+" Item</b> &nbsp;<button class='btn btn-plus float-end rounded-0'  style='padding: 0px 5px;outline: none;box-shadow: none;'><i class='far fa-plus text-success'></i></button></div>";

                    $(".am"+pro_id).html(ab3);

                    $(".amn"+pro_id).html(ab4);

            }

        }else{

                if (obj.qty == "0") {

                    abc ="<a style='background: #710302;color: white;font-weight: 700;font-size: 10px;' class='btn form-control check_data rounded-0' style='background: #fff;color: #710302;border: 1px solid  #710302;' data-new='unit' data-value="+value_id+" data-id="+pid+" data-pro_id="+pro_id+" value="+pid+"> Add To Cart</a>";

                    $(".am"+pro_id).html(abc);

                    $(".amn"+pro_id).html(abc);

                }else{

                    $("."+pid).html(obj.qty);

                }

        }

                // alert(response);

            },

            error:function(){

                alert("Something went wrong.");

            }                    

        });

   })

      

 

	 $(document).on('click','.remove_product',function(){

      var info_id = $(this).attr('data-info');

      $.ajax({

            type: "GET",

            data: {id:info_id},

            url: 'https://leoon.in/home/remove_product',

            success: function(response) {

                var obj = jQuery.parseJSON(response);

                // console.log(obj);

                for (var i =0; i<obj.length; i++) {

                        ab ="<div class='form-check-input check_data'  data-id="+obj[i].id+" type='checkbox' name='gram' value="+obj[i].id+">Add</div>";

                }

                $(".a"+info_id).html(ab);

        		get_cart();

            	get_cart_final_data();

            	get_cart_details();

       			 

            },

            error:function(){

                alert("Something went wrong.");

            }                    

        });

    });

     $(document).on('click','.remove_coupon',function(){

      var coupon_id = $(this).attr('data-id');

      // alert(coupon_id);

      $.ajax({

            type: "GET",

            data: {id:coupon_id},

            url: 'https://leoon.in/home/remove_coupon',

            success: function(response) {

                // console.log(response);

                var ab =[];

                var obj = jQuery.parseJSON(response);

                if (obj != null) {

                    for (var i =0; i<obj.length; i++) {

                        if (obj[i].message == "N") {

                            ab+="<div class='card' style='border: 1px dashed #bbb  ;border-radius: 0px;'><div class='card-body'><div class='mb-3'><span class='coupon-code mx-1 p-2'>"+obj[i].coupon_code+"</span><span class='coupon-text mx-1 p-2'>Get "+obj[i].new_amt+"  off for you</span></div><div class='mb-3'><small class='mx-1'>"+obj[i].new_amt+" Off upto Rs.80 on orders above Rs.200</small></div><div><div class='float-start'><strong class='mx-1'>T & Cs</strong></div><div class='float-end '><div class='mx-1 apply-btn apply_coupon'  data-coupon="+obj[i].coupon_id+"  data-coupon_type="+obj[i].coupon_type+"  data-amt="+obj[i].coupon_amt+"><strong>Apply</strong></div></div></div></div></div><br>";

                        }else{

                            ab+="<div class='card' style='border: 1px dashed #bbb  ;border-radius: 0px;'><div class='card-body'><div class='mb-3'><span class='coupon-code mx-1 p-2'>"+obj[i].coupon_code+"</span><span class='coupon-text mx-1 p-2'>Get "+obj[i].new_amt+"  off for you</span><span class='float-end'><i style='color: red;'class='fa fa-times remove_coupon' style='cursor:pointer;' data-id="+obj[i].coupon_id+"></i></span></div><div class='mb-3'><small class='mx-1'>"+obj[i].new_amt+" Off upto Rs.80 on orders above Rs.200</small></div><div><div class='float-start'><strong class='mx-1'>T & Cs</strong></div><div class='float-end '><div class='mx-1 apply-btn '  data-coupon="+obj[i].coupon_id+"  data-coupon_type="+obj[i].coupon_type+"  data-amt="+obj[i].coupon_amt+"><strong style='color: green;'>Applied</strong></div></div></div></div></div><br>";

                        }

                    }

                $(".coupon_data").html(ab);

                }else{

                    Swal.fire({

                      position: 'top-center',

                      icon: 'error',

                      title: 'First add product',

                      showConfirmButton: false,

                      timer: 1500

                    })

                }

                 

            },

            error:function(){

                alert("Something went wrong.");

            }                    

        });

    });

 $(document).on('click','.remove_favorite',function(){

      var product_id = $(this).attr('data-id');

      $.ajax({

            type: "GET",

            data: {id:product_id},

            url: 'https://leoon.in/home/remove_fav_product',

            success: function(response) {

                get_fav_data();

                location.reload();

            },

            error:function(){

                alert("Something went wrong.");

            }                    

        });

    });



  $(document).on('click','.apply_coupon',function(){

      var coupon_id = $(this).attr('data-coupon');

      var data_amt = $(this).attr('data-amt');

      var coupon_type = $(this).attr('data-coupon_type');

      $.ajax({

            type: "GET",

            data: {coupon_id:coupon_id,coupon_type:coupon_type,data_amt:data_amt},

            url: 'https://leoon.in/home/apply_coupon',

            success: function(response) {

                var ab =[];

                var obj = jQuery.parseJSON(response);

                if (obj != null) {

                    for (var i =0; i<obj.length; i++) {

                        if (obj[i].message == "N") {

                            ab+="<div class='card' style='border: 1px dashed #bbb  ;border-radius: 0px;'><div class='card-body'><div class='mb-3'><span class='coupon-code mx-1 p-2'>"+obj[i].coupon_code+"</span><span class='coupon-text mx-1 p-2'>Get "+obj[i].new_amt+"  off for you</span></div><div class='mb-3'><small class='mx-1'>"+obj[i].new_amt+" Off upto Rs.80 on orders above Rs.200</small></div><div><div class='float-start'><strong class='mx-1'>T & Cs</strong></div><div class='float-end '><div class='mx-1 apply-btn apply_coupon'  data-coupon="+obj[i].coupon_id+"  data-coupon_type="+obj[i].coupon_type+"  data-amt="+obj[i].coupon_amt+"><strong>Apply</strong></div></div></div></div></div><br>";

                        }else{

                            ab+="<div class='card' style='border: 1px dashed #bbb  ;border-radius: 0px;'><div class='card-body'><div class='mb-3'><span class='coupon-code mx-1 p-2'>"+obj[i].coupon_code+"</span><span class='coupon-text mx-1 p-2'>Get "+obj[i].new_amt+"  off for you</span><span class='float-end'><i style='color: red;'class='fa fa-times remove_coupon' style='cursor:pointer;' data-id="+obj[i].coupon_id+"></i></span></div><div class='mb-3'><small class='mx-1'>"+obj[i].new_amt+" Off upto Rs.80 on orders above Rs.200</small></div><div><div class='float-start'><strong class='mx-1'>T & Cs</strong></div><div class='float-end '><div class='mx-1 apply-btn '  data-coupon="+obj[i].coupon_id+"  data-coupon_type="+obj[i].coupon_type+"  data-amt="+obj[i].coupon_amt+"><strong style='color: green;'>Applied</strong></div></div></div></div></div><br>";

                        }

                    }

                $(".coupon_data").html(ab);

                }else{

                    Swal.fire({

                      position: 'top-center',

                      icon: 'error',

                      title: 'First add product',

                      showConfirmButton: false,

                      timer: 1500

                    })

                }

            },

            error:function(){

                alert("Something went wrong.");

            }                    

        });

        		get_cart();



    });

$(document).on('click','.apply_coupon_code',function(){

      var coupon_code = $(".coupon_code").val();

      $.ajax({

            type: "GET",

            data: {coupon_code:coupon_code},

            url: 'https://leoon.in/home/check_coupon',

            success: function(response) {

                // console.log(response);

                var ab =[];

                var obj = jQuery.parseJSON(response);

                if (obj != null) {

                    Swal.fire({

                        position: 'top-center',

                        icon: 'success',

                        title: 'Applied Successfully.',

                        showConfirmButton: false,

                        timer: 1500

                      })

                    for (var i =0; i<obj.length; i++) {

                        if (obj[i].message == "N") {

                            ab+="<div class='card' style='border: 1px dashed #bbb  ;border-radius: 0px;'><div class='card-body'><div class='mb-3'><span class='coupon-code mx-1 p-2'>"+obj[i].coupon_code+"</span><span class='coupon-text mx-1 p-2'>Get "+obj[i].new_amt+"  OFF on your Order. </span></div><div class='mb-3'><small class='mx-1'>"+obj[i].new_amt+" </small></div><div><div class='float-start'><strong class='mx-1'>T & Cs</strong></div><div class='float-end '><div class='mx-1 apply-btn apply_coupon'  data-coupon="+obj[i].coupon_id+"  data-coupon_type="+obj[i].coupon_type+"  data-amt="+obj[i].coupon_amt+"><strong>Apply</strong></div></div></div></div></div><br>";

                        }else{

                            ab+="<div class='card' style='border: 1px dashed #bbb  ;border-radius: 0px;'><div class='card-body'><div class='mb-3'><span class='coupon-code mx-1 p-2'>"+obj[i].coupon_code+"</span><span class='coupon-text mx-1 p-2'>Get "+obj[i].new_amt+"  OFF on your Order. </span><span class='float-end'><i style='color: red;'class='fa fa-times remove_coupon' style='cursor:pointer;' data-id="+obj[i].coupon_id+"></i></span></div><div class='mb-3'><small class='mx-1'>"+obj[i].new_amt+"</small></div><div><div class='float-start'><strong class='mx-1'>T & Cs</strong></div><div class='float-end '><div class='mx-1 apply-btn '  data-coupon="+obj[i].coupon_id+"  data-coupon_type="+obj[i].coupon_type+"  data-amt="+obj[i].coupon_amt+"><strong style='color: green;'>Applied</strong></div></div></div></div></div><br>";

                        }

                    }

                $(".coupon_data").html(ab);

                }else{

                  Swal.fire({

                    position: 'top-center',

                    icon: 'error',

                    title: 'Not Available Coupon.',

                    showConfirmButton: false,

                    timer: 1500

                  })



                }

                 

            },

            error:function(){

                alert("Something went wrong.");

            }                    

        });

    });

    $(document).on('click','.gram',function(){

      var current_price = $(this).attr('data-id');

      var info_id = $(this).attr('data-info_id');

      var product_id = $(this).attr('data-product');

      var sell_price = $(this).attr('data-sell');

      var serve = $(this).attr('data-serve');

      var piece = $(this).attr('data-piece');

        $("."+product_id).html('<i class="far fa-rupee-sign"></i>'+' '+current_price);

        $(".an"+product_id).html('M.R.P: <s>&#8377;'+' '+sell_price+'</s>');

        $(".al"+product_id).html('<small>No of Pieces: '+piece+'</small>');

        $(".alu"+product_id).html('<small>Serves:'+piece+'</small>');

        $("."+product_id).val(info_id);

    });

    $(document).on('click','.gram1',function(){

