  $(document).on('click','.remove_delivery',function(){

      var address_id = $(this).attr('data-address');

      $.ajax({

            type: "GET",

            data: {id:address_id},

            url: 'https://leoon.in/home/remove_address',

            success: function(response) {

                if (response == "success") {

                  // Swal.fire({

                  //   position: 'top-center',

                  //   icon: 'success',

                  //   title: 'Removed Successfully.',

                  //   showConfirmButton: false,

                  //   timer: 1500

                  // })

                  setTimeout(() => {

                            location.reload();

                        }, 2000);

                }else{

                  Swal.fire({

                    position: 'top-center',

                    icon: 'error',

                    title: 'Failed.',

                    showConfirmButton: false,

                    timer: 1500

                  })



                }

                 

            },

            error:function(){

                alert("Something went wrong.");

            }                    

        });

                get_cart();



    });

 

</script>

 

<script>

$(document).ready(function(){

      $("#example-search-input").on("keyup", function() {

        var pro_name = $(this).val();

        $.ajax({

            type: "GET",

            data: {pro_name:pro_name},

            url: 'https://leoon.in/home/get_product_name',

            success: function(response) {

                var c =[];

                var obj = jQuery.parseJSON(response);

               for (var i =0; i<obj.length; i++) {

                 c+="<li><a class='dropdown-item' href='https://leoon.in/product_details/"+obj[i].product_slug+"'>"+obj[i].product_name+"</a></li>";

               }

                $(".product_name11").html(c);

                // console.log(response);

               

            },

            error:function(){

                alert("Something went wrong.");

            }                    

        });

    })

})





$(document).ready(function(){

      $("#lap-search-data").on("keyup", function() {

        var city = $(this).val();

        // alert(pro_name);

        $.ajax({

            type: "GET",

            data: {city:city},

            url: 'https://leoon.in/home/get_city',

            success: function(response) {

                var c =[];

                var obj = jQuery.parseJSON(response);

               for (var i =0; i<obj.length; i++) {

                 c+="<a class='dropdown-item' href='' onclick='document.cookie =\"city1="+obj[i].city+" \"' > "+obj[i].city+" </a>";

               }

                $(".get_city").html(c);

                // console.log(response);

               

            },

            error:function(){

                alert("Something went wrong.");

            }                    

        });

    })

})



$(document).ready(function(){

      $("#search-loc").on("keyup", function() {

        var city = $(this).val();

        // alert(pro_name);

        $.ajax({

            type: "GET",

            data: {city:city},

            url: 'https://leoon.in/home/get_city',

            success: function(response) {

                var c =[];

                var obj = jQuery.parseJSON(response);

               for (var i =0; i<obj.length; i++) {

                 c+="<a class='dropdown-item' href='' onclick='document.cookie =\"city1="+obj[i].city+" \"' > "+obj[i].city+" </a>";

               }

                $(".get_city").html(c);

                // console.log(response);

               

            },

            error:function(){

                alert("Something went wrong.");

            }                    

        });

    })

})

  function data(e){

  $.ajax({

        type: "GET",

        data: {product_id:e.id},

        url: 'https://leoon.in/home/add_favorite',

        success: function(response) {

            get_fav_data();

            var obj = jQuery.parseJSON(response);

             if (obj.message == "success") {

                 c ="<i class='fa fa-heart' style='color: red;'></i>";

             }else{

                 c ="<i class='fa fa-heart' style='color: red;'></i>";

             }

            $("#"+obj.pro_id+"").html(c);

        },

        error:function(){

            alert("Something went wrong.");

        }                    

    });

}

</script>



 <script>

    $(document).ready(function() {

        $('#btn-submit').click(function() {

            var data = new FormData(this.form);

            $.ajax({

                type: "POST",

                data: data,

                processData: false,

                contentType: false,

                url: "https://leoon.in/Code/subscribe",

                success: function(response) {

                  // alert(response);

                  if (response == "success") {

                  Swal.fire({

                    position: 'top-center',

                    icon: 'success',

                    title: 'Subscribed Successfully.',

                    showConfirmButton: false,

                    timer: 1500

                  })

                 }else if (response == "already") {

                  Swal.fire({

                    position: 'top-center',

                    icon: 'error',

                    title: 'Already Subscribed.',

                    showConfirmButton: false,

                    timer: 1500

                  })

                 }else if (response == "not_mail") {

                  Swal.fire({

                    position: 'top-center',

                    icon: 'error',

                    title: 'Please Fill right email.',

                    showConfirmButton: false,

                    timer: 1500

                  })

                 }else{

                  Swal.fire({

                    position: 'top-center',

                    icon: 'error',

                    title: 'All Field Required.',

                    showConfirmButton: false,

                    timer: 1500

                  })



                }

                     

                },

                error: function() {

                    alert('Something went wrong.');

                }

            })

        })

    })

</script>

