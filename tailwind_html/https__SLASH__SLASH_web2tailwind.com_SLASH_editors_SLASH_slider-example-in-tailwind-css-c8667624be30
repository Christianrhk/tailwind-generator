            <iframe id="iframe" class="w-full" style="min-height: 14rem;" srcdoc="&lt;!DOCTYPE html&gt;
&lt;html lang=&quot;en&quot;&gt;

&lt;head&gt;
    &lt;meta charset=&quot;UTF-8&quot;&gt;
    &lt;meta name=&quot;viewport&quot; content=&quot;width=device-width, initial-scale=1.0&quot;&gt;
    &lt;meta http-equiv=&quot;X-UA-Compatible&quot; content=&quot;ie=edge&quot;&gt;
    &lt;title&gt;Document&lt;/title&gt;
	&lt;link rel=&quot;stylesheet&quot; href=&quot;https://unpkg.com/swiper/css/swiper.css&quot;&gt;
	&lt;link rel=&quot;stylesheet&quot; href=&quot;https://unpkg.com/swiper/css/swiper.min.css&quot;&gt;

	&lt;script src=&quot;https://unpkg.com/swiper/js/swiper.js&quot;&gt;&lt;/script&gt;
	&lt;script src=&quot;https://unpkg.com/swiper/js/swiper.min.js&quot;&gt;&lt;/script&gt;
    &lt;link href=&quot;https://unpkg.com/tailwindcss@^1.0/dist/tailwind.min.css&quot; rel=&quot;stylesheet&quot;&gt;
    &lt;script src=&quot;https://cdn.jsdelivr.net/gh/alpinejs/alpine@v2.x.x/dist/alpine.min.js&quot; defer&gt;&lt;/script&gt;
    &lt;link href=&quot;https://fonts.googleapis.com/css?family=Source+Code+Pro|Roboto&amp;display=swap&quot; rel=&quot;stylesheet&quot;&gt;
&lt;/head&gt;

&lt;body style=&quot;font-family:Roboto&quot;&gt;
    &lt;div class=&quot;flex justify-center items-center h-screen md:-mx-4&quot;&gt;
        &lt;div class=&quot;w-full bg-gray-100&quot;&gt;
            &lt;div class=&quot;swiper-container w-100 h-64&quot;&gt;
    &lt;!-- Additional required wrapper --&gt;
    &lt;div class=&quot;swiper-wrapper&quot;&gt;
        &lt;!-- Slides --&gt;
        &lt;div class=&quot;swiper-slide&quot;&gt;
          &lt;img src=&quot;https://source.unsplash.com/weekly?water&quot;&gt;
        &lt;/div&gt;
        &lt;div class=&quot;swiper-slide&quot;&gt;
          &lt;img src=&quot;https://source.unsplash.com/weekly?mountain&quot;&gt;
        &lt;/div&gt;
    &lt;/div&gt;
    &lt;!-- If we need navigation buttons --&gt;
    &lt;div class=&quot;swiper-button-prev&quot;&gt;&lt;/div&gt;
    &lt;div class=&quot;swiper-button-next&quot;&gt;&lt;/div&gt;
    &lt;!-- If we need pagination --&gt;
    &lt;div class=&quot;swiper-pagination&quot;&gt;&lt;/div&gt;   
&lt;/div
        &lt;/div&gt;
    &lt;/div&gt;
&lt;script&gt;
  var mySwiper = new Swiper (&#039;.swiper-container&#039;, {
    // Optional parameters
    direction: &#039;horizontal&#039;,
    loop: true,

    // If we need pagination
    pagination: {
      el: &#039;.swiper-pagination&#039;,
    },

    // Navigation arrows
    navigation: {
      nextEl: &#039;.swiper-button-next&#039;,
      prevEl: &#039;.swiper-button-prev&#039;,
    },

    // And if we need scrollbar
    scrollbar: {
      el: &#039;.swiper-scrollbar&#039;,
    },
  })
  &lt;/script&gt;
&lt;/body&gt;

&lt;/html&gt;"></iframe>
