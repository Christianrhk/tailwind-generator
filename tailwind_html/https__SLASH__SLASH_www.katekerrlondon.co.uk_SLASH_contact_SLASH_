<script>

// Mobile menu
const mobileBurgerButton = document.getElementById( 'mobile-burger' );
const closeMobileMenu = document.getElementById( 'close-mobile-menu' );
const mobileMenu = document.getElementById( 'mobile-menu' );

// open mobile menu
mobileBurgerButton.addEventListener( 'click', function() {
    mobileMenu.classList.toggle( 'hidden' );

    if ( mobileBurgerButton.getAttribute( 'aria-expanded' ) === 'true' ) {
        mobileBurgerButton.setAttribute( 'aria-expanded', 'false' );
    } else {
        mobileBurgerButton.setAttribute( 'aria-expanded', 'true' );
    }
})

// close mobile menu
closeMobileMenu.addEventListener( 'click', function() {
    mobileMenu.classList.toggle( 'hidden' );

    if ( mobileBurgerButton.getAttribute( 'aria-expanded' ) === 'true' ) {
        mobileBurgerButton.setAttribute( 'aria-expanded', 'false' );
    } else {
        mobileBurgerButton.setAttribute( 'aria-expanded', 'true' );
    }
})



// Sub menu
const subMenuButton = document.getElementById( 'sub-menu-button' );
const subMenu = document.getElementById( 'sub-menu-1' );

subMenuButton.addEventListener( 'click', function() {
    subMenu.classList.toggle( 'hidden' );

    if ( subMenuButton.getAttribute( 'aria-expanded' ) === 'true' ) {
        subMenuButton.setAttribute( 'aria-expanded', 'false' );
    } else {
        subMenuButton.setAttribute( 'aria-expanded', 'true' );
    }
})


const subMenuButton2 = document.getElementById( 'sub-menu-button-2' );
const subMenu2 = document.getElementById( 'sub-menu-2' );

subMenuButton2.addEventListener( 'click', function() {
    subMenu2.classList.toggle( 'hidden' );

    if ( subMenuButton2.getAttribute( 'aria-expanded' ) === 'true' ) {
        subMenuButton2.setAttribute( 'aria-expanded', 'false' );
    } else {
        subMenuButton2.setAttribute( 'aria-expanded', 'true' );
    }
})

</script>
