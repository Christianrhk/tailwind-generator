<script>
var checkButton = document.getElementById('check-button');
var checkButtonSpan = document.getElementById('check-button-span');

function toggleCheck() {

    if(checkButton.getAttribute('aria-checked') == 'false') {
        checkButton.classList.add("bg-green-pale");
        checkButton.classList.remove("bg-gray-200");
        checkButtonSpan.classList.add("translate-x-5");
        checkButtonSpan.classList.remove("translate-x-0");
        checkButton.setAttribute('aria-checked', 'true');
    } else {
        checkButton.classList.remove("bg-green-pale");
        checkButton.classList.add("bg-gray-200");
        checkButtonSpan.classList.remove("translate-x-5");
        checkButtonSpan.classList.add("translate-x-0");
        checkButton.setAttribute('aria-checked', 'false');
    }
}


function checkForm() {

  if(checkButton.getAttribute('aria-checked') !== 'true') {
    alert("Please indicate that you accept the privacy policy");
    return false;
  }
  return true;
}


</script>
