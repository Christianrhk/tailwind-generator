<script>

  function loadZowie() {
    $('#zowie-facade g').remove();
    $('#typing-wave').removeClass('hidden');
    (function (d, s, id, cb) {
      var js = d.createElement(s);
      var n = d.createElement("div");
      n.id = id;
      if (js.readyState) {
        js.onreadystatechange = function () { if (js.readyState === "loaded" || js.readyState === "complete") { js.onreadystatechange = null; cb(); } };
      } else { js.onload = function () { cb(); }; }
      js.src = "https://live-chat.chatbotize.com/chatbotize-entrypoint.min.js";
      document.body.appendChild(js);
      document.body.appendChild(n);
    })(document, "script", "chatbotize", function () {
      Chatbotize.init({
        instanceId: "265cc3ff3816493290879fef2fb5b1de",
        startOnOpen: true,
        headerMode: "white",
        onShow: function() {
          setTimeout(function () {
            $('#typing-wave').remove();
            $('#zowie-facade').remove();
          }, 1400)
        },
      });
      setTimeout(function () {
        Chatbotize.open();
      }, 2000);
    });
  }

  function activateZowie() {
    if (sessionStorage.getItem('herochat-status') !== null) {
      (function (d, s, id, cb) {
        var js = d.createElement(s);
        var n = d.createElement("div");
        n.id = id;
        if (js.readyState) {
          js.onreadystatechange = function () { if (js.readyState === "loaded" || js.readyState === "complete") { js.onreadystatechange = null; cb(); } };
        } else {
          js.onload = function () { cb(); };
        }
        js.src = "https://live-chat.chatbotize.com/chatbotize-entrypoint.min.js";
        document.body.appendChild(js);
        document.body.appendChild(n);
      })(document, "script", "chatbotize", function () {
        Chatbotize.init({
          instanceId: "265cc3ff3816493290879fef2fb5b1de",
          startOnOpen: true,
          headerMode: "white",
          ctaText: '',
          ctaHeader: ''
        });
      });
    } else {
      $('#zowie-facade').removeClass('hidden');
    }
  }

  $(function () {
    
    activateZowie();

    $('.activateZowie').click(function(){
      if(typeof Chatbotize === 'object'){
        Chatbotize.open();
      }else{
        loadZowie();
      }
    });

  });
</script>

<script src="//cdn.shopify.com/shopifycloud/shopify/assets/themes_support/option_selection-fe6b72c2bbdd3369ac0bfefe8648e3c889efca213baefd4cfb0dd9363563831f.js" type="text/javascript"></script>

<script src="https://cdnjs.cloudflare.com/ajax/libs/shopify-cartjs/1.1.0/rivets-cart.min.js" type="text/javascript"></script>
<script src="//cdn.shopify.com/s/files/1/0270/0636/9827/t/16/assets/slideout.min.js?v=124021314526455746731658250802" type="text/javascript"></script>

<script>

  var slideout = new Slideout({
    'panel': document.getElementById('panel'),
    'menu': document.getElementById('mobile-menu'),
    'padding': 325,
  });

  document.querySelectorAll('.toggle-button').forEach(i => {
    i.addEventListener('click', function(){
      slideout.toggle();
    });
  });

  slideout.on('beforeopen', function(){
    $('main#panel').prepend('<div id="hide_main" class="w-full min-h-full absolute top-0 left-0 bg-black bg-opacity-75" style="z-index: 320000;"></div>');
  });

  slideout.on('beforeclose', function(){
    $('#hide_main').remove();
  });

