<script>
  window.addEventListener('load',function(){
    jQuery('[href*="mailto:enquiries"]').click(function(){
      google_trackConversion({
        google_conversion_id : 100388001,
        google_conversion_label :"n8NDCNC5rncQzfzX3gM",
        google_remarketing_only :false
      });
    });
