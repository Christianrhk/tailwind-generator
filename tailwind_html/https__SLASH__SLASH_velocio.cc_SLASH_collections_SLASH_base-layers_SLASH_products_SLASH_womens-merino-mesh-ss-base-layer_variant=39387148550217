<script>
  //only run the function if mini cart enabled
    const miniCart = document.querySelector('.mini-cart');
    const announcementBarHeight = document.querySelector('.announcement-bar-container').offsetHeight;
      // Use this function when the announcement bar is enable to correctly position the mini cart on scroll
    function toggleMiniCartFixedPosition() {
        if (window.scrollY >= announcementBarHeight) {
            miniCart.classList.remove('top-announcementBar_and_header');
            miniCart.classList.add('top-mobile_header');
      }
    }
    
    // const announcementBarEnabled = ``
    
    // if(announcementBarEnabled === 'true') {
    //   document.addEventListener('scroll', function() {
    //       //toggleMiniCartFixedPosition() 
    //   });
    // }
  </script>

