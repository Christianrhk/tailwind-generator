cument.getElementById("InstallerChatClose"); var bubbleIntro=bubble.getElementsByClassName("installer-bubble-intro")[0]; var bubbleContent=bubble.getElementsByClassName("installer-bubble-conversation")[0]; var bubbleText=bubbleContent.getElementsByClassName("installer-bubble-text"); var trackEvent=function (eventAction, eventLabel){eventObject={event: "interactionevent", eventcategory: "installer assistant", eventaction: eventAction, eventlabel: eventLabel ? eventLabel : ""}; try{window.dataLayer=window.dataLayer || []; window.dataLayer.push(eventObject);}catch (error){console.warn(error);}}; var openChat=function (){if (isOpen){return;}isOpen=true; chatbot.classList.add("installer-bubble-open", "installer-bubble-loading"); setTimeout(function (){bubbleText[0].classList.add("installer-bubble-text__show"); chatbot.getElementsByClassName("typing-indicator")[0].style.display="none"; bubbleContent.style.display="grid"; chatbot.classList.remove("installer-bubble-loading"); opened=true;}, opened ? 0 : 2000);}; var closeChat=function (event){isOpen=false; chatbot.classList.remove("installer-bubble-open"); avatar.style.animation="none"; trackEvent("Close chat", locationPath); event.stopPropagation();}; var clickBubble=function (){trackEvent("Click", locationPath); openChat();}; var clickAvatar=function (){if (isOpen){return;}trackEvent("Click", locationPath); openChat();}; var clickQuestion=function (index){trackEvent(installerTexts.installerQuestions[index].text, locationPath);}; bubbleIntro.addEventListener("click", clickBubble); avatar.addEventListener("click", clickAvatar); closeChatbot.addEventListener("click", closeChat); (function (){var questionElement=bubble.getElementsByClassName("installer-bubble-question"); var questionContent=installerTexts.installerQuestions; bubble.getElementsByClassName("installer-bubble-intro")[0].innerHTML=installerTexts.findMe; bubble.getElementsByClassName("installer-bubble-text")[0].innerHTML=installerTexts.installerIntro; for (var i=0;