<script>
    function GetDataEnc(KeyData) {

        //if (!KeyValue) {
        //    KeyValue = '8080808080808080';
        //}

        //if (!ivValue) {
        //    ivValue = '8080808080808080';
        //}

        //var key = CryptoJS.enc.Utf8.parse(KeyValue);
        //var iv = CryptoJS.enc.Utf8.parse(ivValue);

        var key = CryptoJS.enc.Utf8.parse("8080808080808080");
        var iv = CryptoJS.enc.Utf8.parse("8080808080808080");

        var encryptedData = CryptoJS.AES.encrypt(CryptoJS.enc.Utf8.parse(KeyData), key,
            {
                keySize: 256,
                iv: iv,
                mode: CryptoJS.mode.CBC,
                padding: CryptoJS.pad.Pkcs7
            });

        return encryptedData;

    }

    function SubmitEncry() {
        var Username = $("#UserName").val();
        var Password = $("#Password").val();
        if ($("#Password").is('input:text')) {
            $("#Password").attr("type", "password");
        }

        //if ((Username.length == 0 || Username == "" || typeof (Username) == 'undefined')) {
        //    var userNameError = "Enter Email";
        //    alert(userNameError);
        //    return false;
        //}

        if (Password.length == 0 || Password == "" || typeof (Password) == 'undefined') {
            /*alert("Enter Password");*/
            return false;
        }


        $("#Password").val(GetDataEnc(Password));
        //$('#loginform').attr('onsubmit', 'return true;');
    }

