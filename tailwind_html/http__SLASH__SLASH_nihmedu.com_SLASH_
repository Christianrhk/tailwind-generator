		.dropdown-menu {
			position: absolute;
			top: 100%;
			left: 0;
			z-index: 1000;
			display: none;
			float: left;
			min-width: 9.5rem;
			padding: .5rem 0;
			margin: .125rem 0 0;
			font-size: 1rem;
			color: #212529;
			text-align: left;
			list-style: none;
			background-color: #f5f5f5;
			background-clip: padding-box;
			border: 1px solid rgba(0, 0, 0, .15);
			border-radius: .25rem;
			text-align: center;
		}

		.submenu-active .submenu {
			display: block;
			border-radius: 0px;
		}

		.submenu-active {
			background-color: transparent !important;
			border-radius: 3px;
		}

		.dropdown-submenu {
			position: relative;
		}

		.dropdown-submenu .dropdown-menu {
			top: -31px;
			left: 100%;
			margin-top: 41px;
		}



		.menu ul li:hover>a {
			background-color: #ff7d5f;
			color: #ffffff !important;
		}

		.menu ul li:hover>ul {
			display: block;
		}

		.menu li ul li a:hover {
			background-color: #ff7d5f;
			color: #ffffff;
		}

		.menu li ul li ul {
			left: 100%;
			top: 0;
		}

		.menu li ul {
			position: absolute;
			top: 100%;
			margin: 0;
			padding: 0;
			list-style: none;
			display: none;
			line-height: normal;
			background-color: #fff;
		}

		.menu li ul li a {
			text-align: left;
