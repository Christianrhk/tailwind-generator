<script>
  $(document).ready(function(){
    $.each($(".mortar-item-text-wrapper"), function(i, e){
      $(e).append("<span class='button outline'>Let's Go</span>");
    });
    
    var location_select = '<select id="mortar-event-location-select" class="inline-block ml-3"><option value="0">All Locations</option></select>';
    var locations = [];
    $(".events-listing-title").after('<div class="bg-grey-lighter rounded flex flex-wrap p-4"><label for="mortar-event-location-select">Select a Location:</label> '+location_select+'<button class="button outline ml-3 inline-block" id="location-selected">Go</button></div>');
    
    $.getJSON('/a/pages/api/locations/events', function(data){
      $.each(data, function(i) {
            locations.push(data[i]);
        	$("#mortar-event-location-select").append('<option value="'+data[i].slug+'">'+data[i].name+'</option>');
        });
    });
    
    $("#location-selected").click(function(e){
      mortarLinkToLocation事件($("#mortar-event-location-select").val());
    });
    
  });
  
  function mortarLinkToLocation事件(location_handle){
    var url = '/a/pages/events/locations/'+location_handle;
    window.location.href = url;
  }
</script>


