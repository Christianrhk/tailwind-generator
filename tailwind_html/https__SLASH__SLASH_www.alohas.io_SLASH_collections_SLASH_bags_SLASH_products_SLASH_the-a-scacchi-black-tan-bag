<script>
  // mobile accordion for tabs content
  $('.color-accordion-toggle', this.$container).click(function () {
    if ($(this).parent().next('div').is(':visible')) {
      $(this).parent().next('div').slideUp('fast');
    } else {
      $(this).parent().next('div').slideDown('fast');
    }
    $(this).find('.accordion__icon').toggleClass('rotate-90');
  });
</script>


