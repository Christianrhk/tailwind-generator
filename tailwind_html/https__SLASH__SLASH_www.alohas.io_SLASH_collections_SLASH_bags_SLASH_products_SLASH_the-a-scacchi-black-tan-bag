    function convertLabelArray(labels){
        for(let i=0; i<labels.length; i++){
            labels[i].tags = labels[i].tags.split(',')
        }
        console.log('arrays tags', labels)
        checkifActive(labels)
    }
    
    function checkifActive(allLabels) {

    if((window.location.href).includes('/collections') || (window.location.href).includes('/search')) {
        for(let i=0; i<allLabels.length; i++){
            if(allLabels[i].active === false){
                console.log(allLabels[i], 'desactivada')
                allLabels.splice(i, 1)
            }
        }
    } else if((window.location.href).includes('/products')){
        for(let i=0; i<allLabels.length; i++){
            if(allLabels[i].active === false && allLabels[i].productPage === false){
                allLabels.splice(i, 1)
            }
        }
    }  
        lookForLabel(allLabels)
    }
    function lookForLabel(activeLabels) {
        activeLabels.forEach(label =>{
            label.tags.forEach(tag =>{
                if(productTags6246482936001.includes(tag)){
                    assignedLabelText6246482936001 = label.label
                    assignedBackgroundColor6246482936001 = label.bgColor
                    assignedTextColor6246482936001 = label.txtColor
                } 
            })
        })
        assignTexts()
    }
    function assignTexts(){
        if(assignedLabelText6246482936001){
            labelContainer6246482936001.style.color = assignedTextColor6246482936001
            labelContainer6246482936001.style.backgroundColor = assignedBackgroundColor6246482936001
            labelContainer6246482936001.innerText = assignedLabelText6246482936001
        }
    }

    convertLabelArray(totalLabels6246482936001)

</script> -->

