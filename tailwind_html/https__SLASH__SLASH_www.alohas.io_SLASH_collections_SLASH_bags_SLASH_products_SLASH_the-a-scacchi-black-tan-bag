<script>
  // We add this script here to not depend innecesarily of the index.js bundle.
  // Inspired on https://shopify.dev/tutorials/customize-theme-support-multiple-currencies?shpxid=894907dd-82C1-490D-12C2-CEA030226892#example-currency-selectors
  document.querySelectorAll('[data-currency-and-language-picker] .shopify-currency-form select').forEach(function(element) {
    element.addEventListener('change', (event) => {
      event.target.form.submit()
    });
  });
