<script>
  let firstTab = document.querySelector('.description-tab')
  firstTab.classList.add("tab__active")

  let firstContent = document.querySelector('.tab__content')
  firstContent.classList.add("content__active")

  let tabs = document.querySelectorAll('.tab')
  let contents = document.querySelectorAll('.tab__content')
  let activeTab

  tabs.forEach(tab => {
    tab.addEventListener("click", (e) => {
      tabs.forEach(tab => tab.classList.remove("tab__active"))
      let elementType = e.target.nodeName
      if(elementType === 'LI'){
        e.target.classList.add("tab__active")
        activeTab = e.target.dataset.infoNumber
      } else {
        let parent = e.target.parentElement
        parent.classList.add("tab__active")
        activeTab = parent.dataset.infoNumber
      }
      
      let activeContent = document.querySelector(`div[data-info-number='${activeTab}']`)
      contents.forEach(content => content.classList.remove("content__active"))
      activeContent.classList.add("content__active")
    });
  })
</script>
