<script>
  window.addEventListener('DOMContentLoaded', addElements);

  function addElements() {
      let sizeContainer = document.querySelectorAll('.product-size-input')
      function closeTab() {
      $('.size-container').slideUp('fast')
      $('.size-arrow').removeClass('rotate-90');
    }
      sizeContainer.forEach(input => input.addEventListener("click", closeTab))
  }
  function checkSizeSelected(event, variant) {
    let selectedSizeSpan = document.querySelector('#size-selected-variant')
    let holderButton =  document.getElementById('placeholder-button')
    if(holderButton){
      document.getElementById('placeholder-button').style.display = 'none'
      document.getElementById('product_submit_container').style.display = 'block'
    }
    selectedSizeSpan.textContent = variant
  }
  function sizeError() {
    document.querySelector('.select-size--error').style.display = "block"
    console.log('select size to continue')
  }
</script>
