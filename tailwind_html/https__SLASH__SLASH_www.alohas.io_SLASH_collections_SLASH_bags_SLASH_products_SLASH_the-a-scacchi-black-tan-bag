  <link rel="stylesheet" href="https://cdn.shopify.com/extensions/9f1ff4c6-d5e2-444a-8465-d0be8bf1323f/6.9.0/assets/govalo.min.css">
  
  

  <script type="text/javascript"
          src="https://cdn.shopify.com/extensions/9f1ff4c6-d5e2-444a-8465-d0be8bf1323f/6.9.0/assets/govalo.min.js"
          defer="defer"></script>

  <script type="text/javascript">
    // if we are the recharge or bold clone
    const rechargeSourceId = "";
    const boldSourceId = "";

    const sourceId = rechargeSourceId || boldSourceId;
    if (sourceId !== "") {
      // fetch all products (/products.json)
      fetch("/products.json")
        .then(response => response.json())
        .then(({products}) => {
          const product = products.find(product => product.id == sourceId);
          window.location = `/products/${product.handle}?gvlo-subscription=true`;
        })
      // filter for the rechargeSourceId
      // redirect to that page `/products/${product.handle}`
    }
  </script>




<!-- END APP EMBED: GOVALO -->


