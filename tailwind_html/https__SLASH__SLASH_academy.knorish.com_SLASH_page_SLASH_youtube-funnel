        $(function () {
            $.ajax({
                type: 'GET',
                url: '/course/getcoursefortracking?id=c7a30702-9e79-40b2-a758-eebaa0c0bc04',
                success: function (data) {
                    if (data.status) {
                        if (data.facebookpixel) {
                            fbq('track', 'ViewContent', {
                                value: data.price,
                                currency: data.currency,
                                content_type: 'product',
                                content_ids: data.courseid
                            });
                        }
                        if (data.googleanalytics) {
                            gtag('event', 'view_item', {
                                'items': [
                                    {
                                        'id': data.courseid,
                                        'name': data.coursename,
                                        'price': data.price
                                    }
                                ]
                            });
                        }
                    }
                }
            });
        });
        </script>
