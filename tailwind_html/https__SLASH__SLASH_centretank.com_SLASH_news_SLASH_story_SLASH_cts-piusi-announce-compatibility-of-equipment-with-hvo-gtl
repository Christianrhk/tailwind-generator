<script>
  window.addEventListener('load',function(){
    jQuery('[href*="mailto:enquiries"]').click(function(){
      google_trackConversion({
        google_conversion_id : 100388001,
        google_conversion_label :"n8NDCNC5rncQzfzX3gM",
        google_remarketing_only :false
      });
    });
  });

	$( document ).ready(function() {


		$('a.view-more').on('click',function(){
			$('.homepage-intro>div').fadeToggle();
			$('.product-cat-desc').fadeToggle();
			$('.product-cat-desc-mobile').toggle();
			return false;
		});

		$('.nProducts > a').on('mouseover hover focus',function(){
			$('.first-products-submenu').removeClass('hover');
			$('.first-products-submenu').addClass('hover');
		});
		$('.first-products-submenu').on('mouseover hover focus',function(){$('.first-products-submenu').removeClass('hover');});

	var win = $(this); //this = window
	//if (win.height() >= 820) { /* ... */ }
	
	if (win.width() <= 1000) {$('.news-article-filters').addClass('news-article-filter-mobile-menu');}
	$(window).on('resize', function(){
		var win = $(this); //this = window
		//if (win.height() >= 820) { /* ... */ }
		if (win.width() > 1000) {
			$('.news-article-filters').removeClass('news-article-filter-mobile-menu');
			$('.news-article-filter-mobile-menu .news-article-filter-menu').show();
		}
		if (win.width() <= 1000) {
			if(!$('.news-article-filters').hasClass('news-article-filter-mobile-menu')){$('.news-article-filters').addClass('news-article-filter-mobile-menu');}
		}
	});
	$('.news_filter_burger_menu').on('click touch',function(){
		$('.news-article-filter-mobile-menu .news-article-filter-menu').fadeToggle();
	});



	});


</script>

