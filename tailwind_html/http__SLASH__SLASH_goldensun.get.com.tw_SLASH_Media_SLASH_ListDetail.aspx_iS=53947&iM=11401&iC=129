		function fShowVideo() {
			//alert("瀏覽器：" + BrowserDetect.browser +"；版本：" + BrowserDetect.version);
			var rml = new RadiantML();
			var mp4H264AAC = rml.mp4H264AAC('high40');
			var mse = rml.mse();
			var m4aHEAACv2 = rml.m4aHEAACv2();

			if (mse && mp4H264AAC && m4aHEAACv2) {
				document.getElementById("player-size").style.display = 'block';
				document.getElementById("rmpPlayer").style = 'width:640px; height:360px;'; //RWD可能不需此段，由CSS控制
