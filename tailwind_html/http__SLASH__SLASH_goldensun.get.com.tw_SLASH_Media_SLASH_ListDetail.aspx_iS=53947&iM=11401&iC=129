<script>
    new Vue({
        el: "#banner_item",
        components: {
            Hooper: window.Hooper.Hooper,
            Slide: window.Hooper.Slide,
            HooperPagination: window.Hooper.Pagination,
        },
        data() {
            return {
                hooperSettings: {
                    initialSlide: 0,
                    infiniteScroll: true,
                    // centerMode: true,
                    autoPlay: true,
                    playSpeed: 3500,
                    breakpoints: {
                        0: {
                            itemsToShow: 1
                        }
                    }
                }
            }
        }
    });

    //開合
    $(".open_press").hide('');

    $(".close_press").click(function() {
        $("#banner_item").addClass("side-active");
        $(".close_press").hide('fast');
        $(".open_press").show('fast');
    });

    $(".open_press").click(function() {    
        $("#banner_item").removeClass("side-active");
        $(".close_press").show('fast');
        $(".open_press").hide('fast');

    });
</script>

