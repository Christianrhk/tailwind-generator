    jQuery(function () {
        jQuery('.swatch :radio').change(function () {
            var optionIndex = jQuery(this).closest('.swatch').attr('data-option-index');
            var optionValue = jQuery(this).val();
            jQuery(this)
                .closest('form')
                .find('.single-option-selector')
                .eq(optionIndex)
                .val(optionValue)
                .trigger('change');
        });
    });

    // Update for RO Compatibility
    (function () {
        var mobileSubscribeLabel = document.querySelector('.nav-link[href*="Subscribe"]')
        mobileSubscribeLabel && mobileSubscribeLabel.addEventListener('click', function (evt) {
            var subscription = mobileSubscribeLabel.closest('form').querySelector('.bold-ro__subscribe-radio-btn')
            subscription && subscription.click()
        })

        var onetimeSubscribeLabel = document.querySelector('.nav-link[href*="purchase"]')
        onetimeSubscribeLabel && onetimeSubscribeLabel.addEventListener('click', function (evt) {
            var oneTime = onetimeSubscribeLabel.closest('form').querySelector('.bold-ro__one-time-radio-btn')
            oneTime && oneTime.click()
        })
    })()
</script>

