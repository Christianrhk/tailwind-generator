<script>
 document.addEventListener("selectedVariantUpdated", function(e) {
   const sel = jQuery(document).find(`option[value='${e.detail.id}']`).parent();
   sel.val(e.detail.id).trigger("change");
   console.log(e.detail);
  });
  jQuery(document).on("change", "select.combined-option", function(e){
    const org_price = jQuery(this).find(":selected").data('price');
   
    const quantity = jQuery(this).parents("form").find("input[name='quantity']").val();
    const selling_plan = jQuery(this).parents("form").find("input[name='selling_plan']:checked").val();
    let price = ((org_price/100) * quantity);
        if(selling_plan.length){
          price = price - ((10/100) * price)
        }
        jQuery(this).parents("form").find("button[type='submit'] span.prichange").html(price.toFixed(2));
    
  });
  jQuery(document).on("change", "input.radin", function(e){
    // const org_price = jQuery(this).parents("form").find("select.combined-option").find(":selected").data('price');
    // const quantity = jQuery(this).parents("form").find("input[name='quantity']").val();
    // const selling_plan = jQuery(this).val();
    // let price = ((org_price/100) * quantity);
    // if(selling_plan.length){
    //   price = price - ((10/100) * price)
    // }
    // jQuery(this).parents("form").find("button[type='submit'] span.prichange").html(price.toFixed(2));
  });
  jQuery(document).on("input", "input.qaty", function(e){
    const org_price = jQuery(this).parents("form").find("select.combined-option").find(":selected").data('price');
    const quantity = parseInt(jQuery(this).val());
    if(quantity && org_price){
      	const selling_plan = jQuery(this).parents("form").find("input[name='selling_plan']:checked").val();
        let price = ((org_price/100) * quantity);
        if(selling_plan.length){
          price = price - ((10/100) * price)
        }
        jQuery(this).parents("form").find("button[type='submit'] span.prichange").html(price.toFixed(2));
    }
    
  });
  jQuery(document).on("click", ".u-plus, .u-minus", function(e){
    const org_price = jQuery(this).parents("form").find("select.combined-option").find(":selected").data('price');
    const quantity = jQuery(this).parents("form").find("input[name='quantity']").val();
    if(quantity && org_price){
      	const selling_plan = jQuery(this).parents("form").find("input[name='selling_plan']:checked").val();
      	let price = ((org_price/100) * quantity);
        if(selling_plan.length){
          price = price - ((10/100) * price)
        }
        jQuery(this).parents("form").find("button[type='submit'] span.prichange").html(price.toFixed(2));
    }
    
  });
