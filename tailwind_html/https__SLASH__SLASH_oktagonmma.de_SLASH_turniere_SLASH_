<script type='text/javascript' src='https://cdnjs.cloudflare.com/ajax/libs/Swiper/8.0.7/swiper-bundle.min.js?ver=8.0.7' id='swiper-bundle-js'></script>
<script>
/* Smooth scroll */
jQuery(document).on('click', '.anchor-link a, a.anchor-link', function(e) {
var id = jQuery(this).attr('href');
var id = id.replace("/", "");
var $id = jQuery(id);
if ($id.length === 0) {
return;
}
e.preventDefault();
if (jQuery(window).width() < 992) {
var cffset = 0;
} else {
var cffset = 0;
}
var pos = $id.offset().top - cffset;
jQuery('body, html').animate({
scrollTop: pos
}, 1000);
});
/* Hamburger button trigger - mobilne menu */
jQuery(document).on('click', '#mobile_menu_trigger', function(e) {
e.preventDefault();
jQuery("#mobile_menu_trigger>svg").toggleClass('active');
jQuery("#challenge_nav").toggleClass('navopen');
jQuery("body").toggleClass('navopen');
});
</script>
<script>
const swiper = new Swiper('.hero-slider', {
// Optional parameters
direction: 'horizontal',
loop: false,
speed: 900,
slidesPerView: 1,
loop: true,
autoplay: {
delay: 7500,
disableOnInteraction: false,
},
spaceBetween: 0,
// Navigation arrows
navigation: {
nextEl: '.swiper-button-next',
prevEl: '.swiper-button-prev',
},
});
// Sticky header after scroll
jQuery(window).scroll(function() {
if (jQuery(this).scrollTop() > 45) {
jQuery('#challenge-header').addClass("header-sticky");
jQuery('body').addClass("header-sticky");
if (jQuery(window).width() > 992) {
//jQuery('.preserve-height').show();
}
} else {
jQuery('#challenge-header').removeClass("header-sticky");
jQuery('body').removeClass("header-sticky");
if (jQuery(window).width() > 992) {
//jQuery('.preserve-height').hide();
}
}
});
</script>
