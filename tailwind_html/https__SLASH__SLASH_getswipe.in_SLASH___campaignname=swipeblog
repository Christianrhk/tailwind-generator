    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-71930908-8"></script>
    <script>
    window.dataLayer = window.dataLayer || [];

    function gtag() {
        dataLayer.push(arguments);
    }
    gtag("js", new Date());
    gtag("config", "UA-71930908-8");
