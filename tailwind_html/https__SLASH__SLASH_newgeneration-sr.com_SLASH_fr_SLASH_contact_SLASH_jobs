    <style>
        #my_pdf_viewer.hide_viewer {
            display: none;
        }
        #canvas_container {
            text-align: center;
        }
    </style>

    <script
        src="https://cdnjs.cloudflare.com/ajax/libs/pdf.js/2.0.943/pdf.min.js">
    </script>
    <script>
                                
        var myState = {
            pdf: null,
            currentPage: 1,
            zoom: 1.5
        }

        function openViewer(pdf_in){
            if(pdf_in) {
                pdfjsLib.getDocument(pdf_in).then((pdf) => {
                    myState.pdf = pdf;
                    render();
                    document.getElementById('my_pdf_viewer').classList.toggle('hide_viewer')
                    document.getElementById('my_pdf_viewer').scrollTo({
                        top: 0
                    })
                    document.querySelector('body').style.overflow = 'hidden'
                });
            }
            else {
                document.getElementById('my_pdf_viewer').classList.toggle('hide_viewer')
                document.querySelector('body').style.overflow = 'auto'
            }
        }

        function render() {
            myState.pdf.getPage(myState.currentPage).then((page) => {        
                var canvas = document.getElementById("pdf_renderer");
                var ctx = canvas.getContext('2d');                
                var viewport = page.getViewport(myState.zoom);

                canvas.width = viewport.width;
                canvas.height = viewport.height;

                page.render({
                    canvasContext: ctx,
                    viewport: viewport
                });
            });
        }

        document.getElementById('go_previous').addEventListener('click', (e) => {
            if(myState.pdf === null || myState.currentPage === 1) return;

            console.log('prev');
            
            myState.currentPage -= 1
            document.getElementById("current_page").value = myState.currentPage;
            render();
        });

        document.getElementById('go_next').addEventListener('click', (e) => {
            if(myState.pdf === null || myState.currentPage >= myState.pdf._pdfInfo.numPages) return;

            console.log('next');
                
            myState.currentPage += myState.currentPage !== 1 ? 1 : 0;
            document.getElementById("current_page").value = myState.currentPage;
            render();
        });

        document.getElementById('current_page').addEventListener('keypress', (e) => {
            if(myState.pdf === null) return;
        
            // Get key code
            var code = (e.keyCode ? e.keyCode : e.which);
        
            // If key code matches that of the Enter key
            if(code === 13) {
                var desiredPage = document.getElementById('current_page').valueAsNumber;
                                
                if(desiredPage >= 1 && desiredPage <= myState.pdf._pdfInfo.numPages) {
                    myState.currentPage = desiredPage;
                    document.getElementById("current_page").value = desiredPage;
                    render();
                }
            }
        });

        document.getElementById('zoom_in').addEventListener('click', (e) => {
            if(myState.pdf === null) return;

            myState.zoom *= myState.zoom < 6 ? 1.1 : 1;

            console.log(myState.zoom);

            render();
        });

        document.getElementById('zoom_out').addEventListener('click', (e) => {
            if(myState.pdf === null) return;

            myState.zoom *=  myState.zoom > 0.3 ? 0.9 : 1;
            console.log(myState.zoom);
            
            render();
        });
    </script>
    
            </section>
        
