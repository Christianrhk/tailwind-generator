<form
        name="contact"
    action="/fr/contact/jobs"
    method="POST"
                      class=" "          >
        
                                      
                          <div class="form-field   mb-6">
            <div class="form-label"><label class="inline" >Nom              <span class="required">*</span></label>
      </div>
        <div class="form-data"
          data-grav-field="text"
  data-grav-disabled=""
  data-grav-default="null"
    >
                        <div class="form-input-wrapper ">
                                    <input
              class="pl-4 py-4 pr-11"
              name="data[name]"
              value=""
                  type="text"
                  class=" "
        placeholder="Entrer&#x20;votre&#x20;nom"        autocomplete="on"          required="required"    
    
    
            />
            <div style="position: absolute; right: 0.6rem; top: 0.8rem" class="button-style-icon hide light">
              <i class="mdi mdi-close text-lg"></i>
            </div>
                                      </div>
                        </div>
  </div>

<script>
{
  let field;
  let input;
  let label;
  let clear;

  if(document.querySelector('input[name="data[name]"]') || document.querySelector('textarea[name="data[name]"]')){
    input = document.querySelector('input[name="data[name]"]') || document.querySelector('textarea[name="data[name]"]')
    clear = input.parentElement.children[1]
    field = input.parentElement.parentElement.parentElement
    label = field.children[0]

    if(document.activeElement === input){
      field.classList.add('focused')
    }
    if(input.value){
      field.classList.add('filled')
      clear.classList.remove('hide')
    }

    if(clear){
      clear.addEventListener('click', () => {
        if(input.value) {
          input.value = ""
          input.focus()
          clear.classList.add('hide')
          field.classList.remove('filled')
        }
      })

      input.addEventListener('keyup', () => {
        if(input.value) {
          clear.classList.remove('hide')
        }
        else {
          clear.classList.add('hide')
        }
      })
    }

    input.addEventListener('focus', () => {
      field.classList.add('focused')

      if(input.value){
        field.classList.add('filled')
      }
    })

    input.addEventListener('blur', () => {
      field.classList.remove('focused')

      if(!input.value){
        field.classList.remove('filled')
      }
      else {
        field.classList.add('filled')
      }
    })
  }
}
</script>

