                                <div id="searchbar" style="position: absolute; top: 5rem; left: 0; width: 100%" class="bg-black pb-6 px-6">
                        <form name="search" data-simplesearch-form>
        <div class="form-field">
            <div class="form-label">Rechercher...</div>
            <div class="form-data">
                <div class="form-input-wrapper">
                    <input
                        name="searchfield"
                        class="search-input pl-4 py-4 pr-11"
                        aria-label="search"
                        type="text"
                         data-min="3"                         required
                        value=""
                        data-search-invalid="Veuillez ajouter au moins 3 caractères"
                        data-search-separator=":"
                        data-search-input="/fr/search/query"
                    />
                                        <div style="position: absolute; right: 0.6rem; top: 0.8rem" class="button-style-icon hide light">
                        <i class="mdi mdi-close text-lg"></i>
                      </div>
                </div>
            </div>
        </div>
    </form>

<script>
    {
      let field;
      let input;
      let label;
      let clear;
    
      if(document.querySelector('input[name="searchfield"]')){
        input = document.querySelector('input[name="searchfield"]')
        clear = input.parentElement.children[1]
        field = input.parentElement.parentElement.parentElement
        label = field.children[0]
    
        if(document.activeElement === input){
          field.classList.add('focused')
        }
        if(input.value){
          field.classList.add('filled')
          clear.classList.remove('hide')
        }
    
        if(clear){
          clear.addEventListener('click', () => {
            if(input.value) {
              input.value = ""
              input.focus()
              clear.classList.add('hide')
              field.classList.remove('filled')
            }
          })
    
          input.addEventListener('keyup', () => {
            if(input.value) {
              clear.classList.remove('hide')
            }
            else {
              clear.classList.add('hide')
            }
          })
        }
    
        input.addEventListener('focus', () => {
          field.classList.add('focused')
    
          if(input.value){
            field.classList.add('filled')
          }
        })
    
        input.addEventListener('blur', () => {
          field.classList.remove('focused')
    
          if(!input.value){
            field.classList.remove('filled')
          }
          else {
            field.classList.add('filled')
          }
        })
      }
    }
    </script>
    
