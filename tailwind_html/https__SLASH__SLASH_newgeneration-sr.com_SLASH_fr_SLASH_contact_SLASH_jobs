<script>
{
  let field;
  let input;
  let label;
  let clear;

  if(document.querySelector('input[name="data[subject]"]') || document.querySelector('textarea[name="data[subject]"]')){
    input = document.querySelector('input[name="data[subject]"]') || document.querySelector('textarea[name="data[subject]"]')
    clear = input.parentElement.children[1]
    field = input.parentElement.parentElement.parentElement
    label = field.children[0]

    if(document.activeElement === input){
      field.classList.add('focused')
    }
    if(input.value){
      field.classList.add('filled')
      clear.classList.remove('hide')
    }

    if(clear){
      clear.addEventListener('click', () => {
        if(input.value) {
          input.value = ""
          input.focus()
          clear.classList.add('hide')
          field.classList.remove('filled')
        }
      })

      input.addEventListener('keyup', () => {
        if(input.value) {
          clear.classList.remove('hide')
        }
        else {
          clear.classList.add('hide')
        }
      })
    }

    input.addEventListener('focus', () => {
      field.classList.add('focused')

      if(input.value){
        field.classList.add('filled')
      }
    })

    input.addEventListener('blur', () => {
      field.classList.remove('focused')

      if(!input.value){
        field.classList.remove('filled')
      }
      else {
        field.classList.add('filled')
      }
    })
  }
}
</script>

