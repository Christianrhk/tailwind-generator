<meta name="robots" content="noindex, follow"><script type="text/javascript">var dataLayer = [{page_id:"CA20FF67-0929-4A1E-8440-BC6A87A12D4E" ,page_type: "GenericPage", country:"Germany",language:"de-DE",http_code: "200", customer_status: "not logged in"}];</script>
<script type="text/javascript" language="javascript">
jQuery(window).load(function(){
	addWindowObj	=	new addWindowObjClass();
	showCodeTextObj	=	new showCodeTextObjClass();
});

/*************************/
/* Voucher Code Response */
/*************************/
function showCodeTextObjClass() {
	this.selfRef			= this;
	this.campaignActive		= true;

	this.CodeInputTarget	= jQuery('input[name=gutschein_code]');
	this.TextTarget_1		= jQuery('div.conditional_text_1');
	this.TextTarget_1b		= jQuery('div.conditional_text_1b');
	this.TextTarget_2		= jQuery('div.conditional_text_2');
	this.TextTarget_3		= jQuery('div.conditional_text_3');
	this.TextTarget_4		= jQuery('div.conditional_text_4');
	this.TextTarget_all		= jQuery('div.conditional_text');

	this.visibleClass		= 'show';

	this.init(this.selfRef);
}

showCodeTextObjClass.prototype = {
	init:function(selfRef) {
		if( (this.CodeInputTarget) && (this.TextTarget_1) && (this.TextTarget_1b) && (this.TextTarget_2) && (this.TextTarget_3) && (this.TextTarget_4) ) {
			this.setChangeHandler(selfRef);
			this.CodeInputTarget.trigger( "change" );
		}
	},

	setChangeHandler: function(selfRef) {
		selfRef.CodeInputTarget.on( "change blur keyup", function( event ) {

			var testString = jQuery.trim(jQuery(this).val());
			/* Mindestlänge des getrimmten Codes = 9 */
			if(testString.length >= 9) {

				// Aktion60+70
				if( (testString == "HH-FR2019") ) {
					selfRef.TextTarget_all.removeClass(selfRef.visibleClass);
					if(selfRef.campaignActive != false) {
						selfRef.TextTarget_1.addClass(selfRef.visibleClass);
					} else {
						selfRef.TextTarget_4.addClass(selfRef.visibleClass);
					}

				// Aktion50+70
				// Special :: handover with URL-String
				} else if( (testString == "VD-FR2019") ) {
					selfRef.TextTarget_all.removeClass(selfRef.visibleClass);
					if(selfRef.campaignActive != false) {
						selfRef.TextTarget_1b.addClass(selfRef.visibleClass);
						//selfRef.CodeInputTarget.attr("readonly", "true");
						//selfRef.CodeInputTarget.attr('style', 'background-color: #efefef !important');
					} else {
						selfRef.TextTarget_4.addClass(selfRef.visibleClass);
					}

				// Pflegeset-Aktion
				} else if( (testString == "HH-PS2018") || (testString == "HH-PS2019") ) {
					selfRef.TextTarget_all.removeClass(selfRef.visibleClass);
					selfRef.TextTarget_2.addClass(selfRef.visibleClass);

				// code not valid
				} else {
					selfRef.TextTarget_all.removeClass(selfRef.visibleClass);
					selfRef.TextTarget_3.addClass(selfRef.visibleClass);
				}

				} else {
					selfRef.TextTarget_all.removeClass(selfRef.visibleClass);
				}
		});
	}

}

/*********************/
/* Window show/hide */
/*********************/
function addWindowObjClass() {
	this.selfRef			= this;
	this.Targets_2Cmns		= jQuery('.form-columns-2');
	this.Targets_3Cmns		= jQuery('.form-columns-3');
	this.Targets_rte		= jQuery('.form-columns-3 .hs-richtext');
	this.btn_next			= jQuery('p.button.next');
	this.btn_prev			= jQuery('p.button.prev');
	this.clickTargetIdent	= 'clickTarget';
	this.visibleClass		= 'formFieldsShow';
	this.windowAll			= 0;
	this.windowCount		= 0;
	this.init(this.selfRef);
}

addWindowObjClass.prototype = {
	init:function(selfRef) {
		if( (selfRef.Targets_2Cmns.length > 0) || (selfRef.Targets_3Cmns.length > 0) ) {
			selfRef.windowAll = selfRef.Targets_3Cmns.length;
			if(this.reorderHTML(selfRef)) {
				this.showHideWindow(selfRef, this.windowCount+1);
				this.enableClickHandler(selfRef);
				this.enableButtonHandler(selfRef);
			}
		}
	},

	reorderHTML: function(selfRef) {
		selfRef.Targets_rte.each(function() {
			var tempTarget = jQuery( this ).parent();
			var tempSource = jQuery( this ).addClass(selfRef.clickTargetIdent).remove();
			
			tempSource.insertBefore( tempTarget );
			
		});
		return true;
	},

	showHideWindow: function(selfRef, amount) {

		if(amount <=0) {
			amount = 1;
		} else if(amount >= (selfRef.Targets_3Cmns.length)/2) {
			amount = selfRef.Targets_3Cmns.length/2;
		}

		/* Click auf Toggler, kein anderes Akkordeon soll geöffnet werden: */
		if( selfRef.windowCount == amount) {
			if( jQuery('.clickTarget').eq(amount-1).hasClass('opened')) {
				selfRef.helper_removeClass(selfRef);
			} else {
				selfRef.helper_removeClass(selfRef);
				selfRef.helper_addClass(selfRef, amount);
			}

		/* Neues Akkordeon öffnen */
		} else {
			selfRef.helper_removeClass(selfRef);
			selfRef.helper_addClass(selfRef, amount);
		}

		selfRef.windowCount = amount;

	},

	helper_removeClass: function(selfRef) {
		//selfRef.Targets_2Cmns.removeClass(selfRef.visibleClass);
		selfRef.Targets_3Cmns.removeClass(selfRef.visibleClass);
		jQuery('.clickTarget').removeClass('opened');
	},

	helper_addClass: function(selfRef, amount) {
		selfRef.Targets_3Cmns.eq(amount*2-1).addClass(selfRef.visibleClass);
		selfRef.Targets_3Cmns.eq(amount*2-2).addClass(selfRef.visibleClass);
		jQuery('.clickTarget').eq(amount-1).addClass('opened');
	},

	enableButtonHandler: function(selfRef) {
			jQuery(selfRef.btn_next).click(function() {
				selfRef.showHideWindow(selfRef, selfRef.windowCount+1);
			});
			jQuery(selfRef.btn_prev).click(function() {
				selfRef.showHideWindow(selfRef, selfRef.windowCount-1);
			});

	},

	enableClickHandler: function(selfRef) {
		jQuery('.clickTarget').click(function() {

			for(var i=0; i< jQuery('.clickTarget').length; i++){

				if( jQuery(this).is(jQuery('.clickTarget').eq(i)) ) {
					selfRef.showHideWindow(selfRef, i+1);
				}
			}

		});

	}
}

</script>

