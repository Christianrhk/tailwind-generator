    $(window).load(function() {
        setTimeout(clickFuntion, 1000);

        function clickFuntion() {
            var windowIndex = 1;
            $('#windowbutton2').click(function() {
                console.log(windowIndex);
                if (windowIndex < 10) {
                    windowIndex++;
                }
                if (windowIndex < 10) {
                    $('.windowNumber' + windowIndex).show();
                    $('.hs_windowreg_typ_0' + windowIndex + '_2019_01').show();
                    $('.hs_windowreg_size_0' + windowIndex + '_2019_01').show();
                    $('.hs_windowreg_imp_0' + windowIndex + '_2019_01').show();
                    $('.hs_windowreg_code_0' + windowIndex + '_2019_01').show();
                    $('.hs_windowreg_date_0' + windowIndex + '_2022_03').show();
                } else {
                    $('.windowNumber' + windowIndex).show();
                    $('.hs_windowreg_typ_' + windowIndex + '_2019_01').show();
                    $('.hs_windowreg_size_' + windowIndex + '_2019_01').show();
                    $('.hs_windowreg_imp_' + windowIndex + '_2019_01').show();
                    $('.hs_windowreg_code_' + windowIndex + '_2019_01').show();
                    $('.hs_windowreg_date_' + windowIndex + '_2022_03').show();

                    $('#windowbutton2').hide();
                }
            });
        }
    });
    </script>
