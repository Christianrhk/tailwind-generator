	function messageRed() {
		fetch("https://lor.paris/message_accepted", {
				  method: 'POST',
				  headers: {
					  'X-CSRF-TOKEN': "gEMZlupiimuqgBbs0d7o1FBo7YqabTCe7ktCiflY",
				  },
				  body: ""
			  })
		.then(value => {
			  if (value.status != 200) {
				console.log('Form sucessfully submitted and negative answer (' + value.status + ')');
			  } else {
				  document.getElementById('cookie_bar').style.display = "none";
			  }
			  },
			  reason => {
				console.log(reason);
			  })
		.catch(() => {
				console.log("error");
			   })
		.finally(() => {
				 })
	}
