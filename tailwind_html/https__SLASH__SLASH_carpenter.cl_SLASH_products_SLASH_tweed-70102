<script>
  (function () {
    const btn = document.querySelector("#AddToCartDesk");

    function disableAddToCart(blockElement = false) {
      if (blockElement) {
        btn.setAttribute("disabled", blockElement);
        return;
      }
      btn.removeAttribute("disabled");
    }

    btn.addEventListener("click", function () {
      disableAddToCart(true);
      setTimeout(() => disableAddToCart(false), 3000);
    });
    window.addEventListener("load", () => disableAddToCart(false));


    disableAddToCart(true);
  })();
</script>
      
