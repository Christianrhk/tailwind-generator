		<script type="text/javascript">
			function isIE() {
			  ua = navigator.userAgent;
			  /* MSIE used to detect old browsers and Trident used to newer ones*/
			  var is_ie = ua.indexOf("MSIE ") > -1 || ua.indexOf("Trident/") > -1;

			  return is_ie;
			}
			/* Create an alert to show if the browser is IE or not */
			if (isIE()){
			  $("#platform-alert").css("display", "block");
			}
</script>
<script>

		 function clear_utms(){
				window.localStorage.removeItem("atento_utm_source")
				window.localStorage.removeItem("atento_utm_medium")
				window.localStorage.removeItem("atento_utm_campaign")
		 }

		 function set_utms(){
					var item = window.localStorage.getItem("atento_utm_source");
					if (( item == '') || (item == null)){
						window.localStorage.setItem("atento_utm_source", "")
					}
					var item = window.localStorage.getItem("atento_utm_medium");
					if (( item == '') || (item == null)){
						window.localStorage.setItem("atento_utm_medium", "")
					}
					var item = window.localStorage.getItem("atento_utm_campaign");
					if (( item == '') || (item == null)){
						window.localStorage.setItem("atento_utm_campaign", "")
					}
		 }

		 if (window.localStorage) {
			 set_utms()
			}
			function init() {
			var imgDefer = document.getElementsByTagName('img');
			for (var i=0; i<imgDefer.length; i++) {
			if(imgDefer[i].getAttribute('data-src')) {
			imgDefer[i].setAttribute('src',imgDefer[i].getAttribute('data-src'));
			} } }
			window.onload = init;
		 </script>

