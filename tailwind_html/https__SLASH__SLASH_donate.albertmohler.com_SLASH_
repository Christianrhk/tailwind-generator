    <script>
        var usingSafari = navigator.vendor && navigator.vendor.indexOf('Apple') > -1 &&
            navigator.userAgent &&
            navigator.userAgent.indexOf('CriOS') === -1 &&
            navigator.userAgent.indexOf('FxiOS') === -1
        function updateStatusDirectly(status) {
            var oldClasses = '', newClasses = '', message = ''
            var submitButton = document.querySelector('form button')
            switch (status) {
                case 'processing':
                    submitButton.classList.add('btn-disabled')
                    submitButton.classList.remove('btn-primary', 'btn-success', 'btn-error')
                    submitButton.innerHTML = 'Processing...'
                    submitButton.disabled = true
                    break
                case 'success':
                    submitButton.classList.add('btn-disabled', 'btn-primary')
                    submitButton.classList.remove('btn-success', 'btn-error')
                    submitButton.innerHTML = 'Success!'
                    submitButton.disabled = true
                    break
                case 'error':
                    submitButton.classList.add('btn-disabled', 'btn-error')
                    submitButton.classList.remove('btn-success', 'btn-primary')
                    submitButton.innerHTML = 'Error'
                    submitButton.disabled = true
                    break
                case 'complete':
                    submitButton.classList.add('btn-success')
                    submitButton.classList.remove('btn-success', 'btn-primary', 'btn-error')
                    submitButton.innerHTML = 'Success!'
                    submitButton.disabled = true
                    break
                case 'ready':
                default:
                    submitButton.classList.add('btn-primary')
                    submitButton.classList.remove('btn-disabled', 'btn-success', 'btn-error')
                    submitButton.innerHTML = 'Give'
                    submitButton.disabled = false
                    break

            }
        }

        function updateErrorsDirectly(errorMessage) {
            document.querySelector('form .error-message').innerHTML = '<p>'+errorMessage+'</p>'
        }

        Vue.use(VeeValidate, {
            classes: true
        })
        Vue.use(VueTheMask)
        Vue.use(VMoney)

        const app = new Vue({
            el: '#donate',
            data: {
                card: null,
                form: {
                    paymentType: 'cc',
                    category: 'Mohler Online General',
                    amount: null,
                    premium: false,
                    honor: false,
                    honoree: null,
                    recurring: false,
                    frequency: null,
                    first: null,
                    last: null,
                    phone: null,
                    email: null,
                    street: null,
                    city: null,
                    state: null,
                    zip: null,
                    country:'US',
                    nonce: null
                },
                money: {
                    decimal: '.',
                    thousands: ',',
                    prefix: '$ ',
                    precision: 2,
                    masked: false /* doesn't work?! according to docs */
                },
                status: 'ready', // ready || processing || success || error || complete
                stripe: null,
                stripeElements: null,
                errorMessage: '',
                year: 2022
            },
            computed: {
                isPremium: function() {
                    return this.getStrippedAmount(this.form.amount) >= 100
                }
            },
            methods: {
                revalidate: function () {
                    setTimeout(function () {
                        app.$validator.validateAll()
                    }, 100)
                },
                getStrippedAmount: function (maskedAmount) {
                    var nonNullAmount = maskedAmount || '0'
                    return parseFloat(nonNullAmount.replace(/[,$$\s]/g, '').trim())
                },
                processDonation: function () {
                    var app = this
                    if (this.status === 'ready') {
                        this.updateStatus('processing', '')

                        var form = {
                            amount: this.getStrippedAmount(this.form.amount),
                            category: this.form.category,
                            city: this.form.city,
                            country: this.form.country,
                            email: this.form.email,
                            first: this.form.first,
                            frequency: this.form.frequency,
                            honor: this.form.honor,
                            honoree: this.form.honoree,
                            last: this.form.last,
                            paymentType: this.form.paymentType,
                            phone: this.form.phone,
                            premium: this.isPremium ? this.form.premium : false,
                            recurring: this.form.recurring,
                            state: this.form.state,
                            street: this.form.street,
                            zip: this.form.zip
                        }

                        if (this.form.recurring && (!! this.form.frequency)) {
                            this.stripe.createPaymentMethod({
                                type: 'card',
                                card: this.card,
                                billing_details: {
                                    email: this.form.email
                                }
                            }).then(function (result, email) {
                                if (result.error) {
                                    app.updateStatus('error', result.error.message)
                                } else {
                                    form.payment_method = result.paymentMethod.id
                                    if (window.dataLayer &&
                                        window.dataLayer.push &&
                                        (typeof window.dataLayer.push === 'function')
                                    ) {
                                        window.dataLayer.push({
                                            'event': 'Donation',
                                            'transactionId': result.paymentMethod.id,
                                            'transactionAffiliation': form.category +' - '+form.frequency+'ly',
                                            'transactionTotal': form.amount
                                        })
                                    }
                                    app.$http.post('/api/recurring', {form: form}).then(function (customer) {
                                        app.updateStatus('success', '')
                                        app.complete()
                                    }).catch(function (error) {
                                        app.updateStatus('error', 'Could not process recurring donation.')
                                    })
                                }
                            }).catch(function (error) {
                                console.log(error)
                                app.updateStatus('error', 'Could not contact payment processor. Please try again.')
                            })
                        } else {
                            // register the client
                            this.$http.post('/api/client', {form: form}).then(function (response) {
                                // tell Stripe about the registered client and tell it to process the card
                                app.stripe.confirmCardPayment(response.data.client, {
                                    payment_method: {
                                        card: app.card,
                                        billing_details: {
                                            email: app.form.email
                                        }
                                    }
                                }).then(function (result) {
                                    // if everything went well registering the client with Stripe then post the
                                    //  charge to be recorded in the database, send emails, etc.
                                    // note: app.complete is delayed in hopes that the user does not close the
                                    //  window without confirmation
                                    if (result.error) {
                                        app.updateStatus('error', result.error.message)
                                    } else {
                                        if (window.dataLayer &&
                                            window.dataLayer.push &&
                                            (typeof window.dataLayer.push === 'function')
                                        ) {
                                            window.dataLayer.push({
                                                'event': 'Donation',
                                                'transactionId': result.paymentIntent.id,
                                                'transactionAffiliation': form.category,
                                                'transactionTotal': form.amount
                                            })
                                        }
                                        // everything seems to have gone well, record it on the back-end
                                        app.$http.post('/api/record', {form:form}).then(function () {
                                            app.updateStatus('success', '')
                                            app.complete()
                                        }).catch(function (error) {
                                            // this is the worst case scenario
                                            // - Stripe charged them and something
                                            //  broke while we were trying to
                                            //  record it; hopefully the error
                                            //  log caught it
                                            app.updateStatus('success', '')
                                            app.complete()
                                        })
                                    }
                                })
                            })
                        }
                    }
                },
                complete: function () {
                    // display the success message until this setTimeout is over
                    setTimeout(function () {
                        app.updateStatus('complete', '')
                        if (usingSafari) {
                            document.getElementById('thankYou').style.display = 'block'
                            document.querySelector('.main').style.display = 'none'
                        }
                        // give vue time to render the dom changes from the status change
                        setTimeout(function () {
                            // scroll to the top of the donate, which should now have the thank you message
                            document.getElementById('thankYou').scrollIntoView({behavior: 'smooth'})
                            setTimeout(function () {
                                // hide hovering header
                                var header = document.getElementById('js-site-header')
                                if (header) {
                                    header.classList.add('site-header-up')
                                }
                            }, 600)
                        }, 200)
                    }, 600)
                },
                updateCategory: function (amount) {
                    if (amount < 25) {
                        this.form.category = this.form.category.replace('Book offer', 'Year-End 2020')
                    }
                },
                updateState: function () {
                    if (! usingSafari) return;
                    if (this.form.country === 'US') {
                        document.getElementById('state_select').style.display = 'block'
                        document.getElementById('state_free').style.display = 'none'
                    } else {
                        document.getElementById('state_select').style.display = 'none'
                        document.getElementById('state_free').style.display = 'block'
                    }
                },
                updateStatus: function (status, message) {
                    this.status = status
                    this.errorMessage = message
                    if (usingSafari) {
                        updateStatusDirectly(status)
                        updateErrorsDirectly(message)
                    }
                }
            },
            mounted: function () {
                // TODO better style for a background-color
                // https://stripe.com/docs/stripe-js
                // https://stripe.com/docs/js/appendix/style
                var style = {
                    base: {
                        color: "#333",
                        fontFamily: '"Helvetica Neue", Helvetica, sans-serif',
                        fontSmoothing: "antialiased",
                        fontSize: "16px",
                        "::placeholder": {
                            color: "#aab7c4"
                        },
                        'card': {
                            border: "1px solid #ddd",
                            backgroundColor: '#fff'
                        }
                    }
                }
                this.stripe = Stripe('pk_live_msJEWV9xPPsAdWvrw03PyNt3')
                this.stripeElements = this.stripe.elements()
                this.card = this.stripeElements.create('card', {style: style})
                this.card.mount('#card_element')
                this.$validator.validateAll()

                // Safari helper to enable/disable book selection based upon minimum donation amount
                if (usingSafari) {
                    const va = this
                    const amountElement = document.getElementById('amount')

                    amountElement.addEventListener('input', function (element) {
                        const value = va.getStrippedAmount(element.target.value)
                        const bookElementList = document.getElementsByName('book-selector')
                        const isDisabled = value < 100

                        for (const element of bookElementList) {
                            element.disabled = isDisabled
                        }

                        document.getElementById('book-container').style.display = isDisabled ? 'none' : ''
                        document.getElementById('book-requirement').style.display = !isDisabled ? 'none' : ''
                    })
                }
            }
        })
    </script>

