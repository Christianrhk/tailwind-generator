                        <div class="field">
                            <label class="label" for="amount">Gift amount</label>
                            <input class="format-currency" id="amount" type="text" v-validate="'required'" v-model.lazy="form.amount" v-money="money">
                        </div>
                    </div>
                                        <div class="full column">
                        <div class="field">
                            <label><input type="checkbox" v-model="form.honor"> &nbsp; This gift is in memory of someone</label>
