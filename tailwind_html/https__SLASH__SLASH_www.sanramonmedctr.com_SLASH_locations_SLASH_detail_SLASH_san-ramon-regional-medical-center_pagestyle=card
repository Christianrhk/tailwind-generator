<script>
    document.getElementById("fakeheader").style.height = "0px";
    function openLocMenu() {
        var locMenu = document.getElementById("openLocMenu");
        var locMenuIcon = document.getElementById("locMenuIcon");
        if (locMenu.style.display === "" || locMenu.style.display === "none") {
            locMenu.setAttribute('style', 'display: block !important');
            locMenu.setAttribute('aria-expanded', 'true');
            locMenuIcon.innerHTML = "-";
        } else {
            locMenu.setAttribute('style', 'display: none !important');
            locMenu.setAttribute('aria-expanded', 'false');
            locMenuIcon.innerHTML = "+";
        }
    }
    var locationId = "65d7df70-b415-4e20-b3c7-e6439e36dec3"
    var UrlName = "san-ramon-regional-medical-center"
</script>






