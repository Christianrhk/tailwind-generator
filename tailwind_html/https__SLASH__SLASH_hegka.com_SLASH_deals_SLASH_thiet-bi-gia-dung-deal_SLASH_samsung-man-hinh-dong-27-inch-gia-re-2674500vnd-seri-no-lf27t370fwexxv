    $(document).ready(function () {
        window.livewire.on('modalUpdate', (e) => {
            var image = e.image;
            var parentId = e.parentId;
            $('#kt_modal_update .form-select').val(parentId).trigger('change');
            $('#kt_modal_update .image-input-wrapper').css("background-image", 'url(' + image + ')');
        });
    });
