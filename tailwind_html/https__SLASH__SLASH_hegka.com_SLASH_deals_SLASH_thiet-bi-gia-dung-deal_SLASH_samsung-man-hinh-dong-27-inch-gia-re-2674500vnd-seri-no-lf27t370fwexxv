    function reCaptchaCallback(response) {
        Livewire.emitTo('user.deal.add-review', 'setRecaptcha', response)
    }
    </script>
    <script>
    window.livewire.on('addReviewDealModalHide', (e) => {
        $('#modal-add-deal-review').modal('hide');
    })
    const swiper = new Swiper(".mySwiper", {
        slidesPerView: 3,
        spaceBetween: 16,
        slidesPerGroup: 3,
        // loop: true,
        loopFillGroupWithBlank: true,
        pagination: {
            el: ".mySwiper-pagination",
            clickable: true
        },
        navigation: {
            nextEl: ".swiper-next",
            prevEl: ".swiper-prev"
        }

    });
    const rightSwiper = new Swiper(".rightSwiper", {
        navigation: {
            nextEl: ".rightSwiper-next",
            prevEl: ".rightSwiper-prev"
        }
    });
    rightSwiper.on('activeIndexChange', function(e) {
        $('.right-review .rightbar-header .slider-control .swiper-count').html(e.activeIndex + 1);
    });
    swiper.on('activeIndexChange', function(e) {
        $('.card-coupon .coupon-review .slider-control .current-pagination').html(e.snapIndex + 1);
    });


    $(document).on('click', '.card-coupon .coupon-box .coupon-image .coupon-reaction i.fa-heart', function() {
        $(this).toggleClass('active');
    })
    </script>
    
<script>
    // click to Question button and Back to Top For New Right bar
    const questionBtns = document.querySelectorAll('.right-control .item')
    const questionBtn = document.querySelectorAll('.right-control .item')[2];
    let newBackToTop = document.querySelectorAll('.right-control .item')[0];
    if(questionBtns.length) {
        newBackToTop.onclick = () => {
            window.scrollTo({
                top: 0,
                behavior: 'smooth',
            })
        }
        if(questionBtn) {
            questionBtn.onclick = (e) => {
                e.stopPropagation();
                const img = questionBtn.querySelectorAll('img');
                img[0].style.opacity = '0';
                img[1].style.opacity = '1';
                const tagBox = document.querySelector('.right-control .tags-box');
                if (!tagBox.classList.contains('active')) {
                    tagBox.classList.add('active');
                    img[0].style.opacity = '0';
                    img[1].style.opacity = '1';
                } else {
                    tagBox.classList.remove('active');
                    img[0].style.opacity = '1';
                    img[1].style.opacity = '0';
                }
            }

            // make a hover after clicking question button
            questionBtn.addEventListener('mouseover', () => {
                const img = questionBtn.querySelectorAll('img');
                img[0].style.opacity = '0';
                img[1].style.opacity = '1';
            })
            questionBtn.addEventListener('mouseout', () => {
                const tagBox = document.querySelector('.right-control .tags-box');
                const img = questionBtn.querySelectorAll('img');
                if (!tagBox.classList.contains('active')) {
                    img[0].style.opacity = '1';
                    img[1].style.opacity = '0';
                } else {
                    img[0].style.opacity = '0';
                    img[1].style.opacity = '1';
                }
            })
        }
        // Turn off Tags Box When clicking to the child of TagsBox
        const tagsInBox = document.querySelectorAll('.copyright a');
        tagsInBox.forEach(ele => {
            ele.onclick = () => {
                const itemLength = document.querySelectorAll('.right-control .item').length;
                const tagBox = document.querySelector('.right-control .tags-box');
                tagBox.classList.remove('active');
                const img = document.querySelectorAll('.right-control .item')[itemLength-1].querySelectorAll('img');
                img[0].style.opacity = '1';
                img[1].style.opacity = '0';
            }
        })

        // StopPropagation when clicking the button inside the tagsInBox
        const tagsBox = document.querySelector('.tags-box .copyright');
        const tagsInBoxBtn = document.querySelector('.copyright .btn-group .create-company');
        tagsBox.onclick = (e) => {
            e.stopPropagation();
        }
        tagsInBoxBtn.onclick = () => {
            const itemLength = document.querySelectorAll('.right-control .item').length;
            const tagBox = document.querySelector('.right-control .tags-box');
            tagBox.classList.remove('active');
            const img = document.querySelectorAll('.right-control .item')[itemLength-1].querySelectorAll('img');
            img[0].style.opacity = '1';
            img[1].style.opacity = '0';
        }
        // turn off Tags Box when clicking to the body
        document.body.onclick = () => {
            const itemLength = document.querySelectorAll('.right-control .item').length;
            const tagBox = document.querySelector('.right-control .tags-box');
            tagBox.classList.remove('active');
            const img = document.querySelectorAll('.right-control .item')[itemLength-1].querySelectorAll('img');
            img[0].style.opacity = '1';
            img[1].style.opacity = '0';
        }
    }
    $(document).on('click', '.dropdown .delete-deal', function (e) {
        $(this).parents('article').remove();
        $(this).parents('.swiper .swiper-slide').remove();
    })
    $(document).on('click', '.copy-button', function (e) {
        e.stopPropagation();
        e.preventDefault();
        var $input = $(this).parent().find('.copy-input');
        $input.select();
        document.execCommand("copy");
    })
    const isHover = (e) => e.parentElement.querySelector(":hover") === e;
    function showLikeBtn() {
        $(document).on('mouseenter','.newly-post', function () {
            $(this).find('.reaction-article-btn').removeClass('hidden');
        })
        $(document).on('mouseleave','.newly-post', function () {
            let $this = $(this);
            setTimeout(function () {
                $this.find('.reaction-article-btn').addClass('hidden');
            }, 1000)
        })
    }
    showLikeBtn()
</script>
</body>
</html>
