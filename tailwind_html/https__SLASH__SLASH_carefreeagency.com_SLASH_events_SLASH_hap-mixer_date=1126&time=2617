                })
    </script>
    
    <script type="text/javascript">
        $(document).on('click', '.redirect-to-link', function() {
            var url = $(this).attr('data-link');
            $("#externalLinkForm").attr('action', url);
            $("#externalLinkForm").submit();
        });
        $(document).ready(function() {
            $('.rtsdetector').click(function(event) {
                if ($(this).hasClass('guest-only')) {
                    event.preventDefault();
                    $('#fixed-ahip').removeClass('hidden');
                    return;
                }
                if ($(this).hasClass('not-rts')) {
                    event.preventDefault();
                    $('#fixed-ahip-2').removeClass('hidden');
                    return;
                }
            });
        });
    </script>

    <script src="/js/v2.js?id=e7309d6f1cda2e6712b1"></script>
    <script>
        document.addEventListener("DOMContentLoaded", () => {
            const externalLinkDisclaimers = document.getElementsByClassName('popover__disclaimer')
            if(externalLinkDisclaimers.length <= 0) {
                const el = document.getElementById("externalDisclaimerMessage")
                el.className = 'hidden'
            }else {
                for(let i = 0; i < externalLinkDisclaimers.length; i++){
                    const classes = externalLinkDisclaimers[i].className.split(' ').splice(0, 1).map(c => `tw-${c}`).join(' ');
                    externalLinkDisclaimers[i].className = `popover__disclaimer ${classes}`
                }
            }
        })
    </script>
</body>

</html>
