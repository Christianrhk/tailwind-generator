                   };
                   
                   rm_idnumber_exists(this,rm_ajax_url,data,"THIS ENTRY HAS ALREADY BEEN USED. PLEASE USE A DIFFERENT ONE");
                  
                 });
  jQuery("#form_2_1-element-13").change(function(){
                   var data = {
                           'action': 'idnumber_yoxla',
                           'phone': jQuery(this).val(),
                           'attr': 'data-rm-valid-username',
