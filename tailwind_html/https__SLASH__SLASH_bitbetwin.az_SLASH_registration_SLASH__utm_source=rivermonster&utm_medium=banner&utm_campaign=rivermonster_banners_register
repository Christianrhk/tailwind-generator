<script>
document.getElementById("form_2_1-element-13").value = "+1";
var readOnlyLength = $('#form_2_1-element-13').val().length;

$('#output').text(readOnlyLength);

$('#form_2_1-element-13').on('keypress, keydown', function(event) {
  var $field = $(this);
  $('#output').text(event.which + '-' + this.selectionStart);
  if ((event.which != 37 && (event.which != 39)) &&
    ((this.selectionStart < readOnlyLength) ||
      ((this.selectionStart == readOnlyLength) && (event.which == 8)))) {
    return false;
  }
});

</script>
