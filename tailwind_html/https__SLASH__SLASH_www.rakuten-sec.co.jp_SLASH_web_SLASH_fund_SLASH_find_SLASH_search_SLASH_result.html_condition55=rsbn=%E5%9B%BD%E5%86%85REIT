    /*
    function UpdateSelectedBunrui(val)
    {   
        var data_add = "<li class=\"pcm-fsearch-condition-select__tag__list pcm-fsearch-tag__list pcm-fsearch-tag__list--default\">";
            data_add += "<span>" + val + "</span><a href=\"Javascript:LoadData('')\" class=\"pcm-fsearch-tag__dismiss\" role=\"button\" aria-label=\"削除する\"><span class=\"rex-icon-close\" aria-hidden=\"true\"></span></a>";
