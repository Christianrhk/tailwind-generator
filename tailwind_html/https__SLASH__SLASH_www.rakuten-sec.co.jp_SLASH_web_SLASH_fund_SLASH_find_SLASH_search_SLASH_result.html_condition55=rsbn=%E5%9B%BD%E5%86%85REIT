        $("ul[data-tag='pcm-fsearch-rakuten-bunrui']").append(data_add);        
    }*/

    function AddBunrui()
    {   
        //clear first
        $("ul[data-tag='pcm-fsearch-rakuten-bunrui']").html("");

        var cnt = 0;
        $("div[data-tag='bunrui_list'] input:checkbox").each(function () {
            if($(this).data('ignore') != "yes"){
                if($(this).prop('checked')){
                    var text = GetBunruiText(this);    
                    //this will be the checkbox id
                    var removeID = "bunrui-selection" + cnt;
                    //this is the id of the selected bunrui text
                    var selectedID = "show-selected-bunrui" + cnt;
                    $(this).attr("id",removeID);

                    
                    var data_add = "<li class=\"pcm-fsearch-condition-select__tag__list pcm-fsearch-tag__list pcm-fsearch-tag__list--default\" id=\"" + selectedID + "\" >";
                    data_add += "<span>" + text + "</span><a href=\"#!\" class=\"pcm-fsearch-tag__dismiss\" role=\"button\" aria-label=\"削除する\"><span class=\"rex-icon-close\" aria-hidden=\"true\"></span></a>";
                    data_add += "</li>";
                    $("ul[data-tag='pcm-fsearch-rakuten-bunrui']").append(data_add);                    
                    $('#'+ selectedID).click(function(){ RemoveBunrui("#" + removeID); });
                }
            }
            cnt++;
        });   
    }

    function RemoveBunrui(id)
    {        
        $(id).prop('checked',false);      
        AddBunrui();
        LoadData('');
    }

    function GetBunruiText(id)
    {
        var text = $(id).next().html();
        var loc = text.indexOf("<span>");
        return text.substring(0,loc);        
    }

    /*
    function ClearSelectedConditions(reloadData)
    {
        if(reloadData === undefined)
            reloadData = true;

        $("#kw").val("");

        $("input:checkbox").each(function () {
            if($(this).prop('checked'))
                $(this).prop('checked',false);
        });
        
        console.log("reload:" + reloadData);
        if(reloadData)
            LoadData('');
    }
    */

</script>

