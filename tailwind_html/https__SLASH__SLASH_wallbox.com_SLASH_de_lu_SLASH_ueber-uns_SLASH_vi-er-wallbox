        (function () {
            let StoreLangChanger = (() => {
                let obj = {};
                obj.changeToHome = () => {
                    window.location = '/?update=1'
                }
                obj.changeToSamePage = () => {
                    let currentPath = window.location.pathname + window.location.search;
                    let storeMatch = currentPath.match(/^\/([a-z]{2}_(?:[a-z]{2}|catalog))\/.*/);
                    let storeCode = storeMatch[1];
                    currentPath = currentPath.replace(storeCode + '/', '');
                    window.location = '/?update=1'
                        + '&redirect=' + encodeURIComponent(currentPath)
                        + '&from_store=' + storeCode;
                }
                return obj;
            })();
            document.getElementById('country-switcher1527668002').addEventListener('click', function (e) {
                e.preventDefault();
                StoreLangChanger.changeToHome()
            })
        }());

    </script>


