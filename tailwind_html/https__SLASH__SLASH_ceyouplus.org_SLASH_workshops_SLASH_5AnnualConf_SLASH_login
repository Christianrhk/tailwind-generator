</div></span>
						<a id="location" href="https://ceyouplus.org/workshops/5AnnualConf/view">https://ceyouplus.org/workshops/5AnnualConf/view</a>
					</div>
				</div>
				<script>
					let $ = str => document.getElementById(str);

					if ("5AnnualConf" == "ADHDTreatment") {
						$('calendar_button').style.display = "none";
						}

					window.addEventListener('click', (e) => {
						if (e.target.id == "calendar_button" && !$('calendars').classList.contains('active_calendars')) {
							$('calendars').classList.add('active_calendars');
							$('calendar_button').classList.add('hidden');
							}
						else {
							$('calendars').classList.remove('active_calendars');
							$('calendar_button').classList.remove('hidden');
							}
						});

					function convertFromTimezoneToUserTimezone(time, webinar_timezone) {
						var offset = 0;
						switch (webinar_timezone) {
							case "IST": // UTC +2
								offset = -2;
								break;
							case "IDT": // UTC +3
								offset = -3;
								break;
							case "EDT" : case "EST" : //UTC - 4
                                // offset = 5;
                                offset = 4;
								break;
							case "CDT": // UTC -5
								offset = 5;
								break;
							case "CST": // UTC -6
								offset = 6;
								break;
                    		default : break;
						}
						var date = new Date();
						var diff = date.getTimezoneOffset() / 60;
						var calculatedOffset = 0;
						if (Math.sign(diff) == 1 && Math.sign(offset) == 1) {
							calculatedOffset = offset - diff;
						} else if (Math.sign(diff) == -1 && Math.sign(offset) == -1) {
							calculatedOffset = Math.abs(diff) + offset;
						}
						else{
							calculatedOffset = Math.abs(diff) + Math.abs(offset);
						}
						return calculatedOffset;
					}

					String.prototype.toGoogleDateFormat = function() {
						return this.replaceAll('-', '').replaceAll(':', '').replace(' ', 'T');
					}

					String.prototype.toOutlookDateFormat = function() {
						var date = new Date(this);
						return date.toISOString().replace(/-|:|\.\d+/g, '');
					}

					String.prototype.toYahooDateFormat = function() {
						var date = new Date(this);
						return date.toISOString().replace(/-|:|\.\d+/g, '');
					}

					String.prototype.toYahooDurationFormat = function() {
						var yahooHourDuration = this < 600 ? '0' + Math.floor((this / 60)) : Math.floor((this / 60)) + '';
						var yahooMinuteDuration = this % 60 < 10 ? '0' + this % 60 : this % 60 + '';
						return yahooHourDuration + yahooMinuteDuration;
					}

					var time = "2022-08-09 09:30:00";
					var tz = "EST";
					var duration = 60;
					var offset = convertFromTimezoneToUserTimezone(time, tz);

					var d = new Date(new Date(time).getTime() + (offset * 60 * 60 * 1000));
					time = d.getFullYear().toString() + "-" + ((d.getMonth() + 1).toString().length == 2 ? (d.getMonth() + 1).toString() : "0" + (d.getMonth() + 1).toString()) + "-" + (d.getDate().toString().length == 2 ? d.getDate().toString() : "0" + d.getDate().toString()) + " " + (d.getHours().toString().length == 2 ? d.getHours().toString() : "0" + d.getHours().toString()) + ":" + ((parseInt(d.getMinutes() / 5) * 5).toString().length == 2 ? (parseInt(d.getMinutes() / 5) * 5).toString() : "0" + (parseInt(d.getMinutes() / 5) * 5).toString()) + ":00";
					d = new Date(new Date(time).getTime() + duration * 60000);
					var end_time = d.getFullYear().toString() + "-" + ((d.getMonth() + 1).toString().length == 2 ? (d.getMonth() + 1).toString() : "0" + (d.getMonth() + 1).toString()) + "-" + (d.getDate().toString().length == 2 ? d.getDate().toString() : "0" + d.getDate().toString()) + " " + (d.getHours().toString().length == 2 ? d.getHours().toString() : "0" + d.getHours().toString()) + ":" + ((parseInt(d.getMinutes() / 5) * 5).toString().length == 2 ? (parseInt(d.getMinutes() / 5) * 5).toString() : "0" + (parseInt(d.getMinutes() / 5) * 5).toString()) + ":00";

					// Add to google:
					$("google").href = encodeURI("https://calendar.google.com/calendar/u/0/r/eventedit?text=" + $('title').innerText + "&dates=" + time.toGoogleDateFormat() + "/" + end_time.toGoogleDateFormat() + "&location=" + $('location').innerText + "&pli=1" + "&details=" + $('description').innerText);

					// Add to outlook:
					var href = encodeURI(
						'data:text/calendar;charset=utf8,' + [
							'BEGIN:VCALENDAR',
							'VERSION:2.0',
							'BEGIN:VEVENT',
							'URL:' + document.URL,
							'DTSTART:' + (time.toOutlookDateFormat() || ''),
							'DTEND:' + (end_time.toOutlookDateFormat() || ''),
							'SUMMARY:' + ($('title').innerText || ''),
							'DESCRIPTION:' + ($('description').innerText || ''),
							'LOCATION:' + ($('location').innerText || ''),
							'END:VEVENT',
				'END:VCALENDAR'].join('\n'));
					$('outlook').href = href;

					// Add to yahoo:
					href = encodeURI([
						'http://calendar.yahoo.com/?v=60&view=d&type=20',
						'&title=' + ($('title').innerText || ''),
						'&st=' + time.toYahooDateFormat(),
						'&dur=' + (duration.toString().toYahooDurationFormat() || ''),
						'&desc=' + ($('description').innerText || ''),
						'&in_loc=' + ($('location').innerText || '')
					].join(''));
					$("yahoo").href = href;
				</script>
