$( document ).ready(function() {

  
    $.js = function (el) {
        return $('[data-js=' + el + ']')
    };

    function carousel() {
      $.js('timeline-carousel').slick({
        infinite: false,
        arrows: true,
        dots: false,
        autoplay: false,
        speed: 1100,
        slidesToShow: 3,
        slidesToScroll: 3,
        responsive: [
          {
            breakpoint: 800,
            settings: {
              slidesToShow: 1,
              slidesToScroll: 1
            }
          }]
      });
    }
	carousel();
  
});  
</script>

