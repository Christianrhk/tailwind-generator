<script>
        //カテゴリーの親子階層リストをアコーディオンにする
        $(function() {
          $(".sp_footer_accordion .dlink").click(function(){
              var click = $("+ul",this);
              click.slideToggle();
              $("#accordion ul").not(click).slideUp();
              return false;
          });
        });
</script>


