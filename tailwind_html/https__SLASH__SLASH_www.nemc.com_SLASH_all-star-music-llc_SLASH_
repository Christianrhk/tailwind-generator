<script>
    $('.setSchoolBtn').click(function () {
       /* showLoader();*/
        var schCode, schName;
        schCode = encodeURIComponent($(this).attr('data-schCode'));
        schName = encodeURIComponent($(this).attr('data-schName'));
        var pageType = $(this).parents(".modal-content").attr("data-pageType");
        setSchoolCode(schCode, schName, pageType);
    });
    function setSchoolCode(schCode, schName, pageType) {
       /* hideLoader();*/
        var browser, urlrent;
        urlrent = "rents/cart";
        if (/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent)) {
            var browser = "M";
        } else {
            var browser = "D";
        }
        var SchoolOpenFlag = ('; ' + document.cookie).split(`; SchoolOpenFlag=`).pop().split(';')[0];
        if (SchoolOpenFlag == "") {
            SchoolOpenFlag = 0;
        }
        if (SchoolOpenFlag == 1) {
            
            $.ajax({
                type: "POST",
                /* url: "/RentalCheckout/RefreshCartItemsOnSchoolChange?refSchoolcode=" + encodeURIComponent(schCode) + " &refSchoolname=" + encodeURIComponent(schName),*/
                url: "/RentalCheckout/RefreshCartItemsOnSchoolChange?refSchoolcode=" + (schCode) + " &refSchoolname=" + (schName),
                datatype: 'json',

                success: function (result, status, xhr) {
                    $('.loader-overlay').hide();
                    /* $("#cartItemsPartial").html(result);*/
                    $('.modal').removeClass('show');
                    /*window.location.href = result;*/
                    $.ajax({
                        type: "POST",
                        url: "/Pages/AddToCart?instrumentCode=" + $('#instrumentCode').val() + " &priceScheduleId=" + $('#priceScheduleId').val() + " &browser=" + browser,
                        datatype: 'html',

                        success: function (result, status, xhr) {

                            //if (result == urlrent) {
                            //    window.location.href = result;
                            //}
                            if (result.includes(urlrent)) {
                                window.location.href = result;
                            }
                            else {
                                $("#showmessage").html(result);
                                $('#_ShowMessagePartial').find(".modal-content").attr("data-pageType", pageType);
                                $('#_ShowMessagePartial').addClass('show');
                                document.cookie = 'SchoolOpenFlag' + '=; Path=/; Expires=Thu, 01 Jan 1970 00:00:01 GMT;';
                                document.cookie = 'ModelCloseFlag' + '=; Path=/; Expires=Thu, 01 Jan 1970 00:00:01 GMT;';
                            }

                        },
                        error: function (error, status, xhr) {
                            $("#MsgModal").html(error);

                        }
                    });
                },
                error: function () {
                    console.log("Result: " + status + " " + error + " " + xhr.status + " " + xhr.statusText);
                }
            });
        }
        else {
          
            $.ajax({
                type: "POST",
               /* url: "/Pages/SetSchoolCookies?Schoolcode=" + encodeURIComponent(schCode) + "&Schoolname=" + encodeURIComponent(schName),*/
                url: "/Pages/SetSchoolCookies?Schoolcode=" + (schCode) + "&Schoolname=" + (schName),
                datatype: 'json',

                success: function (result, status, xhr) {
                    location.reload(true);
                },
                error: function () {
                    console.log("Result: " + status + " " + error + " " + xhr.status + " " + xhr.statusText);
                }
            });
        }

    }
    $("#ShowMoreSchoolPartial .modal-close").click(function () {
        hideLoader();
        var pageType = $(this).parents(".modal-content").attr("data-pageType");
        var SchoolOpenFlag = ('; ' + document.cookie).split(`; SchoolOpenFlag=`).pop().split(';')[0];
        if (SchoolOpenFlag == "") {
            SchoolOpenFlag = 0;
        }
        // console.log(SchoolOpenFlag);
        if (SchoolOpenFlag == 1) {
            var  browser, urlrent;

            urlrent = "rents/cart";
            if (/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent)) {
               var browser = "M";
            } else {
               var  browser = "D";
             }
            document.cookie = "ModelCloseFlag=1";
            $.ajax({
                type: "POST",
                url: "/Pages/AddToCart?instrumentCode=" + $('#instrumentCode').val() + " &priceScheduleId=" + $('#priceScheduleId').val() + " &browser=" + browser,
                datatype: 'html',

                success: function (result, status, xhr) {

                    //if (result == urlrent) {
                    //    window.location.href = result;
                    //}
                    if (result.includes(urlrent)) {
                        window.location.href = result;
                    }
                    else {
                        $("#showmessage").html(result);
                        $('#_ShowMessagePartial').find(".modal-content").attr("data-pageType", pageType);
                        $('#_ShowMessagePartial').addClass('show');
                        document.cookie = 'SchoolOpenFlag' + '=; Path=/; Expires=Thu, 01 Jan 1970 00:00:01 GMT;';
                        document.cookie = 'ModelCloseFlag' + '=; Path=/; Expires=Thu, 01 Jan 1970 00:00:01 GMT;';
                       
                    }

                },
                error: function (error, status, xhr) {
                    $("#MsgModal").html(error);

                }
            });
        }
        $('.modal').removeClass('show');
    });

</script>

<div id="showmessage">
    <div class="modal fade fixed top-0 left-0 hidden w-full h-full outline-none overflow-x-hidden overflow-y-auto"
         id="_ShowMessagePartial" tabindex="-1" role="dialog" aria-labelledby="_ShowMessagePartialLabel" aria-hidden="true" aria-model="true">
        <div class="modal-dialog  modal-dialog-centered modal-dialog-scrollable relative w-auto pointer-events-none" role="document">
            <div class="modal-content border-none shadow-lg relative flex flex-col w-full pointer-events-auto bg-white bg-clip-padding rounded-md outline-none text-current">
                <div class="modal-header flex flex-shrink-0 items-center justify-between p-4 border-b border-gray-200 rounded-t-md">
