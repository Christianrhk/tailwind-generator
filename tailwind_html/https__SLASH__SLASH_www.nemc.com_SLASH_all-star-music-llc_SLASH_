     $(document).ready(function () {
         /*CheckCartSessionExistOrNot1();*/

         $('#sessioninactiveclose').click(function () {
             window.location.href = "https://www.nemc.com/";
         });

         $('#sessioninactivecancel').click(function () {
             window.location.href = "https://www.nemc.com/";
         });
