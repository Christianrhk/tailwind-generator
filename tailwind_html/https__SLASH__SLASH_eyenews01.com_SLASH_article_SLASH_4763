        if(time == 0 || time == 1 || time == 7 || time == 8 || time == 9 || time == 11 || time == 12 || time == 13 || time == 16 || time >= 18 ){
        top_dialog_num = 33; 
        }else{
        a = Math.floor(Math.random()*(1200)) + 400; // 8 + 5
        top_dialog_num = 50;
        }

