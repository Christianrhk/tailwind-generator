<script type="text/javascript">
    jQuery(document).ready(function() {

        //header menu.
        jQuery('.open-menu-btn').click(function(e) {
            e.preventDefault();
            jQuery(this).addClass('show');
            jQuery('.menu-wrapper').addClass('show');
        });


        jQuery('.menu-close-btn').click(function(e) {
            e.preventDefault();
            jQuery('.open-menu-btn').removeClass('show');
            jQuery('.menu-wrapper').removeClass('show');
        });


        jQuery('.artist-menu-item').click(function(e) {
            e.preventDefault();
            jQuery(this).toggleClass('opened');
            jQuery('.artits-header-menu-section').slideToggle(500);
        });

        //masonary grid.
        var $grid = jQuery('.masonry-grid-home-page').imagesLoaded(function() {
            // init Masonry after all images have loaded
            $('.masonry-grid-home-page').removeClass('hidden');
            $('.home-masnory-skaleton').addClass('hidden');
            $grid.masonry({
                // options...
                itemSelector: '.grid-item',
                gutter: 5,
                fitWidth: true,
            });
        });



        var $grid_artist = jQuery('.masonry-grid-artist-page').imagesLoaded(function() {
            // init Masonry after all images have loaded
            $grid_artist.masonry({
                // options...
                itemSelector: '.grid-item',
                gutter: 5,
                fitWidth: true,
            });
        });

        var $grid_single_artist = jQuery('.masonry-grid-single-artist-page').imagesLoaded(function() {
            // init Masonry after all images have loaded

            // $('div.masonry-grid-single-artist-page').removeClass('hidden');

            $grid_single_artist.masonry({
                // options...
                itemSelector: '.grid-item',
                gutter: 5,
                fitWidth: true,
            });
        });


        $(window).on('resize', function() {
            $grid.masonry('destroy')
            $grid.masonry({
                // options...
                itemSelector: '.grid-item',
                gutter: 5,
                fitWidth: true
            });


            $grid_artist.masonry('destroy')
            $grid_artist.masonry({
                // options...
                itemSelector: '.grid-item',
                gutter: 5,
                fitWidth: true,
            });


            $grid_single_artist.masonry('destroy')
            $grid_single_artist.masonry({
                // options...
                itemSelector: '.grid-item',
                gutter: 5,
                fitWidth: true,
            });


        });


        $('.top-slider-section').on('init', function(event, slick) {
            $('div.top-banner-skaleton').addClass('hidden')
        });

        jQuery('.top-slider-section').slick({
            infinite: true,
            autoplay: true,
            speed: 500,
            autoplaySpeed: 2000,
            fade: true,
            cssEase: 'linear',
            arrows: false,
            dots: false
        });

        $('.logos-slider-section').on('init', function(event, slick) {
            $('div.logos-skaleton').addClass('hidden')
        });

        jQuery('.logos-slider-section').slick({
            infinite: true,
            autoplay: true,
            speed: 700,
            autoplaySpeed: 3000,
            vertical: true,
            slidesToShow: 1,
            slidesToScroll: 1,
            arrows: false,
            dots: false
        });
        $('.cases-slider-section').on('init', function(event, slick) {
            $('.cases-banner-skaleton').addClass('hidden')
        });
        jQuery('.cases-slider-section').slick({
            infinite: true,
            autoplay: true,
            speed: 500,
            autoplaySpeed: 2000,
            fade: false,
            cssEase: 'linear',
            arrows: false,
            dots: false
        });






        jQuery('input.tnp-name').attr('placeholder', 'Name');
        jQuery('input.tnp-email').attr('placeholder', 'Email');


        var prevScrollpos = window.scrollTop;
        jQuery(window).scroll(function() {
            let currentScrollPos = $(this).scrollTop();
            if (currentScrollPos > prevScrollpos && currentScrollPos > 100) {
                jQuery('.site-header').addClass('scrolled-header');
            } else {
                jQuery('.site-header').removeClass('scrolled-header');
            }

            prevScrollpos = currentScrollPos;
        });

        //load more transferblog articles page
        var lastScrollTop = 0;
        var ajax_requests = 1;


        $(window).on('scroll', function() {

            if ($('.masonry-grid-single-artist-page').length) {
                var st = $(this).scrollTop();
                if (st > lastScrollTop) {
                    // downscroll code
                    if (st >= $('.masonry-grid-single-artist-page').offset().top + $('.masonry-grid-single-artist-page').outerHeight() - window.innerHeight - 200) {

                        let limit = jQuery('input.load-more-artist-images').data('limit');
                        let offset = jQuery('input.load-more-artist-images').data('offset');
                        let post_id = jQuery('input.load-more-artist-images').data('post-id');
                        let total = jQuery('input.load-more-artist-images').data('total');
                        let newOffset = limit + offset;

                        // let pages = total / limit;
                        // pages = parseInt(pages) + 1;
                        // pages = pages * limit;
                        if (ajax_requests === 1) {

                            jQuery.ajax({
                                type: "post",
                                url: "https://charliecharlie.se/wp-admin//admin-ajax.php",
                                data: {
                                    action: 'load_more_artist_images_ajax',
                                    limit: limit,
                                    offset: offset,
                                    post_id: post_id
                                },
                                success: function(html) {
                                    if (html) {
                                        $grid_single_artist.masonry('destroy')

                                        $grid_single_artist.imagesLoaded(function() {
                                            $grid_single_artist.masonry({
                                                // options...
                                                itemSelector: '.grid-item',
                                                gutter: 5,
                                                fitWidth: true,
                                            });
                                        })

                                        jQuery('div.masonry-grid-single-artist-page').append(html);
                                        jQuery('input.load-more-artist-images').data('offset', newOffset);

                                        // $(document).scrollTop($('.masonry-grid-single-artist-page').outerHeight() - 200);

                                        // update new offset.
                                        ajax_requests = 1;

                                        //     jQuery('div.masonry-grid-single-artist-page').masonry('on', 'layoutComplete', function() {
                                        //     console.log('layout is complete');
                                        // });

                                        // console.log($('.masonry-grid-single-artist-page').outerHeight());
                                        // $(document).scrollTop($('.masonry-grid-single-artist-page').outerHeight() - 200);
                                    }
                                }
                            });

                        }

                        ajax_requests = 0;
                        // alert('end reached');
                    }
                } else {
                    // upscroll code
                }
                lastScrollTop = st;

            }
        });
    });
</script>
