<!-- calltouch request-->
<script type="text/javascript">
  jQuery(document).on('click', 'input[type="submit"]', function() {
    var m = jQuery(this).closest('form');
    var fio = m.find('input[name*="FIELDS[FIO_FID1]"]').val();
    var phone = m.find('input[name*="FIELDS[PHONE_FID1]"]').val();
    // var comment = m.find('textarea[name*="field_b6e202c"]').val();
    var ct_site_id = '47045';
    var sub = 'Заявка c ' + location.hostname;
    var ct_data = {
      fio: fio,
      phoneNumber: phone,
      // comment: comment,
      subject: sub,
      requestUrl: location.href,
      sessionId: window.call_value
    };
    console.log(ct_data);
    if (!!phone) {
      jQuery.ajax({
        url: 'https://api.calltouch.ru/calls-service/RestAPI/requests/' + ct_site_id + '/register/',
        dataType: 'json',
        type: 'POST',
        data: ct_data,
        async: false
      });
    }
  });
</script>
