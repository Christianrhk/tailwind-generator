  <div class="sticky_button hidden" productStickyContainer>
    <div class="sticky-info" stickyInfo></div>
    <div class="sticky-close-btn" stickyClose><svg xmlns="http://www.w3.org/2000/svg" aria-hidden="true" focusable="false" role="presentation" class="icon icon-close" fill="none" viewBox="0 0 20 20">
  <path fill-rule="evenodd" clip-rule="evenodd" d="M10 9.55556L19.5556 0L20 0.444444L10.4444 10L20 19.5556L19.5556 20L10 10.4444L0.444444 20L0 19.5556L9.55556 10L0 0.444444L0.444444 0L10 9.55556Z" fill="currentColor"/>
</svg>
</div>
    <div class="text-center button w-full p-3 mb-3 bg-black font-noway uppercase text-lg text-white"
         addToCartbtn>&lt;tc&gt;Ausverkauft&lt;/tc&gt;
</div>
  </div>
  
  <script>
    (function(){
      const mainAddToCartBtn = document.querySelector('[addToCart]');
      const productForm = document.querySelector('.buy-buttons');
      const stickyContainer = document.querySelector('[productStickyContainer]');
      const stickyAddBtn = document.querySelector('[addToCartbtn]');
	  const stickyInfoWrapper = document.querySelector('[stickyInfo]');
      const stickyVariant = document.querySelector('[stickyVariant]');
      const stickyOverlay = document.querySelector('[stickyOverlay]');
      const productTitle = document.querySelector('[stickyTitle]');
      const productPrice = document.querySelector('[stickyPrice]');
      const closestickyBtn = document.querySelector('[stickyClose]');
      // Wrappers for prepend elements
      const priceWrapper = document.querySelector('.price__wrapper');
      const variantWrapper = document.querySelector('.variant__wrapper');
      const titleWrapper = document.querySelector('.title__wrapper');
      const buttonWrapper = document.querySelector('.buy-buttons__wrapper');
      if (!stickyAddBtn) return;
      
      stickyAddBtn.addEventListener('click', function(){
        
        if (!stickyVariant) {
          addToCart();
          return;
        }
        
       const data = stickyVariant.querySelector('[type="application/json"]');
        
       if (!data) return;
        
       const variantData = JSON.parse(data.textContent);
        
       if (!variantData) return;
       
        if (variantData.length < 2) {
          addToCart();
          return;
        }
        
        // prevent page scroll
        document.querySelector('html').classList.add('remove-scroll');
        
        // add active class to the parent
        stickyContainer.classList.add('sticky-button__visible');
        
        
        // Enable overlay 
        if (stickyOverlay) stickyOverlay.classList.add('sticky-overlay__visible');
        
        // Show close button 
         if (closestickyBtn) closestickyBtn.classList.add('sticky-close-btn__visible');
        
        //prepend product info in the sticky Wrapper
        stickyContainer.prepend(stickyVariant);
        if (productForm) stickyContainer.append(productForm);
        if (stickyInfoWrapper) {
          stickyInfoWrapper.prepend(productPrice);
          stickyInfoWrapper.prepend(productTitle);
        }
        
        // hide the stickyButton 
        stickyAddBtn.style.display = 'none';
      
      });
      
      function addToCart() {
      	if (!mainAddToCartBtn) return;
      	mainAddToCartBtn.click(); 
      };
      
      function closeSticky() {
      	document.querySelector('html').classList.remove('remove-scroll');
        stickyAddBtn.style.display = 'block';
      	stickyOverlay.classList.remove('sticky-overlay__visible');
        stickyContainer.classList.remove('sticky-button__visible');
        if (closestickyBtn) closestickyBtn.classList.remove('sticky-close-btn__visible');
        if (buttonWrapper && productForm) buttonWrapper.prepend(productForm);
        if (priceWrapper && productPrice) priceWrapper.prepend(productPrice);
        if (titleWrapper && productTitle) titleWrapper.prepend(productTitle);
        if (variantWrapper && stickyVariant) variantWrapper.prepend(stickyVariant);
      }
      
      stickyOverlay.addEventListener('click', closeSticky);
      closestickyBtn.addEventListener('click', closeSticky)
      
    })();
    
    (function(){
      
      const productForm = document.querySelector('.buy-buttons');
      const stickyContainer = document.querySelector('[productStickyContainer]');
      var position = productForm.getBoundingClientRect();

      var observer = new IntersectionObserver(function(entries) {
        var position = productForm.getBoundingClientRect();
        // isIntersecting is true when element and viewport are overlapping
        // isIntersecting is false when element and viewport don't overlap
        if(entries[0].isIntersecting === true) {
          stickyContainer.classList.add('hide__mobile');
        }
        if(entries[0].isIntersecting === false && position.bottom >= 1) {
          stickyContainer.classList.remove('hide__mobile');
        }
        
      }, { threshold: [0] });

      observer.observe(productForm);
      
   
    })()
  </script>


  
  <script>
    (function(){
      const variantRadio = document.querySelector('[stickyvariant]');
      if (!variantRadio) return;

	  const fieldsets = [...variantRadio.querySelectorAll('fieldset')];

      if (!fieldsets.length > 1) return;
      fieldsets.forEach(el => {
      	const inputs = [...el.querySelectorAll('input')];
        if (inputs.length < 2) el.classList.add('visually-hidden'); // hide if input is less than 2
      });

    })();
  </script>



