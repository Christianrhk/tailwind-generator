        function myFunction() {
            var copyText = document.getElementById("current-page");
            copyText.select();
            copyText.setSelectionRange(0, 99999);
            document.execCommand("copy");
            $('#current-page').tooltip({title:"Copied: " + copyText.value});
        }

        function outFunc() {
            var tooltip = document.getElementById("myTooltip");
            tooltip.innerHTML = "Copy to clipboard";
        }
    </script>

