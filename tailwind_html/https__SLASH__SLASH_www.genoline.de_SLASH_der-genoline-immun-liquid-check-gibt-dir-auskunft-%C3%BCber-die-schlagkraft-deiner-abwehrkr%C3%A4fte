function fetchResult(searchTerm) {
  var queryData = '{ products(search: "' + searchTerm + '", limit: 4 ) { title uri slug } entries(search: "' + searchTerm + '", limit: 4 ) { title uri slug }}';
  console.log(queryData);
  console.log(JSON.stringify({query: queryData}))
  fetch('/search-api', {
    method: 'POST',
    headers: {
      'Content-Type': 'application/json',
      'Accept': 'application/json',
      'Authorization': 'Bearer kv6fkiCGtRyDDKq-2q3nK6L-ZIez8S3Y'
    },
    body: JSON.stringify({query: queryData})
  }).then(r => r.json()).then(data => afterFetch(data.data))
}

function afterFetch(data) {
  if (data.products.length || data.entries.length) {
    searchSuggestion.innerHTML = '';
    data.products.forEach((item, i) => {
      searchSuggestionHTML(item.title, item.uri, item.slug);
    });

    data.entries.forEach((item, i) => {
      searchSuggestionHTML(item.title, item.uri, item.slug);
    });
  }
}

function searchSuggestionHTML(title, uri , slug) {
  var querySelector = '.search-item.' + slug;
  if (!document.querySelector(querySelector)) {
    var item = document.createElement('a');
    item.className = 'search-item ' + slug;
    item.href = '/' + uri;
    item.innerHTML = title
    searchSuggestion.appendChild(item);
  }
}

const searchContainer = document.getElementById('search-wrapper');
const searchInput = document.getElementById('search-input');
const searchSuggestion = document.getElementById('search-suggestion');
const searchCloseOverlay = document.getElementById('close-btn-search');

searchInput.addEventListener("input", function(e) {
  var searchTerm = e.target.value;
  if (searchTerm.length > 2) {
    fetchResult(searchTerm);
  }
});

// Keyboard Input
searchInput.addEventListener("keydown", function(e) {
  var suggestionItems = document.querySelectorAll('.search-item');
  var focusedItem = document.querySelector('.search-item.is-focused');
  var index = Array.prototype.indexOf.call(suggestionItems, focusedItem);

  if (index != -1) {
    if (e.keyCode == 38) {
      //up
      if (index == 0) {
        focusedItem.classList.remove('is-focused');
      } else {
        var index = index - 1;
        focusedItem.classList.remove('is-focused');
        suggestionItems[index].classList.add('is-focused');
      }
    }
    if (e.keyCode == 40) {
      //down
      var index = (
        index == (suggestionItems.length - 1)
          ? 0
          : index + 1
      );
      focusedItem.classList.remove('is-focused');
      suggestionItems[index].classList.add('is-focused');
    }

    if (e.keyCode == 13) {
      e.preventDefault();
      window.location.href = focusedItem.href; //relative to domain
    }
  } else {
    if (e.keyCode == 40) {
      suggestionItems[0].classList.add('is-focused');
    }
  }
});
searchInput.addEventListener("focus", function(e) {
  searchSuggestion.classList.remove('hidden');
  if (window.innerWidth < 768) {
    searchContainer.classList.add('full-screen');
    searchCloseOverlay.classList.remove('hidden');
    toggleBodyOverflow();
  }
});

document.getElementById('search-btn').addEventListener("click", function(e) {
  toggleClass(searchContainer, 'hidden');
});

searchContainer.addEventListener("mouseleave", function(e) {
  window.setTimeout(function () {
    toggleClass(searchSuggestion,'hidden');
  },750);
});

searchCloseOverlay.addEventListener("click", function(e) {
  searchContainer.classList.remove('full-screen');
  searchCloseOverlay.classList.add('hidden');
  searchSuggestion.classList.add('hidden');
  toggleBodyOverflow();
});

function toggleClass(element, klass) {
  element.classList.toggle(klass);
}
</script></header><script>
//Fügt bei pageYOffset Klassen zum Header hinzu
document.addEventListener("DOMContentLoaded", function() {
  window.onscroll = function() {
    var nav = document.getElementById('header');
    if (window.pageYOffset > 1) {
      nav.classList.add("h-shadow","sticky", "z-20");
    } else {
      nav.classList.remove("h-shadow");
    }
  }
});


function toggleBodyOverflow() {
  document.body.classList.toggle('overflow-hidden');
}
// End Sticky Header JS //
