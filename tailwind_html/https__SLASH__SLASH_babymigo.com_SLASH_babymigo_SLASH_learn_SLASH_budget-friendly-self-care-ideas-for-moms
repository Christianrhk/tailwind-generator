<script>
    function toggleUserPersona() {
        $(document).ready(function () {

            var $due_date_div = $('#due_date_div'); //cache this selector into a variable to avoid a case of duplicity
            var $baby_birth_day_div = $('#baby_birth_day_div'); //cache this selector into a variable to avoid a case of duplicity

            //initially hide the two divs
            $due_date_div.hide();
            $baby_birth_day_div.hide();

            var user_persona_value = $('#user_persona').val();

            if (user_persona_value == 2 /* for Pregnant persona*/) {
                if ($baby_birth_day_div.is(':visible')) {
                    $baby_birth_day_div.hide();
                }
                if (!$due_date_div.is(':visible')) {
                    $due_date_div.show();
                    //$due_date_div.effect("highlight", {}, 1000);
                    //$due_date_div.css({paddingBottom: '1px'});
                }
            }
            else if (user_persona_value == 3 /* for Parent persona*/) {
                if ($due_date_div.is(':visible')) {
                    $due_date_div.hide();
                }
                if (!$baby_birth_day_div.is(':visible')) {
                    $baby_birth_day_div.show();
                    //$baby_birth_day_div.effect("highlight", {}, 1000);
                    //$baby_birth_day_div.css({paddingBottom: '1px'});
                }
            }
            else {
                /* for Trying to Conceive persona*/
                //Hide the two divs
                $baby_birth_day_div.hide();
                $due_date_div.hide();
            }
        });
    }

    function taglineDueDateValidator(elem) {

        var today = todaysDate();

        var due_date = $("#"+elem).val();
        if (Date.parse(due_date)) {
            var date = new Date(due_date);
            var days = getDaysBetween(today, date);
            if (days > 280) {
                $('#babyDueDateError').empty();
                $('#babyDueDateError').append("Error: Wrong due date; it must be within 9 months. Try again.");
                $("input[type=date]").val("");
            }
            else if (days < -1) {
                $('#babyDueDateError').empty();
                $('#babyDueDateError').append("Error: Wrong due date; it must be a future date. Try again.");
                $("input[type=date]").val("");
            }
            else {
                $('#babyDueDateError').empty();
            }
        }
    }

    function taglineBabyDateValidator(elem) {
        var today = todaysDate();
        var baby_birth_day = $("#"+elem).val();
        if (Date.parse(baby_birth_day)) {
            var date2 = new Date(baby_birth_day);
            var days = getDaysBetween(today, date2);
            if (days > -1) {
                $('#childDOBError').empty();
                $('#childDOBError').append("Error: Wrong date; it must be in the past. Try again.");
                $("input[type=date]").val("");
            }
            else {
                $('#childDOBError').empty();
            }
        }
    }

    function dueDateValidator() {

        var today = todaysDate();

        var due_date = $("#due_date").val();
        if (Date.parse(due_date)) {
            var date = new Date(due_date);
            var days = getDaysBetween(today, date);
            if (days > 280) {
                $('#due_date_error').empty();
                $('#due_date_error').append("Error: Wrong due date; it must be within 9 months. Try again.");
                $("input[type=date]").val("");
            }
            else if (days < -1) {
                $('#due_date_error').empty();
                $('#due_date_error').append("Error: Wrong due date; it must be a future date. Try again.");
                $("input[type=date]").val("");
            }
            else {
                $('#due_date_error').empty();
            }
        }
    }

    function babyDateDateValidator() {

        var today = todaysDate();

        var baby_birth_day = $("#baby_birth_day").val();
        if (Date.parse(baby_birth_day)) {
            var date2 = new Date(baby_birth_day);
            var days = getDaysBetween(today, date2);
            if (days > -1) {
                $('#baby_birth_day_error').empty();
                $('#baby_birth_day_error').append("Error: Wrong date; it must be in the past. Try again.");
                $("input[type=date]").val("");
            }
            else {
                $('#baby_birth_day_error').empty();
            }
        }
    }

    function getDaysBetween(firstDate, secondDate) {

        var startDay = new Date(firstDate);
        var endDay = new Date(secondDate);
        var millisecondsPerDay = 1000 * 60 * 60 * 24;

        var millisBetween = endDay.getTime() - startDay.getTime();
        var days = millisBetween / millisecondsPerDay;

        // Round down.
        return Math.floor(days);

    }

    function todaysDate() {
        return new Date();
    }
</script>



