  $('video').each(function () {
    if($(this).attr('autoplay') == "autoplay"){
      $(this).trigger('play');
    }
    $(this).attr('playsinline', true);
    $(this).attr('webkit-playsinline', true);
    $(this).attr('x5-video-player-type', 'h5');
  });
}
