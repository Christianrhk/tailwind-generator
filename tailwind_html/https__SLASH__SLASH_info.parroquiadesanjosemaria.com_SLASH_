<div class="container flex flex-col lg:flex-row content-center pt-5 pb-10">
    <div class="calendar-container w-full mr-0 md:mr-2 mb-4 md:mb-0">
        <link href='/css/CalendarMain.css' rel='stylesheet' />
        <style>
            #calendar {
                max-width: 1000px;
                margin: 0 auto;
            }
        
            .calendario--noticia {
                margin-top: 10px;
                display: grid;
                grid-template-columns: 70% 30%;
                gap: 10px;
                margin-left: 5%;
                margin-right: 5%;
            }
        
            .fc-event-main {
                width: 100%;
            }
        
            .fc-toolbar-title:first-letter {
                text-transform: capitalize;
            }
        
            .fc-direction-ltr .fc-toolbar>*> :not(:first-child) {
                margin: 0 !important;
            }
        
            .fc-header-toolbar>.fc-toolbar-chunk:last-child {
                display: flex;
                flex-direction: row-reverse;
            }
        
            .fc-view-harness.fc-view-harness-active {
                border-radius: 0.125rem;
                --tw-shadow: 0 4px 6px -1px rgba(0, 0, 0, 0.1), 0 2px 4px -1px rgba(0, 0, 0, 0.06);
                /* box-shadow: var(--tw-ring-offset-shadow, 0 0 #0000), var(--tw-ring-shadow, 0 0 #0000), var(--tw-shadow); */
                overflow: hidden;
            }
        
            .fc-view-harness.fc-view-harness-passive {
                border-radius: 0.125rem;
                --tw-shadow: 0 4px 6px -1px rgba(0, 0, 0, 0.1), 0 2px 4px -1px rgba(0, 0, 0, 0.06);
                /* box-shadow: var(--tw-ring-offset-shadow, 0 0 #0000), var(--tw-ring-shadow, 0 0 #0000), var(--tw-shadow); */
                overflow: hidden;
            }
        
            .fc-scroller.fc-scroller-liquid-absolute {
                overflow: hidden !important;
            }
        
            .fc-theme-standard .fc-scrollgrid {
                border: none;
            }
        
            .fc-theme-standard td,
            .fc-theme-standard th {
                border: none;
            }
        
            /* TODO  button today*/
            .fc-today-button {
                width: 100%;
                background-color: rgb(217, 217, 217) !important;
                border: none !important;
                outline: none !important;
                color: black !important;
                font-weight: bold !important;
                transition: all 0.3s;
            }
        
            .fc-today-button:hover {
                background-color: rgb(164, 164, 164) !important;
            }
        
        
        
            .noticia {
                display: block;
                border: 2px;
                border-color: aqua;
                border-radius: 3px;
                width: 100%;
                background-color: aquamarine;
                height: 100px;
            }
        
            @media (max-width: 1082px) {
                .calendario--noticia {
                    display: inline;
                }
            }
        
            .fc-col-header-cell {
                background-color: white !important;
                font-size: 12px !important;
                text-transform: uppercase;
                color: #99a1a7 !important;
                text-align: center !important;
                border-bottom: 2px solid rgba(166, 168, 179, 0.12) !important;
                line-height: 50px !important;
            }
        
            .fc-col-header-cell-cushion {
                font-weight: lighter !important;
            }
        
            .fc-event-time {
                width: 100%;
                font-size: small;
            }
        
            .fc-daygrid-body tr {
                background-color: white !important;
                border-bottom: 1px solid rgba(166, 168, 179, 0.12);
            }
        
        
            .fc-daygrid-day-frame {
                margin: 0 5px;
                border-right: 1px solid rgba(166, 168, 179, 0.12);
                font-size: 12px;
                box-sizing: border-box;
                color: #98a0a6;
                position: relative;
                z-index: 1;
            }
        
            .fc-event-title,
            .fc-daygrid-day-number {
                font-size: medium;
                font-weight: 100 !important;
            }
        
            /*  .fc-day-sun {
                    background-color: rgb(193, 193, 230);
                }
        
                .fc-day-sat {
                    background-color: rgb(240, 220, 220);
                } */
        
            .fc-event-title {
                width: 100%;
                /* force events to be one-line tall */
                white-space: nowrap !important;
                overflow: hidden !important;
                text-overflow: ellipsis;
            }
        
            .fc-prev-button.fc-button,
            .fc-next-button.fc-button {
                display: flex;
                justify-content: center;
                align-items: center;
                width: 2.5rem;
                height: 2.5rem;
                background: transparent !important;
                border: 0;
                border-radius: 100%;
                transition: all 0.3s ease-in-out;
            }
        
            .fc .fc-button:focus {
                outline: none !important;
                border: none !important;
                box-shadow: none !important;
            }
        
            .fc-prev-button.fc-button:hover,
            .fc-next-button.fc-button:hover {
                background: #e8e8e8 !important;
            }
        
            .fc-prev-button.fc-button span,
            .fc-next-button.fc-button span {
                color: gray;
                font-weight: bold;
            }
        
            .fc-daygrid-event {
                display: flex;
                flex-direction: column;
                align-items: start;
                justify-content: center;
                background: #4786ff;
                border: 0;
                border-radius: 4px;
                grid-column: 3/span 3;
                grid-row: 4;
                align-self: end;
                color: #fff;
                box-shadow: 0 10px 14px rgba(71, 134, 255, 0.4);
            }
        
            .fc-event-time {
                display: flex;
                justify-content: center;
                align-items: center;
                max-width: min-content !important;
            }
        
            .fc-daygrid-event-dot {
                display: none;
            }
        
            .fc-daygrid-event .fc-event-title {
                white-space: normal !important;
            }
        
            .modal-event.show {
                transform: translateX(0rem);
            }
        </style>
        
        
        <div id="calendar"></div>
        
        
        <script src='./js/locales-all.js'></script>    </div>
