                    <script>function replaceSvg(svg_image, replacement_url) {
    var $img = jQuery(svg_image);
    var imgID = $img.attr('id');
    var imgClass = $img.attr('class');
    var imgURL =  replacement_url; 
		var width = $img.attr('width');
		var height = $img.attr('height');
		var onclick = $img.attr('onclick');
    jQuery.get(imgURL, function(data) {
        // Get the SVG tag, ignore the rest
        var $svg = jQuery(data).find('svg');

        // Add replaced image's ID to the new SVG
        if(typeof imgID !== 'undefined') {
            $svg = $svg.attr('id', imgID);
        }
        // Add replaced image's classes to the new SVG
        if(typeof imgClass !== 'undefined') {
            $svg = $svg.attr('class', imgClass+' replaced-svg');
        }
				
        // Remove any invalid XML tags as per http://validator.w3.org
        $svg = $svg.removeAttr('xmlns:a');
				
        // Replace image with new SVG
				//$svg.attr('viewbox','0 0 10 10');
        $img.replaceWith($svg);
				viewbox = $svg.attr('viewbox');
				$svg = $svg.attr('height', height);
				$svg = $svg.attr('width',  width);
				$svg = $svg.attr('onclick', onclick);
				//$svg.attr('viewbox', viewbox);
				$(".replaced-svg").attr('viewBox', viewbox);
    }, 'xml');
}
$().ready(function() {
jQuery('img.svg').each(function(){
    var $img = jQuery(this);
		replaceSvg(this, $img.attr('src'));
		$("#calculator-shape-rectangle").click();	
});
});
calculator_do_change = function() {}

shapes = {
	"Owal": {
		params: 2,
		exp: function (p1, p2, ro) {
			return ((p1 - p2) * p2 + p2 * p2 * Math.PI / 4) * ro / 1000;
		},
		heigth: 40,
		width: 90
	},
	"Kwadrat": {
		params: 1,
		exp: function (p1, ro) {
			return p1 * p1 * ro / 1000;
		},
		heigth: 60,
		width: 60
	},
	"Semiowal": {
		params: 2,
		exp: function (p1, p2, ro) {
			return ((p1 - p2 / 2) * p2 + p2 * p2 * Math.PI / 8) * ro / 1000 ;
		},
		heigth: 40,
		width: 70
	},
	"Koło": {
		params: 1,
		exp: function (p1, ro) {
			return p1 * p1 * Math.PI / 4 * ro / 1000;
		},
		heigth: 60,
		width: 60
	},
	"Prostokąt": {
		params: 2,
		exp: function (p1, p2, ro) {
			return p1 * p2 * ro / 1000;
		},
		heigth: 40,
		width: 70
	},
	"Elipsa": {
		params: 2,
		exp: function (p1, p2, ro) {
			return p1 * p2 * Math.PI / 4 * ro / 1000;
		},
		heigth: 50,
		width: 80
	},
};

function calculator_exp2(shape, p1, p2, p3, ro) {
	if (shape.params == 1) 
		return shape.exp(p1, ro) - shape.exp(p1 - 2 * p3, ro);
	else 
		return shape.exp(p1, p2, ro) - shape.exp(p1 - 2 * p3, p2 - 2 * p3, ro);
}

var calculator_selected_shape;
var calculator_selected_shape_type = 1;
function calculator_choose_shape_type(which) {
	if (which == 1) {
		$("#calculator-shape-type-switch-1").css("background-color", "#e6e6e6");
		$("#calculator-shape-type-switch-2").css("background-color", "white");
		$("#calculator-shape-switch svg").children().attr("fill", "grey");
		calculator_selected_shape_type = 1;
		$(".calculator-param3").hide();
		//$("#calculator-size-content-svg-box-image").attr("fill", "grey");
	} else {
		$("#calculator-shape-type-switch-2").css("background-color", "#e6e6e6");
		$("#calculator-shape-type-switch-1").css("background-color", "white");
		$("#calculator-shape-switch svg").children().attr("fill", "white");
		calculator_selected_shape_type = 2;
		$(".calculator-param3").show();
		//$("#calculator-size-content-svg-box-image").attr("fill", "white");
	}
	calculator_do_change();
}

function calculator_choose_shape(shapename, element) {
	$("#calculator-shape-switch svg").css("background-color", "white");
	calculator_selected_shape = shapename;
	$(element).css("background-color", "#e6e6e6");
	calculator_show_params();
	calculator_do_change();
	//$("#calculator-size-content svg").hide();
	$("#calculator-size-content-svg-box-image").attr("xlink:href", "/wp-content/uploads/2021/05/" + element.id.substr(17) + ".svg");
	//replaceSvg($("#calculator-size-content-svg-box-image"), "/wp-content/uploads/2021/05/" + element.id.substr(17) + ".svg");
	//$("#calculator-size-content-svg-box-image").attr("heigth", "300px").attr("x", "100").attr("y", "100"). attr("width", "300px").attr("viewbox", "0 0 99.9 99.9");
	var shape = shapes[calculator_selected_shape];
	var width = shape.width;
	var heigth = shape.heigth;

	$('#calculator-yarrow').attr("y1", 100 + (100 - heigth) * 3/2).attr("y2", 400 - (100 - heigth) * 3/2).css("display", "block");
	$("#calculator-yarrow-uline").attr("y1", 100 + (100 - heigth) * 3/2 - 2)
	.attr("y2", 100 + (100 - heigth) * 3/2 - 2)
	.css("display", "block");
	$("#calculator-yarrow-dline").attr("y1", 400 - (100 - heigth) * 3/2 + 2)
	.attr("y2", 400 - (100 - heigth) * 3/2 + 2)
	.css("display", "block");
	$("#calculator-yarrow-head1").attr("y", 100 + (100 - heigth) * 3/2 - 5).css("display", "block");
	$("#calculator-yarrow-head2").attr("y", 400 - (100 - heigth) * 3/2 - 5).css("display", "block");

	$('#calculator-xarrow').attr("x1", 100 + (100 - width) * 3/2).attr("x2", 400 - (100 - width) * 3/2);
	$("#calculator-xarrow-lline").attr("x1", 100 + (100 - width) * 3/2 - 2)
	.attr("x2", 100 + (100 - width) * 3/2 - 2);
	$("#calculator-xarrow-rline").attr("x2", 400 - (100 - width) * 3/2 + 2)
	.attr("x1", 400 - (100 - width) * 3/2 + 2);
	$("#calculator-xarrow-head1").attr("x", 100 + (100 - width) * 3/2 - 5);
	$("#calculator-xarrow-head2").attr("x", 400 - (100 - width) * 3/2 - 5);
	calculator_show_params();
	//$("#" + element.id + "-big").show();
}

function calculator_show_params() {
	if (typeof(calculator_selected_shape) == "undefined") return;
	var shapetype = calculator_selected_shape_type - 1;
	if (shapetype == 1) 
		$('.calculator-param3').show();
	else
		$('.calculator-param3').hide();
	var shape = calculator_selected_shape;
	var params = shapes[shape].params;
	if (params >=2) 
		$('.calculator-param2').show();
	else 
		$('.calculator-param2').hide();

	if (params >=1) 
		$('#calculator-input-param1-container').show();
	else 
		$('#calculator-input-param1-container').hide();
	calculator_do_change();
}

function calculator_do_weight() {
	if (typeof(calculator_selected_shape) == "undefined") return;
	calculator_do_change = calculator_do_weight;
	var shapetype = calculator_selected_shape_type - 1;
	var shape = calculator_selected_shape;
	var ro = $('#calculator-input-ro').val();
	var param1 = $('#calculator-input-param1').val();
	var param2 = $('#calculator-input-param2').val();
	var param3 = $('#calculator-input-param3').val();
	var length = $('#calculator-input-length').val();
	var weight;
	if (shapetype == 0) {
		if (shapes[shape].params == 2 )
			weight = shapes[shape].exp(param1, param2, ro) * length;
		else 
			weight = shapes[shape].exp(param1, ro) * length;
	}
	else {
		weight = length * calculator_exp2(shapes[shape], param1, param2, param3, ro);
	}
	if (!isNaN(weight))
	$('#calculator-input-weight').val(parseFloat(weight).toFixed(2));
}

function calculator_do_length() {
	if (typeof(calculator_selected_shape) == "undefined") return;
	calculator_do_change = calculator_do_length;
	var shapetype = calculator_selected_shape_type - 1;
	var shape = calculator_selected_shape;
	var ro = $('#calculator-input-ro').val();
	var param1 = $('#calculator-input-param1').val();
	var param2 = $('#calculator-input-param2').val();
	var param3 = $('#calculator-input-param3').val();
	var weight = $('#calculator-input-weight').val();
	var length;
	if (shapetype == 0) {
		if (shapes[shape].params == 2 )
			length = 1 / shapes[shape].exp(param1, param2, ro) * weight;
		else 
			length = 1 / shapes[shape].exp(param1, ro) * weight;
	}
	else {
		length = 1 / calculator_exp2(shapes[shape], param1, param2, param3, ro) * weight;
	}
	if (!isNaN(length))
	$('#calculator-input-length').val(parseFloat(length).toFixed(2));
}
</script>		
    
