        <script>
    $( document ).ready(function() {
        $('.open-mobile-menu').on('click', function() {
            $('#mobile-menu').toggleClass('hidden');
            $('.buger-icon').toggleClass('block').toggleClass('hidden');
            $('.close-icon').toggleClass('block').toggleClass('hidden');
        });

        if ($('body > .h-full > div:first-child').attr('data-section') && $('body > .h-full > div:first-child').attr('data-section').indexOf('Banner') >= 0 && $('body > .h-full > div:first-child').attr('data-section').indexOf('Hero Banner') < 0) {
            $('body > .h-full > div:first-child').addClass('relative');
            $('body > .h-full > div:first-child').append(`<div class="nextElement arrow-container animated fadeInDown"><div class="arrow-2"><svg class="fill-current h-12 w-12 mx-auto mt-2" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20">
                                    <path d="M9.293 12.95l.707.707L15.657 8l-1.414-1.414L10 10.828 5.757 6.586 4.343 8z"></path>
                                </svg></div><div class="arrow-1 animated hinge infinite zoomIn"></div></div>`);

            $('body').on('click', '.nextElement', function() {
                $('html,body').animate({ scrollTop:$('body > .h-full > div:first-child + div').offset().top -  65}, 'slow');
            });
        }

        $.fn.is_on_screen = function(){
            var win = $(window);
            var viewport = {
                top : win.scrollTop(),
                left : win.scrollLeft()
            };
            viewport.right = viewport.left + win.width();
            viewport.bottom = viewport.top + win.height();

            var bounds = this.offset();
            if (!bounds) return null;
            bounds.right = bounds.left + this.outerWidth();
            bounds.bottom = bounds.top + this.outerHeight();

            return (!(viewport.right < bounds.left || viewport.left > bounds.right || viewport.bottom < bounds.top || viewport.top > bounds.bottom));
        };

        $(window).scroll(function () {

            if($('body > .h-full > div:first-child').is_on_screen()) {
                $('.nextElement').show();
            } else {

                $('.nextElement').hide();
            }
        });


        $('body').on('click', '.open-send-invite', function(e) {
            e.preventDefault();
            $('.modal').removeClass('flex')
                .addClass('hidden');
            $('.send-invite-modal').removeClass('hidden')
                .addClass('flex');
            $('#sendinvite_email').val($('#prefill_email').val());
        });

        $('.sendinvite-submit').on('click', function(e) {

            e.preventDefault();
            if ($('#sendinvite-form1').valid()) {

                $(this).text('Loading...').addClass('opacity-25 pointer-events-auto cursor-wait');

                var formData = new FormData();
                var data1 = $('#sendinvite-form1').serializeArray();
                for (var i = 0; i < data1.length; i++)
                    formData.append(data1[i].name, data1[i].value);

                $('#sendinvite-form1 input, #sendinvite-form2 checkbox').prop('disabled', true);

                fetch(SITE_URL + "/parent/send-invite", {
                    method: "POST",
                    headers: {
                        'X-CSRF-TOKEN': $('meta[name="csrf-token"]').attr('content')
                    },
                    body: formData,
                })
                    .then(response =>  response.json())
                    .then(response => {
                        if (response.status) {
                            $('.sendinvite-error, .sendinvite-title, .paging').hide();
                            $('.sendinvite-step1').hide();
                            $('.sendinvite-step2').fadeIn();
                            parentId = response.data.id;

                            $.get(SITE_URL + "/parent/dashboard", function( data ) {
                                $('#children-list').replaceWith($(data).find('#children-list').children());
                                $('.parent-menu').replaceWith($(data).find('.parent-menu').children());
                            });
                        } else {
                            $('.sendinvite-error').show();
                            $('.sendinvite-error .list-disc').html('');
                            var errorsList = response.message.split("\n");
                            errorsList.forEach(error => {
                                if (error) {
                                    $('.sendinvite-error .list-disc').append('<li>' + error + '</li>');
                                }
                            });
                        }

                        $('#sendinvite-form1 input, #sendinvite-form1 checkbox').prop('disabled', false);
                        $(this).text('Send Invite').removeClass('opacity-25 pointer-events-auto cursor-wait');
                    });
                }
        });


        $('body').on('click','.re-send-invite', function(e) {
            e.preventDefault();
            var student_id = $(this).attr('data-id');
            var formData = new FormData();
            formData.append('student_id', student_id);
            fetch(SITE_URL + "/parent/re-sent-invite", {
                    method: "POST",
                    headers: {
                        'X-CSRF-TOKEN': $('meta[name="csrf-token"]').attr('content')
                    },
                    body: formData,
                })
                    .then(response =>  response.json())
                    .then(response => {
                        if (response.status) {
                            alert('Invitation email has been resent.');
                        } else {
                            alert(response.message);
                        }
                    });
        });

        $('body').on('click','.login-show-password', function(e) {
            $('.login-show-password').hide();
            $('.login-hide-password').show();
            $('#lpassword').prop('type', 'text');
        });

        $('body').on('click','.login-hide-password', function(e) {
            $('.login-hide-password').hide();
            $('.login-show-password').show();
            $('#lpassword').prop('type', 'password');
        });

        
        Validate('.form-validate', {
            errorElement: "span",
            errorClass: "invalid text-red-500 text-sm",
            errorPlacement: function(error, element) {
                error.appendTo( element.parent() );
            }
        });
    });
    </script>
</body>

</html>
