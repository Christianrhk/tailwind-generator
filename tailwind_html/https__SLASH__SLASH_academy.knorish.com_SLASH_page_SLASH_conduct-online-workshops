        <div id="goToTop" class="btn kno-btntop"><i class="fa fa-caret-up"></i></div>

    
        <script src="https://knorish-cdn.azureedge.net/public/d0bda472-e00f-493e-b811-21729901b034/jsfile/626um.js"></script>
    <script>
var floatbox = document.createElement("div");
floatbox.classList.add("scrollToTopBtn");
document.querySelector('body').appendChild(floatbox);
var scrollToTopBtn = document.querySelector(".scrollToTopBtn");
var element = document.getElementById('floatbtn').innerHTML;
scrollToTopBtn.innerHTML = element;
var rootElement = document.documentElement

function handleScroll() {
  // Do something on scroll
  var scrollTotal = rootElement.scrollHeight - rootElement.clientHeight
  if ((rootElement.scrollTop / scrollTotal ) > 0.05) {
    // Show button
    scrollToTopBtn.classList.add("showBtn")
  } else {
    // Hide button
    scrollToTopBtn.classList.remove("showBtn")
  }
}

function scrollToTop() {
  // Scroll to top logic
  rootElement.scrollTo({
    top: 0,
    behavior: "smooth"
  })
}
scrollToTopBtn.addEventListener("click", scrollToTop)
document.addEventListener("scroll", handleScroll)
</script>

</body>
</html>