    <script>
    $(document).ready(function(){

        $('body').on('click', '#thing_to_click', function() {
           // alert('click');
            var username = $('#clubusername').val();
			if(!$.trim(username.value).length) {
              	var untocheck = username.toUpperCase();
                if (untocheck == 'DYBFC' ) {
                    window.location = "https://core-37.com/collections/darlington-young-bulls"; 
                }
                else if (untocheck == 'RCWFC' ) {
                    window.location = "https://core-37.com/collections/racing-club-warwick-fc";
                }
                else if (untocheck == 'RCWSHOP' ) {
                    window.location = "https://core-37.com/collections/racing-club-warwick-fc";
                }
                else if (untocheck == 'NSRFC' ) {
                    window.location = "https://core-37.com/collections/north-shields-rfc";
                }
                else if (untocheck == 'NBFC' ) {
                    window.location = "https://core-37.com/collections/newcastle-benfield-juniors-fc";
                } 
                else if (untocheck == 'KYPC' ) {
                    window.location = "https://core-37.com/collections/killingworth-young-peoples-club";
                } 
                else if (untocheck == 'DHJFC' ) {
                    window.location = "https://core-37.com/collections/dulwich-hamlet-fc";
                } 
                else if (untocheck == 'NWR' ) {
                    window.location = 'https://core-37.com/collections/nascot-wood-rangers';
                } 
                else if (untocheck == 'SNBJ--' ) {
                    window.location = 'https://core-37.com/collections/junior-south-north-bulls';
                } 
                else if (untocheck == 'SNBS--' ) {
                    window.location = 'https://core-37.com/collections/senior-south-north-bulls';
                } 
                else if (untocheck == 'SNBW--' ) {
                    window.location = 'https://core-37.com/collections/womens-south-north-bulls';
                } 
                else if (untocheck == 'SNBYB--' ) {
                    window.location = 'https://core-37.com/collections/young-bulls-south-north-bulls';
                }
                else {
                   alert('Club not found');
                }
            }

        });
    });
    </script>
