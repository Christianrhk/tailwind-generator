<script>
    $('#reg-otp2').hide();
    $('#regOTP').hide();
    $('#loginOTP').hide();
    $('#not-registerd').hide();
    // setInterval(function(){
    //   load();
    // },3000);
    $("#login-opt1").click(function() {
    var mobile = $("#form3Example3").val();
    if (!mobile) {
      $("#mnum").text("Please Enter Mobile No.");
      $("#mnum").css({"color":"red"});
      
      setTimeout(function(){ 
        $("#mnum").text("Mobile Number");
        $("#mnum").css({"color":"#111"}); 
      }, 2000);

    }else{
      $.ajax({
            type: "GET",
            data: {mobile:mobile},
            url: 'https://leoon.in/home/sent_otp',
            success: function(response) {
              // alert(response);
                if (response == "success") {
                  countdown();
                  $('#UserLoginModal').removeClass('show');
                  $('#UserLoginModal').css({"visibility":"hidden"});
                  $('#GetOTPModal').addClass('show');
                  $('#GetOTPModal').addClass('visible');
                  $('#loginOTP').show();
                  $('#regOTP').hide();
                  $('.modal-backdrop').removeClass('show');
                  $('.fade').removeClass('modal-backdrop');
                  // $('.modal-backdrop').addClass('invisible');
                  // $('.modal-backdrop').removeClass('modal-backdrop');
                  // $('.modal-backdrop').addClass('show');
                  //     position: 'top-center',
                  //     icon: 'success',
                  //     title: 'Otp Sent successfully',
                  //     showConfirmButton: false,
                  //     timer: 1500
                  //   })
                }else{
                  $('#login-opt1').hide();
                  $('#not-registerd').show();
                  $('#reg-otp2').show();
                  $('#regOTP').show();
                  $('#loginOTP').hide();
                  // Swal.fire({
                  //     position: 'top-center',
                  //     icon: 'error',
                  //     title: 'Not Registered',
                  //     showConfirmButton: false,
                  //     timer: 1500
                  //   })
                }
                // alert(response);
            },
            error:function(){
                alert("Something went wrong.");
            }                    
        });
    }
  });
    $(document).on('click', '#close-otp', function(){
      $('#GetOTPModal').removeClass('show');
      $('#GetOTPModal').removeClass('visible');
      $('.modal-backdrop').removeClass('show');
    })
  $("#reg-otp2").click(function() {
    var name = $("#form3Example1").val();
    var email = $("#form3Example2").val();
    var mobile = $("#form3Example3").val();
      $.ajax({
            type: "GET",
            data: {name:name,email:email,mobile:mobile},
            url: 'https://leoon.in/home/sent_otp_register',
            success: function(response) {
              // alert(response);
                if (response == "success") {
                  countdown();
                  $('#UserLoginModal').removeClass('show');
                  $('#UserLoginModal').css({"visibility":"hidden"});
                  $('#GetOTPModal').addClass('show');
                  $('#GetOTPModal').addClass('visible');
                  $('.modal-backdrop').removeClass('show');
                  $('.fade').removeClass('modal-backdrop');
                  // $('.modal-backdrop').addClass('invisible');
                  // $('.modal-backdrop').removeClass('modal-backdrop');
                  // $('.modal-backdrop').addClass('show');
                  //     position: 'top-center',
                  //     icon: 'success',
                  //     title: 'Otp Sent successfully',
                  //     showConfirmButton: false,
                  //     timer: 1500
                  //   })
                }else{
                  $('#login-opt1').hide();
                  $('#not-registerd').show();
                  $('#reg-otp2').show();

                  // Swal.fire({
                  //     position: 'top-center',
                  //     icon: 'error',
                  //     title: 'Not Registered',
                  //     showConfirmButton: false,
                  //     timer: 1500
                  //   })
                }
                // alert(response);
            },
            error:function(){
                alert("Something went wrong.");
            }                    
        });
  });
  function countdown() {
        var counter = 20;
        var interval = setInterval(function() {
            counter--;
            // Display 'counter' wherever you want to display it.
            if (counter <= 0) {
                clearInterval(interval);
                $('#timers').html("<h3 onclick='countdown()' style='color:#862421;cursor:pointer'>Resend OTP</h3>");
                $('#timess').css({"display":"none"});
                return;
            }else{
                $('#timers').html(" ");  
                $('#timess').css({"display":"block"});
                $('#timess').html("Resend OTP : " +counter);
                // console.log("Timer --> " + counter);
            }
        }, 1000);
      }
</script>

