        <script>
         $(document).ready(function(){
          $("#all_Show").hide();
          $("#all_ShowBtn").hide();
          $(document).click(function(){
          $("#all_HideBtn").click(function(){
            $("#all_Hide").hide();
            $("#all_Show").slideDown();
            $("#all_ShowBtn").show();
            $("#all_HideBtn").hide();
          });

          $("#all_ShowBtn").click(function(){
            $("#all_Show").hide();
            $("#all_Hide").slideDown();
            $("#all_ShowBtn").hide();
            $("#all_HideBtn").show();
          })
         })
         })
       </script>
