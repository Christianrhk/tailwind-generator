<script type="text/javascript">
  document.addEventListener("DOMContentLoaded", function (event) {
  const linkColor = document.querySelectorAll('#details .gram')

    function colorLink() {
        if (linkColor) {
            linkColor.forEach(l => l.classList.remove('active'))
            this.classList.add('active')
        }
    }
    linkColor.forEach(l => l.addEventListener('click', colorLink))
    });
  </script>
  <script type="text/javascript">
       $(document).ready(function(){
        $("#clickToShow121").hide();
        $(document).on('click','.clickBtn121',function(){
        // $(".clickBtn121").click(function(){
          $(".clickBtn121").hide();
          $("#clickToShow121").slideDown();
        })
        $(".addCart").click(function(){
          $("#clickToShow121").hide();
          $(".clickBtn121").slideDown();
        })
       })
     </script>
