                            location.reload();

                        }, 2000);

                }else{

                  Swal.fire({

                    position: 'top-center',

                    icon: 'error',

                    title: 'Failed.',

                    showConfirmButton: false,

                    timer: 1500

                  })



                }

                 

            },

            error:function(){

                alert("Something went wrong.");

            }                    

        });

                get_cart();



    });

 

