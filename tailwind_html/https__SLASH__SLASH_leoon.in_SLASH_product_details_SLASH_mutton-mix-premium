    <script>

        $(document).ready(function() {

  

            setTimeout(function() {

                $('#ctn-preloader').addClass('loaded');

                // Una vez haya terminado el preloader aparezca el scroll

                $('body').removeClass('no-scroll-y');



                if ($('#ctn-preloader').hasClass('loaded')) {

                  // Es para que una vez que se haya ido el preloader se elimine toda la seccion preloader

                  $('#preloader').delay(1000).queue(function() {

                    $(this).remove();

                  });

                }

            }, 3000);

          

        });

    </script>



<!-- Resend Counter Start -->



<script type="text/javascript">

    function countdown() {

        var counter = 20;

        var interval = setInterval(function() {

            counter--;

            // Display 'counter' wherever you want to display it.

            if (counter <= 0) {

                clearInterval(interval);

                $('#timers').html("<h3 onclick='countdown()' style='color:#862421;cursor:pointer'>Resend OTP</h3>");

                $('#timess').css({"display":"none"});

                return;

            }else{

                $('#timers').html(" ");  

                $('#timess').css({"display":"block"});

                $('#timess').html("Resend OTP : " +counter);

                // console.log("Timer --> " + counter);

            }

        }, 1000);

      }

      // countdown();

</script>



<!-- Resend Counter End -->



<script type="0208d782a0ae677d2a099056-text/javascript" src="https://maps.googleapis.com/maps/api/js?libraries=places&key=AIzaSyCdlb2k17PxjKjOcvWMhrY1GobweAGp4Xs"></script>

<input type="hidden" id="lati" name="" value="">

<input type="hidden" id="lngi" name="" value="">

<input type="hidden" id="cur_lc" name="" value="">

<script type="text/javascript">

    function getLocation(){

        // alert('1');

        if (navigator.geolocation) {

            navigator.geolocation.getCurrentPosition(showPosition);

            // alert(window.lat);

        } else { 

            x.innerHTML = "Geolocation is not supported by this browser.";

        }

    }

    function showPosition(position) {

        var lat = position.coords.latitude; //latitude

        var lng = position.coords.longitude; //longitude

        $.ajax({

                type: "POST",

                url: "https://maps.googleapis.com/maps/api/geocode/json?latlng="+lat+","+lng+"&sensor=true&key=AIzaSyCdlb2k17PxjKjOcvWMhrY1GobweAGp4Xs",

                success: function(data){

                var data_num=data.results[0].address_components.length-1;

                var pin = data.results[0].address_components[data_num].long_name;

                var state = data.results[0].address_components[data_num-2].long_name;

                var city = data.results[0].address_components[data_num-3].long_name;

                var address = data.results[0].formatted_address;

                // console.log(data);

                $("#cur_lc").val(city);

                document.cookie = 'city='+city;

                // document.cookie = 'city1='+city;

                sessionStorage.setItem("city",city);

                sessionStorage.getItem(city,city);

                // console.log(city);

                document.cookie = 'state='+state;

                document.cookie = 'pin='+pin;

                document.cookie = 'address='+address;

            }

        });

    }

    getLocation();

    if ("geolocation" in navigator){ //check geolocation available 

            //try to get user current location using getCurrentPosition() method

            navigator.geolocation.getCurrentPosition(function(position){

            document.getElementById('lati').value=position.coords.latitude;

            document.getElementById('lngi').value=position.coords.longitude;

            // alert('pos');

            // alert(position);

        });

    }else{

        console.log("Browser doesn't support geolocation!");

    }

</script>