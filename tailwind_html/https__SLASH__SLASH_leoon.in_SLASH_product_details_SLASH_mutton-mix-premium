                          <script type="text/javascript">
                $(document).ready(function() {
                 $('#chik154').change(function() {
                    var gramn = $('#chik154').val();
                     splitArr = gramn.replace(/(\,\s+)/g, ',').split(',');
                       l = splitArr.length;
                       var ab =[];
                      for (var i =0; i<l; i++) {
                              product_id =splitArr[0];
                              current_price =splitArr[1];
                              info_id =splitArr[2];
                              sell_price =splitArr[3];
                      }
                      // alert(gramn);
                  $(".chik1"+product_id).html('<i class="far fa-rupee-sign"></i>'+' '+current_price);
                  $(".chik2"+product_id).html('<s>&#8377;'+' '+sell_price+'</s>');
                  $("."+product_id).val(info_id);
                  
              });
                });
               </script>
