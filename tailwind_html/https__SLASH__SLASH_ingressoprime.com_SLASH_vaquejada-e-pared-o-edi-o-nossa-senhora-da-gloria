<script>
new Vue({
  el:"#header",
  data() {
    return {
      menu: false,
      account: false,
      search: '',
      events: [],
      date: {
        start: '',
        end: '',
      }
    }
  },
  mounted() {
    $('#datepicker').daterangepicker({
      "locale": {
          "format": "DD/MM/YYYY",
          "separator": " - ",
          "applyLabel": "Selecionar",
          "cancelLabel": "Cancelar",
          "fromLabel": "De",
          "toLabel": "Para",
          "customRangeLabel": "Custom",
          "weekLabel": "S",
          "daysOfWeek": [
              "Dom",
              "Seg",
              "Ter",
              "Qua",
              "Qui",
              "Sex",
              "Sab"
          ],
          "monthNames": [
              "Janeiro",
              "Fevereiro",
              "MarÃ§o",
              "Abril",
              "Maio",
              "Junho",
              "Julho",
              "Augosto",
              "Setembro",
              "Outubro",
              "Novembro",
              "Dezembro"
          ],
          "firstDay": 1
      },
      "minDate": new Date(),
      "drops": "auto",
    }, (start, end) => {
      this.date.start = start._d;
      this.date.end = end._d;
    });
    axios.get('https://api.evenyx.io/v1/events', {
      headers: {
        'Content-Type': 'application/json',
        'Authorization': 'Bearer x6H0PwXRtvfE589omde4pjsGuFDIgZK1',
      }
    }).then((res) => {
      this.events = res.data;
    }).catch((err) => {
      console.error(err);
    })
  },
  computed: {
    getFiltered() {
      const sRegex = new RegExp(this.search, 'i');
      const toISOFormat = date => new Date(date).toISOString().slice(0, 10);
      const filteredDate = (events) => {
        if (this.date.start === '' && this.date.end === '') {
          return events;
        }
        return events.filter(
          item => toISOFormat(item.startDateTime) >= toISOFormat(this.date.start)
            && toISOFormat(item.endDateTime) <= toISOFormat(this.date.end),
        )
      };
      return filteredDate(this.events.filter(
          item => item.title.toLowerCase().indexOf(this.search.toLowerCase()) !== -1
            || item.city.toLowerCase().indexOf(this.search.toLowerCase()) !== -1
            || sRegex.test(item.attractions ? item.attractions.map(attr => attr.name) : ''),
      ));
    },
  }
});
</script>
