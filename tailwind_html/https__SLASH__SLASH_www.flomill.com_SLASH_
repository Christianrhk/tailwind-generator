			/* Basic Rules */
			.switch input { 
				display:none;
			}
			.switch {
				display:inline-block;
				width:40px;
				height:20px;
				margin:2px;
				transform:translateY(50%);
				position:relative;
				
			}
			/* Style Wired */
			.slider {
				position:absolute;
				top:0;
				bottom:0;
				left:0;
				right:0;
				border-radius:50px;
				cursor:pointer;
				border:4px solid transparent;
				overflow:hidden;
				transition:.4s;
				background:#9FA5BC;
			}
			.slider:before {
				position:absolute;
				content:"";
				width:50%;
				height:100%;
				background:#fff;
				border-radius:50px;
				transform:translateX(-20px);
				transition:.4s;
			}

			input:checked + .slider:before {
				transform:translateX(4px);
				background:#fff;
			}
			input:checked + .slider {
				background:#FA3D7F;9
			}

			/* Style Flat */
			.switch.flat .slider {
			 box-shadow:none;
			}
			.switch.flat .slider:before {
			  background:#FFF;
			}
			.switch.flat input:checked + .slider:before {
			 background:white;
			}
			.switch.flat input:checked + .slider {
			  background:#FA3D7F;
			}
