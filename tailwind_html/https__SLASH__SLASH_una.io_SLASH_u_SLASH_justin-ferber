  $( document ).ready(function() {
		$( ".bx-btn" ).click(function() {
			var elm=$(this);
			elm.attr('ant-click-animating-without-extra-node', true);
			setTimeout(function() { elm.attr('ant-click-animating-without-extra-node', false);}, 1000); 
		});
   });
</script>

