            }).always(function () {
            });

            $(this).blur();
            return false;
        });

        var submitContactForm = function () {
            $('#8f3dbc65c9f1a1efcb1082af135cf772-form button[type=submit]').attr('disabled', 'disabled');
            $("#st-result-menu a[href='#st-link-load'] .spovr").removeClass("d-none");
