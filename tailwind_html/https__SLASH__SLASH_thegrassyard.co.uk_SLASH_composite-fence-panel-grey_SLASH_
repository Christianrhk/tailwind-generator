<script>
    //CALCULATOR
    //Formula = (Length X Width X (Thickness / 12)) / 27
    jQuery(document).ready(function () {

        var baseRate =
        12.99;

        var widthParams = null;

        //Get injected dropdown width - 1st select
        if(jsContext.productOptionsList.length){
            widthParams = jsContext.productOptionsList[0].values;
        }
        // console.log(widthParams);
        //Populate Width Dropdown
        // jQuery('#turf_width_field').append(jQuery('<option>').text("Select"));
        jQuery.each(widthParams, function(i, obj){
                jQuery('#turf_width_field').append(jQuery('<option>').text(obj.label).attr('value', obj.data.replace('M','')));
        });

        //Product Grass
        function calculate() {

            setTimeout(function() {
                var baseRateNew = jQuery('.price-for-calc [data-product-price-with-tax]').text();
     
                var baseRateClean = baseRateNew.replace('£', '')
                        //    console.log(baseRateClean);
        

            

            if (
                jQuery("#turf_length_field").children("option:selected").val() != "" &&
                jQuery("#turf_length_field").children("option:selected").val() != undefined &&
                jQuery("#turf_width_field").children("option:selected").val() != "" &&
                jQuery("#turf_width_field").children("option:selected").val() != undefined

            ) {
                var result =
                    jQuery("#turf_length_field").children("option:selected").val() *
                    jQuery("#turf_width_field").children("option:selected").val();

                //Enable btn
                jQuery('.productView-options #form-action-addToCart').removeAttr('disabled');

                // console.log(result);
                var quantity = "0.66";
                var total = result;
                var totalR = Math.round(total);
                // console.log(totalR)
                jQuery(".form-input--incrementTotal").val(totalR);
                jQuery(".turf-result").html(
                    jQuery("#turf_length_field").children("option:selected").val() *
                    jQuery("#turf_width_field").children("option:selected").val() +
                    "&#x33a1; of artificial grass"
                );
                
                var totalLabel = (baseRateClean.replace(',', '') * totalR);

                jQuery('.total-price').html('£' + totalLabel.toLocaleString("en"));
            } else {
                jQuery(".turf-result").html("Please select all sizes.");
            }

            }, 1500);

        }

        //Product Decking
        function calculateDecking() {
            
            var result =
            jQuery("#decking_length_field").val() *
            jQuery("#decking_width_field").val();

            //Enable btn
            jQuery('.productView-options #form-action-addToCart').removeAttr('disabled');

            var total = result;
            var totalR = Math.round(total) * 2; //We need to double here as 2 boards = 1m squared

            jQuery(".form-input--incrementTotal").val(totalR);

            jQuery(".decking-result").html(
            jQuery("#decking_length_field").val() *
            jQuery("#decking_width_field").val() +
            "&#x33a1; of decking"
            );

            jQuery('.total-price').html('£' + (baseRate * totalR).toFixed(2));
            // var baseratecleaner = baseRate.replace(',', '');
            // var totalLabels = (baseRate.replace(',', '') * totalR);
            // jQuery('.total-price').html('£' + totalLabels.toLocaleString("en"));


            // else {
            // jQuery(".decking-result").html("Please select all sizes.");
            // }

        }

        //On change
        jQuery('#turf_width_field').on('change', function () {

            var result = null;
            // console.log(this.selectedIndex);
            if (this.value == '2') {
                result = 364;
            } else if (this.value == '4') {
                result = 365;
            } else if (this.value == '5') {
                result = 366;
            }
            // var sel = jQuery('.productView-options .form .form-field:first-of-type .form-select').val(result).change();
            var sel = jQuery('.productView-options .form .form-field:first-of-type .form-select').prop('selectedIndex', this.selectedIndex).trigger('change');

            //Need to detect trigger in product-details.js - Line 55.

            calculate();

        });

        jQuery('#turf_length_field').on('change', function () {
            calculate();
        });

        //Decking Calc
        jQuery('#decking_length_field').on('input', function () {
            calculateDecking();
        });
        jQuery('#decking_width_field').on('input', function () {
            calculateDecking();
        });

        jQuery('.productView-options #form-action-addToCart').on('click', function () {
            jQuery('#turf_length_field').val('');
            jQuery('#turf_width_field').val('');
            jQuery('.turf-result').html('--');
            jQuery('#decking_length_field').val('');
            jQuery('#decking_width_field').val('');
            jQuery('.decking-result').html('--');
            jQuery('.total-price').html(' -- ');
            setTimeout(function () { jQuery('.productView-options #form-action-addToCart').attr('disabled', 'disabled'); },
                1000);

        });

        //Grass Calc
        if (jQuery('.breadcrumbs li.breadcrumb:nth-of-type(2) .breadcrumb-label:contains("Artificial Grass")').length > 0) {
            
            if (!jQuery('.breadcrumbs li.breadcrumb:nth-of-type(3) .breadcrumb-label:contains("Full Rolls")').length > 0)
            {
                
            if (!jQuery('.breadcrumbs li.breadcrumb:nth-of-type(3) .breadcrumb-label:contains("Accessories")').length > 0) {
            
            jQuery(".productView-options .form div .form-field").addClass("hidden");
            jQuery(".turf-calc").addClass("visible");
            jQuery(".sample-link").addClass("visible");
            jQuery(".usp-icons").addClass("hidden");
            jQuery(".productView-dimensionTabLink").addClass("hidden");
            jQuery(".productView-options .form .form-field.form-field--increments").addClass("hidden");

            //Disable the Add to cart unless l/w selects are chosen
            jQuery('.productView-options #form-action-addToCart').attr('disabled', 'disabled');

            // jQuery(".form-field.form-field--increments").addClass("hidden");

            //Show the colour select on play grass
                    if (jQuery('.breadcrumbs li.breadcrumb:nth-of-type(3) .breadcrumb-label:contains("Play Grade Coloured Grass")').length > 0) {
                        jQuery(".productView-options .form div .form-field:last-of-type").removeClass("hidden");
                    }
                }
            }
        }

        //Decking Calc
        if (jQuery('.breadcrumbs li.breadcrumb:nth-of-type(2) .breadcrumb-label:contains("Composite Decking")').length > 0) {

            if (!jQuery('.breadcrumbs li.breadcrumb:nth-of-type(3) .breadcrumb-label:contains("Composite Decking Accessories")' ).length > 0) {
                jQuery(".sample-link--decking").addClass("visible");
                if (!jQuery('.breadcrumbs li.breadcrumb:nth-of-type(3) .breadcrumb-label:contains("Oakio")' ).length > 0) {
                
                        
                jQuery(".productView-options .form div .form-field").addClass("hidden");
                jQuery(".decking-calc").addClass("visible");
                jQuery(".sample-link--decking").addClass("visible");
                jQuery(".usp-icons").addClass("hidden");
                jQuery(".productView-dimensionTabLink").addClass("hidden");
                jQuery(".productView-options .form .form-field.form-field--increments").addClass("hidden");

                //Disable the Add to cart unless l/w selects are chosen
                jQuery('.productView-options #form-action-addToCart').attr('disabled', 'disabled');
                
                }

            }
            
             if (jQuery('.breadcrumbs li.breadcrumb:nth-of-type(3) .breadcrumb-label:contains("Accessories")' ).length > 0) {
                 
                  jQuery(".productView-options .form div .form-field").removeClass("hidden");
                jQuery(".decking-calc").removeClass("visible");
                jQuery(".sample-link--decking").removeClass("visible");
                jQuery(".usp-icons").removeClass("hidden");
                jQuery(".productView-dimensionTabLink").removeClass("hidden");
                jQuery(".productView-options .form .form-field.form-field--increments").removeClass("hidden");
                
                //Disable the Add to cart unless l/w selects are chosen
                jQuery('.productView-options #form-action-addToCart').removeAttr('disabled');
                 
             }
                    
        }
        
        if (jQuery('.breadcrumbs li.breadcrumb:nth-of-type(2) .breadcrumb-label:contains("Composite Fencing")').length > 0) {
            jQuery(".sample-link--fencing").removeClass("hidden");
            jQuery(".sample-link--fencing").addClass("visible");
        }
        
                
        if (jQuery('.breadcrumbs li.breadcrumb:nth-of-type(2) .breadcrumb-label:contains("Composite Cladding")').length > 0) {
            jQuery(".sample-link--cladding").removeClass("hidden");
            jQuery(".sample-link--cladding").addClass("visible");
        }

        // if (jQuery('.breadcrumbs li.breadcrumb:nth-of-type(2) .breadcrumb-label:contains("Artificial Grass")').length > 0) {
            
        //     if (!jQuery('.breadcrumbs li.breadcrumb:nth-of-type(3) .breadcrumb-label:contains("Accessories")' || '.breadcrumbs li.breadcrumb:nth-of-type(3) .breadcrumb-label:contains("Full Rolls")').length > 0) {
                
            
        //     jQuery(".productView-options .form div .form-field").addClass("hidden");
        //     jQuery(".turf-calc").addClass("visible");
        //     jQuery(".sample-link").addClass("visible");
        //     jQuery(".usp-icons").addClass("hidden");
        //     jQuery(".productView-dimensionTabLink").addClass("hidden");
        //     jQuery(".productView-options .form .form-field.form-field--increments").addClass("hidden");

        //     //Disable the Add to cart unless l/w selects are chosen
