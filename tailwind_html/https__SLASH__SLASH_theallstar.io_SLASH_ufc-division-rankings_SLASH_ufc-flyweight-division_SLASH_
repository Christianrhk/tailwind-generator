				var eras = [{"start_date":{"milliseconds":1348272000,"year":2012,"month":9,"day":22,"display_date":"<span>Demetrious Johnson<\/span> &bull; Sep, 2012"},"end_date":{"milliseconds":1533340800,"year":2018,"month":8,"day":4,"display_date":"<span>Demetrious Johnson<\/span> &bull; Aug, 2018"},"text":{"headline":"Demetrious \"Mighty Mouse\" Johnson"}},{"start_date":{"milliseconds":1533340800,"year":2018,"month":8,"day":4,"display_date":"<span>Henry Cejudo<\/span> &bull; Aug, 2018"},"end_date":{"milliseconds":1547856000,"year":2019,"month":1,"day":19,"display_date":"<span>Henry Cejudo<\/span> &bull; Jan, 2019"},"text":{"headline":"Henry \"The Messenger\" Cejudo"}},{"start_date":{"milliseconds":1595116800,"year":2020,"month":7,"day":19,"display_date":"<span>Deiveson Figueiredo<\/span> &bull; Jul, 2020"},"end_date":{"milliseconds":1623456000,"year":2021,"month":6,"day":12,"display_date":"<span>Deiveson Figueiredo<\/span> &bull; Jun, 2021"},"text":{"headline":"Deiveson \"Deus da Guerra\" Figueiredo"}},{"start_date":{"milliseconds":1623456000,"year":2021,"month":6,"day":12,"display_date":"<span>Brandon Moreno<\/span> &bull; Jun, 2021"},"end_date":{"milliseconds":1660003200,"year":0,"month":0,"day":0,"display_date":"<span>Brandon Moreno<\/span> &bull; Jun, 2021"},"text":{"headline":"Brandon \"The Assassin Baby\" Moreno"}}];
