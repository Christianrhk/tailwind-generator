    $(document).ready(function(){
        const swiper_medal = new Swiper('.swiper_medal',{
            grabCursor: true,
            effect: "creative",
            creativeEffect: {
                prev: {
                    shadow: true,
                    translate: [0, 0, -400],
                    opacity: 0,
                },
                next: {
                    translate: ["100%", 0, 0],
                },
            },
            navigation: {
                nextEl: '.medal_swiper-button-next',
                prevEl: '.medal_swiper-button-prev'
            }
        });
