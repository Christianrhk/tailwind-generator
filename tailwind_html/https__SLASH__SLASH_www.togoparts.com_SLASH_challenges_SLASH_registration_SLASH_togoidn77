<script type="text/javascript">
    
    $(document).ready(function(){
        const swiper_jersey = new Swiper('.swiper_jersey',{
            grabCursor: true,
            // effect: "coverflow",
            effect: "creative",
            // coverflowEffect: {
            //     rotate: 30,
            //     slideShadows: false
            // },
            creativeEffect: {
                prev: {
                    shadow: true,
                    translate: [0, 0, -400],
                    opacity: 0,
                },
                next: {
                    translate: ["100%", 0, 0],
                },
            },
            // Navigation arrows
            navigation: {
                nextEl: '.jrsy_swiper-button-next',
                prevEl: '.jrsy_swiper-button-prev'
            }
        });
    
        $("#vMdlJersey,.elMdlJersey").on('click',function(){
            $("#jersey3dModal").modal('show');
            return false;
        })
        $("#closeJrsyModal").on('click',function(e){
            e.preventDefault();
            $("#jersey3dModal").modal('hide');
        })
    })
</script>
