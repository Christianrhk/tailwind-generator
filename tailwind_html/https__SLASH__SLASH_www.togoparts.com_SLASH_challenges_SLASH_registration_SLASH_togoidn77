<script type="text/javascript">
    $(document).ready(function(){
        $("#vMdltee3dModal,.vMdlf3dBtn").on('click',function(){
            const tee_swiper = new Swiper('.tee_swiper',{
                // Navigation arrows
                navigation: {
                    nextEl: '.tee_swiper-button-next',
                    prevEl: '.tee_swiper-button-prev',
                }
            });
            $("#tee3dModal").modal('show');
            return false;
        })
        $("#closeTeeModal").on('click',function(e){
            e.preventDefault();
            $("#tee3dModal").modal('hide');
        })
    })
</script>
