<script type="text/javascript" src="https://www.togoparts.com/challenges/assets/js/2022/idn77/script.js?t=1659922052"></script>
<script type="text/javascript">
var sourceP = '0-2958-200-91751';
var totalPrice = 0;

var challengeId = 58;
var upcl = '01-201415';
var upclM = "994-5919";
var countryV = $("#country").val();
var countryVal = $("#country").val();
var addBCountrySelected = $("#country").val();;
var addBPhCodeSelected = '1';
var upldVaccineCard = 'UPLOAD PROOF OF VACCINATION';
var tRewT__9910_mQ = '2';

var tRewJr__9910_mQ = '2';

var tRewS__9910_mQ = '3';

var tRewJr__00191_cQ = 0;
var tRewT__00191_cQ = 0;


var tRewBo__989891 = 0;
var jrRewBo__989891 = 0;
var sRewBo__989891 = 0;

var alChecked = '';
$(document).ready(function(){
    
    if($(".membership-dc input[type='checkbox']:checked").length > 0){
        var alChecked = $(".membership-dc input[type='checkbox']:checked").attr('id').replace('chck__','');
    }else{
        $(".rewSectionCtr").fadeOut();
        $(".rew_tee_size, .rew_jersey_size").val('').attr('disabled',true);
    }
    
     
    if(alChecked != ''){
        if(alChecked == 'medal_tee_jersey'){
            $(".sleeveCtr").addClass('sleeve_selected');
        }
        $(".rew_jersey_size").val('');
        $(".rew_tee_size").val('');
        $(".rew_shorts_size").val('');
        
        calculateTotalPrice($("#coupon_code").val(),'58'); 
    }
    
    
    
        
    $("#togoidn77-registration").validate({
        rules: {
            "dob": {
                required: true
            },
            "gender": {
                required: true
            }
        },
        submitHandler: function(form) {
            var membershipChecked = false;
            var memC = '';
            $("input[name='membership']").each(function(){
                if($(this).is(':checked')){
                    val = true;
                    memC = $(this).val();
                    membershipChecked = true;
                }
            })
            
            if(!membershipChecked && countryV != 'Indonesia'){
                $("input[name='membership']").addClass('error');
                $("html,body").animate({scrollTop : $("input[name='membership']").offset().top - 300});
                return false;
            }
            
            
            var inv = false;
            var invE = '';
         
            
            if(membershipChecked && memC != ''){
                if(memC == 134 || memC == 136){
                    if($("select[name='tee_size_"+memC+"']").val() == ''){
                        $("select[name='tee_size_"+memC+"']").addClass('error');
                        invE = "select[name='tee_size_"+memC+"']";
                        
                        inv = true;
                    }
                }
                if(memC == 135 || memC == 136){
                    if($("select[name='jersey_size_"+memC+"']").val() == ''){
                        $("select[name='jersey_size_"+memC+"']").addClass('error');
                        invE = "select[name='jersey_size_"+memC+"']";
                        
                        inv = true;
                    }
                }
                
                $("input[name='addon_membership[]']").each(function(){
                    if($(this).is(':checked')){
                        var v = $(this).val();
                        if(v == 132 && $("select[name='shorts_size_0']").val() == ''){
                            $("select[name='shorts_size_0").addClass('error');
                            invE = "select[name='shorts_size_0";        
                            inv = true;
                        }
                        
                        if(v == 130 && $("select[name='tee_size_0']").val() == ''){
                            $("select[name='tee_size_0']").addClass('error');
                            invE = "select[name='tee_size_0']";        
                            inv = true;
                        }
                        
                        if(v == 131 && $("select[name='jersey_size_0']").val() == ''){
                            $("select[name='jersey_size_0").addClass('error');
                            invE = "select[name='jersey_size_0";        
                            inv = true;
                        }
                        
                    }
                })
                
                
                
            }
            
            if(inv){
                $("html,body").animate({scrollTop : $(invE).offset().top - 300});
                return false;
            }
            
            if((membershipChecked) && $("#addressfl").val() <= 0){
                $(".ab__010_address").addClass('error');
                $("html,body").animate({scrollTop : $(".ab__010_address").offset().top - 300});
                return false;        
            }
            
            if($("#teamParticipateV").is(':checked')){
                if($.trim($("#team_name_sel").val()) == ''){
                    $("#team_name_sel").addClass('error');
                    $("html,body").animate({scrollTop : $("#team_name_sel").offset().top - 300});
                    return false;
                }
            }
            
            if($(".teamImemC:checked").length > 0){
                if(($.trim($("#team_name").val()) == '' && $.trim($("#team_name_sel").val()) == '') || $("#teamParticipateV:checked").length <= 0){
                    $("#team_name").addClass('error');
                    $("#teamParticipateV").addClass('error');
                    $("#team_name_sel").addClass('error');
                    $("html,body").animate({scrollTop : $("#teamParticipateV").offset().top - 300});
                    return false; 
                }
            }
            
            
            if($('#terms-conditions').is(':checked')) {
                $(".error").removeClass('error');
                $("#loadingDiv").removeClass('loadingDivReg').show();
                fbq('track', 'InitiateCheckout'); //track fb checkout event
                $("#loadingDiv").show();
                $("#regSaveBtn").val('Processing').css('background','#ccc !important').attr('disabled',true);
                form.submit();
            } else {
                $('.terms-error').css('display', 'inline-block');
            }
        }
    });
    
})




</script>
