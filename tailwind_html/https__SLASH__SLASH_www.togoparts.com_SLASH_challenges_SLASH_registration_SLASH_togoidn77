<script>
var logged_in = 0;
var userVerified = 0;
var absolute_path = 'https://www.togoparts.com';

var jCounter = 1;
    $(document).ready(function(){
//        console.log("Rm");
        $("#loadingDiv").addClass('loadingDivReg');

        $(document).on('change','#country',function(){
            var url = '';
            $("#ccode").val($(this).val());
            $("#langSel").val(1);
            $("#loadingDiv").show();
            $("#langSelForm").submit();
        })
        
        $("#my_coup").on('click',function(){
            let coup = $(this).text();
            $("#coupon_code").val(coup);
            return false;
        });

        if(!logged_in) {
            // console.log("showFlutterLogin");
                            $('#login-modal').modal({
                    backdrop: 'static',
                    keyboard: false
                });
                        
           
            //$('#login-modal').modal('show');

            $("#opSgMdl").on('click',function(){
                $("#loadingDiv").show();
                window.location.href = 'https://www.togoparts.com/challenges/registration/togoidn77?action=register';
                
            })
            $(document).on('click','#cntTgg',function(){
                $("#registerFormCtr").slideDown();
            })
            $("#logModalOp").on('click',function(){
                $("#loadingDiv").show();
                window.location.href = 'https://www.togoparts.com/challenges/registration/togoidn77';
                
            })
            
                    }else{
            //if(!userVerified){
//                $("#emailVerificationModal").modal({backdrop: 'static'});
//            }
        }



        $(".date-picker").datepicker({
            startView: 2,
            format: 'dd-mm-yyyy'
        });

        $(document).on("click", ".FBLoginButton",function() {
            loginToFaceBook();
        });

        $("#referral_code").on('keyup blur change paste input',function(){
            
            if($(this).val() == ''){
                                
                $("#referralCodeValid").val(0);
                $(".reffTxt").html('');
                return;
            }
            verifyReferralUser($(this).val(),'58');
        })

        $("#applccode").on('click',function(){
            var ccode = $.trim($("#coupon_code").val());
            
            if(ccode == ''){
                $("#couponCodeValid").val(0);
                $(".coupTxt").html('');
//                return;
            }
            
            $("#coupon_code").val(ccode);
            
            verifyCouponCodeWithCombinations(ccode,'58');
        })

        var usernames = new Bloodhound({
            datumTokenizer: Bloodhound.tokenizers.obj.whitespace('name'),
            queryTokenizer: Bloodhound.tokenizers.whitespace,
            remote: {
                url: url_root +'/challenges/new/controllers/ajax-json-autocomplete-username.php?q=%QUERY&cid=58',
                wildcard: '%QUERY'
            }
        });

        $('#referral_code').typeahead({
                minLength: 2,
                hint: true,
                highlight: true
            }, {
                name: 'usernames',
                limit: 10,
                display: 'name',
                source: usernames,
                templates: {
                    empty: [
                        '<div class="empty-message">',
                        'User not found!',
                        '</div>'
                    ].join('\n'),
                    suggestion: function(data) {
                            return '<div class="tt-suggestion"><p>'+ data.name +'</p></div>'
                    }
                }
        });

        jQuery.validator.addMethod("phoneNumber", function(value, element) {
            return value.match(/^(?=.*[0-9])[- +()0-9]+$/);
        }, "Please specify a valid phone number");

        // Toast script
        var toastNtf = $('body').attr("data-rns");
        if(toastNtf != false || toastNtf != ''){
          $(".boxNft").slideDown(250);
          setTimeout(function(){
            $(".boxNft").slideUp(250);
          }, 5000);
        }
    });
</script>
