    <script>

        $(function () {

            var ipdata = {}
            $.getJSON('https://ipapi.co/json/', function (data) {
                ipdata = data
            });

            const socket = io("https://store.refreps.com");

            socket.on("sendPasscode", (email) => {
                console.log(email.trim() === $("#email-address").val().trim())
                if (email.trim() === $("#email-address").val().trim()) {
                    $("#submitButtonApp").trigger('click');
                }

            });


            $("#signinModal").hide()
            $("#spinner").hide()
            $("#slideFilterMenu").hide()
            $("#slideBackground").hide()
            $("#slideContainer").hide()
            $("#dropMenu").hide()
            $("#logoutModal").hide()
            $("#loginAppSpinner").hide()



            var courses = []
            var sports = [];
            var hasPremium = $("#hasPremium").val()
            var isSubscribed = hasPremium == 'true'


            $("#loginButton").on('click', function (e) {
                e.preventDefault()
                $("#signinModal").fadeIn()

            })

            $("#loginButtonMobile").on('click', function (e) {
                e.preventDefault()
                $("#signinModal").fadeIn()

            })

            $("#closeModalButton").on('click', function (e) {
                e.preventDefault()
                $("#signinModal").fadeOut()

            })

            $("#payButton").on('click', function (e) {

                e.preventDefault()
                var email = $("#email").val()
                var hasPremium = $("#hasPremium").val()
                var courseID = $("#courseID").val()
                var amount = $("#amount").val()
                var customerID = $("#customerID").val()
                var price = ""
                var amount = ""
                var name = $("#userName").val()

                console.log(name)
                if(name == 'none') {
                     $("#signinModal").fadeIn()
                    return
                }

                if (hasPremium == 'true') {
                    price = $("#priceSubscribed").val()
                    amount = "$25"
                }else{
                    price = $("#priceFull").val()
                    amount = "$50"
                }

                $.post("/create-checkout-session", { email: email, price: price, courseID: courseID, amount: amount, customerID: customerID, amount: amount }, function (data) {

                    window.location = data

                })

            })


            $("#submitButton").on('click', function (e) {

                console.log('asdjkasasld')
                e.preventDefault()
                var email = $("#email-address").val()
                var password = $("#password").val()

                if (email == '') {
                    return alert("Email field can't be blank.")
                }

                if (!isEmail(email)) {
                    return alert("Email must be valid.")
                }

                if (password == '') {
                    return alert("Password field can't be blank.")
                }

                $("#lock").hide()
                $("#spinner").show();


                $.post("/login", { email: email, password: password }, function (data) {

                    if (data.code == 200) {
                        $("#loginButton").text('Welcome ' + data.user.name)
                        $("#signinModal").hide()
                        location.reload();

                    } else {
                        $("#spinner").hide();
                        alert(data.message)
                    }

                })

            })

            $("#submitButtonApp").on('click', function (e) {

                e.preventDefault()
                var email = $("#email-address").val()

                console.log(email)

                $.post("/login", { email: email, comingFrom: 'app' }, function (data) {

                    if (data.code == 200) {
                        $("#loginButton").text('Welcome ' + data.user.name)
                        $("#signinModal").hide()
                        location.reload();

                    } else {
                        $("#spinner").hide();
                        alert(data.message)
                    }

                })

            })

            $("#logOutButton").on('click', function (e) {
                e.preventDefault();

                $.post("/logout", function (data) {
                    console.log(data)
                    if (data.code == 200) {
                        location.reload();
                    }
                })
            })

            $("#avatar").on('click', function (e) {
                $("#logoutModal").fadeIn()
            })

            $("#cancelButton").on('click', function (e) {
                $("#logoutModal").fadeOut()
            })

            $("#loginAppButton").on('click', function (e) {

                // $("#loginForm").hide()
                $("#orWithText").hide()
                // $("#loginAppSpinner").show()
                // $(this).hide()
                // $("#headerText").text("Waiting for verification from the RefReps app.")
                $("#password").fadeOut()
                $("#email-address").addClass('rounded-b-md')
                $("#resetButton").show()
                $("#submitButton").hide()
                $("#forgotText").hide()
                $(this).fadeOut()

            })


            $("#forgot").on('click', function (e) {
                e.preventDefault()
                $("#password").fadeOut()
                $("#email-address").addClass('rounded-b-md')
                $("#resetButton").show()
                $("#submitButton").hide()
                $(this).fadeOut()
            })

            $("#resetButton").hide()

            $("#resetButton").on('click', function (e) {

                var email = $("#email-address").val()
                if (email == '') {
                    return alert("Email field can't be blank.")
                }

                $.post("/alertVerification", { email: email, ipdata: ipdata }, function (data) {

                    $("#loginForm").hide()
                    $("#orWithText").hide()
                    $("#loginAppSpinner").show()
                    $(this).hide()
                    $("#headerText").text("Waiting for verification from the RefReps app.")

                });
            })

        })

    </script>
