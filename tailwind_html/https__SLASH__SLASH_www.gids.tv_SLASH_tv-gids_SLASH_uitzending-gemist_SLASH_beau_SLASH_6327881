        $('.poster-img').each(function() {
            var $this = $(this);
            $this.on('error', function() {
                if ($this.data('src-fallback') && $this.data('src-fallback') !== '') {
                    $this.attr('src', $this.data('src-fallback'));
                    $this.data('src-fallback', '');
                } else {
                    $this.remove();
                }
            }).on('load', function() {
                $(this).parent().find('.no-img').hide();
            }).attr('src', $this.data('src'));
        });

        $('.add-to-watchlist button').each(function() {
            var $this = $(this);

            $this.tooltip({
                html : true,
                delay : {
                    hide : 500
                },
                trigger : 'manual'
            }).on('shown.bs.tooltip', function() {
                var tooltipId = $this.attr('aria-describedby');
                $('#'+tooltipId).hover(
                    function() {
                        $this.addClass('active');
                    },
                    function() {
                        setTimeout(function() {
                            if ($this.data('hover') !== 'on') {
                                $this.removeClass('active');
                            }
                        }, 500);
                    }
                );
            });

            $this.hover(
                function() {
                    $this.addClass('active');
                    $this.data('hover', 'on');
                    if (!$this.attr('aria-describedby')) {
                        $this.tooltip('show');
                    }
                },
                function() {
                    $this.data('hover', 'off');
                    setTimeout(function() {
                        var tooltipInterval = setInterval(function() {
                            if (!$this.hasClass('active')) {
                                $this.tooltip('hide');
                                clearInterval(tooltipInterval);
                            }
                        }, 10);
                    }, 500);
                    $this.removeClass('active');
                }
            );
        });
    });
</script>
<script src="https://www.gids.tv/js/video.js?v=1558376467"></script>
<script type="text/plain" data-cookie-dependant>
    if ($('#video-plyr').length > 0) {
        const player = new Plyr('#video-plyr', {
            controls : [
                'play-large',
                'play',
                'progress',
                'current-time',
                'mute',
                'volume',
                //'pip',
                'fullscreen'
            ],
            fullscreen: {
                enabled: true,
                fallback: true,
                iosNative: true
            },
            ads: {
                enabled: false,
                publisherId: '',
                tagUrl: ''
            }
        });

        player.on('ready', function(event) {
            var $player = $(this);

            var $ratio = $player.parent().find('.ratio');
            var height = $ratio.height();

            $ratio.hide();
            $('.plyr--video').css({
                'height' : height,
            });
            $('.plyr__video-wrapper').attr('style', '');
            $('#video-plyr').show();
        });
    }

    $('video[data-src]').each(function() {
        var video = this;
        const player = new Plyr(video, {captions: {active: true, update: true, language: 'en'}});
        if (!Hls.isSupported()) {
            $video.src = source;
        } else {
            // For more Hls.js options, see https://github.com/dailymotion/hls.js
            const hls = new Hls();
            hls.loadSource($(video).data('src'));
            hls.attachMedia(video);
        }

        var $ratio = $(video).closest('.h_iframe').find('.ratio');
        var height = $ratio.height();

        $('.plyr__video-wrapper').css({
            'height' : height,
        });

        $ratio.hide();
    });
</script>
