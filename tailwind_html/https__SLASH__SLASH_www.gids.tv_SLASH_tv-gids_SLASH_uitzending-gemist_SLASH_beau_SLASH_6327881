<script>
    $(function () {
        setTimeout(function() {
            setupRequest().then(function(options) {
                options.method = 'PUT';
                options.url    = '/tracker/archived-broadcast/6327881/event';
                options.data   = {
                    event : 'show'
                };

                $.ajax(options);
            });
        }, 200);

        if ($('.npo-thumbnail').length > 0) {
            var thumbnail = $('.npo-thumbnail').data('src');
            $('.npo-thumbnail img').attr('src', 'https://images.gids.tv/video-thumbnail.jpg?url='+thumbnail+'&width=728').height($('#npovideo-player .ratio').height());
            $('#npovideo-player .ratio').hide();
        }

        /*if ($('#npovideo').length > 0) {

            npoplayer('npovideo').setup({
                width: '100%',
                height: Math.round(Number($('#npovideo-player .ratio').height()))+'px',
                prid: $('#npovideo-player').data('player-id'),
                options: {
