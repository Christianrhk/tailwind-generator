<script src="https://www.togoparts.com/challenges/assets/js/elevate.js?t=3" type="text/javascript"></script>
<script type="text/javascript">
    var chId = '58';
    var hash = window.location.hash;

    $(document).ready(function(){
        var upTeamMdL = new URLSearchParams(window.location.search);
        var getTeamInfo = upTeamMdL.get('updateinfo');
        if(getTeamInfo){
            $("#updTeamDModal").modal('show');
        }

        if(hash != ''){
            scrollTop(hash.replace('#',''));
        }
    });

    $(document).on('click','.langSel',function(e){
        e.preventDefault();
        if($(this).hasClass('selected')){
            return;
        }
        var vall = $(this).attr('data-value');
        $("#lang").val(vall);
        $("form[name='langForm']").submit();
        return false;
    });
    $("button[data-toggle='offcanvas']").click(function(){
        $('.row-offcanvas').toggleClass('active');
        if($( ".row-offcanvas" ).hasClass( "active" )){
            $('body').css("overflow", "hidden");
        }
        else{
            $('body').css("overflow", "");
        }
    });
</script>
