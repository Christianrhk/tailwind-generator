            //           $(".notifContainer").slideDown(250);
            //           $('body').css("overflow","hidden");
            //       }
            //   }else{
            //       if($('.notfDHrsWrpCnt:visible').length){
            //           $(".notfDHrsWrpCnt").slideUp(250);
            //           $('body').css("overflow","auto");
            //       }else{
            //           $(".notfDHrsWrpCnt").slideDown(250);
            //           $('body').css("overflow","hidden");
            //       }
            //   }
            // });
    });
    
    

    // For WebView Login Page Redirection
    </script>


