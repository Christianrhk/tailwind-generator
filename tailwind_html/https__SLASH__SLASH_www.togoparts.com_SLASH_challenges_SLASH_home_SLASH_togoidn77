<script>
    let d = false;
    function shownavmenu(){
      let h = $("#mndDev").height();
        if (!d) {
            $("#mobileDrps").toggle().animate({
              height: '100vh',
              'overflow-y': 'scroll',
              'margin-bottom':'10px',
              'padding-bottom':'100px'
            },10);
            $('body').css("overflow","hidden");
            d = true;
        }else{
            $("#mobileDrps").toggle().animate({
              height: '0px',
              'overflow': 'hidden',
              'padding-bottom':'0px'
            },10);
            $('body').css("overflow","auto");
            d = false;
        }
    }
    
    $(document).ready(function(){
            
            // $(document).on('click',"#notfDHrsWro", function(){
            //
            //   if($(document).width() > 1024){
