<script>
var showStandard; var showStandardPlus; var showPremium; var showAll;

jQuery(function ($) {
$("img.lazy").lazyload({
    threshold : 10000
});

var elems = ["#TO", "#BO", "#TOMF", "#BOMF"];


var showAndHide = function(bar) {
    var id = bar; 

    if(id != "all") {
      var i;
      for (i = 0; i < elems.length; ++i) {
          if(!(elems[i] == ($(id))) ) {
            $(elems[i]).parent().hide();
            $(elems[i] + "i").parent().hide();
          }
      }
      $(id).parent().show();
      $(id + "i").parent().show();
    } else {
      for (i = 0; i < elems.length; ++i) {
            $(elems[i]).parent().show();
            $(elems[i]+"i").parent().show();
      }    
    }
};

showTO = function(){
  showAndHide("#TO");
};

showBO = function(){
  showAndHide("#BO");
};

showTOMF = function(){
  showAndHide("#TOMF");
};

showBOMF = function(){
  showAndHide("#BOMF");
};

showAll = function(){
  showAndHide("all");
};

});
</script>

