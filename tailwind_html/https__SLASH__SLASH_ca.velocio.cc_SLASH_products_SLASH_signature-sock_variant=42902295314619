<script>
    //Script for the PDP to activate the hidden modal which shows return policy information on click
    var returnModalLink = document.querySelector('.return-modal__link');
    var returnModal = document.querySelector('.return-modal');

    //Function returns if return link does not exist, else adds click listener to close modal if open if click occurs not on link, or open if click occurs on link
    function listenReturnModal() {
        if(!returnModalLink) {
            return;
        } else {
            window.onclick = function(event) {
                if(event.target !== returnModalLink) {
                    if(!returnModal.classList.contains('return-modal__hidden')) {
                        returnModal.classList.add('return-modal__hidden');
                    }   
                } else {
                    returnModal.classList.toggle('return-modal__hidden');
                }   
            }
        }
    }

    listenReturnModal();
</script>
<script src="//cdn.shopify.com/s/files/1/0506/8429/7403/t/3/assets/shopify-money.js?v=119736788877478344001643199582" type="text/javascript"></script>

<script>

var currentProductID = '7475491733691';
var zaiusXApiKey = `VpZJv5--zAhCDEu3c03Bqw`

const createAndAppendDOMElements = (elementType, props, nest) => {
  const elementVariableName = document.createElement(elementType);
  if(props) {
        for(var name in props) {
            if(name.indexOf("on") === 0) {
              elementVariableName.addEventListener(name.substr(2).toLowerCase(), props[name], false)
            } else {
              elementVariableName.setAttribute(name, props[name]);
            }
        }
    }

    if (!nest) {
        return elementVariableName;
    }
    return nester(elementVariableName, nest)
}

const nester = (element, nested) => {
    if (typeof nested === "string") {
        var text = document.createTextNode(nested);
        element.appendChild(text);
    } else if (nested instanceof Array) {
        for(var i = 0; i < nested.length; i++) {
            if (typeof nested[i] === "string") {
                var text = document.createTextNode(nested[i]);
                element.appendChild(t);
            } else if (nested[i] instanceof Node){
              element.appendChild(nested[i]);
            }
        }
    } else if (nested instanceof Node){
        element.appendChild(nested)
    }
    return element;
}

const buildProductRecStructure = (url, image, altImage, name, price, handle) => {
  const productRecsContainer = document.getElementById("zaius-product-recs-wrapper");
  const productWrapper = createAndAppendDOMElements("DIV", { class: "p-2 w-1/2 lg:w-auto relative"});
  const productWrapLink = createAndAppendDOMElements("A", { href: url, class: "product-rec-link"});
  const productImage = createAndAppendDOMElements("IMG", { src: image, alt: image.alt, width: "400px", height: "400px", loading: "lazy", class: "lazyload first-rec-image"});
  const productAltImage = createAndAppendDOMElements("IMG", { src: altImage, alt: image.alt, width: "400px", height: "400px", loading: "lazy", class: "lazyload absolute top-0 left-0 p-2 second-product-image"});
  const productName = createAndAppendDOMElements("H3", {class: "text-center mt-4"}, name);
  const productPrice = createAndAppendDOMElements("P", {class: "text-center text-black mb-0"}, price);
  const productSwatchContainer = createAndAppendDOMElements("DIV", { class: "swatch-container swatch-container-"+ handle + " lg:w-2/3 m-auto flex flex-wrap justify-center "});
  const productVariantImageContainer = createAndAppendDOMElements("DIV", { class: "variant-container-"+handle+" flex justify-center"});
  productRecsContainer.appendChild(productWrapper).appendChild(productWrapLink).appendChild(productImage);
  productWrapLink.appendChild(productAltImage);
  productWrapLink.appendChild(productName);
  productWrapLink.appendChild(productPrice);
  productWrapLink.appendChild(productSwatchContainer);
  productWrapLink.appendChild(productVariantImageContainer);
}

function getCookie(cname) {
    const name = `${cname}=`;
    const decodedCookie = decodeURIComponent(document.cookie);
    const splitCookie = decodedCookie.split(';');
    for (let i = 0; i < splitCookie.length; i++) {
        let cookieIndex = splitCookie[i];
        while (cookieIndex.charAt(0) === ' ') {
            cookieIndex = cookieIndex.substring(1);
        }
        if (cookieIndex.indexOf(name) === 0) {
            return cookieIndex.substring(name.length, cookieIndex.length);
        }
    }
    return '';
}

let zaiusVUID = getCookie('vuid').split('|')[0];
zaiusVUID = zaiusVUID.replace(/-/g,''); //removing dashes

const getVariantURLSlug = (productURL, variantID) => {
  try{
    const variantURL =  `${productURL}?variant=${variantID}`;
    return variantURL
  }
  catch(error){
    console.log(error)
  }
}

const fetchShopifyAndZaiusData = async () => {
    const zaiusProduct = await fetch(
        `https://api.zaius.com/v3/recommendations/products?vuid=${zaiusVUID}&product_ids=${currentProductID}&limit=6&type=contextual`,
        {
            method: "GET",
            headers: {'x-api-key': zaiusXApiKey},
            mode: 'cors',
        }
    ).then((response) => response.json());

    const shopifyPromises = [];
    
    zaiusProduct.forEach((zProduct) => {
        shopifyPromises.push(
          fetch(`/products/${zProduct.handle}.js`).then((response) => {
              return response.json();
            }).catch(error => {
              console.log(error)
            })
        );
    });

    
    const shopifyProducts = [];
    await Promise.all(shopifyPromises)
        .then((response) => {
          if(response.status !== 404) {
              shopifyProducts.push(response)
          }
        })
        .catch((error) => console.log(error));

    let counter= 0
    shopifyProducts.forEach(product => {
      product.forEach(item => {
        if (item !== undefined) {
          if (counter >= 4) {
            return;
          }
          counter += 1
          const productName = item.title;
          const productHandle = item.handle;
          const productPrice = Shopify.formatMoney(item.price);
          const productURL = item.url;
          const productFeaturedImage = item.featured_image.replace(".jpg", "_400x.jpg");
          let productAltImage;
          if(!item.images[1]) {
            productAltImage = item.images[0].replace(".jpg", "_400x.jpg");
          } else {
            productAltImage = item.images[1].replace(".jpg", "_400x.jpg");
          }

          buildProductRecStructure(productURL, productFeaturedImage, productAltImage, productName, productPrice, productHandle);
          
          //variants
          const productRecVariants = item.variants;

          //Setting swatches variable as variant colors based on where 'Color' falls in product options.
          let swatches;
          if(item.options[0].name.toLowerCase() === 'size') {
            swatches = item.options[1].values;
          } else {
            swatches = item.options[0].values;
          }
          
          swatches.forEach(swatch => {
            let swatchName = swatch.toString().toLowerCase();
            if(swatchName.includes(' / ')) {
              swatchName = swatchName.replaceAll(' / ','-');
            } else if(swatchName.includes('/')) {
                swatchName = swatchName.replaceAll('/','-');
            }
            if(swatchName.includes(' ')) {
                swatchName = swatchName.replaceAll(' ','-');
            } 

            const swatchBackgroundColor = swatch.toString().replace(" ","").toLowerCase();
            const swatchHandle = swatchName +'.png';
            let swatchURL = "//cdn.shopify.com/s/files/1/0506/8429/7403/t/3/assets/swatchHandle?1144"
            swatchURL = swatchURL.replace('swatchHandle',swatchHandle);
            const productLink = item.url;
            const variantIDs = [];

          productRecVariants.every(variant =>{
              if(swatch.toString() !== variant.option2) {
                return true;
              } else {
              variantIDs.push(variant.id);
              return false;
              }
            })

            const variantLink = getVariantURLSlug(productLink, variantIDs[0]);
            const productVariantLink = createAndAppendDOMElements("A", { href: variantLink, 'aria-label': 'variant-swatch' });
            const productSwatchContainer = createAndAppendDOMElements("DIV", { class: "p-0.5 flex items-center justify-center "});
            const productSwatchContainerInner = createAndAppendDOMElements("DIV", { class: "rounded-full border border-gray-400 "});
            const productSwatchImage = createAndAppendDOMElements("DIV", { class: "w-5 h-5 lg:w-6 lg:h-6 m-0.5 border border-gray-300 rounded-full ", style: "background-image: url(" + swatchURL + "); background-color: "+ swatchBackgroundColor +";" });
            // const productSwatchImage = createAndAppendDOMElements("IMG", { src: swatchURL, alt: swatch.alt, width:"30px", height:"30px", class: "p-0.5 m-1 rounded-full border border-gray-400 max-w-rec_swatch_width max-h-rec_swatch_height " + swatchName});
            
          // Adding each swatch to the right product
            const currentContainer = document.querySelector(`.swatch-container-${productHandle}`)
            currentContainer.appendChild(productVariantLink).appendChild(productSwatchContainer).appendChild(productSwatchContainerInner).appendChild(productSwatchImage);
            
            //Adding in Variant Images
            productRecVariants.every(variant=> {
              const variantColor = variant.option2;
              
              if(variantColor !== swatch) {
                return true;
              } else {
                const featuredImage = variant.featured_image.src;
                const smallerImage = featuredImage.replace(".jpg", "_400x.jpg");
                const productVariantImage = createAndAppendDOMElements("IMG", { src: smallerImage, alt: featuredImage.alt, width: "400px", height: "400px:", loading: "lazy", class: "lazyload absolute top-0 left-0 p-2 " + swatchName+"-image hidden"});
                //Images for each variant, hidden on top of the main product image
                const productVariantImageContainer = document.querySelector(`.variant-container-${productHandle}`);
                productVariantImageContainer.appendChild(productVariantImage);
                return false;
              }
            })
            //Swapping to the variant color on swatch hover
            productVariantLink.addEventListener('mouseenter',function(){
              const varContainer = this.parentElement.nextElementSibling;
              const varImages = Array.from(varContainer.querySelectorAll('img'));
              const varImage = varContainer.querySelector(`.${swatchName}-image`)

              varImages.forEach(image => {
                image.classList.add("hidden");
              })
              varImage.classList.remove("hidden");
            })
            productVariantLink.addEventListener('mouseleave',function(){
            const varContainer = this.parentElement.nextElementSibling;
            const varImages = Array.from(varContainer.querySelectorAll('img'));

            varImages.forEach(image => {
              image.classList.add("hidden");
            })
            })
          })
        }
        
      }) 
    })
};

fetchShopifyAndZaiusData();

      </script>

