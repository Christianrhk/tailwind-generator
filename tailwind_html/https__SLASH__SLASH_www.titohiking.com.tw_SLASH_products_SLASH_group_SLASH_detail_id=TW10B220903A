    var $input = $(this);

    var $textarea = $('<textarea></textarea>')
    .prop('id', $input.prop('id'))
    .prop('name', $input.prop('name'))
    .attr('data-rel', 'memo')
    .css('min-height', '150px')
    .addClass('form-control')
    .text('※ 請務必填寫'+'\r\n'+'緊急聯絡人：'+'\r\n'+'緊急連絡電話：'+'\r\n'+'(請舉例)兩年內登山經驗：'+'\r\n'+'(請舉例)兩年內運動經驗：'+'\r\n'+'抬頭：'+'\r\n'+'統編：'+'\r\n'+'(若不需要可留白)'+'\r\n'+'如何得知此行程：'+'\r\n'+'(Google搜尋/LINE帳號/FB粉專/IG/朋友推薦/其他)');

    $input.prop('outerHTML', $textarea.prop('outerHTML'));
  });
 });</script>
