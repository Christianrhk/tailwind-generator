  document. getElementById("emav").style.display = "none";
  document. getElementById("secode1").style.display = "none";
  document. getElementById("itimer").style.display = "none";

  function suall(nm){
    var myval = document.getElementById('uema').value;
    var myemva = myval;
    var subat = "@";
    if(myemva.indexOf(subat) !== -1){

    document. getElementById("emav").style.display = "none";
    document. getElementById("secode1").style.display = "block";
    document. getElementById("itimer").style.display = "block";
    timedCount();

    alert("The email address that you have entered is "+myval+"  Please verify in 03:00 minutes");

      var actv = 'https://skytod.com/skytod/testemailavail';
      $.ajax({
        type:"POST",
        url:"https://skytod.com/skytod/ltests/verifymail.php",
        data:{email:myval},
        success:function(response){
          if(response=="false"){
           alert('Wrong code.'); location.reload();
          } else {
            document.getElementById('secode2').value= response.trim();
          }
        }
      });
    }else{
      alert("Email is not correct.");
    }

}
  function codechecker(){
    // alert('hi');
    var myval1 = document.getElementById('secode1').value;
    var myval1 = document.getElementById('otp').value;
    var myval2 = document.getElementById('secode2').value;
    if(myval1 != ''){
      if(myval1==myval2){ alert('Your Email is verified');
        document. getElementById("emav"). style. display = "block";
        document. getElementById("timer1"). innerHTML = "Email verified";
        document. getElementById("itimer"). style. display = "none";

      }else{ alert('Wrong code.'); location.reload();
      }
    }else{
          alert('Email/Code can not be empty!');
          document.getElementById('secode1').value="";
          document.getElementById("secode1").focus();
    }
  }
  function showemailinfo(){
    // alert('hi');
      document. getElementById("shema"). style. display = "block";
      document. getElementById("itimer"). style. display = "block";

   }




  </script>
