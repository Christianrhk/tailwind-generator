function confirmaddtocart2() {
	if (document.frmaddtocart.pstatus.value == "soldout") {
		alert ("สินค้าหมด");
		return false;
	}
	if (document.frmaddtocart.Quantity.value == "" || isNaN(document.frmaddtocart.Quantity.value) == true) {
		alert("กรุณาระบุ เป็นตัวเลข");
		frmaddtocart.Quantity.focus();
		return false;
	}
	 
}




function confirmorder() {
	
	if (document.frmorder.pstatus.value != "soldout") {
		if (document.frmorder.Quantity.value == "" || isNaN(document.frmorder.Quantity.value) == true) {
			alert("กรุณาระบุ เป็นตัวเลข");
			document.frmorder.Quantity.focus();
		} else {		
			var NetPrice = 7400;
			var PName = "Set 4 น้ำมังคุด BIM และ Mylife 3 ขวด";	
			var PDesc = "";	
			/* if using multiple price */
			if (document.frmorder.p_price_type.value == "multiple") {		
				myOption = -1;							
				for (i=document.frmorder.p_price_selected.length-1; i > -1; i--) {
					if (document.frmorder.p_price_selected[i].checked) { myOption = i+1; }
				}		
				if (myOption == 1) {			
					PDesc += " (" + document.frmorder.p_price_head1.value + ") "; 
					NetPrice = document.frmorder.p_price_multiple1.value;			
				} else if (myOption == 2) {			
					PDesc += " (" + document.frmorder.p_price_head2.value + ") "; 
					NetPrice = document.frmorder.p_price_multiple2.value;
				} else if (myOption == 3) {			
					PDesc += " (" + document.frmorder.p_price_head3.value + ") "; 
					NetPrice = document.frmorder.p_price_multiple3.value;
				} else if (myOption == 4) {			
					PDesc += " (" + document.frmorder.p_price_head4.value + ") "; 
					NetPrice = document.frmorder.p_price_multiple4.value;
				} else if (myOption == 5) {			
					PDesc += " (" + document.frmorder.p_price_head5.value + ") "; 
					NetPrice = document.frmorder.p_price_multiple5.value;			
				}		
			}
			window.location="/template/action_order_process.php?cart=add&SKU=probim-set4&PName=" + PName + "&PDesc=" + PDesc + " (" + frmorder.opt_value.value + ")&Price=" + NetPrice +"&ShippingPrice=0&Quantity=" + frmorder.Quantity.value  + "";
		}
		
	} else {
		alert ("สินค้าหมด");
	}
}
</SCRIPT>


 
