<SCRIPT LANGUAGE="JavaScript">
$(document).ready(function(){
	$('form[name="frmaddtocart"]').submit(function(){
		
		if ($('input[name="pstatus"]').val() == 'soldout') {
			alert ("สินค้าหมด");
			return false;
		}
		
		var Quantity = $('input[name="Quantity"]').val();
		
		if (typeof Quantity != 'undefined' && (Quantity == "" || isNaN(Quantity) == true)) {
			alert("กรุณาระบุ เป็นตัวเลข");
			$('input[name="Quantity"]').focus();
			return false;
		}
		
		if($('#use_stock_alert').val() == 'true'){
			var qtyInCart = $('#quantity_in_cart').val() * 1;
			var stockQuantity = $('#stock_quantity').val() * 1;
	        var sumQuantity = $('#Quantity').val() * 1;
	        
	        if(stockQuantity - sumQuantity < 0){
	            alert('ขณะนี้มีสินค้าใน stock เพียง  ' + stockQuantity + '  หน่วย เท่านั้น');
	            $('#Quantity').focus();
	            return false;
	        }
		}
	});	
});


