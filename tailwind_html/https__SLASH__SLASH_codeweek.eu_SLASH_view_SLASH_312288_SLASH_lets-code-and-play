            _calculatePeriods: function (c, d, e, f) {
                c._now = f;
                c._now.setMilliseconds(0);
                var g = new Date(c._now.getTime());
                if (c._since) {
                    if (f.getTime() < c._since.getTime()) {
                        c._now = f = g
                    } else {
                        f = c._since
                    }
                } else {
                    g.setTime(c._until.getTime());
                    if (f.getTime() > c._until.getTime()) {
                        c._now = f = g
                    }
                }
