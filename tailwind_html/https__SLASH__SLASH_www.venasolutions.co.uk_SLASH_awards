<script>
  function checkDOMChange() {
      if ($('.leadinModal-theme-top').length) {
        $('.navigation').css( "top", "46px" );
      } else {
        $('.navigation').css( "top", "0" );
      }

      // call the function again after 100 milliseconds
      setTimeout( checkDOMChange, 10 );
  }

  checkDOMChange();
</script>  

