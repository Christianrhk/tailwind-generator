<script>
    var menu = document.querySelectorAll('.list-none.justify-center.font-semibold > li div');
    Array.prototype.slice.call(menu).forEach(function(element) {
        element.addEventListener('click', function(event) {
            var items = document.querySelectorAll('.list-none.justify-center.font-semibold > li div');
            Array.prototype.slice.call(items).forEach(function(item) {
                item.classList.remove('active');
            });
            event.target.classList.add('active');
            var rel = event.target.getAttribute('data-rel');
            var sections = document.querySelectorAll('.g-cols.offset_default');
            Array.prototype.slice.call(sections).forEach(function(section) {
                if (rel === 'all') {
                    section.classList.remove('hidden');
                    section.classList.add('grid');
                } else {
                    if (rel !== section.getAttribute('data-rel')) {
                        section.classList.add('hidden');
                        section.classList.remove('grid');
                    } else {
                        section.classList.remove('hidden');
                        section.classList.add('grid');
                    }
                }
            })
        })
    });
</script>

