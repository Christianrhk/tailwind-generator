<script>
  // if (window.location.href.indexOf("awning") > -1) {
  //   jQuery('#awning input[type="checkbox"]').prop('checked', true);
  // }

  // jQuery("a.deselect").each(function() {
  //   this.search = "";
  // });


  jQuery(document).ready(function() {
    jQuery('.woo-category-page .sidebar .yith-woocommerce-ajax-product-filter a').click(function() {
      let selectedItem = this.closest('li');
      console.log(selectedItem);
      jQuery(selectedItem).toggleClass('chosen');
    })

    // mobile slider for features component
    jQuery('.products-features-slider').slick({
      autoplay: false,
      dots: false,
      arrows: false,
      centerMode: true,
      centerPadding: '0',
      slidesToShow: 5,
      infinite: false,

      responsive: [{
        breakpoint: 640,
        settings: {
          autoplay: false,
          centerMode: false,
          slidesToShow: 1,
          slidesToScroll: 1,
        }
      }],
    });

    // hide the category page's own category card at the bottom
    const getCatSlug = {
        config: {
            targetSlug: "windows"
        }
    }
    console.log("cat card is: .category-", getCatSlug.config.targetSlug);
    jQuery('.category-' + getCatSlug.config.targetSlug).hide();
    
    const catUrl = location.href;
    const split1 = catUrl.split('product_cat=');
    // console.log(split1[1]);
    const split2 = split1[1].split('&');
    const finalCat = split2[0];
    // console.log(finalCat);
    console.log("cat card is: .category-", getCatSlug.config.targetSlug);
    jQuery('.category-' + getCatSlug.config.targetSlug).hide();
    console.log("cat card is: .category-", finalCat);
    jQuery('.category-' + finalCat).hide();
  });
</script>


