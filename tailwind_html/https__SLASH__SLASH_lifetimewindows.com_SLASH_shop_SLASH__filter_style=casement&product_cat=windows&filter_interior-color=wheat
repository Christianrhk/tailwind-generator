<script>
	jQuery(document).ready(function() {
		jQuery('.quick-view-modal').hide();

		jQuery('.quick-view-button').unbind().on('click', function(e) {
			e.preventDefault();
			let quickViewId = jQuery(this).attr('id');
			let productId = quickViewId.split('quick-view-button-');

			// console.log('productId', productId[1]);
			let modalId = '#quick-view-modal-' + productId[1];
			jQuery(modalId).show().css('opacity', '1');

			let selectedModal = document.querySelector( '#quick-view-modal-' + productId[1] );
			// console.log('selectedModal', selectedModal);

			let largeProductImage = selectedModal.querySelector( '.large-product-image-src' );
			let productImageGallery = selectedModal.querySelector( '.product-image-gallery' );
			let productImageGalleryArray = productImageGallery.querySelectorAll( 'img' );
			// console.log('largeProductImage', largeProductImage);
			// console.log('productImageGalleryArray', productImageGalleryArray);

			productImageGalleryArray.forEach(img => {
				img.addEventListener('click', function(e) {

					let selectedImageUrl = e.target.currentSrc;
					let w1 = selectedImageUrl.split(',w_');
					let h1 = selectedImageUrl.split(',h_');
					// console.log('h1', h1);

					let wVal = w1[1].split(',');
					let hVal = h1[1].split(',');
					wVal[0] = '500';
					hVal[0] = '610';
					// console.log('hVal', hVal.slice(1).join(','));
					
					let largeSelectedImageUrl = w1[0] + ',w_' + wVal[0] + ',h_' + hVal[0] + ',' + hVal.slice(1).join(',');
					console.log('largeSelectedImageUrl', largeSelectedImageUrl);


					largeProductImage.setAttribute('src', largeSelectedImageUrl);
					jQuery('.large-product-image-src').hide();
					jQuery('.large-product-image-src').fadeIn();
				});
			});
		})

		jQuery('.quick-view-modal__overlay, .quick-view-modal__close-button').on('click', function(e) {
			// console.log('clicked', jQuery(this));
			jQuery('.quick-view-modal').hide();
		})

		document.addEventListener('keydown', function(e) {
			if(e.key === "Escape") {
					// console.log('esc clicked', e);
					jQuery('.quick-view-modal').unbind().hide();
				}
		});

	});

</script></ul>
