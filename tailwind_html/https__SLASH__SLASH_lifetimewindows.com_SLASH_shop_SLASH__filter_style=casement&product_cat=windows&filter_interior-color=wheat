    <script>

    // https://developer.mozilla.org/en-US/docs/Web/API/MutationObserver
    // function waitForElm(selector) {
    //   return new Promise(resolve => {
    //       if (document.querySelector(selector)) {
    //           return resolve(document.querySelector(selector));
    //       }

    //       const observer = new MutationObserver(mutations => {
    //           if (document.querySelector(selector)) {
    //               resolve(document.querySelector(selector));
    //               observer.disconnect();
    //           }
    //       });

    //       observer.observe(document.body, {
    //           childList: true,
    //           subtree: true
    //       });
    //   });
    // }

    // Hide Siding Menu when user is in Arizona
    waitForElm('.class').then((el) => {
      // console.log("el is ready", el);

      // if (localStorage.getItem('CurrentState') == 'Arizona') { 
      //   el.style.display = 'none';
      // }
    });

    const footerTagline = document.querySelector('.footer-tagline');

    // Location based Footer logo
    if (localStorage.getItem('CurrentState') == 'Arizona') 
    { 
      waitForElm('.footer-logo--arizona').then((el) => {
        const logoALink = el.closest('a');
        logoALink.style.display = "inline-block";
        footerTagline.textContent = "Built for Arizona";
      });
    } else if (localStorage.getItem('CurrentState') == 'Colorado') 
    { 
      waitForElm('.footer-logo--colorado').then((el) => {
        const logoALink = el.closest('a');
        logoALink.style.display = "inline-block";
        footerTagline.textContent = "Denver\’s Most Trusted Window & Siding Company";

      });
    } else if (localStorage.getItem('CurrentState') == 'Texas') 
    { 
      waitForElm('.footer-logo--texas').then((el) => {
        const logoALink = el.closest('a');
        logoALink.style.display = "inline-block";
        footerTagline.textContent = "Serving North Texas";

      });
    } else {
      waitForElm('.footer-logo--default').then((el) => {
        const logoALink = el.closest('a');
        logoALink.style.display = "inline-block";
      });
    }

    </script>
