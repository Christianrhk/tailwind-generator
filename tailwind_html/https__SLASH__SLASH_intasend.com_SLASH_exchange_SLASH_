<script type="text/javascript" src="https://website-assets.intasend.com/js/website.js"></script>
<script>
    function getQuery(q) {
        return (window.location.search.match(new RegExp('[?&]' + q + '=([^&]+)')) || [, null])[1];
    }

    let foreignAmount = document.getElementById('foreignAmount')
    let exchangeAmount = document.getElementById('exchangeAmount')
    let exchangeRate = document.getElementById('exchangeRate')
    let foreignCurrency = document.getElementById('foreignCurrency')
    let exchangeCurrency = document.getElementById('exchangeCurrency')

    foreignAmount.onkeyup = function () {
        if (exchangeRate.value) {
            exchangeAmount.value = (this.value * exchangeRate.value).toFixed(2)
        }
    }
    exchangeAmount.onkeyup = function () {
        if (exchangeRate.value) {
            foreignAmount.value = (this.value / exchangeRate.value).toFixed(2)
        }
    }

    let amount = Number(getQuery("amount") || 1)
    if (amount) {
        if (exchangeRate.value) {
            exchangeAmount.value = (amount * exchangeRate.value).toFixed(2)
        }
    }

    function changePairs() {
        if (exchangeCurrency.value && foreignCurrency.value) {
            if (exchangeCurrency.value === foreignCurrency.value) {
                exchangeRate.value = 1
                exchangeAmount.value = foreignAmount.value
            } else {
                window.location.replace("/exchange/" + foreignCurrency.value + "-to-" + exchangeCurrency.value +
                    "-rate/")
            }
        }
    }
</script>


