   <script src="https://cdn.jsdelivr.net/combine/npm/vue@2.6.11/dist/vue.js,npm/lodash@4.17.15"></script>
            <script src="https://cdn.jsdelivr.net/npm/vee-validate@3.3.0/dist/vee-validate.full.js"></script>
            <script src="https://cdn.jsdelivr.net/npm/moment@2.26.0/moment.min.js"></script>
            <script>

            window.csrfTokenName = "CRAFT_CSRF_TOKEN";
            window.csrfTokenValue = "TAqow8LwyCudGuKPqG9hZs8_TEySIMDrTRcHrW7D43zAwiH0TCjHGQk_5\u002D67l5lS6yzQxPcrEheCcng2yHqHshpPTPcc9oc3t\u002D9EtghYhjQ\u003D";
            // Register the component globally.
            Vue.component('validation-provider', VeeValidate.ValidationProvider);

            
            function objectToQuerystring (obj) {
            
              for (var propName in obj) { 
                if (obj[propName] === null || obj[propName] === undefined) {
                  delete obj[propName];
                }
              }
            
              return new URLSearchParams(obj).toString();
            }



             Vue.component('pl-filters', {
                  props: ['attrs','url','query'],
                  data:function(){
                    return { 
                      price_range:[
                        {'label':'less than 1000',value:'<1000'},
                        {'label':'1000 to 2000',value:'1000-2000'},
                        {'label':'lmore than 2000',value:'>2000'},
                      ],
                      sortby: [
                        {'label':'Latest',value:'latest'}
                      ],
                      expand:{sizes:false,colour:false,price:false,orderby:false}
            
                    }
                  },
                  computed:{
                   

                  },
                  watch: {

                   
                  
                        
                  },
                  methods: {

                    buildQuery : function (data) {

                      // If the data is already a string, return it as-is
                      if (typeof (data) === 'string') return data;

                      // Create a query array to hold the key/value pairs
                      var query = [];

                      // Loop through the data object
                      for (var key in data) {
                        if (data.hasOwnProperty(key)) {

                          // Encode each key and value, concatenate them into a string, and push them to the array
                          query.push(encodeURIComponent(key) + '=' + encodeURIComponent(data[key]));
                        }
                      }

                      // Join each item in the array with a `&` and return the resulting string
                      return query.join('&');

                    },

                    getObj :function(key,value)
                    {
                      var temp = {};
                      temp[key] = value;
                      return temp;
                    },

                    getFilter:function(obj)
                    {
                      var parts = this.url.split('?');
                      var search =parts[1];
                      if(search)
                      var params =JSON.parse('{"' + search.replace(/&/g, '","').replace(/=/g,'":"') + '"}', function(key, value) { return key===""?value:decodeURIComponent(value) });
                      else 
                      var params = {};
                      params = _.merge(params,obj);

                      return parts[0]+'?'+this.buildQuery(params);

                    },
                    
                    toggleFilter:function(key)
                    {
                      this.expand[key] = !this.expand[key];
                    },
                    
                    getClearFilter:function(key)
                    {
                      var parts = this.url.split('?');
                      var search = _.cloneDeep(this.query);
                      if(search)
                      var params =search;
                      else 
                      var params = {};

                      delete params['p']

                      if(key=='orderby')
                      {
                        delete params['order'];
                      }
                      // params = _.merge(params,obj);

                      delete params[key];

                      return parts[0]+'?'+this.buildQuery(params);
                    }
                  },
                  filters:{

                    price:function(value){
                      return 'HK$ '+parseFloat(value).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                    }
                    
                  },
                  mounted: function(){
                    var q = this.query;
                    var expand = {};
                    for (var prop in q) {
                        if (Object.prototype.hasOwnProperty.call(q, prop)) {
                           expand[prop] = true;
                        }
                    }
                    this.expand = _.merge(this.expand,expand);

                  }
             });
            
             new Vue({
                 el: '#vue-app', 
                 delimiters: ["<%","%>"]
            }); 
            
            
            </script>


