  <div class="flex flex-wrap -mx-4">
    <div class="w-full md:w-3/12 px-4 mb-10 md:mb-0">
      
      <div class="filters">
        <div class="filter " v-for="attr in attrs">
          <a href="javascript:void(0);" class="heading flex justify-between w-full" v-on:click.prevent="toggleFilter(attr.handle)" v-bind:class="{expanded:expand[attr.handle]}" v-if="!expand[attr.handle]||!query[attr.handle]"><span><%attr.label%></span> <span>ALL</span></a>
          <a  class="heading flex justify-between w-full" :href="getClearFilter(attr.handle)" v-bind:class="{expanded:expand[attr.handle]}" v-else><span><%attr.label%></span> <span>Clear</span></a>
          <ul class="list-none" v-bind:class="{'show':expand[attr.handle]}">
            <li v-for="option in attr.options"><a :href="getFilter(getObj(attr.handle,option.id))" v-bind:class="{'active':query[attr.handle]==option.id}"><%option.label%></a></li>
          </ul>
        </div>
        <div class="filter ">
          <a href="javascript:void(0);" class="heading flex justify-between w-full" v-on:click.prevent="toggleFilter('price')" v-bind:class="{expanded:expand['price']}" v-if="!expand['price']||!query['price']"><span>Price</span> <span>ALL</span></a>
          <a class="heading flex justify-between w-full" :href="getClearFilter('price')" v-bind:class="{expanded:expand['price']}"  v-else><span>Price</span> <span>Clear</span></a>
          <ul class="list-none" v-bind:class="{'show':expand['price']}">
            <li><a :href="getFilter({'price':'lt-1000'})" v-bind:class="{'active':query['price']=='lt-1000'}">less than 1000</a></li>
            <li><a :href="getFilter({'price':'1000-2000'})" v-bind:class="{'active':query['price']=='1000-2000'}">1000 to 2000</a></li>
            <li><a :href="getFilter({'price':'gt-2000'})" v-bind:class="{'active':query['price']=='gt-2000'}">more than 2000</a></li>
          </ul>
        </div>
        <div class="filter ">
          <a href="javascript:void(0);" class="heading flex justify-between w-full" v-on:click.prevent="toggleFilter('orderby')" v-bind:class="{expanded:expand['orderby']}" v-if="!expand['orderby']||!query['orderby']"><span>Sort By</span> <span>Latest</span></a>
          <a class="heading flex justify-between w-full" :href="getClearFilter('orderby')" v-bind:class="{expanded:expand['orderby']}" v-else><span>Sort By</span> <span>Clear</span></a>
          <ul class="list-none" v-bind:class="{'show':expand['orderby']}">
            <li><a :href="getFilter({'orderby':'postDate','order':'desc'})"  v-bind:class="{'active':query['orderby']=='postDate'}">Latest</a></li>
            <li><a :href="getFilter({'orderby':'defaultPrice','order':'asc'})"  v-bind:class="{'active':query['orderby']=='defaultPrice'&&query['order']=='asc'}">Price (Low to High)</a></li>
            <li><a :href="getFilter({'orderby':'defaultPrice','order':'desc'})"  v-bind:class="{'active':query['orderby']=='defaultPrice'&&query['order']=='desc'}">Price (High to Low)</a></li>
          </ul>

        </div> 
      </div>
   
      
