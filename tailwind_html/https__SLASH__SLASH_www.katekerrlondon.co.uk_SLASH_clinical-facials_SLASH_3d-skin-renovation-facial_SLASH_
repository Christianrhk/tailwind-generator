<script>

var acc = document.getElementsByClassName("accordion");
var i;

for (i = 0; i < acc.length; i++) {
  acc[i].addEventListener("click", function() {

    var plusSvg = this.children[0].children[1].children[0];
    var minusSvg = this.children[0].children[1].children[1];

    // var test = this.children[0].children[1].children[0];
    // console.log(test);

    plusSvg.classList.toggle("block");
    plusSvg.classList.toggle("hidden");
    minusSvg.classList.toggle("block");
    minusSvg.classList.toggle("hidden");


    /* Toggle between hiding and showing the active panel */
    var panel = this.nextElementSibling;
    if (panel.style.display === "block") {
      panel.style.display = "none";
    } else {
      panel.style.display = "block";
    }
  });
}

</script>
