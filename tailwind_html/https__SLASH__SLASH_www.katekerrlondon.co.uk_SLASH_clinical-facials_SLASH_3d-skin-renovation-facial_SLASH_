<script>

// Mobile menu
const mobileBurgerButtonWhite = document.getElementById( 'mobile-burger-white' );
const closemobileMenuWhite = document.getElementById( 'close-mobile-menu-white' );
const mobileMenuWhite = document.getElementById( 'mobile-menu-white' );

// open mobile menu
mobileBurgerButtonWhite.addEventListener( 'click', function() {
    mobileMenuWhite.classList.toggle( 'hidden' );

    if ( mobileBurgerButtonWhite.getAttribute( 'aria-expanded' ) === 'true' ) {
        mobileBurgerButtonWhite.setAttribute( 'aria-expanded', 'false' );
    } else {
        mobileBurgerButtonWhite.setAttribute( 'aria-expanded', 'true' );
    }
})

// close mobile menu
closemobileMenuWhite.addEventListener( 'click', function() {
    mobileMenuWhite.classList.toggle( 'hidden' );

    if ( mobileBurgerButtonWhite.getAttribute( 'aria-expanded' ) === 'true' ) {
        mobileBurgerButtonWhite.setAttribute( 'aria-expanded', 'false' );
    } else {
        mobileBurgerButtonWhite.setAttribute( 'aria-expanded', 'true' );
    }
})



// Sub menu
const subMenuButtonWhite = document.getElementById( 'sub-menu-button-white' );
const subMenuWhite = document.getElementById( 'sub-menu-white' );

subMenuButtonWhite.addEventListener( 'click', function() {
    subMenuWhite.classList.toggle( 'hidden' );

    if ( subMenuButtonWhite.getAttribute( 'aria-expanded' ) === 'true' ) {
        subMenuButtonWhite.setAttribute( 'aria-expanded', 'false' );
    } else {
        subMenuButtonWhite.setAttribute( 'aria-expanded', 'true' );
    }
})


const subMenuButtonWhite2 = document.getElementById( 'sub-menu-button-white-2' );
const subMenuWhite2 = document.getElementById( 'sub-menu-white-2' );

subMenuButtonWhite2.addEventListener( 'click', function() {
    subMenuWhite2.classList.toggle( 'hidden' );

    if ( subMenuButtonWhite2.getAttribute( 'aria-expanded' ) === 'true' ) {
        subMenuButtonWhite2.setAttribute( 'aria-expanded', 'false' );
    } else {
        subMenuButtonWhite2.setAttribute( 'aria-expanded', 'true' );
    }
})

