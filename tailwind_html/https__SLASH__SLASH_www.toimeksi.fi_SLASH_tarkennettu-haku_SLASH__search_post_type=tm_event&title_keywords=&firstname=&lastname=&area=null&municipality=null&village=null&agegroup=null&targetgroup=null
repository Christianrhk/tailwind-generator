<script>
jQuery(function($) {

	/*$('.post_types input[type="checkbox"]').on('change', function() {
	   $('input[type="checkbox"]').not(this).prop('checked', false);
	});*/

	post_type_change();

	$( '.pt_checkbox' ).change(function() {

		post_type_change();
		clear_field();

	});

	//Klikkaamalla premise-sisältötyyppiä, varustelu ja toimitilatyyppi value pyyhkiytyy
	//Ehdotus .prop('checked',false)

	function clear_field() {
		$(':input','#advanced-search').not(':button, :submit, :reset, :hidden, input[name=search_post_type], select[id=area], select[id=municipality], select[id=village], input[name=premise_equipment[]], input[name=premise_type[]]').val('').removeAttr('selected');
	}



	function post_type_change() {

		if ( $('#p_post').is(':checked') || $('#p_page').is(':checked')  || $('#tm_person').is(':checked')  /* || $('#tm_project').is(':checked') */) {
			$('.area-info').hide();
			$('.content-order').hide();
            $('.show-date').hide();
		} else {
			$('.area-info').show();
            $('.content-order').show();
		}

		if ( $('#tm_person').is(':checked') ) {
			$('.hide-person').hide();
			$('.person').show();
            $('.show-date').hide(300);
		} else {
			$('.hide-person').show();
			$('.person').hide();
		}

		if ( $('#tm_event').is(':checked') || $('#tm_activity').is(':checked') || $('#tm_organization').is(':checked') || $('#tm_job').is(':checked') || $('#tm_project').is(':checked') ) {
			$('.event-activity-organization').show();
		} else {
			$('.event-activity-organization').hide();
		}

		if ( $('#p_post').is(':checked')) {
			$('.post_fields').show();
		} else {
			$('.post_fields').hide();
		}

		if ( $('#tm_job').is(':checked')) {
			$('.job').show();
			$('.hide-in-job').hide();
            $('.show-date').show(300);
		} else {
			$('.job').hide();
			$('.hide-in-job').show();
		}

		if ( $('#tm_volunteer').is(':checked')) {
			$('.volunteer').show();
			$('.event-activity-organization').hide();
            $('.show-date').show(300);
		} else {
			$('.volunteer').hide();
		}



		if ( $('#tm_event').is(':checked') || $('#tm_activity').is(':checked')) {
			$('.event-activity').show();
            $(".date_descending").show(300);
            $(".date_ascending").show(300);
            $('.show-date').show(300);

        } else {
			$('.event-activity').hide();
		}

		if ( $('#tm_organization').is(':checked') || $('#p_page').is(':checked') /* || $('#tm_event').is(':checked') */ /* || $('#tm_activity').is(':checked') */ || $('#volunteer_job').is(':checked') /* || $('#tm_job').is(':checked') */ /* || $('#tm_premise').is(':checked') */ ) {
			$('.hide-organization').hide();

		} else {
			$('.hide-organization').show();
		}

		if ( $('#tm_organization').is(':checked')) {
			$('.industry').show();
            $(".date_descending").hide(300);
            $(".date_ascending").hide(300);
		} else {
			$('.industry').hide();
		}

		if ( $('#tm_project').is(':checked')) {
			$('.project-info').show();
			$('.cost-info').hide();
            $(".date_descending").show(300);
            $(".date_ascending").show(300);
		}

		if ( $('#tm_project').is(':checked')) {
			$('.hide-project').hide();
            $('.show-date').show();
		} else {
			$('.hide-project').show();
		}

        if ( $('#tm_premise').is(':checked')) {
            $('.premise').show();
            $('.date_descending').hide(300);
            $('.date_ascending').hide(300);
            $('.show-date').hide();

        } else {
            $('.premise').hide();
        }

	if ( $('#tm_event').is(':checked') || $('#tm_activity').is(':checked') || $('#tm_job').is(':checked') ) {
		$('.industry').hide();
	}

	if ( $('#tm_job').is(':checked') ) {
		$('.hide-in-job').hide();
	}

        if ( $('#p_post').is(':checked') || $('#tm_organization').is(':checked') || $('#tm_activity').is(':checked') || $('#tm_event').is(':checked') || $('#tm_volunteer').is(':checked') || $('#tm_job').is(':checked') || $('#tm_project').is(':checked')) {
    $('.keywords').show();

} else {
    $('.keywords').hide();
}

	}
});
</script>

