        <script type="text/javascript">
    $(document).ready(function() {
	$("#blog_content").on("click", ".more", function(e) {
	    var me = $(this);
	    me.html('<i class="fas fa-spinner fa-spin"></i> Loading ...');
	    me.attr("disabled", "disabled");
	    history.pushState({}, '', me.attr("href") + "&from=1");
	    try {
	    $("#loadmore").load(me.attr("href") + "&ajax=1", function() {
		$("#loadmore .grid").addClass("pt-8");
		me.hide();
		$("#blog_content").append($("#loadmore").html());
		$("#loadmore").html("");
	    });
	    } catch(e) {
		console.log(e);
	    }
	    e.preventDefault();
	    //e.stopPropagation();
	    //return false;
	});
    });
</script>
