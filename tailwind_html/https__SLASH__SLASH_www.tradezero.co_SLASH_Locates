<script>
    
    (function() {
        // Desktop and mobile layouts
        function checkMediaQuery() {
            if (window.innerWidth >= 1340) {
                $('.header-account').removeClass('is-mobile').addClass('is-desktop');
                $('.header-account.is-desktop').insertAfter('.header-hamburger');
            } else {
                $('.header-account').removeClass('is-desktop').addClass('is-mobile');
                $('.header-account.is-mobile').appendTo('.header-navbar');
            }
        }
        checkMediaQuery();
        window.addEventListener('resize', checkMediaQuery);

        // Banner for IE and EdgeHTML
        if (window.document.documentMode) {
            document.getElementById('unsupported-browser').style.display='block';
        }

        // Mobile hamburger menu
        $('#hamburger').click(function(e) {
            $(this).toggleClass('active');
            $('#navigation').toggleClass('open');
            e.preventDefault();
        });

        // Highlight active menu item
        $('#navigation a').filter(function() {
            return this.href == window.location.href;
        }).addClass('active');

        // Toggle submenu
        $('.header-toggle').click(function(e) {
            $(this).toggleClass('active');
            $(this).next('.header-submenu').toggleClass('open');
            e.preventDefault();
        });
    }());

    
    (function() {
        // Login utilities
        const queryString = window.location.search;
        const urlParams = new URLSearchParams(queryString);

        var usernamePlaceholder = '';
        var passwordPlaceholder = '';
        var fullNamePlaceholder = '';

        if (urlParams.get('username')) {
            usernamePlaceholder = urlParams.get('username');
        }

        require([
                '/assets/common/js/knockout.min.js?v=3.5.1'
            ],
            function(ko) {
                var viewModel = {
                    userName: ko.observable(usernamePlaceholder),
                    fullName: ko.observable(fullNamePlaceholder),
                    showPassword: ko.observable(false),
                    loading: ko.observable(true),
                    openAccount: ko.observable('<a class="sign-up" id="ancSignUpNew" href="#">Open Account</a>'),
                    signIn: ko.observable('<a class="sign-in" id="ancSignInNew" href="#">Sign In</a>'),
                    intlRegistrationUrl: ko.observable("https://www.tradezero.co/v2/registration/" + queryString),
                    usaRegistrationUrl: ko.observable("https://us.tradezero.co/v2/registration/" + queryString),
                    canRegistrationUrl: ko.observable("https://www.tradezero.ca/v2/registration/" + queryString),
                    intlUrl: ko.observable("https://www.tradezero.co/" + queryString),
                    usaUrl: ko.observable("https://us.tradezero.co/" + queryString),
                    canUrl: ko.observable("https://www.tradezero.ca/" + queryString),
                };
                var unFromQueryStr = getQueryStrValueFromWnd('username');
                if (unFromQueryStr && unFromQueryStr.length > 0) {
                    viewModel.userName = unFromQueryStr;
                }
                viewModel.passwordFieldType = ko.pureComputed(function() {
                    return viewModel.showPassword() ? 'text' : 'password'
                });
                viewModel.welcomeFullName = ko.pureComputed(function() {
                    return 'Welcome ' + viewModel.fullName();
                });
                viewModel.hasAvatar = ko.pureComputed(function() {
                    return viewModel.loading() ? 'has-arrow' : 'has-avatar has-arrow';
                });
                viewModel.accountButtonsLoaded = function() {
                    $('#ancSignUpNew, [data-target="#signup-modal"]').click(function(e) {
                        $('body').addClass('signup-modal-on');
                        $('#signup-modal').fadeIn('fast');
                        e.preventDefault();
                    });
                    $('#ancSignInNew').click(function(e) {
                        
                            window.location.replace("/account/login");
                        
                    });
                }
                ko.applyBindings(viewModel);
                //debugger;
                var cookieVal = getCookie('portal_jwt');
                if (cookieVal && cookieVal.length > 0) {
                    $.ajax({
                        url: "/api/Account/GetName",
                        type: "get",
                        headers: { "Authorization": cookieVal },
                        success: function(response, textStatus, jqXHR) {
                            viewModel.fullName(response.Data.FullName);
                            viewModel.userName(response.Data.UserName);
                        },
                        error: function(jqXHR, textStatus, errorThrown) {
                            viewModel.fullName(fullNamePlaceholder);
                            viewModel.userName(usernamePlaceholder);
                        },
                        complete: function() {
                            viewModel.loading(false);
                        }
                    });
                } else {
                    viewModel.fullName(fullNamePlaceholder);
                    viewModel.userName(usernamePlaceholder);
                    viewModel.loading(false);
                }
            }
        );

        if (window.location.pathname == '/signin' || window.location.pathname == '/login') {
         window.location.replace('/account/login');
        }

        function closeSignupModal() {
            $('body').removeClass('signup-modal-on');
            $('#signup-modal').fadeOut('fast');
        }

        $('#signup-modal').click(function(e) {
            if (e.target == e.currentTarget) {
                closeSignupModal();
            }
        });

        $('#signup-close').click(function(e) {
            closeSignupModal();
            e.preventDefault();
        });
    }());

    
    (function() {
        // Mobile footer menu
        document.querySelectorAll('.site-colophon .menu-unit dt').forEach((el) => {
            el.classList.add('menu-collapse');
            el.addEventListener('click', () => {
                el.classList.toggle('menu-expand');
            }, false);
        });
    }());

    
    (function() {
        // Open demo reg modal via url
        if (window.location.href.indexOf('/trading-software?demo=true') != -1) {
            $('#DemoRegistration').modal('show');
        }

        // Commission disclosure modal
        $('.open-disclosure-modal').click(function(e) {
            e.preventDefault();
            $('#disclosure-modal').modal('show');
        });

        
    }());

    (function () {

        var adParams = ['utm_campaign', 'utm_medium', 'utm_source', 'utm_content', 'utm_term', 'tz_ag', 'url_referrer', 'irclickid', 'mic_graduation_code'];
        var queryString = window.location.search;
        var urlParams = new URLSearchParams(queryString);

        var registrantParams = {};
        var i;
        for (i = 0; i < adParams.length; i++) {
            if (urlParams.get(adParams[i])) {
                registrantParams[adParams[i]] = urlParams.get(adParams[i]);
            }
        }
        if (Document.referrer) {
            var sameSite = document.referrer.indexOf(location.protocol + "//" + location.host) === 0;
            if (!sameSite) {
                registrantParams['url_referrer'] = Document.referrer;
            }
        }
        if (!isEmptyObject(registrantParams)) {
            setCookie('registrant_url_params', JSON.stringify(registrantParams), 365);
        }
    }());


</script>


    <script defer>
        // Cookie consent banner
        window.addEventListener("load", function() {
            window.wpcc.init({
                "colors": {
                    "popup": { "background": "#f6f6f6", "text": "#000000", "border": "#555555" },
                    "button": { "background": "#555555", "text": "#ffffff" }
                },
                "content": { "link": "Privacy Policy", "button": "Accept", "href": "https://www.tradezero.co/manuals/TZI_Privacy_Policy.pdf" },
                "margin": "none",
                "padding": "none",
                "fontsize": "large"
            });
        });
    </script>

