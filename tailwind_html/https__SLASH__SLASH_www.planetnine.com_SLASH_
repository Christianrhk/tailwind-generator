      <style>
         .csscard {
         overflow: hidden;
         position: relative;
         }
         .csscard-content {
         position: absolute;
         inset: 0;
         background-color: rgba(0, 0, 0, .75);
         transition: all .6s;
         opacity: 0;
         display: flex;
         justify-content: center;
         align-items: center;
         }
         .csscard-content svg {
         color: white;
         width: 45px;
         height: 45px;
         cursor: pointer;
         }
         .csscard:hover .csscard-content {
         opacity: 1;
         }
         .service-card img {
         height: 80px;
         }

         /* client owl nav */
         .my-owl-nav {
         display: flex;
         justify-content: space-around;
         align-items: center;
         color: #222;
         max-width: 120px;
         margin: 40px auto;
         }
         .my-owl-button  {
         border-radius: 50%;
         width: 50px;
         height: 50px;
         display: flex;
         justify-content: center;
         align-items: center;
         color: #777;
         transition: all .2s;
         border: solid 3px #222;
         }
         .my-owl-button i {
         font-size: 35px;
         color: #222;
         }
         .my-owl-button:hover {
         border-color: #4B4E6D;
         color: #4B4E6D;
         }
         .my-owl-nav span:hover {
         cursor: pointer;
         }
         .client-image {
         filter: grayscale(100%);
         transition: all .2s;
         cursor: pointer;
         }
         .client-image:hover {
         filter: grayscale(0%);
         transition: all .2s;
         }


         /* testimonial owl nav */
         .testimonial-owl-nav {
         display: flex;
         justify-content: space-around;
         align-items: center;
         color: #222;
         max-width: 120px;
         margin: 40px auto;
         }
         .my-testimonial-owl-button  {
         border-radius: 50%;
         width: 50px;
         height: 50px;
         display: flex;
         justify-content: center;
         align-items: center;
         color: #777;
         transition: all .2s;
         border: solid 3px #222;
         }
         .my-testimonial-prev-button {
             margin-right: 15px;
         }
         .my-testimonial-owl-button i {
         font-size: 35px;
         color: #222;
         }
         .my-testimonial-owl-button:hover {
         border-color: #4B4E6D;
         color: #4B4E6D;
         }
         .testimonial-owl-nav span:hover {
         cursor: pointer;
         }

         .loaderbg{
            display: none;
            position: fixed;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            z-index: 99;
            overflow: hidden;
            background: rgb(0, 0, 0, 0.8);
         }
         .loader {
            position: fixed;
            top: 50%;
            left: 50%;
            margin-left: -4em;
            border: 16px solid #f3f3f3;
            border-radius: 50%;
            border-top: 16px solid #3498db;
            width: 120px;
            height: 120px;
            -webkit-animation: spin 2s linear infinite; /* Safari */
            animation: spin 2s linear infinite;
         }

         /* Safari */
         @-webkit-keyframes spin {
            0% { -webkit-transform: rotate(0deg); }
            100% { -webkit-transform: rotate(360deg); }
         }

         @keyframes  spin {
            0% { transform: rotate(0deg); }
            100% { transform: rotate(360deg); }
         }
         
         .js-cookie-consent{
            display: block;
            position: fixed;
            bottom: 0;
            padding: 10px;
            text-align: center;
            width: 100%;
            z-index: 9999;
            background-color: #fffbdb;
            border-color: #fffbdb;
            color: #4b4e6d;
         }

         .js-cookie-consent-agree{
            padding: 5px 10px 5px  10px;
            border: 1px black;
            border-radius: 10px;
            color: white;
            background-color: #4b4e6d;
         }
      </style>
