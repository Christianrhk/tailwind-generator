function BtnLoading(elem) {
        $(elem).attr("data-original-text", $(elem).html());
        var loadingtext = $(elem).attr("data-loading");
        $(elem).prop("disabled", true);
        $(elem).html(loadingtext);
    }

    function BtnReset(elem) {
        $(elem).prop("disabled", false);
        $(elem).html($(elem).attr("data-original-text"));
    }
</script>


<script type="text/javascript">
    $(function () {
      var isJobSeekerVerified = "";
      if(isJobSeekerVerified){
        $("#transferMsg").modal("show");
      }
    });
</script>

<script>
      $(function () {
        $("#submitbtn").click(function(){

            var errors = false;
            var errormessage = "";
            if($.trim($("#firstname").val())==""){
                errors = true;
                errormessage +="First name required<br/>";
            }
            if($.trim($("#lastname").val())==""){
                errors = true;
                errormessage +="Last name required<br/>";
            }
            if($.trim($("#sEmail").val())==""){
                errors = true;
                errormessage +="Email required<br/>";
            }
            if($.trim($("#sPassword").val())==""){
                errors = true;
                errormessage +="Password required<br/>";
            }
            if($.trim($("#sPasswordConfirm").val())==""){
                errors = true;
                errormessage +="Please confirm your password<br/>";
            }
            if($("#sPasswordConfirm").val()!= $("#sPassword").val()){
                errors = true;
                errormessage +="Passwords do not match<br/>";
            }
            if(errors){
                $(".errortext").html(errormessage);
                $(".errorAlert").show();
                return false;
            }

            // change submit button text while its processing
            $("#submitbtn").html("Processing...");
            $("#submitbtn").attr("disabled",'disabled');

            // $("#completeMsg").show();
            //orgregisteruser
           $.post('https://videobio.me/c5service',
            {'mode':'tenantadduser', 'fn': $("#firstname").val(), 'ln':$("#lastname").val(), 'uEmail': $("#sEmail").val(),
                    'uPassword':$("#sPassword").val(), 'uPasswordConfirm':$("#sPasswordConfirm").val(), 'plan':"jobseeker"
            },
                    function(response) {
                        console.log(response);
                        if(response.success=="success"){
                        $(".errorAlert").hide();
                            setTimeout(function() {
                                 window.location = "/login?m=regsuc";
                            }, 3000);

                        }else{
                            var $html = "";
                            $.each(response.errors, function(i,obj){
                                $html+=obj.message+"\n";
                            })

                          $(".errortext").html($html);
                          $(".errorAlert").show();
                          // $("#completeMsg").hide();
                          $("#submitbtn").html( "Register");
                          $("#submitbtn").removeAttr("disabled");
                        }
                    }, 'json');

            });
    });
    function validateEmail(email) {
        var re = /^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
        return re.test(String(email).toLowerCase());
    }

</script>
