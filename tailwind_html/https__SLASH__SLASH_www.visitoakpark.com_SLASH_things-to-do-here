console.log(post_list);  
  
spl = post_list.sort(function(a, b){
  let x = a.name.toLowerCase();
  let y = b.name.toLowerCase();
  if (x < y) {return -1;}
  if (x > y) {return 1;}
  return 0;
});
  
  
posts_to_show_count = 8;
function set_html(list_to_iterate, neighborhood, tag){
  var post_counter = 0;
  var current_count = 0;
  var html = '';
  
  list_to_iterate.forEach(function(post){
    var tag_bool = false;
    if (current_count < posts_to_show_count) {
      post.tags.forEach(function(t){
        if (t == tag){
          tag_bool = true;
        }
      });
      
      var updated_name = post.nbhd.replace('-', ' ');
      if (updated_name == 'Lagrange' || updated_name == 'lagrange' || updated_name == 'LaGrange'){
        updated_name = 'La Grange';
      }
      if (updated_name == 'Lagrange park' || updated_name == 'lagrange park' || updated_name == 'LaGrange park'){
        updated_name = 'La Grange Park';
      }
      
      if ((post.nbhd == neighborhood || neighborhood == 'All' || neighborhood == 'Location') && (tag_bool || tag == "All" || tag == "Tag")) {
        if ( (post.name + ' ' + post.content).toLowerCase().includes( $('#search-keyword-widget_1656003964603 > input').val().toLowerCase() ) ){
          post_counter++;
          current_count++;
          html += '<div class="post-content-container">';
            html += '<a class="no-dec text-gray-009" href="' + post.url + '">';
              html += '<div style="aspect-ratio:1; overflow: hidden;" class="flex flex-col items-center justify-center"><img style="object-fit:cover; min-width: 100%;min-height: 100%;" src="' + post.img + '" loading="lazy"></div>';
              html += '<div class="location-display text-sm text-white font-semibold">'+ updated_name +'</div>';
              html += '<div class="post-inner-content flex flex-col">';
                html += '<div class="post-name no-dec text-black font-semibold my-1">' + post.name + '</div>';
                html += '<div class="no-dec text-gray-009" >' + post.content + '</div>';
              html += '</div>';
            html += '</a>';
          html += '</div>';
        }
      }
    }
  });
  
  if (post_counter == 0){
    html = "No posts found!"
  }
  return html;
}

  
$(document).ready(function(){
  posts_to_show_count = 8;
  if (posts_to_show_count >= spl.length){
    posts_to_show_count = spl.length;
    $('#LoadMoreButton').remove();
  }
  $('.locations-container').html(set_html(spl,"All","All"));
});
  
$('#search-location-widget_1656003964603 > select').change(function(){
  var selected_location = $(this).val();
  var selected_tag = $('#search-category-widget_1656003964603 > select').val();
  $('.locations-container').html(set_html(spl,selected_location,selected_tag));
});
  
$('#search-category-widget_1656003964603 > select').change(function(){
  var selected_location = $('#search-location-widget_1656003964603 > select').val();
  var selected_tag = $(this).val();
  $('.locations-container').html(set_html(spl,selected_location,selected_tag));
});
  
$('#search-keyword-widget_1656003964603 > input').change(function(){
  var selected_location = $('#search-location-widget_1656003964603 > select').val();
  var selected_tag = $('#search-category-widget_1656003964603 > select').val();
  $('.locations-container').html(set_html(spl,selected_location,selected_tag));
});
  
$('#LoadMoreButton').click(function(){
  posts_to_show_count += 8;
  
  if (posts_to_show_count >= spl.length){
    posts_to_show_count = spl.length;
    
    $(this).remove();
  }

  var selected_location = $('#search-location-widget_1656003964603 > select').val();
  var selected_tag = $('#search-category-widget_1656003964603 > select').val();
  $('.locations-container').html(set_html(spl,selected_location,selected_tag));
  
});

</script>

</section></div>

</div><!--end widget-span -->
</div><!--end row-->
</div><!--end row-wrapper -->

</div><!--end widget-span -->
</div><!--end row-->
</div><!--end row-wrapper -->

</div><!--end widget-span -->
</div>
</div>
</div>
  </div>
</section> 


      </div>

      
