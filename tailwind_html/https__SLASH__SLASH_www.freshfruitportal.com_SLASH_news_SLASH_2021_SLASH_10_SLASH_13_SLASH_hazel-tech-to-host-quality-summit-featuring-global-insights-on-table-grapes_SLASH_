                <img width="100%" class="h-460px w-full object-cover" alt="" title="" src="https://cdn.freshfruitportal.com/2021/10/2021October-GrapeSummit.jpg">
                <div class="background-primary text-white p-2 px-6">
                    Ag-Innovation                </div>
                <div class="p-6 bg-white">
                    <h1 class="font-medium pb-4">
                        Hazel Tech to host Quality Summit featuring global insights on table grapes                    </h1>

                    <div class="flex justify-center">
                        <div class='sgp-banner sgp-desktop mt-6 mb-6' line_item_id='62cd9d9c6f6eae0021336151' id='sgp-62f19b1387049'>
            <a href='https://www.globalgrapesummit.com/' target='_blank' style='width: 728px; height:90px; display: inline-block;'>
                <img src='https://sgp-cdn.yentzengroup.com/uploads%2Fbanner%2Fsource%2F62cec9456f6eae002135e7c8%2Fsource_1657719109.jpeg' width='100%' height='100%' style='object-fit: cover;'>
            </a>
        </div>
        <script type='text/javascript'>
            if (jQuery('[line_item_id=62cd9d9c6f6eae0021336151]#sgp-62f19b1387049').is(':visible') ) {
                const payload = {
                    'lineItemId': '62cd9d9c6f6eae0021336151',
                    'event': 'print'
                }

                jQuery.ajax('https://www.freshfruitportal.com/wp-json/sgp/stats/new_event', {
                    dataType: 'json',
                    method: 'post',
                    data: JSON.stringify(payload),
                })
                console.log('print - 62cd9d9c6f6eae0021336151');
            }

            jQuery('[line_item_id=62cd9d9c6f6eae0021336151]#sgp-62f19b1387049').on('click', function() {
                
                const payload = {
                    'lineItemId': '62cd9d9c6f6eae0021336151',
                    'event': 'click'
                }

                jQuery.ajax('https://www.freshfruitportal.com/wp-json/sgp/stats/new_event', {
