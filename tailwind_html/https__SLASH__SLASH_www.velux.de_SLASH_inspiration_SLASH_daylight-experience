    <script type="text/javascript">

        function analyticsbuilder(emailValue) {
            if (emailValue === "") {
                return;
            }
            var etmc = window._etmc = window._etmc || [];
            etmc.push(["setOrgId", 510003975]);
            etmc.push(["setUserInfo", { "email": emailValue }]);
            etmc.push(["trackEvent", { "name": "Form submit" }]);
        }

        function activateTracking() {
            var form = document.forms[0];
            if (form) {
                var submitButton = form.querySelector('input[type="submit"]');

                if (submitButton) {
                    submitButton.addEventListener("click", function () {
                        analyticsbuilder(getEmailValue(form));
                    });
                }
            }
        }

        function getEmailValue(form) {
            var emailElement = form.querySelector('input[type="email"]');
            if (emailElement) {
                return emailElement.value;
            }
            return "";
        }

        activateTracking();
    </script>
                <input type="hidden" data-sc-fxb-condition value='{}' />
</form>
