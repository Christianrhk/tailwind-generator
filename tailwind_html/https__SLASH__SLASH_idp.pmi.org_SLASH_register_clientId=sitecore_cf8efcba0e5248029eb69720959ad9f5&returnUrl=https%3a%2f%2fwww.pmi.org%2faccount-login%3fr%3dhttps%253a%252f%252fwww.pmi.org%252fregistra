    <script src="/lib/jquery/jquery.min.js"></script>
    <script src="/js/idp.min.js?v=OTo8FVvnxtb495ObomsdIoVbEH3msZyV0LP14HGoBIc"></script>
    <script src="https://ajax.aspnetcdn.com/ajax/jquery.validate/1.16.0/jquery.validate.min.js"></script>
    <script src="https://ajax.aspnetcdn.com/ajax/jquery.validation.unobtrusive/3.2.6/jquery.validate.unobtrusive.min.js"></script>
    
        <script type="text/javascript">
            function logAnalyticsEvent(event) {
                if (!event) { return }

                if (typeof window.raiseAnalyticsTracking === "function") {
                    window.raiseAnalyticsTracking("pmi-add-tracking", event);
                } else {
                    console.warn("window.raiseAnalyticsTracking not found");
                }
            }

            $(function () {
                // Catch Model Validation Error
                var $modelValidationEl = $("#RegistrationModelError");
                var modelValidationError = $modelValidationEl.text();

                if (modelValidationError) {
                    var eventModelErrorDataBlock = {
                        "event": "sso-form-interaction",
                        "eventInfo": "sso-form-interaction",
                        "form": {
                            "action": "form-error",
                            "formName": "create-account",
                            "formError": modelValidationError,
                            "ssoplatform": ""
                        }
                    };

                    setTimeout(function () {
                        logAnalyticsEvent(eventModelErrorDataBlock);
                    }, 500);
                }

                // Send Form Start
                var registerFornElementFocused = false;
                $('form#RegistrationForm :input').focus(function (e) {
                    if (registerFornElementFocused) return;

                    var eventRegisterFormDataBlock = {
                        "event": "sso-form-interaction",
                        "eventInfo": "sso-form-interaction",
                        "form": {
                            "action": "form-start",
                            "formName": "create-account",
                            "formError": "",
                            "ssoplatform": ""
                        }
                    };

                    logAnalyticsEvent(eventRegisterFormDataBlock);
                    registerFornElementFocused = true;
                });
            });
        </script>

    <script type="text/javascript">
        var processing = false;

        $(function () {

            $("#RegistrationForm").on("submit", function () {
                var $submitBtn = $("button.registerButton");
                var isFormValid = $("#RegistrationForm").valid();


                        var errorsToSend = [];
                                    var els = $("input.segment-question, select.segment-question");
                var answersValid = true;
                var firstError = true;
                $.each(els, function (idx, el) {
                    var value = $(this).val();
                    var $parent = $(this).parent();
                    if ($parent.is(":visible")) {
                        if ($(el).hasClass("required") && value.trim().length === 0) {
                            answersValid = false;
                            $("span.field-validation-error", $parent).first().show();
                            if (isFormValid && firstError) {
                                firstError = false;
                                $(this).focus();
                            }


                                    errorsToSend.push(el.id);
                                                        }
                        else {
                            $("span.field-validation-error", $parent).first().hide();
                        }
                    }
                });

                var ageAndWorkExperienceAreValid = true;
                if ($("select#2").length > 0 && $("select#2").val().trim().length > 0 &&
                    $("select#1").length > 0 && $("select#1").val().trim().length > 0) {
                    var birthYear = parseInt($("select#2 option:selected").data("value"));
                    var age = (new Date()).getFullYear() - birthYear;
                    var experience = parseInt($("select#1 option:selected").data("value"));
                    if (age < experience) {
                        isFormValid = false;
                        ageAndWorkExperienceAreValid = false;
                        if (firstError) {
                            firstError = false;
                            $("select#2").focus();
                        }
                        var yearValidationErrorText = "Years of work experience must be less than age";
                        $("div.form-group.2 span.year-validation").show();
                        $("div.form-group.1 span.year-validation").text(yearValidationErrorText);
                        $("div.form-group.1 span.year-validation").show();


                                errorsToSend.push(["1","2"]);
                            
                    }
                    else {
                        $("div.form-group.2 span.year-validation").hide();
                        $("div.form-group.1 span.year-validation").hide();
                    }
                }
                if ($("select#1").length && $("select#1").val().trim().length > 0 &&
                    $("select#4").length > 0 && $("select#4").val().trim().length > 0) {
                    var experience = parseInt($("select#1 option:selected").data("value"));
                    var pmExperience = parseInt($("select#4 option:selected").data("value"));
                    if (experience < pmExperience) {
                        isFormValid = false;
                        if (firstError) {
                            firstError = false;
                            $("select#1").focus();
                        }
                        if (ageAndWorkExperienceAreValid === true) {
                            var ageAndWorkExperienceErrorText = "Years of full-time work experience must be greater than or equal to the value entered for years of managing projects over 4 weeks";
                            $("div.form-group.1 span.year-validation").text(ageAndWorkExperienceErrorText);
                            $("div.form-group.1 span.year-validation").show();


                                    errorsToSend.push(["1"]);
                                
                        }
                        $("div.form-group.4 span.year-validation").show();

                                errorsToSend.push(["4"]);
                                                }
                    else {
                        $("div.form-group.4 span.year-validation").hide();
                    }
                }


                        if (!isFormValid) {
                            var validator = $("#RegistrationForm").validate();
                            var errorList = validator.errorList;

                            $.each(errorList, function (index, err) {
                                errorsToSend.push(err.element.id);
                            });


                            var uniqueErrorsToSend = [];
                            $.each(errorsToSend, function(i, el){
                                if($.inArray(el, uniqueErrorsToSend) === -1) uniqueErrorsToSend.push(el);
                            });

                            var eventRegistrationFormErrorDataBlock = {
                                "event": "sso-form-interaction",
                                "eventInfo": "sso-form-interaction",
                                "form": {
                                    "action": "form-error",
                                    "formName": "create-account",
                                    "formError": uniqueErrorsToSend.join(),
                                    "ssoplatform": ""
                                }
                            };

                            logAnalyticsEvent(eventRegistrationFormErrorDataBlock);
                        }
                    
                var isValid = isFormValid && answersValid;
                if (!isValid) {
                    $submitBtn.removeClass("disabled");
                }
                processing = isValid;
                return isValid;
            });

        });

        function collectAnswers() {
            if (processing === true) {
                console.log("duplicate submit ignored...")
                return false;
            }

            processing = true;
            var $submitBtn = $("button.registerButton");
            $submitBtn.addClass("disabled");

            var answers = [];
            var els = $("input.segment-question, select.segment-question");
            $.each(els, function (idx, el) {
                var value = $(this).val();
                answers.push({ questionId: $(el).attr("questionid"), answerId: value });
            });
            $("#AnswersJson").val(JSON.stringify(answers));
            return true;
        }
    </script>

            <script>
            $(function() {
                $("#5").on("change", function() {
                    if ($(this).val() === "172") {
                        $("#6").parent().show();
                    }
                    else {
                        $("#6").val("");
                        $("#6").trigger("change");
                        $("#6").parent().hide();
                    }
                });

                $("#5").trigger("change");
            });
            </script>
            <script>
            $(function() {
                $("#6").on("change", function() {
                    if ($(this).val() === "175") {
                        $("#7").parent().show();
                    }
                    else {
                        $("#7").val("");
                        $("#7").trigger("change");
                        $("#7").parent().hide();
                    }
                });

                $("#6").trigger("change");
            });
            </script>
    
    <script src="/lib/tooltip/tooltip.js"></script>
    <script src="/lib/tooltip/popover.js"></script>
    <script src="/lib/tooltip/modal.js"></script>
    <script>
        $(function () {
            // Add tooltip
            var isMobile = false;
            if (window.innerWidth < "992") isMobile = true;
            if (isMobile) $(".tooltip-trigger").attr("data-toggle", "modal");
            else $(".tooltip-trigger").removeAttr("data-toggle").popover();
            
            // Save tooltip state on resize
            $(window).resize(function () {
                if ((window.innerWidth < "992") & !isMobile) {
                    isMobile = true;
                    if ($(".popover").hasClass("in")) {
                        $(".tooltip-trigger")
                        .popover("hide")
                        .next(".pmi-modal-help")
                        .modal("show");
                    }
                    $(".tooltip-trigger")
                    .popover("destroy")
                    .attr("data-toggle", "modal");
                } else if ((window.innerWidth >= "992") & isMobile) {
                    isMobile = false;
                    $(".tooltip-trigger").removeAttr("data-toggle").popover();
                    
                    if ($(".pmi-modal-help").hasClass("in")) {
                        $(".pmi-modal-help.in")
                        .modal("hide")
                        .prev(".tooltip-trigger")
                        .popover("show");
                    }
                }
            });
            
            // Close the modal on backdrop click
            $(".pmi-modal-help").on("shown.bs.modal", function (e) {
                $(".modal-backdrop.fade.in").click(function () {
                    console.log("close modal");
                    $(".pmi-modal-help").modal("hide");
                });
            });
        });
    </script>


