<script type="text/javascript">
    $(document).ready(function() {
        $("#sCountry").on("change", function () {
            var pk_c_code = $(this).val();
                        var url = 'https://omomakay.com/index.php?page=ajax&action=regions&countryId=' + pk_c_code;
                        var result = '';

            if (pk_c_code != '') {

                $("#sRegion").attr('disabled', false);
                $("#sCity").attr('disabled', true);

                $.ajax({
                    type: "POST",
                    url: url,
                    dataType: 'json',
                    success: function (data) {
                        var length = data.length;

                        if (length > 0) {

                            result += '<option value="">Select a region...</option>';
                            for (key in data) {
                                result += '<option value="' + data[key].pk_i_id + '">' + data[key].s_name + '</option>';
                            }

                            $("#region").before('<select class="form-control" name="sRegion" id="sRegion" ></select>');
                            $("#region").remove();

                            $("#city").before('<select class="form-control" name="sCity" id="sCity" ></select>');
                            $("#city").remove();

                            $("#sRegion").val("");

                        } else {

                            $("#sRegion").before('<input type="text" name="region" id="region" />');
                            $("#sRegion").remove();

                            $("#sCity").before('<input type="text" name="city" id="city" />');
                            $("#sCity").remove();

                        }

                        $("#sRegion").html(result);
                        $("#sCity").html('<option selected value="">Select a city...</option>');
                    }
                });

            } else {

                // add empty select
                $("#region").before('<select class="form-control" name="sRegion" id="sRegion" ><option value="">Select a region...</option></select>');
                $("#region").remove();

                $("#city").before('<select class="form-control" name="sCity" id="sCity" ><option value="">Select a city...</option></select>');
                $("#city").remove();

                if ($("#regionId").length > 0) {
                    $("#regionId").html('<option value="">Select a region...</option>');
                } else {
                    $("#region").before('<select class="form-control"  name="sRegion" id="sRegion" ><option value="">Select a region...</option></select>');
                    $("#region").remove();
                }
                if ($("#cityId").length > 0) {
                    $("#cityId").html('<option value="">Select a city...</option>');
                } else {
                    $("#city").before('<select class="form-control"  name="sCity" id="sCity" ><option value="">Select a city...</option></select>');
                    $("#city").remove();
                }
                $("#regionId").attr('disabled', true);
                $("#cityId").attr('disabled', true);
            }
        });

        $("#sRegion").on("change", function () {
            var pk_c_code = $(this).val();
                        var url = 'https://omomakay.com/index.php?page=ajax&action=cities&regionId=' + pk_c_code;
            
            var result = '';

            if (pk_c_code != '') {

                $("#sCity").attr('disabled', false);
                $.ajax({
                    type: "POST",
                    url: url,
                    dataType: 'json',
                    success: function (data) {
                        var length = data.length;
                        // exist $("#city") ? (hack?)
                        if ($("#city").length == 0 && length > 0) {
                            $("#sCity").before('<input type="text" name="city" id="city" />');
                            $("#sCity").remove();
                            console.log('log');
                        }

                        if (length > 0) {
                            result += '<option selected value="">Select a city...</option>';
                            for (key in data) {
                                result += '<option value="' + data[key].pk_i_id + '">' + data[key].s_name + '</option>';
                            }

                            $("#city").before('<select class="form-control" name="sCity" id="sCity" ></select>');
                            $("#city").remove();
                        } else {
                            result += '<option value="">No results</option>';
                            $("#sCity").before('<input type="text" name="city" id="city" />');
                            $("#sCity").remove();
                        }
                        $("#sCity").html(result);
                    }
                });
            } else {
                $("#cityId").attr('disabled', true);
            }
        });

        if ($("#sRegion").attr('value') == "") {
            $("#sCity").attr('disabled', true);
        }

        if ($("#countryId").length != 0) {
            if ($("#countryId").prop('type').match(/select-one/)) {
                if ($("#countryId").attr('value') == "") {
                    $("#sRegion").attr('disabled', true);
                }
            }
        }
    });
</script>
        <script>
    $('#sOrder').change(function(){
        var url = $(this).val();
        if (url) {
            window.location = url; // redirect
        }
        return false;
    });
    $('button.sub_button_click').click(function(e){
        e.preventDefault();
        $('form#sub_alert').toggleClass('hide');
    });
</script>
