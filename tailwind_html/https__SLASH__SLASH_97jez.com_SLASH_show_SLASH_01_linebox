    <script type="text/javascript">
        let isSending = false;
        function sendSms() {
            if (isSending === true) return ;
            let phoneInputElement = document.getElementById('phone');
            if (phoneInputElement.validity.patternMismatch) phoneInputElement.setCustomValidity('號碼格式必須是 09 開頭的十碼數字');
            else phoneInputElement.setCustomValidity('');
            if (!phoneInputElement.reportValidity()) return ;

            let request = new XMLHttpRequest();
            isSending = true;
            request.open('GET', "https://97jez.com/sendAuthSms?phone=" + phoneInputElement.value, true);
            request.onload = function() {
                let res = JSON.parse(request.responseText);
                if (request.status >= 200 && request.status < 400) {
                    if (res.success === true && res.code === 1) {
                        setTimeout(function(){
                            alert('已發送驗證碼, 請在手機上收取簡訊');
                            isSending = false;
                            document.getElementById('valid_code').focus();
                        }, 3000);
                    } else if (res.success === true && res.code === 2) {
                        setTimeout(function(){
                            alert('本門號先前已使用過');
                            isSending = false;
                                // that.isSentCode = true;
                            }, 3000);
                    } else {
                        alert(res.msg);
                        isSending = false;
                    }
                } else if (request.status == 429) {
                    alert('請直接聯繫客服');
                    isSending = false;
                } else {
                    alert('請直接聯繫客服');
                    isSending = false;
                }
            };
            request.onerror = function() {
                isSending = false;
                alert("失敗, 請直接聯繫客服");
            };
            isSentCode = false;
            request.send();
        }
    </script>
