<script>
        function shareNow() {
            $('.quick-share').toggleClass('shareActive');
        }

        $('#main').on('click', function() {
            $('.quick-share').removeClass('shareActive');
        })

        $(".linktext").on('click', function(event) {
            if (this.hash !== "") {
                event.preventDefault();
                var hash = this.hash;
                $('html, body').animate({
                    scrollTop: $(hash).offset().top
                }, 800, function(){

                    window.location.hash = hash;
                });
            }
        });

        $(document).ready(function($) {
            setTimeout(() => {
                $('#botCta').css('transform','translateY(0)')
            }, 2000);

            // setTimeout(() => {
            //     $('#botCta').removeClass('expanded');
            // }, 5000);

            $('#botCtaArrow').on('click', () => {
                $('#botCta').toggleClass('expanded')
            })

            $('#botCtaOfferBtn').on('click', () => {
                $('#offerField').show();
                $('#offerBtn').hide();
            })
        })

    </script>
