                var x = this.x || this.point.x
                var tooltip = tooltips[x] || null;
                if(tooltip){
                    //console.warn(this.point)
                    //debugger
                    return tooltip
                }
            }
            
            return this.y +'count <br/>on average<br/>on '+this.x;
        
