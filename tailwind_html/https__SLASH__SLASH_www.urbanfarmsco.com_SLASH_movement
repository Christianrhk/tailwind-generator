	  $(document).ready(function(){
	      var url = $("#cartoonVideo1").attr('src');
	      $("#cartoonVideo1").attr('src', '');
	    
	      $("#myModal1").on('shown.bs.modal', function(){
	          $("#cartoonVideo1").attr('src', url);
	      });
	      $("#myModal1").on('hide.bs.modal', function(){
	          $("#cartoonVideo1").attr('src', '');
	      });

	  });
