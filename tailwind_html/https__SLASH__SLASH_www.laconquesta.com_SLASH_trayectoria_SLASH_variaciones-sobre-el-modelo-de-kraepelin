  <script defer src="https://unpkg.com/alpinejs@3.2.3/dist/cdn.min.js"></script>
<script type="text/javascript" src="https://cdn.jsdelivr.net/npm/blazy@1.8.2/blazy.min.js"></script>
<script type="text/javascript" src="./assets/js/functions.js"></script>  <script src="https://player.vimeo.com/api/player.js"></script>

  <script src="https://unpkg.com/swiper/swiper-bundle.min.js"></script>

  <script>
    var swiper;

    function openSlider(id) {
      swiper = new Swiper(`#slide${id}`, {
        spaceBetween: 30,
        navigation: {
          prevEl: `#slide${id}-prev`,
          nextEl: `#slide${id}-next`,
        },
        preloadImages: false,
        lazy: true,
        observer: true,
        observeParents: true,
        observeSlideChildren: true,
        on: {
          slideChange: function() {
            if (this.isBeginning) {
              this.navigation.prevEl.classList.remove("flex");
              this.navigation.prevEl.classList.add("hidden");
            } else {
              this.navigation.prevEl.classList.remove("hidden");
              this.navigation.prevEl.classList.add("flex");
            }
            if (this.isEnd) {
              this.navigation.nextEl.classList.remove("flex");
              this.navigation.nextEl.classList.add("hidden");
            } else {
              this.navigation.nextEl.classList.remove("hidden");
              this.navigation.nextEl.classList.add("flex");
            }
          }
        }
      });
    }
  </script>

</body>

</html>