<script>
function cryptoPayment() {
    const total = $("#checkoutForm #totalAmount").text()
    $.ajax({
        type: "post",
        url: "/spirit/app/api/cryptoPayment.php",
        data: {
            amount: total
        },
        success: function(data) {
            $("#cryptoPayment #content").html(data)
        }
    });
    launchModal('cryptoPayment');
}
</script>

<script>
function checkIfLoggedUser() {
    console.log("Check")
    const user = $("#loggedInUser").val()
    const toHide = [
        '#loginInputs',
        /*'.login-actions-wrapper',*/
        '.existing-account-texts',
        '#notLoggedInputs'
    ]
    const toNotRequire = [
        "#loginInputs input[name='email']",
        "#loginInputs input[name='password']",
        "#notLoggedInputs input[name='first-name']",
        "#notLoggedInputs input[name='last-name']",
        "#notLoggedInputs input[name='company-name']",
        "#notLoggedInputs input[name='phone']",
        "#notLoggedInputs input[name='address']",
        "#notLoggedInputs input[name='city']",
        "#notLoggedInputs input[name='state']",
        "#notLoggedInputs input[name='country']",
        "#notLoggedInputs input[name='postal-code']",
        "#testInputEmail"
    ]
    const toMakeValueOne = [
        '#existingAccountInput',
        '#correctLoginInput'
    ]

    if (user.length > 1) { //existing user
        toHide.forEach(el => {
            $(el).addClass("hidden")
        })

        $(".login-actions-wrapper").addClass("hidden")

        toNotRequire.forEach(el => {
            $(el).prop('required', false)
            //$(el).removeAttr('required')
        })

        toMakeValueOne.forEach(el => {
            $(el).val('1')
        })
    } else {
        toHide.forEach(el => {
            $(el).removeClass("hidden")
        })

        toNotRequire.forEach(el => {
            $(el).prop("required", true)
            //$(el).addAttr('required')
        })

        toMakeValueOne.forEach(el => {
            $(el).val('0')
        })
    }
}

$(document).ready(function() {
    checkIfLoggedUser()
})

window.onload = function() {
    checkIfLoggedUser()
    $("#testInputEmail").prop('required', 'false')
    console.log("test")
}
</script>
<script>
$("#checkoutForm button").on("click", function(e) {
    e.preventDefault()
    checkIfLoggedUser()
    const source = $(this).attr('data-source')
    const total = $("#totalAmount").html()
    if (parseInt(total) > 0) {
        if (source === 'stripe') {
            $('#checkoutForm').attr('action', "/spirit/app/stripe/server/public/create-checkout-session.php")
                .submit();
        }
        if (source === 'paypal') {
            $('#checkoutForm').attr('action', "/spirit/app/paypal/checkout.php").submit();
        }
        if (source === 'crypto') {
            cryptoPayment()
        }
    } else {
        console.log("Error! The cart is empty!")
    }
});
</script>
<script>
$(document).ready(function() {
    $("#checkoutForm").validate()
});

/*
$("#checkoutForm").submit(function(e) {

    e.preventDefault();

    var form = $(this);
    var url = form.attr('action');

    if ($("#checkoutForm").valid()) {

    } else {
        e.preventDefault();
    }

});
*/

function tryToLogin() {
    console.log("Try to login")
    const email = $("#loginInputs input[name='email']").val()
    const password = $("#loginInputs input[name='password']").val()

    $.ajax({
        type: "GET",
        url: 'ajax-login.php?email=' + email + '&password=' + password,
        success: function(response) {
            var jsonData = JSON.parse(response);
            console.log(response)
            if (jsonData.success == "1") {
                $("#loggedInUser").val(email)
                $("#incorrectLogin").addClass("hidden")
                $(".logged-in-username").removeClass("hidden")
                $("#loggedInNoticeEmail").text(email)
                checkIfLoggedUser()
            } else {
                $("#loggedInUser").val('')
                $("#incorrectLogin").removeClass("hidden")
            }
        }
    });
}
</script>
