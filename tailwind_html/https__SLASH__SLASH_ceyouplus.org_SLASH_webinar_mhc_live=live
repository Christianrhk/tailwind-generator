	<script type="text/javascript">
		window.addEvent('domready',function(){
			var timezone = Cookie.read("jstz_timezone");
			if (!timezone && window.jstz) {
				timezone = jstz.determine();
				timezone = timezone.name();
				if (timezone == "Asia/Beirut") timezone = "Asia/Jerusalem";
				Cookie.write("jstz_timezone", timezone, {duration:7});
			}
			document.body.addClass(Browser.name+" v"+Browser.version+" "+Browser.platform);

			var tosPopup;
			if (window.LightFace && true) {
			    tosPopup = new LightFace.IFrame({
                    //constrain:true,
                    width: Math.min.attempt(Object.values(window.getSize()).map(function(n){ return n*.95;})),
                    height: 600,
                    url: '/assets/tos/ce.html',
                    buttons: [
                        {
                            title: 'Print',
                            event: function () {
                                var tos = $$('.lightface iframe')[0].contentWindow;
                                tos.focus();
                                tos.print();
                            }
                        },
                        {
                            title: 'Close',
                            event: function () {
                                this.close();
                            }
                        }
                    ]
                });
            }
			$$('a[href="terms/"]').addEvent('click', function(e){
				e.stop();
				var size = window.getSize();
				tosPopup.options.height = size.y *.9;
				tosPopup ? tosPopup.open() : window.open('/assets/tos/ce.html',"popup","height=600,width=800,status=1");
			});
		});
	</script>

	
	<script>window.fb_app_id = '1640808992795734';</script><script>
	function open_closeDisclosures(e){
		$$(".opened").removeClass("opened");
		$(e.target).addClass('opened');
	}

	var googleAd = ''
	  , extraTasks = function(){}
	  , useOMS = false	  , mapInit
	  , req;
	function arm(){
		var loadToId = "filtersContainer";
		if (!req)
			req = new Request.HTML(
				{ link:	"cancel",
                  update: loadToId,
                  evalScripts: false,
                  url:" webinars?ajax=1",
                  onRequest: function(){
                    $('directory').tween('opacity',0.5);
                    openPopup();
                    var adEl = $('googleAd');
                    if (adEl) googleAd = adEl.innerHTML || '';
				}, onSuccess: function(){ arm(); if (useOMS) runOMS(); }
			});

		$$('#filterContainer a').addEvent('click',function(e){
			var val = decodeURIComponent(this.get('href')).split('?')[1].split('=');
			var obj = {};
			obj[val[0].trim()] = (this.get('text') == 'Reset' ? '' : val[1].trim());
			req.get(obj);
			e.stop();
			});
		$$('.pgsMenu a').addEvent('click',function(e){
			var href = decodeURIComponent(this.get('href'));
			if (!href) return;
			var arr = href.split('?')[1].split('&');
			var obj = {};
			arr.forEach(val=>{
				var val=val.split('=');
				obj[val[0].trim()] = val[1].trim();
			})

			req.get(obj);
			e.stop();
			// window.location.href = "directory.php#topMenu";
			});
		$$('#filterContainer select').addEvent('change',function(e){
			var val = this.value
			  , obj = {};
			obj[this.name.trim()] = val.trim();
			req.get(obj);
			e.stop();
		});
		$$('#filterBox input').addEvent('keydown',function(e){
			if (e.key != "enter") return;
			var val = [this.get('name'),this.get('value')];
			var obj = {};
			obj[val[0].trim()] = val[1].trim();
			req.get(obj);
			e.stop();
			});

		// Search functions
		$$('#listingSearchVal').addEvent('keydown',function(e){
			if (e.key != "enter") return;
			$('listingSearchGo').fireEvent('click');
		});
		$$('#listingSearchGo').addEvent('click',function(){
			var val = $('listingSearchVal').value
			  , obj = {};
			const urlParams = new URLSearchParams(window.location.search);
			const string = urlParams.get('string');
			const category = urlParams.get('category');
			obj['string'] = string;
			obj['category'] = category;
			obj['search'] = val.trim().replace('(','\\(').replace(')','\\)');
			obj['searchParam'] = ($$('input[name=listingSearchParam]:checked').map(function(e) { return e.value; }))[0];
			req.get(obj);
		});
		$$('#listingSearchReset').addEvent('click',function(){
			var obj = {};
			obj['search'] = '';
			obj['reset'] = 1;
			req.get(obj);
		});
		// window.history.pushState('', '', '/directory.php');
		if (googleAd != '') $('googleAd').innerHTML = googleAd;

		var filterBox = $('filterBox');
		if (filterBox) filterBox.addEvent('click:relay(.expand-button)', function(event, el){
            el=$(el);
            var contentDiv = el.getParent().getNext().getFirst();
            var toggled = el.get('html') != '+';
            if (toggled == false){
                el.set('html','-');
                contentDiv.setStyle('max-height', '110px');
            }
            else {
                el.set('html', '+');
                contentDiv.setStyle('max-height', '0');
            }

        });

		Function.attempt(extraTasks);
		if (useOMS) {
			var mapContainer = new Element('div#mapContainer.loading',{html:"Click the map to search in that area:"})
				.adopt(new Element('div#map'));
						$('filterContainer').adopt(mapContainer);

            if ($('OMSloader')) {
                $('map').empty();
                mapContainer.removeClass('loading');
            }
            else window.addEvent('load',function(){
				document.head.adopt(new Element('script[src="assets/js/OpenLayers.js"]',{async:false})
					.addEvent('load',function(){
						document.head.adopt(new Element('script[src="assets/js/OMSloader.js"]#OMSloader',{async:false})
							.addEvent('load',function(){ mapInit = initOMS(); runOMS(); mapContainer.removeClass('loading'); }));
					}));
			});
		}

		var mainSection = $('mainSection');
		if (!mainSection) mainSection = $('center');
		if(window.scrollY > mainSection.offsetTop) mainSection.scrollIntoView();
	}
	function runOMS() {
		if (!$('map')) {
			$('filterContainer').adopt(new Element('div#mapContainer.loading',{html:"Click the map to search in that area:"})
				.adopt(new Element('div#map')));
		}
		$('directory').grab(new Element('input#coord',{type:"hidden",value:""}),"after");
		if (typeOf(mapInit) == "function") mapInit('map',$('coord').value,12);
	}
	window.addEvent('domready',arm);

	// Set function to run on map click
	function onMapClick(longitude, lattitude) {
		var obj = {'map': longitude+','+lattitude};
		req.get(obj);
	}

	function openPopup() {
		$('loadingBox').adopt(new Element('div',{
			'id':"loadingPopup"
		   ,'style':"border-radius:5px; box-sizing:content-box; position:absolute; padding:5px; height:30px; width:30px; "+
				"top:130px; left:34%; box-shadow:0 0 6px #333; font-style:italic; z-index:10;"
		   ,'html':"<img src='assets/img/loading.gif'>"}));
	}

	// All webinars - Reset search bar.
    function allWebinars(){
		var obj = {};
		obj['search'] = ' ';
		obj['reset'] = 1;
		req.get(obj);
		setTimeout(function(){
			window.location = 'webinars';
		},800);

	}

	window.onload = () => {
		let categoriesBoxes = document.querySelectorAll(".box-1");
		Array.from(categoriesBoxes).forEach(e => {
			e.addEventListener('click', (event) => {
				let a = event.target.querySelector("a");
				window.location = a.href;
			});
		})
	}
	function filterbyStateprofession(r) {
		if($_('.stateFilter').selectedIndex==0 || $_('.professionFilter').selectedIndex==0)return;
		r.preventDefault()
		location.href=location.fullpath+'?state='+($_('.stateFilter').value)+'&profession='+($_('.professionFilter').value);
	}
	function resetFilters(r) {
		r.preventDefault()
		location.href='/webinars/';}
