    {
        "@context": "https://schema.org",
        "@type": "Article",
        "headline": "No Surprises Act &amp; Good Faith Estimates",
        "image": "",
        "keywords": "0 CCB Hidde",
        "wordcount": "854",
        "url": "https://www.saintfrancisbartlett.com/no-surprises-act-good-faith-estimates",
