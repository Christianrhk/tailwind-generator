    const killAddThis = () => {
      const script = document.querySelector('#add-this-script');

      if (script) {
        script.remove();
      }

      const addThis = document.querySelectorAll('.addthis_inline_share_toolbox');

      console.log(addThis.length)

      addThis.forEach(el => {
        el.innerHTML = '';
      });
    }

    const handleAddThis = cookieHub => cookieHub.hasConsented('analytics')
        ? appendAddThisScript()
        : killAddThis();

