<script type="text/javascript">

        var replyToUserId = 0;

        
        $(document).ready(function() {

            $('textarea[name=status]', 'textarea[name=about]').autosize();

            $("textarea[name=postText]").bind('keyup mouseout', function(e) {
                var input = $("textarea[name=postText]");
                var count = input.val().length;
                var max_char = input.attr('maxlength');

                $("span#word_counter").empty();
                $("span#word_counter").html(max_char - count);
                //console.log('Something dey go down!');
                e.preventDefault();
            });

            $(".page_verified").tipsy({gravity: 'w'});
            $(".verified").tipsy({gravity: 'w'});

        });

    </script>
