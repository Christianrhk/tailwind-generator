<script type="text/javascript">
  var navbar = document.querySelector('#masthead');

  function pageScroll() {
    if (window.pageYOffset > 0) {
      navbar.classList.add('bg-coolGray-900')
      navbar.classList.remove('bg-transparent')
    } else {
      navbar.classList.remove('bg-coolGray-900')
      navbar.classList.add('bg-transparent')
    }
  };
  if (navbar) {
    window.onscroll = function() {
      pageScroll();
    }
    pageScroll();
  }
  window.addEventListener('load', function() {
    $(document).on('click', '.navbar-menu', function(e) {
      if(!$(e.target).not(this).length){
        $('.navbar-menu').addClass('hidden')
      }
    });
  });
</script>

