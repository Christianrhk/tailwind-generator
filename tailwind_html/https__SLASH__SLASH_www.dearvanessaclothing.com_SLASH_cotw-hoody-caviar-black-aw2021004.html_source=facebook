  $(document).ready(function() { 
    setProductImageHeight();
    lazyLoadImages( $('.lazyload') );
  });
  
  window.addEventListener('resize', setProductImageHeight);
  window.addEventListener('scroll', setProductImageHeight);
  
  var lazyLoadImages = function( imageElements ) {
    imageElements.each( function() {
        setImageSrcFromDataAttribute( $( this ) );
    });
  }
  
  var setImageSrcFromDataAttribute = function( imageElement ) {
    let imageUrl = imageElement.data( 'img' );
    imageElement.attr('src', imageUrl);
    setProductImageHeight()
  }
  
  // Make product-container same size as product-image
  function setProductImageHeight() {
    $('.product-container').height($('.product-image').outerHeight());
    $('.product-container').height($('.product-backlay').outerHeight());
  }
  
  function daisyAnimationCookie() {
    if (getCookie('daisy') == 'true')
    {
    } else {
      //start animation
      setCookie('daisy', true, 1);
    }
  }
  
  function getCookie(cname) 
  {
    let name = cname + "=";
    let decodedCookie = decodeURIComponent(document.cookie);
    let ca = decodedCookie.split(';');
    for(let i = 0; i <ca.length; i++) {
      let c = ca[i];
      while (c.charAt(0) == ' ') {
        c = c.substring(1);
      }
      if (c.indexOf(name) == 0) {
        return c.substring(name.length, c.length);
      }
    }
    return "";
  }
  
  function setCookie(cname, cvalue, exdays) 
  {
    const d = new Date();
    d.setTime(d.getTime() + (exdays*24*60*60*1000));
    let expires = "expires="+ d.toUTCString();
    document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
  }
