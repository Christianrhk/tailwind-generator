            {
                widgetCatalog = grecaptcha.render('recaptcha-custom-catalog', {
                    'sitekey' : '6Lf3F7AaAAAAAJiqdIuZNKCWLXBywCAZ1Gt-m7Vc',
                    'theme' : 'light'
                });
            }
        };

