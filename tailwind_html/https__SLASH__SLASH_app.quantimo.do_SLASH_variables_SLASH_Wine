        "formatter": function() {
            //debugger;
            var data = this.point.series.data;
            var i = this.point.index;
            var point = data[i - 1];
            if(!point){
                return this.point.name;
            }
            return point.tooltip;
        },
        "followPointer": false,
        "style": {
            "pointerEvents": "auto"
        },
        "useHtml": true,
        "hideDelay": 2000
    },
    "useHighStocks": false,
