        var loading = false;
        var o_max = 136;
        var loaded =10;
        var my_comments;
        var comments = [];
        window.onscroll = function (ev) {
            if ((window.innerHeight + window.scrollY) >= document.body.offsetHeight && loaded < o_max && !loading) {
                loading = true;
                fetch('/mc?ii=ec124091-9952-4ee9-9826-56292acfb7c1&x=' + loaded)
                    .then((resp) => resp.json())
                    .then((data) => { comments.push.apply(comments, data); loaded += data.length; my_comments.update({ "comments": comments }) }).catch((err) => alert(err)).then(() => loading = false);
            }
        };
    </script>
    <script>
        var photo_viewer;
        var time = 0;
        var imgs = [{"uuid":"f03745c1-ce5b-4a2a-aab3-e9e6fea4674a","portada":1}];
        var myPrivTok = null;

        function displayImg(index) {
            if (photo_viewer) {
                if (time == 0) {
                    photo_viewer.update({ items: imgs });
                    time = 1;
                }
                photo_viewer.update({ index: index, visible: true });
            }
        }
        function displayAvatar(av) {
            if (av && av.length > 0) {
                time = 0;
                photo_viewer.update({ index: -1, items: [] });
                photo_viewer.update({ src: av, visible: true });
            }
        }                        
    </script>
