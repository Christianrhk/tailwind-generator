  </style><script type="text/javascript">
    let preferences = document.getElementById("preferences");
    let statistics = document.getElementById("statistics");
    let marketing = document.getElementById("marketing");

    let consensoCookie = {
      technical: "ok",
      statistics: "no",
      marketing: "no"
    };

    window.addEventListener('DOMContentLoaded', (event) => {

      document.querySelector('#gdpr-all').addEventListener('click', function(e) {
        let consensoCookie = {
          technical: "ok",
          statistics: "ok",
          marketing: "ok"
        };
        optInCookiesConsented();
        setCookieGdpr("gdprConsent", JSON.stringify(consensoCookie));
        showThankYouWindow();

      });

        document.querySelector('#gdpr-ok').addEventListener('click', function(e) {
          showThankYouWindow();

          technical.checked
            ? consensoCookie.technical = "ok"
            : consensoCookie.technical = "no";
          marketing.checked
            ? consensoCookie.marketing = "ok"
            : consensoCookie.marketing = "no";

          if (marketing.checked) {
            optInCookiesConsented();
          }
          setCookieGdpr("gdprConsent", JSON.stringify(consensoCookie));
        });

        document.querySelector('#gdpr-reject').addEventListener('click', function(e) {
          showThankYouWindow();

          technical.checked
            ? consensoCookie.technical = "ok"
            : consensoCookie.technical = "no";
          marketing.checked
            ? consensoCookie.marketing = "no"
            : consensoCookie.marketing = "no";

          setCookieGdpr("gdprConsent", JSON.stringify(consensoCookie));

        });

    });

    function getConsensoCookie() {
      var stringa = getCookieGdpr("gdprConsent");
      var consenso = JSON.parse(stringa);
      return consenso;
    }

    function getCookieGdpr(cname) {
      var name = cname + "=";
      var ca = document.cookie.split(';');
      for (var i = 0; i < ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0) == ' ') {
          c = c.substring(1);
        }
        if (c.indexOf(name) == 0) {
          return c.substring(name.length, c.length);
        }
      }
      return "";
    }

    function setCookieGdpr(cname, cvalue, exdays) {
      var expires = "";
      if (exdays != undefined) {
        var d = new Date();
        d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
        expires = "expires=" + d.toUTCString();
      } else {
        expires = "expires = Fri, 31 Dec 9999 23:59:59 GMT";
      }
      document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
    }

    function showThankYouWindow() {
      document.querySelector('#gdpr-popup-container').classList.add('pulse');
      setTimeout(function() {
        document.querySelector('#gdpr-popup-container').style.display = 'none';
      }, 700);
      document.querySelector('#cookie-thank-you').classList.add('pulse');
      setTimeout(function() {
        document.querySelector('#cookie-thank-you').classList.remove('pulse');
        setTimeout(function() {
          document.querySelector('#cookie-thank-you').style.display = 'none';
        }, 700);
      }, 2100);
    }

    function optInCookiesConsented() {
      let scriptTag = document.createElement('script');
      scriptTag.setAttribute('src', 'https://www.googletagmanager.com/gtag/js?id=UA-191221551-1');
      document.head.appendChild(scriptTag);

      window.dataLayer = window.dataLayer || [];
      function gtag() {
        dataLayer.push(arguments);
      }
      gtag('js', new Date());
      gtag('config', 'UA-191221551-1', {'anonymize_ip': true});
    };
  </script><script type="application/ld+json">{"@context":"http://schema.org","@graph":[{"@type":"WebSite","author":{"@id":"#identity"},"copyrightHolder":{"@id":"#identity"},"copyrightYear":"2021","creator":{"@id":"#creator"},"dateModified":"2022-04-14T11:50:17+02:00","datePublished":"2021-09-17T11:30:00+02:00","description":"Die ECE Downpipes f√ºr das Tuning. üí® Mit den Downpipes aus Edelstahl ein neues Fahrgef√ºhl entdecken. Jetzt Mehrleistung mit Soundqualit√§t sp√ºren!","headline":"ECE Downpipes | Tuning","inLanguage":"de","mainEntityOfPage":"https://hjs-motorsport.de/produkte/tuning/ece-downpipe","name":"ECE Downpipes | Tuning","publisher":{"@id":"#creator"},"url":"https://hjs-motorsport.de/produkte/tuning/ece-downpipe"},{"@id":"#identity","@type":"LocalBusiness","name":"HJS Motorsport und Tuning","priceRange":"$"},{"@id":"#creator","@type":"Organization"},{"@type":"BreadcrumbList","description":"Breadcrumbs list","itemListElement":[{"@type":"ListItem","item":"https://hjs-motorsport.de","name":"Startseite","position":1},{"@type":"ListItem","item":"https://hjs-motorsport.de/produkte/tuning","name":"Tuning","position":2},{"@type":"ListItem","item":"https://hjs-motorsport.de/produkte/tuning/ece-downpipe","name":"ECE Downpipes","position":3}],"name":"Breadcrumbs"}]}</script><script src="/assets/js/lazysizes.min.js?v=1631721703" async></script></body></html>