<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
<script src="/js/onewaycargo.js"></script>
<script type="text/javascript">
    $('#regionCentralPanel').ready(() => {
        counter('#maritimoCentral', 17)
        counter('#aereoCentral', 4.1)
        counter('#correspondenciaCentral', 2)
    })

    let regionCentralSelector = () => {
        triggerClassesOnRegionSelect()
        counter("#maritimoCentral", 17)
        counter('#aereoCentral', 4.1)
        counter('#correspondenciaCentral', 2)
        counter('#maritimoRestoDelPais', 20)
        counter('#aereoRestoDelPais', 4.9)
        counter('#correspondenciaRestoDelPais', 2.5)
    }

    let regionRestoDelPaisSelector = () => {
        triggerClassesOnRegionSelect()
        counter('#maritimoRestoDelPais', 20)
        counter('#aereoRestoDelPais', 4.9)
        counter('#correspondenciaRestoDelPais', 2.5)
        counter('#maritimoCentral', 0)
        counter('#aereoCentral', 0)
        counter('#correspondenciaCentral', 0)
    }

    let mobileRegionSelector = () => {
        $('#mobileRegionSelector').val() == 'regionCentral'
            ? regionCentralSelector()
            : regionRestoDelPaisSelector()
    }

    let triggerClassesOnRegionSelect = () => {
        $('#restoDelPaisSelector').toggleClass([
            'bg-yellow-100',
            'border-yellow-600',
        ])
        $('#regionCentralSelector').toggleClass([
            'bg-yellow-100',
            'border-yellow-600',
        ])
        $('#regionRestoDelPaisPanel').toggleClass('hidden')
        $('#regionCentralPanel').toggleClass('hidden')
        $('#centralCities').toggleClass('hidden')
        $('#restoDelPaisCities').toggleClass('hidden')
    }

    let calculadora = () => {
        tarifa_aerea = 4.1
        tarifa_maritima = 17
        minimo_maritimo = 30

        peso = parseFloat($('#peso').val())
        largo = parseFloat($('#largo').val())
        ancho = parseFloat($('#ancho').val())
        alto = parseFloat($('#alto').val())

        if (
            typeof peso === 'number' &&
            typeof largo === 'number' &&
            typeof ancho === 'number' &&
            typeof alto === 'number'
        ) {
            if (peso > 0 && largo > 0 && ancho > 0 && alto > 0) {
                pesovol = Math.round((largo * alto * ancho) / 166)
                peso = Math.max(peso, pesovol)
                total_aereo = peso * tarifa_aerea

                pies = Math.round((largo * alto * ancho) / 1728)
                total_maritimo = Math.max(
                    minimo_maritimo,
                    pies * tarifa_maritima
                )
                counter('#piescalculados', pies, true)
                counter('#librascalculadas', peso, true)
                counter('#aereocalculado', total_aereo, true)
                counter('#maritimocalculado', total_maritimo, true)
            } else {
                counter('#piescalculados', 0, true)
                counter('#librascalculadas', 0, true)
                counter('#aereocalculado', 0, true)
                counter('#maritimocalculado', 0, true)
            }
        }
    }

    $('.calcinput').keyup(() => {
        calculadora();
    })

    calculadora();
  </script>
<script type="text/javascript">
    fbq('track', 'ViewContent');
  </script>
