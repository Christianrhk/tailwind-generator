<script>
jQuery(window).on("load", function(){

    // var doc_ready = jQuery.Deferred();

    console.log( "load running..." );

    jQuery.when( jQuery.ready ).then( function(){ 

    var v = jQuery('#content.site-content').css('top');

    v = v.replace( '-', '+');

    console.log( 'value: ' + v );

    w = jQuery('body').width();

    if(w <= 550 )
    {
        jQuery('#content.site-content').css('margin-top', v);
    }

    }); 

    jQuery(window).on('resize', function(){

        var win = $(this); 

        if (win.width() <= 550) 
        { 
            var v = jQuery('#content.site-content').css('top');

            v = v.replace( '-', '+');

            jQuery('#content.site-content').css('margin-top', v);
        }
        else jQuery('#content.site-content').css('margin-top', 0);

    });

})
</script>

