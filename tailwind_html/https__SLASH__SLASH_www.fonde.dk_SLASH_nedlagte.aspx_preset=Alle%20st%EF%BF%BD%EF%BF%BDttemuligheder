<script>
    //checkUserPermission();
    function checkUserPermission(redirectLink, buttonClickOnTrueId ) {
        var currentUserId = document.getElementById("CurrentUserId").value;

        var data_get = {
            userId: currentUserId
        };
        $.ajax({
            url: "/Security/CheckMemberPermission",
            data: data_get,
            dataType: 'json',
            type: 'GET',
            contentType: 'application/json;charset=utf-8',
            success: function (datas) {
                console.log("CheckMemberPermission : ", datas);
                if (redirectLink == "" && buttonClickOnTrueId == "") { return datas.Result; }

                if (datas.Result == false) {
                    if (datas.Message == "User not login") {
                        if (redirectLink != "" && redirectLink != null) {
                              location.href = "/Account/loginMember?returnUrl=" + redirectLink;
                            return false;
                        } else {
                            location.href = "/Account/loginMember?returnUrl=/";
                            return false;
                        }
                        return false;
                    }
                    $("#userPermissionMessage").html(datas.Message);
                    $("#userPermissionRedirectUrl").attr("href", datas.Model.Redirect);
                    $("#userPermissionModal").modal("show");
                    return false;
                } else {
                    console.log("CheckMemberPermission : ", datas);
                    if (redirectLink != "" && redirectLink != null) {
                    
                        const PathArr = redirectLink.split("/");
                        if (PathArr[0] == "fonde" || PathArr[0] == "vejviser" || PathArr[0] == "nedlagte") {
                            window.open(redirectLink)
                        } else {
                            location.href = redirectLink;
                        }
                  
                        return false;
                    } else if (buttonClickOnTrueId != "" && buttonClickOnTrueId != null) {
                        $("#" + buttonClickOnTrueId).click();
                    } else {
                        return true;
                    }
                }
            },
            error: function (error) {
                console.log('There was an error.' + error.responseText);
                return false;
            }
        }); // end $.ajax
        return false;
    }
</script>


<script>
    var data_get = {
            userId: ''
    };
    $.ajax({
        url: "/Security/CheckMemberPermission",
        data: data_get,
        dataType: 'json',
        type: 'GET',
        contentType: 'application/json;charset=utf-8',
        success: function (datas) {
            if (datas.Result == false) {
                $.ajax({
                    url: "/Security/CheckMemberPermission",
                    data: data_get,
                    dataType: 'json',
                    type: 'GET',
                    contentType: 'application/json;charset=utf-8',
                    success: function (datas) {
                        if (datas.Result == false) {
                            $(".gem-fond").html("");
                            $(".check-access").attr("onclick", "javascript:void(0)");
                            $("a.check-access").attr("href", "javascript:void(0)")
                            $("select.check-access").attr("onchange", "javascript:void(0)");
                        }
                    },
                    error: function (error) {
                        console.log('There was an error.' + error.responseText);
                        return false;
                    }
                }); // end $.ajax
            }
        },
        error: function (error) {
            console.log('There was an error.' + error.responseText);
            return false;
        }
    }); // end $.ajax
</script>

<script>
    function alwaysactive() {
        var PresetGroupName = $('#FoundationSortBy').val();
        $("input[type=checkbox][name *= 'months']").prop('checked', false);
        checkval();
        if (PresetGroupName == 'always-active') {
            $('#FoundationSortBy').val('');
            $("#search-bar").submit();
        }
        if (PresetGroupName != 'always-active' || PresetGroupName == null) {
            $('#FoundationSortBy').val('always-active');

            $("#search-bar").submit();
        }
    }

    $(".panel-heading").parent('.accord').hover(
        function () {
            $('.collapse').collapse('hide');
            $(this).children('.collapse').collapse('show');
        }, function () {
            $(this).children('.collapse').collapse('hide');
        }
    );
    var sortby_element = $("#FoundationSortBy");
    setSortByElement();
    function setSortByElement() {
        //Reset
        $(".foundationSortBy").removeClass("actived");
        //Set actived class
        var active_element = $("#" + sortby_element.val());
        active_element.addClass("actived");
    }

    function changeSortByValue(value) {
        sortby_element.val(value);
        setSortByElement();
        $("#search-bar").submit();
    }
</script>
