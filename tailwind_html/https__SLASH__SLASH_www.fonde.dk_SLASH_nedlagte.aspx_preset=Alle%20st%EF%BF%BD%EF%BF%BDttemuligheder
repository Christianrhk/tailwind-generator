<script>

    $(document).mouseup(e => {
        if (!$('.dropdown-button-wrapper').is(e.target) && $('.dropdown-button-wrapper').has(e.target).length === 0 ) {
            $('.menu-level-1').removeClass('block');
            $('.menu-item-1').removeClass('menu-item-1-active');
            $('.menu-level-2').removeClass('has-level-2-show-active');
        }

    })

       var arr = "daa992dc-e63e-4069-b5a9-b814ef4b9302,6ea6f14f-d8fd-4b72-b1bd-bfab2bd2d4be,13f18d45-f5b4-4821-8892-0aea43d3fce7,644d3a65-fb38-4439-b3b5-9eec3c94cc06,14b049d1-4e9d-499f-b96c-f1253ed9f9f1,522d579a-80f3-4235-87a1-6a00fc94c791,c4555e23-4697-41f4-8119-bb7315959456,077aca6d-e188-43d8-b7a2-ef00ea559652,4f303136-800a-4bdd-8f84-261012ad8030,f646a74d-40ec-42e9-ab2f-9a196a05d06d,c1161516-6b19-4c59-a3f8-fd09da565005,5d016460-d2b6-4070-9072-c4e91aea5d4f,c2abb8b5-c5d0-4e2f-b2ae-9c0c533ab256,6aefa503-5773-4b44-83ff-fdaab37f45f3,66d971cd-a0c5-4dc4-9134-fb201a8d0dd0,5c944633-37d3-4ffa-8d44-42c80ba143d8,b2d525cf-7111-4c80-961f-4077b22705be,9dc4dc7a-aa67-42c3-94c0-523f6de2062d,8b7444f6-64b7-4deb-8261-1573407eb103,72187542-5235-4030-805b-f0b821502492";

