            GetAppliedMineFonde(val);
            GetFondefavorite(val);

      });

    function CreateFondeMyList(id) {  
        var Name = $("#Name_" + id).val();
        var FondeList = $("#FondeList_" + id).val();
        if (Name == '' || Name == null) {
            $("#Name_" + id).css({ 'border-color':'red'});
            return false;
        } 
        $("#Name_CreateListForm").val(Name);
        $("#FondeList_CreateListForm").val(FondeList); 
        GoogleTagManagerPush('CreateNewList');
        $.post('/FondeMyList/AddListFondedirectory', $('#CreateListFrom').serialize()) 
        $("#Name_" + id).val("");
        //get 1
        GetAppliedMineFonde(id);
        //get 2
        setTimeout(function () {
            GetAppliedMineFonde(id)
        }, 2000);
    }


    function checkval() {
        $("input[type=checkbox][name *= 'months']").each(function () {
            if (this.checked) {
                $(this).parent().parent().css("background", "linear-gradient(#ff8827, #f15c22)");
                $(this).parent().parent().css("color", "white");
            } else {
                $(this).parent().parent().css("background", "");
                $(this).parent().parent().css("color", "");
            }
        });
        if ($('#FoundationSortBy').val() == 'always-active') {
            $('#FoundationSortBy').val('');
            $("#afventer-dato-stigende").css("color", "");
        }

    }

    function AddFondeMyList(id) { 
        var ListID = $("#ListID_" + id).val();
        var FondeList = $("#FondeListAdd_" + id).val();
        $("#ListID_AddListForm").val(ListID);
        $("#FondeList_AddListForm").val(FondeList);

        if ($("#ListID_" + id).val() !== "") {
            GoogleTagManagerPush('AddToMyLists');
            $.post('/FondeMyList/AddListFondedirectory', $('#AddListFrom').serialize())

            //get 1
            GetAppliedMineFonde(id);
            ////get 2
            setTimeout(function () {
                GetAppliedMineFonde(id)
            }, 2000);

        }
        else {
            $("#ListID_" + id).css("background", "#ff151533");
        }


    }

    function GetAppliedMineFonde(id)
    {
        $.get('/API/Foundation/GetAppliedMineFonde?id=' + id+'&username='+'',  // url
            function (data) {  // success callback

                if (data.AppliedAmount == 0)
                {
                    $(".AppliedAmount-id-" + id).hide();
                    return false;

                }

                if (data.AppliedAmount == 1) {
                    $("#AppliedAmount-id-" + id).html("TILFÃ˜JET â€“ " + data.AppliedAmount + " LISTE");
                }
                else {
                    $("#AppliedAmount-id-" + id).html("TILFÃ˜JET â€“ " + data.AppliedAmount + " LISTER");
                }

                $(".menu-level-1").hide("slow");
                $(".menu-level-1").removeClass('block');

                $(".AppliedAmount-id-" + id).show();
                $("#AppliedAmount-id-" + id).show();

            });

    }


    function Fondefavorite(id) {
        if ($("#fav-id-" + id).hasClass("fa-heart-o")) {
            $.post('/FondeMyList/AddFavorite', { FondeID: id, type: "vejviser" });
            $("#fav-id-" + id).removeClass("fa-heart-o").addClass("fa-heart");
        } else {
            $.post('/FondeMyList/DelFavorite', { FondeID: id, type: "vejviser" });
            $("#fav-id-" + id).removeClass("fa-heart").addClass("fa-heart-o");
        }
    }

    function GetFondefavorite(id) {

        $.post('/FondeMyList/GetFavorite', { FondeID: id, type: "vejviser" })
            .done(function (data) {
                if (data != 0) {
                    $("#fav-id-" + id).removeClass("fa-heart-o").addClass("fa-heart");
                }
            });
    }

</script>

