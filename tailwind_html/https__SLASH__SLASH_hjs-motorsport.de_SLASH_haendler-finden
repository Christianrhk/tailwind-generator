  function appendMarker(map, latitude, longitude, text, id) {
    mapEl = document.querySelector('#map');
    var pos = {
      lat: latitude,
      lng: longitude
    };
    var markerOption = {
      position: pos,
      map: map
    };
    var text = '<p>' + text + '</p><button id="button-' + id + '" name="openPopUp" onclick="openPopUp(' + id + ');" class="mt-3 btn btn-filled-blue">H√§ndler kontaktieren</a>';
    var infowindow = new google.maps.InfoWindow({content: text});
    var marker = new google.maps.Marker(markerOption);
    marker.hjs = id;

    marker.addListener('click', function() {
      if (activeInfoPopup) {
        activeInfoPopup.close();
      }
      infowindow.open(map, marker);
      activeInfoPopup = infowindow;
    });

    document.querySelector('.partner-list-item[data-id="' + id + '"] button[name="showTrader"]').addEventListener('click', function() {
      if (activeInfoPopup) {
        activeInfoPopup.close();
      }
      activeInfoPopup = infowindow;

      infowindow.open(map, marker);
      mapEl.scrollIntoView({ behavior: "smooth" });
    });

    document.querySelector('.partner-list-item[data-id="' + id + '"] button[name="openPopUp"]').addEventListener('click', function() {
      if (activeInfoPopup) {
        activeInfoPopup.close();
      }
      mapEl.scrollIntoView({ behavior: "smooth" });
      activeInfoPopup = infowindow;

      openPopUp(id);
    });

    markers.push(marker);
  }

  function openPopUp(id) {
    popup = document.getElementById('popup-wrapper');

    popup.classList.add('active');
    document.body.classList.add('overflow-hidden');
    setPopUpData(id);
  }

  function setPopUpData(id) {
    popup = document.querySelector('#popup-wrapper');

    var tmpObj = dataMap.get(id);

    popup.querySelector('#partnerName').innerHTML = tmpObj.title;
    popup.querySelector('#partnerEmail').innerHTML = tmpObj.email;
    popup.querySelector('#partnerEmail').href = 'mailto:' + tmpObj.email;
    popup.querySelector('#partnerAddress--street').innerHTML = tmpObj.street;
    popup.querySelector('#partnerAddress--zipCode').innerHTML = tmpObj.zipCode;
    popup.querySelector('#partnerAddress--location').innerHTML = tmpObj.location;
    popup.querySelector('#partnerAddress--country').innerHTML = tmpObj.country;
    popup.querySelector('#partnerPhone').innerHTML = tmpObj.phone;
    popup.querySelector('#partnerPhone').href = 'tel:' + tmpObj.phone;
    popup.querySelector('#partnerWebsite').innerHTML = tmpObj.website.replace('http://', '');
    popup.querySelector('#partnerWebsite').href = tmpObj.website;
    popup.querySelector('#partnerMotorsport').innerHTML = tmpObj.motorsport ? '‚àö' : 'X';
    popup.querySelector('#partnerTuning').innerHTML = tmpObj.tuning ? '‚àö' : 'X';
    popup.querySelector('#toEmailField').value = tmpObj.emailHashed;
  }

