</style><script type="text/javascript">

  function searchInputChanged(el) {
    document.querySelectorAll('.partner-list-item').forEach(item => {
      showItem = true;

      if (!item.getAttribute('data-title').toLowerCase().includes(el.value.toLowerCase()) && !item.getAttribute('data-city').toLowerCase().includes(el.value.toLowerCase())) {
        showItem = false;
      }

      for (var i = 0; i < filter.length; i++) {
        if (item.getAttribute('data-' + filter[i]) != 1) {
          showItem = false;
          break;
        }
      }
      if (showItem) {
        item.classList.remove('hidden');
        findObjectByKey(markers,'hjs',parseFloat(item.dataset.id)).setVisible(true);
      } else {
        item.classList.add('hidden');
        findObjectByKey(markers,'hjs',parseFloat(item.dataset.id)).setVisible(false);
      }
    });
  }

  function filterChanged() {
    var number = 0;
    document.querySelectorAll('.partner-list-item').forEach(item => {
      var showItem = true;

      switch (motorsportTuningFilter) {
        case 'tuning':{
          if (!item.dataset.tuning) {
            showItem = false;
          }
        };break;
        case 'motorsport':{
          if (!item.dataset.motorsport) {
            showItem = false;
          }
        };break;
      }

      if (countryFilter.length) {
        if (item.dataset.country != countryFilter) {
          showItem = false;
        }
      }

      if (tuningWorkshopFilter.length) {
        if (!item.dataset.tuningworkshop) {
          showItem = false;
        }
      }

      if (individualAcceptanceFilter.length) {
        if (!item.dataset.individualacceptance) {
          showItem = false;
        }
      }

      if (showItem) {
        item.classList.remove('hidden');
        findObjectByKey(markers,'hjs',parseFloat(item.dataset.id)).setVisible(true);
        number++;
      } else {
        item.classList.add('hidden');
        findObjectByKey(markers,'hjs',parseFloat(item.dataset.id)).setVisible(false);
      }
    });

    if (countryFilter.length) {
      const request = {
        query: countryFilter,
        fields: ["geometry"],
      };

      service = new google.maps.places.PlacesService(map);
      service.findPlaceFromQuery(request, (results, status) => {
        if (status === google.maps.places.PlacesServiceStatus.OK && results) {
          map.setCenter(results[0].geometry.location);
        }
      });
    }else {
      map.setCenter({ lat: 51.07029925578371, lng: 11.279187197763951, zoom: 5.5 });
    }

    document.getElementById('number-trader').innerHTML = number;
  }

  motorsportTuningFilter = '';
  countryFilter = '';
  tuningWorkshopFilter = '';
  individualAcceptanceFilter = '';

  tuningWorkshop = document.querySelector('label[for="tuningWorkshop"]');
  individualAcceptance = document.querySelector('label[for="individualAcceptance"]');

  document.querySelectorAll('.filter-option').forEach(item => {
    item.addEventListener('change', event => {
      switch (item.id) {
        case 'motorsportTuning': {
          if (item.value == 'all') {
            motorsportTuningFilter = '';
          }else {
            motorsportTuningFilter = item.value;
          }

          if (item.value == 'tuning') {
            tuningWorkshop.classList.remove('hidden');
            individualAcceptance.classList.remove('hidden');
          }else {
            tuningWorkshop.classList.add('hidden');
            individualAcceptance.classList.add('hidden');

            tuningWorkshopFilter = '';
            individualAcceptanceFilter = '';
          }
        }break;
        case 'country': {
          if (item.value == 'all') {
            countryFilter = '';
          }else {
            countryFilter = item.value;
          }
        }break;

        case 'tuningWorkshop': {
          if (item.checked) {
            tuningWorkshopFilter = '1';
          }else {
            tuningWorkshopFilter = '';
          }
        }break;

        case 'individualAcceptance': {
          if (item.checked) {
            individualAcceptanceFilter = '1';
          }else {
            individualAcceptanceFilter = '';
          }
        }break;
      }

      filterChanged();
    });
  });

  function findObjectByKey(array, key, value) {
    for (var i = 0; i < array.length; i++) {
      if (array[i][key] === value) {
        return array[i];
      }
    }
    return null;
  }
</script></div><div class="partner-list-item"
