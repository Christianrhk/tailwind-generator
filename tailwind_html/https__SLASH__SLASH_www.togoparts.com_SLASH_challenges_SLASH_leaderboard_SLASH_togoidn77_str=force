        })

        $("#feedbackBtn").on('click',function(){
            var url = '';
            window.open(url,'_blank');
        })
        $(".uLLnk").on('click',function(){
            var url = 'https://www.togoparts.com/user/profile/';
            window.location.href = url;
        })
        var objDiv = document.getElementById("fcommCtr");

        objDiv.scrollTop = objDiv.scrollHeight;
        var objDiv1 = document.getElementById("mobModalCommentsContent");
        objDiv1.scrollTop = objDiv1.scrollHeight;

        $(document).on('keypress','#leaderBoardSearchField',function(e){
            if(e.which == 13){
                $("#leaderBoardSearchBtn").trigger('click');
            }
        })
                $("#bibBtnPanel").on('click',function(){
                        $("#login-modal").modal('show');
                    })
        $(document).on('click','.uRideFlag',function(){
            if($(this).hasClass('disabled') || $(this).attr('disabled')){
                return false;
            }
            $('#owner_id').val($(this).data('ownerid'));
            $('#activity_cid').val($(this).data('activitycid'));
            $('#activity_id').val($(this).data('activityid'));
            $('#challenge_name').val($(this).data('challengename'));
            $('#flagsCount').val($(this).attr('data-acount'));
        });
        $('.flag-ride-btn').click(function(){
            var flag_comment = $('#flag_comment').val(),
                owner_id = $('#owner_id').val(),
                activity_cid = $('#activity_cid').val(),
                activity_id = $('#activity_id').val(),
                challenge_name = $('#challenge_name').val();
            flagsCount     = parseInt($("#flagsCount").val());
            /*console.log(flag_comment);
            console.log(owner_id);
            console.log(activity_cid);
            console.log(activity_id);
            console.log(challenge_name);*/

            var wordcount = flag_comment.split(' ');
            wordcount = wordcount.length;

            //alert('.flag-icon-'+activity_id);

            if(flag_comment != '' && wordcount >= 5) {
                $.ajax({
                    url: 'https://www.togoparts.com/members/ajax_add_flag_comments.php',
                    method: 'POST',
                    data: { owner_id: owner_id, activity_id: activity_id, activity_cid: activity_cid, flag_comment: flag_comment, challenge_name: challenge_name },
                    success: function(response){
                        if(response > 0) {
                            $('.flag-message').show();
                            $('.flag-message').addClass('text-success');
                            $('.flag-message').addClass('bg-success');
                            $('.flag-message').html('Thank you for sharing this with us. We will check on this immediately.');
                            //alert('Thank you for sharing this with us. We will check on this immediately.');
                            setTimeout(function(){ $('#flag-modal').modal('hide'); }, 1000);
                            $(".uRideFlag-"+activity_id).html('<i class="fa fa-flag flag-icon-'+activity_id+' mrgr5"></i>'+(++flagsCount));
                            $('.uRideFlag-'+activity_id).attr('disabled', true);
                            $('.uRideFlag-'+activity_id).addClass('disabled');
                            $('.uRideFlag-'+activity_id).attr('title','Already Flagged');
                            $('.uRideFlag-'+activity_id).removeAttr('data-toggle');
                            $('.uRideFlag-'+activity_id).removeAttr('data-target');
                            //location.reload();
                        } else {
                            //alert('Something went wrong. Please try again.');
                            $('.flag-message').show();
                            $('.flag-message').addClass('text-danger');
                            $('.flag-message').addClass('bg-danger');
                            $('.flag-message').html('Something went wrong. Please try again.');
                        }

                        //console.log(response);
                    },
                    error: function() {
                        //alert('Something went wrong. Please try again.');
                        $('.flag-message').show();
                        $('.flag-message').addClass('text-danger');
                        $('.flag-message').addClass('bg-danger');
                        $('.flag-message').html('Something went wrong. Please try again.');
                    }
                });
            } else {
                //alert('Please share your thoughts about this ride for at least 5 or more words.');
                $('.flag-message').show();
                $('.flag-message').addClass('text-danger');
                $('.flag-message').addClass('bg-danger');
                $('.flag-message').html('Please share your thoughts about this ride for at least 5 or more words.');
            }
        });

        if(viewType == 'leaderboard' || viewType == '' || viewType == 'list'){
                            getLeaderBoardContent('58',$("#pagedVal").val(),'','','','outdoor','','',0);
                        
        }else if(viewType == 'gallery'){
            getLeaderBoardActivities('58',$("#pagedVal").val(),'','','','outdoor','','',0);
        }else if(viewType == 'region'){
            var att = 'all';
            if(chId == 72){
                att = 'overall';
            }
            getRegionalLeaderboardContent(chId,$("#pagedVal").val(),'',att,'','',0);
        }else if(viewType == 'level'){
            var aParam = '';
            var actType = '';
            if(chId == 56){
                aParam = $("#lSchoolLvlSel").val();
            }
            getLevelLeaderboardContent(chId,$("#pagedVal").val(),'',actType,'','',0,aParam);
        }else if(viewType == 'team'){
            getTeamLeaderboardContent(chId,$("#pagedVal").val(),'','all','','',0);
        }

        $(document).on('click','#snt__vt',function(){
            var vall = $(this).attr('data-val');
            if(vall == ''){return false;}

            if(vall == 't1mmapp'){
                getLeaderBoardContent('58',$("#pagedVal").val(),'scf_team','','','outdoor','','',0);
                $(this).attr('data-val','t2mmapp');
                $("#snt__vt span").text('Show Leaderboard');
            }else if(vall == 't2mmapp'){
                getLeaderBoardContent('58',$("#pagedVal").val(),'','','','outdoor','','',0);
                $(this).attr('data-val','t1mmapp');
                $("#snt__vt span").text('Show National Team');
            }

            return false;
        })

        
        $('.event_end_stats').click(function(){
            var searchtext = $(this).text();
            $("#leaderBoardSearchField").val(searchtext);
            if (window.matchMedia("(max-width: 767px)").matches) {
                $([document.documentElement, document.body]).animate({
                    scrollTop: $(".uLListViewContainer").offset().top-150
                }, 1000);
            } else {
                $([document.documentElement, document.body]).animate({
                    scrollTop: $(".uLListViewContainer").offset().top-180
                }, 1000);
            }


        });

        $(document).on('click','.lHeroAchContainer',function(){
            var url = $(this).attr('data-href');
            if(url == ''){
                return false;
            }

            window.location.href = url;
        })

        $(document).on('click','.langSel',function(e){
            e.preventDefault();
            if($(this).hasClass('selected')){
                return;
            }
            var vall = $(this).attr('data-value');
            $("#lang").val(vall);
            $("form[name='langForm']").submit();
            return false;
        });
    });

</script>
