        pauseOnHover : false,
        nextArrow: '<a class="slick-next tgNext" aria-label="Next"><i class="fa fa-chevron-right"></i></a>',
        prevArrow: '<a class="slick-prev tgPrev" aria-label="Previous"><i class="fa fa-chevron-left"></i></a>'
