<script type="text/javascript">
    $(document).ready(function(){
        $("#vlsFedSMdL, .clsFedBck").on('click',function(e){
            e.preventDefault();
            $("#feedSMdL").modal('hide');
        })
    })
</script>
<script type="text/javascript">
    var getParamMdL = new URLSearchParams(window.location.search);
    var getParamFeedbackForm = getParamMdL.get('eventFed');
    $(document).ready(function(){
        $(document).on('click','#bottomSheetFeedback', function(e){
            e.stopPropagation();
            if ( $( "#mobEventFeedbackForm" ).is( ":hidden" ) ) {
                $('.feedBackdrop').show();
                // $('body,html').css('overflow','hidden');
                $( "#mobEventFeedbackForm" ).slideDown( 400 );
            }
        });
        if(getParamFeedbackForm){
            setTimeout(function(){
                if($(window).width() < 768){
                    if ( $( "#mobEventFeedbackForm" ).is( ":hidden" ) ) {
                        $('.feedBackdrop').show();
                        $( "#mobEventFeedbackForm" ).slideDown( 400 );
                    }
                }else{
                    if ( $( "#eventFeebackForm" ).is( ":hidden" ) ) {
                        // $('.feedBackdrop').show();
                        $( "#eventFeebackForm" ).modal('show')
                    }
                }
                console.log('Timeout Is param');
            },400);
            console.log('Is param');
        }

        $(document).on('click',".closeEventMobileFeedbackModal",function(e){
            e.preventDefault();
            if ( $( "#mobEventFeedbackForm" ).is( ":hidden" ) ) {
            } else {
                $('.feedBackdrop').hide();
                $( "#mobEventFeedbackForm" ).slideUp( 400 );
            }
        })

        $(document).on('click','.feedBackdrop',function(e){
            e.stopPropagation();
            if ( $( "#mobEventFeedbackForm" ).is( ":hidden" ) ) {
                // Do nothing
            } else {
                $('.feedBackdrop').hide();
                // $('body,html').css('overflow','initial');
                $( "#mobEventFeedbackForm" ).slideUp( 400 );
            }
        })
        $(document).on('click',".closeEventFeedbackModal",function(e){
            e.preventDefault();
            $("#eventFeebackForm").modal('hide');
        })

        $('.iContentDescription').change(function () {
            var origiN = $(this).attr("data-from");
            var formF = '';
            if(origiN == 'mobile'){
                formF = '#mobEventFeedbackForm';
            }else{
                formF = '#fedDesktop';
            }
            if ($.trim($(formF + ' .iContentDescription').val()).length > 1) {
                err = 0;
                $(formF + " .issueDescription").slideUp();

            }
        });

        $(document).on('click','.submitSupportRequest', function(e){
            e.preventDefault();
            var origiN = $(this).attr("data-from");
            var checkErr = $("#checkAll").attr("data-err");

            var formF = '';
            if(origiN == 'mobile'){
                formF = '#mobEventFeedbackForm';
            }else{
                formF = '#fedDesktop';
            }
            var err = 0;
            if($.trim($(formF + ' .iContentDescription').val()).length < 1 ){
                $(formF + " .issueDescription").slideDown();
                err = 1;
            }else{
                err = 0;
                $(formF + " .issueDescription").slideUp();
            }

            if($.trim($(formF + ' .userNameField').val()).length == 0){
                $(formF + " .userNameNoValMsg").slideDown();
                err = 1;
            }else{
                err = 0;
                $(formF + " .userNameNoValMsg").slideUp();
            }

            if(err == 1 || checkErr == 1){
                return false;
            }else{
                var cid = $(formF + " select[name='queryForEvent'] option:selected").val();
                var userid = $(formF + ' input[name="userid"]').val();
                var username = $(formF + ' input[name="username"]').val();
                var query = $(formF + " select[name='query'] option:selected").text();
                var issueDescription = $.trim($(formF + ' .iContentDescription').val());

                var fdata = new FormData();
                fdata.append('action','supportRequest');
                fdata.append('cid',cid);
                fdata.append('userid',userid);
                fdata.append('username',username);
                fdata.append('query',query);
                fdata.append('issueDescription',issueDescription);
                sendajaxrequest('supportRequest',fdata,'users');
            }
            // alert("Submitting");
        })

        $(document).on('keyup','input[name="username"]',function(e){
            var username = e.target.value;
            $(".userNameNoValMsg").slideUp();

            if(e.keyCode == 13){
                e.preventDefault();
                return false;
            }
            checkUsername(username);
        })

        function checkUsername(username){
            var fdata = new FormData();
            fdata.append('action','validateUsername');
            fdata.append('vall',username);
            fdata.append('feed',1);
            sendajaxrequest('validateUsername',fdata,'users');
        }
    })
</script>
