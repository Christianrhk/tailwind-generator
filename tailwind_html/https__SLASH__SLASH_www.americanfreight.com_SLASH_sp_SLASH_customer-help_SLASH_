    $(subMenu).parent().attr("data-toggle","dropdown");
    $(subMenu).before('<a class="caret" href="javascript:void(0);"><i class="fa fa-chevron-down"></i></a>');

    $('.caret').click(function(){
      if($(subMenu).hasClass('open')){
        $(subMenu).removeClass('open')
