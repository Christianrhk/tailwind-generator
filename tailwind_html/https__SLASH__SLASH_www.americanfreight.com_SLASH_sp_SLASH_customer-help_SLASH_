<script>
  function toggleNav() {
    var mask = document.getElementById('mask');
    var nav = document.getElementById('mobileNav');

    mask.classList.toggle('hidden');
    nav.classList.toggle('hidden');
  }

  function buildMobileCatLinks(i) {
    var catHtml = '';

    for (var s = 0; s < nav.vertical[i].dept.length; s++) {
      catHtml = catHtml + "<div class='p-2'><a href='https://americanfreight.com" + nav.vertical[n].dept[s].url + "'>" + nav.vertical[n].dept[s].title + buildMobileSubCatLinks(nav.vertical[i].dept[s]) + "</a></div>";
    }

    return catHtml;
  };

  function buildMobileSubCatLinks(s) {
    return '';
    // return "<a tabindex='-1' class='block whitespace-nowrap pl-2 text-md pb-2' href='https://americanfreight.com/br/plp/" + s.deptName + "/" + s.dept_name.replace(/[^a-zA-Z0-9]/g, '-').replace(/\--/g, '').toLowerCase() + "/" + s.category_name.replace(/[^a-zA-Z0-9]/g, '-').replace(/\--/g, '-').toLowerCase() + "/" + s.category_id + "?pno=1&sortBy=1&psize=25'>" + s.category_name + "</a>";
  }

  function toggleSubcat(el, index) {
    el.querySelector('.mobileFlyout').classList.toggle('hidden');
  }

  function buildMobileDeptLink(v, index) {
    return "<button onclick='toggleSubcat(this)' class='p-2 block text-xl pt-2 pb-2 border-b w-full text-left border-b border-t-0 border-gray-400'>" + v.title + "&nbsp;&nbsp;<i class='fal fa-angle-right float-right text-black text-3xl pt-2' aria-hidden='true'></i>&nbsp;&nbsp;<div class='mobileFlyout hidden'><a class='bg-gray-200 block p-2' href='https://americanfreight.com" + v.url + "'>Shop All " + v.title + "</a>" + buildMobileCatLinks(index) + "</div></button>";
  }

  var mobileDeptHtml = '';

  for (var n = 0; n < nav.vertical.length; n++) {
    mobileDeptHtml = mobileDeptHtml + buildMobileDeptLink(nav.vertical[n], n); // set the main depts
  }

  setTimeout(() => {
    document.getElementById('mobileNavLinks').innerHTML = mobileDeptHtml;
  }, 1000);
</script>
