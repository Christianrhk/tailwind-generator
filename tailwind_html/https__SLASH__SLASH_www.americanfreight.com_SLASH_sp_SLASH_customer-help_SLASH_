<style>
  .flyout {
    display: none !important;
    position: absolute;
    top: 99%;
    width: 100%;
    min-width: 800px;
    min-height: 250px;
    background: rgb(255, 255, 255);
    left: 0px;
    padding: 8px;
    padding-top: 5px;
    z-index: 9999;
  }

  #nav span:hover .flyout {
    display: block !important;
  }
  /* #nav span:hover .flyout, #nav span:focus .flyout, .flyout--trigger:focus + .flyout {
    display: block !important;
  } */

  .flyout--trigger[aria-expanded="true"] + .flyout {
    display: block !important;
  }
</style>


<script type="text/javascript" src="https://www.americanfreight.com/sp/wp-content/themes/ascendio-child-redesign/library/components/nav.js"></script>


<script>
  function buildDeptLink(v) {
    return "<a class='pr-6 pb-2 pt-2 inline-block' href='" + v.url + "'>" + v.title + "&nbsp;&nbsp;&nbsp;&nbsp;</a><button type='button' class='flyout--trigger' style='border: none;height: 25px;display: inline-block; margin-left: -32px;margin-top: 8px; margin-right: 15px;'><i class='fal fa-angle-down text-gray-500' aria-hidden='true'></i></button>";
  }

  function buildCatLink(c) {
    return "<div><div class='whitespace-nowrap p-2 bg-gray-100 mb-3'><a class='block whitespace-nowrap' href='" + c.url + "'>" + c.title + "<br /><small>Shop All</small></a></div></div>";
  }

  function buildSubCatLink(s) {
      return "<a class='block whitespace-nowrap pl-2 text-md pb-2' href='https://americanfreight.com/br/plp/" + s.deptName + "/" + s.dept_name.replace(/[^a-zA-Z0-9]/g, '-').replace(/\--/g, '').toLowerCase() + "/" + s.category_name.replace(/[^a-zA-Z0-9]/g, '-').replace(/\--/g, '-').toLowerCase() + "/" + s.category_id + "?pno=1&sortBy=1&psize=25'>" + s.category_name + "</a>";
  }

  var deptHtml = '';

  for (var n = 0; n < nav.vertical.length; n++) {
    // build the depts
    deptHtml = deptHtml + '<span class="inline-block">';
    deptHtml = deptHtml + buildDeptLink(nav.vertical[n]); // set the main depts

    deptHtml = deptHtml + '<div class="flyout shadow-lg w-screen container pt-6"><div class="grid grid-rows-2 grid-flow-col gap-2">';
    // build the cats
    for (var c = 0; c < nav.vertical[n].dept.length; c++) {
      deptHtml = deptHtml + '<div>';
      deptHtml = deptHtml + buildCatLink(nav.vertical[n].dept[c]);

      // build the subcats
      for (var s = 0; s < nav.vertical[n].dept[c].cat.length; s++) {
        deptHtml = deptHtml + buildSubCatLink(nav.vertical[n].dept[c].cat[s]);
      }
      deptHtml = deptHtml + '</div>';
    }

    deptHtml = deptHtml + '</div></div>';
    deptHtml = deptHtml + '</span>';
  }

  setTimeout(() => {
    document.getElementById('nav').innerHTML = deptHtml;

    var focusEl  = jQuery('#header--logo');

    // accessibility items
    jQuery('.flyout--trigger').on('keyup', function (e) {
      if(e.key == 'Enter' || e.key == ' ') {
        e.preventDefault();
        var isExpanded = jQuery(this).attr('aria-expanded');

        focusEl = jQuery(this);

        jQuery('.flyout--trigger').attr('aria-expanded',  false); // close all others
        jQuery(this).attr('aria-expanded', (isExpanded !== 'true') ? "true" : "false");

        if (isExpanded !== 'true') {
          jQuery(this).next().find('a').first().focus()
        } else {
          jQuery(this).focus();
        }
      }
    });

    jQuery('#nav').on('mouseout', function() {
      jQuery('.flyout--trigger').attr('aria-expanded',  false);
    });

    jQuery(document).on('keydown', function(event) {
      if (event.key == "Escape") {
        jQuery('.flyout--trigger').attr('aria-expanded',  false);
        focusEl.focus();
      }
    });

    // if focus leaves the navs, close them all
    jQuery('#menu-item-4875').on('focusin', function() {
      jQuery('.flyout--trigger').attr('aria-expanded',  false);
    });

    jQuery('.pr-6.pb-2.pt-2.inline-block').on('focusin', function() {
      jQuery('.flyout--trigger').attr('aria-expanded',  false);
    })


  }, 1000);
</script>

