   $('#menu .nav li').hover(function(){
      if($(this).find('.sec li').length>0)
      {
         $(this).find('.sec').show();
      }
      
   },function(){
       if($(this).find('.sec li').length>0)
       {
          $(this).find('.sec').hide();
       }
   })

