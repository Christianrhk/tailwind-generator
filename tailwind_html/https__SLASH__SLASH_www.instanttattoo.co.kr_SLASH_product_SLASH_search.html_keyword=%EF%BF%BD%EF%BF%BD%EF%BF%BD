<script>
    const images = document.querySelectorAll("img.image-lazy"); // Î™®Îì† Ïù¥ÎØ∏ÏßÄ ÌååÏùº ÏÑ†ÌÉù
    const lazyLoad = (target) => {
        const io = new IntersectionObserver((entries, observer) => {
            entries.forEach((entry) => { 
                if (entry.isIntersecting) {
                    //console.log("reached!");
                    const img = entry.target; // Ïù¥ÎØ∏ÏßÄ ÏóòÎ¶¨Î©òÌä∏Î•º Í∞ÄÏ†∏ÏòµÎãàÎã§. 
      
                    img.src = img.getAttribute("data-src");
                    img.classList.add("lazy-loaded");
                    observer.disconnect();

                    const parent = img.parentElement.parentElement.parentElement
                    const el = parent.querySelector('.description > .spec')
                    setProductPrice(el) 
                }
                else{
                    //console.log('ÌôîÎ©¥ÏóêÏÑú Ï†úÏô∏Îê®');
                }
            });
        });
        io.observe(target);
    }; 
    images.forEach(lazyLoad);


    const sort_method = getUrlParameter('order_by')
    const sort_node =  document.getElementById('sort')
    if(sort_method){
        let sortdic = {
            'recent' : 0,
            'priceasc' : 1,
            'price' : 2,
            'favor' : 3,
            'review' : 4,
            'hit_count' : 5,
        }
        sort_node.selectedIndex = sortdic[sort_method]
    }

    function onchangeSort(e){
        let selected = (e.options[e.selectedIndex])
        let keyword = getUrlParameter('keyword') 
        location.href =  `/product/search.html?search_type=product_name&keyword=${keyword}&order_by=${selected.value}`
    }


    let hideitem = []
    for(hi of hideitem){
        let node = document.getElementById(`anchorBoxId_${hi}`)
        if(node){
            node.remove();
        }
    }   

