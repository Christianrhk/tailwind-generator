function isSeqNoKeyExpiretime(iExpiretime)
{
var sDate = new Date();
var iNow = Math.floor(sDate.getTime() / 1000);
// Ïú†Ìö®ÏãúÍ∞Ñ ÌôïÏù∏
if (iExpiretime > iNow) {
return false;
}
return true;
}
