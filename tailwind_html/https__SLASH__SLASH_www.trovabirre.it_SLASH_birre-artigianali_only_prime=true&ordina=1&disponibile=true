<script>
    const url = window.location;
    const urlSearcher = new URLSearchParams(url.search);
    const openmodal = document.querySelectorAll('.modal-open')
    const closemodal = document.querySelectorAll('.modal-close')
    const overlay = document.querySelector('.modal-overlay')

    const htmlAggiungiAlCarrello = ``
        + `<div class="flex text-red-700" role="alert">`
        + `<span class="m-2"><svg xmlns="http://www.w3.org/2000/svg" class="w-6 h-6" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 9v2m0 4h.01m-6.938 4h13.856c1.54 0 2.502-1.667 1.732-3L13.732 4c-.77-1.333-2.694-1.333-3.464 0L3.34 16c-.77 1.333.192 3 1.732 3z" /></svg>
           </span>`
        + `<div>`
        + `<p>Hai raggiunto la quantit massima disponibile</p`
        + `</div>`
        + `</div>`


    const insertParam = (key, value, encode = true) => {
        if (encode) {
            key = encodeURI(key);
            value = encodeURI(value);
        }
        urlSearcher.set(key, value);
        window.history.replaceState({}, document.title, `?${urlSearcher}`);
        if (value === '' || value == null) {
            urlSearcher.delete(key);
            window.history.replaceState({}, document.title, `?`);
        }
    }

    const deleteParam = (key, encode = true) => {
        encode && (key = encodeURI(key));
        urlSearcher.delete(key);
        window.history.replaceState({}, document.title, `?${urlSearcher.toString()}`);
    }

    // const deleteAllParam = () => {
    //     window.history.replaceState({}, document.title, `?`);
    // }

    const appendParam = (key, value, encode = true) => {
        if (encode) {
            key = encodeURI(key);
            value = encodeURI(value);
        }
        urlSearcher.append(key, value);
        window.history.replaceState(null, null, `?${urlSearcher}`);
    }

    const toggleModal = () => {
        const body = document.querySelector('body')
        const modal = document.querySelector('.modal')
        modal.classList.toggle('opacity-0')
        modal.classList.toggle('pointer-events-none')
        body.classList.toggle('modal-active')
    }

    const setModal = (title) => {
        document.getElementById("title_modal").innerHTML = title;
    }

    async function aggiungiAlCarrello(id, sku, increment) {
        let quantity = increment ?? 1;

        let data = {
            "_token": "aMFxfYrc6oI5eDgI1YHRU4MzXlXpL0ux4YxCq1QW",
            "sku": sku,
            "qty": quantity,
        };

        let xhttpFirst = new XMLHttpRequest();
        xhttpFirst.onreadystatechange = function () {
            if (this.readyState === 4 && this.status === 200) {
                let response = JSON.parse(this.response);
                if (!response) {
                    document.getElementById("title_modal").innerHTML = htmlAggiungiAlCarrello;
                    toggleModal();
                    return;
                }
                let xhttp = new XMLHttpRequest();
                xhttp.onreadystatechange = function () {
                    if (this.readyState === 4 && this.status === 200) {
                        let response = JSON.parse(this.response);
                        if (response.status) {
                            badgeCartCount.innerText = response.qty;
                            document.getElementById("title_modal").textContent = 'Prodotto aggiunto al carrello';
                            toggleModal();
                        } else {
                            alert('ERROR')
                        }
                    }
                };
                if (id) data.id = id;

                xhttp.open("POST", "/addtocart", true);
                xhttp.setRequestHeader("Content-type", "application/json");
                xhttp.send(JSON.stringify(data));
            }
        };
        xhttpFirst.open("POST", "/check-addtocart", true);
        xhttpFirst.setRequestHeader("Content-type", "application/json");
        xhttpFirst.send(JSON.stringify(data));
    }

    const init = () => {
        let xhttp = new XMLHttpRequest();
        let badgeCartCount = document.getElementById("badgeCartCount");
        xhttp.onreadystatechange = function () {
            if (this.readyState == 4 && this.status == 200) {
                let response = JSON.parse(this.response);
                if (response.status) {
                    badgeCartCount.innerText = response.qty;
                }

            }
        }
        let data = {
            "_token": "aMFxfYrc6oI5eDgI1YHRU4MzXlXpL0ux4YxCq1QW",
        };
        xhttp.open("POST", "/checkCart", true);
        xhttp.setRequestHeader("Content-type", "application/json");
        xhttp.send(JSON.stringify(data));

        if (overlay != null && openmodal != null && closemodal != null) {
            overlay.addEventListener('click', toggleModal)
            for (let i = 0; i < openmodal.length; i++) {
                openmodal[i].addEventListener('click', function (event) {
                    event.preventDefault()
                    toggleModal()
                })
            }

            for (let i = 0; i < closemodal.length; i++) {
                closemodal[i].addEventListener('click', toggleModal)
            }

            document.onkeydown = function (evt) {
                evt = evt || window.event
                let isEscape = "key" in evt ? (evt.key === "Escape" || evt.key === "Esc") : (evt.keyCode === 27);
                if (isEscape && document.body.classList.contains('modal-active'))
                    toggleModal()
            };
        }
    }

    init();
</script>

