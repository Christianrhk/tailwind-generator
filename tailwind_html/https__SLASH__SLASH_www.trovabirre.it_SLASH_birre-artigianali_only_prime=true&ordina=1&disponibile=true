        const filtraURL = (name) => {
            let i = 0;
            let arr = [];
            while (urlSearcher.has(`${name}[${i}]`))
                arr.push(urlSearcher.get(`${name}[${i++}]`));
            return arr;
        }
        const mostraDiPiu = (e) => {
            // if (e == 'stilelistadown' || e == 'stilelistaup') {
            //     showHideStili(e);
            // }
            if (e == 'producttypologylistaButtondown' || e == 'producttypologylistaButtonup') {
                showHideTipologiaProdotto(e);
            }
            if (e == 'regionelistaButtondown' || e == 'regionelistaButtonup') {
                showHideRegione(e);
            }
            if (e == 'birrificiolistaButtondown' || e == 'birrificiolistaButtonup') {
                showHideBirrificio(e);
            }
            if (e == 'formatolistaButtondown' || e == 'formatolistaButtonup') {
                showHideFormato(e);
            }

        }


