                listMobile += `<div class="flex items-center cursor-pointer my-2 producttypologylistadiv"    >`;
                listMobile += `<input id="mobiletipologia-${el.id}"  onclick="customCheckBox('mobiletipologia-${el.id}','input')"  name="tipologia" value="${el.id}" type='checkbox' class=' w-5 h-5 text-yellow-500 bg-gray-100 rounded border-yellow-300 focus:ring-white cursor-pointer' ${forEach(filtriSelezionati.tipologia, el.id) ? 'checked' : ''} >`;
                listMobile += ` <label for="checked-mobile${el.name}-${el.id}"   onclick="customCheckBox('mobiletipologia-${el.id}')"  class="ml-2 text-md font-medium text-gray-900 dark:text-gray-300 cursor-pointer">${el.name}</label>`;
                listMobile += `</div>`
                count++;
            })
            list += `<div class="flex justify-end w-full flex row items-center gap-2 cursor-pointer producttypologylistaButtondown">`;
            list += `    <div class="font-bold  " onclick="mostraDiPiu('producttypologylistaButtondown')">VEDI TUTTI</div>`;
            list += `    <span class="iconify " data-icon="akar-icons:chevron-down" onclick="mostraDiPiu('producttypologylistaButtondown')" data-width="22" data-height="22"></span>`;
            list += `</div>`;
            list += `<div class="flex justify-end w-full flex row items-center gap-2 cursor-pointer ${hiddenRow} producttypologylistaButtonup">`;
            list += `  <div class="font-bold " onclick="mostraDiPiu('producttypologylistaButtonup')">VEDI MENO</div>`;
            list += ` <span class="iconify " onclick="mostraDiPiu('producttypologylistaButtonup')"data-icon="akar-icons:chevron-up" data-width="22" data-height="22"></span>`;
            list += `</div>`;
            document.getElementById('listTipologia').innerHTML += list
            document.getElementById('listTipologiaMobile').innerHTML += listMobile
            list = "";
            count = 0;
            hiddenRow = '';
            listMobile = "";
            regioni.forEach(el => {
                if (count == 4)
                    hiddenRow = "hidden";
                list += `<div class="flex items-center cursor-pointer my-2 ${hiddenRow} regionelistadiv "   >`;
                list += `<input id="regione-${el.id}"  onclick="customCheckBox('regione-${el.id}','input')"   name="regione" value="${el.id}" type='checkbox' class=' w-5 h-5 text-yellow-500 bg-gray-100 rounded border-yellow-300 focus:ring-white cursor-pointer' ${forEach(filtriSelezionati.regione, el.id) ? 'checked' : ''} >`;
                list += ` <label for="checked-${el.name}-${el.id}" onclick="customCheckBox('regione-${el.id}')" class="ml-2 text-md font-medium text-gray-900 dark:text-gray-300 cursor-pointer">${el.name}</label>`;
                list += `</div>`
                listMobile += `<div class="flex items-center cursor-pointer my-2 regionelistadiv "   >`;
                listMobile += `<input id="mobileregione-${el.id}"  onclick="customCheckBox('mobileregione-${el.id}','input')"   name="regione" value="${el.id}" type='checkbox' class=' w-5 h-5 text-yellow-500 bg-gray-100 rounded border-yellow-300 focus:ring-white cursor-pointer' ${forEach(filtriSelezionati.regione, el.id) ? 'checked' : ''} >`;
                listMobile += ` <label for="checked-mobile-${el.name}-${el.id}" onclick="customCheckBox('mobileregione-${el.id}')" class="ml-2 text-md font-medium text-gray-900 dark:text-gray-300 cursor-pointer">${el.name}</label>`;
                listMobile += `</div>`
                count++;
            })
            list += `<div class="flex justify-end w-full flex row items-center gap-2 cursor-pointer regionelistaButtondown">`;
            list += `    <div class="font-bold  " onclick="mostraDiPiu('regionelistaButtondown')">VEDI TUTTI</div>`;
            list += `    <span class="iconify " data-icon="akar-icons:chevron-down" onclick="mostraDiPiu('regionelistaButtondown')" data-width="22" data-height="22"></span>`;
            list += `</div>`;
            list += `<div class="flex justify-end w-full flex row items-center gap-2 cursor-pointer ${hiddenRow} regionelistaButtonup">`;
            list += `  <div class="font-bold " onclick="mostraDiPiu('regionelistaButtonup')">VEDI MENO</div>`;
            list += ` <span class="iconify " onclick="mostraDiPiu('regionelistaButtonup')"data-icon="akar-icons:chevron-up" data-width="22" data-height="22"></span>`;
            list += `</div>`;
            document.getElementById('listREGIONE').innerHTML += list
            document.getElementById('listREGIONEMobile').innerHTML += listMobile

            listMobile = "";
            list = "";
            count = 0;
            hiddenRow = '';

            birrifici.forEach(el => {//
                if (count == 4)
                    hiddenRow = "hidden";
                list += `<div class="flex items-center cursor-pointer my-2 ${hiddenRow}  birrificiolistadiv  birrificio-${el.id}"  id="birrificio-${el.slug}">`;
                list += `<input id="birrificio-${el.id}"   onclick="customCheckBox('birrificio-${el.id}','input')"      name="birrificio" value="${el.id}" type='checkbox' class=' w-5 h-5 text-yellow-500 bg-gray-100 rounded border-yellow-300 focus:ring-white cursor-pointer' ${forEach(filtriSelezionati.birrificio, el.id) ? 'checked' : ''} >`;
                list += ` <label for="checked-${el.name_activity}-${el.id}"  onclick="customCheckBox('birrificio-${el.id}')"  class="capitalize ml-2 text-md font-medium text-gray-900 dark:text-gray-300 cursor-pointer">${el.name_activity}</label>`;
                list += `</div>`
                listMobile += `<div class="flex items-center cursor-pointer my-2 birrificiolistadiv  birrificio-${el.id}"  id="birrificio-${el.slug}">`;
                listMobile += `<input id="mobilebirrificio-${el.id}"   onclick="customCheckBox('mobilebirrificio-${el.id}','input')"      name="birrificio" value="${el.id}" type='checkbox' class=' w-5 h-5 text-yellow-500 bg-gray-100 rounded border-yellow-300 focus:ring-white cursor-pointer' ${forEach(filtriSelezionati.birrificio, el.id) ? 'checked' : ''} >`;
                listMobile += ` <label for="checked-mobile${el.name_activity}-${el.id}"  onclick="customCheckBox('mobilebirrificio-${el.id}')"  class="capitalize ml-2 text-md font-medium text-gray-900 dark:text-gray-300 cursor-pointer">${el.name_activity}</label>`;
                listMobile += `</div>`
                count++;
            })
            list += `<div class="flex justify-end w-full flex row items-center gap-2 cursor-pointer birrificiolistaButtondown">`;
            list += `    <div class="font-bold  " onclick="mostraDiPiu('birrificiolistaButtondown')">VEDI TUTTI</div>`;
            list += `    <span class="iconify " data-icon="akar-icons:chevron-down" onclick="mostraDiPiu('birrificiolistaButtondown')" data-width="22" data-height="22"></span>`;
            list += `</div>`;
            list += `<div class="flex justify-end w-full flex row items-center gap-2 cursor-pointer ${hiddenRow} birrificiolistaButtonup">`;
            list += `  <div class="font-bold " onclick="mostraDiPiu('birrificiolistaButtonup')">VEDI MENO</div>`;
            list += ` <span class="iconify " onclick="mostraDiPiu('birrificiolistaButtonup')"data-icon="akar-icons:chevron-up" data-width="22" data-height="22"></span>`;
            list += `</div>`;
            document.getElementById('listBIRRIFICIO').innerHTML += list
            document.getElementById('listMobileBIRRIFICIO').innerHTML += listMobile
            list = "";
            count = 0;
            hiddenRow = '';
            listMobile = "";
            formati.forEach(el => {
                if (count == 4)
                    hiddenRow = "hidden";
                list += `<div class="flex items-center cursor-pointer my-2 ${hiddenRow} formatolistadiv"   >`;
                list += `<input id="formato-${el.id}"  onclick="customCheckBox('formato-${el.id}','input')"      name="formato" value="${el.id}" type='checkbox' class=' w-5 h-5 text-yellow-500 bg-gray-100 rounded border-yellow-300 focus:ring-white cursor-pointer' ${forEach(filtriSelezionati.formato, el.id) ? 'checked' : ''} >`;
                list += ` <label for="checked-${el.name}-${el.id}" onclick="customCheckBox('formato-${el.id}')" class="ml-2 text-md font-medium text-gray-900 dark:text-gray-300 cursor-pointer">${el.format} ${el.um}</label>`;
                list += `</div>`
                listMobile += `<div class="flex items-center cursor-pointer my-2  formatolistadiv"   >`;
                listMobile += `<input id="mobileformato-${el.id}"  onclick="customCheckBox('mobileformato-${el.id}','input')"      name="formato" value="${el.id}" type='checkbox' class=' w-5 h-5 text-yellow-500 bg-gray-100 rounded border-yellow-300 focus:ring-white cursor-pointer' ${forEach(filtriSelezionati.formato, el.id) ? 'checked' : ''} >`;
                listMobile += ` <label for="checked-mobile${el.name}-${el.id}" onclick="customCheckBox('mobileformato-${el.id}')" class="ml-2 text-md font-medium text-gray-900 dark:text-gray-300 cursor-pointer">${el.format} ${el.um}</label>`;
                listMobile += `</div>`
                count++;
            })
            list += `<div class="flex justify-end w-full flex row items-center gap-2 cursor-pointer formatolistaButtondown">`;
