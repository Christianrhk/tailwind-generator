        let filtriSelezionati = {
            priming: urlSearcher.has("priming") ? urlSearcher.get("priming") : "",
            stile: urlSearcher.has("stile[]") ? urlSearcher.getAll("stile[]") : filtraURL('stile'),
            formato: urlSearcher.has("formato[]") ? urlSearcher.getAll("formato[]") : filtraURL('formato'),
            regione: urlSearcher.has("regione[]") ? urlSearcher.getAll("regione[]") : filtraURL('regione'),
            birrificio: urlSearcher.has("birrificio[]") ? urlSearcher.getAll("birrificio[]") : filtraURL('birrificio'),
            tipologia: urlSearcher.has("tipologia[]") ? urlSearcher.getAll("tipologia[]") : filtraURL('tipologia'),
            nome: urlSearcher.has("nome") ? urlSearcher.get("nome") : "",
            ordina: urlSearcher.has("ordina") ? urlSearcher.get("ordina") : "",
            gluten_free: urlSearcher.has("gluten_free") ? urlSearcher.get("gluten_free") : "",
            // natale: urlSearcher.has("natale") ? urlSearcher.get("natale") : "",
            colore: urlSearcher.has("colore") ? urlSearcher.get("colore") : "",
            alcol: urlSearcher.has("alcol") ? urlSearcher.get("alcol") : "",
            amaro: urlSearcher.has("amaro") ? urlSearcher.get("amaro") : "",
            aroma: urlSearcher.has("aroma") ? urlSearcher.get("aroma") : "",
            equilibrio: urlSearcher.has("equilibrio") ? urlSearcher.get("equilibrio") : "",
            disponibile: urlSearcher.has("disponibile") ? urlSearcher.get("disponibile") : "",
        }

        function app() {
            return {
                modal: {
                    open: false,
                    title: null,
                    content: null,
                    arrName: null,
                },

                reload: 0,

                constructor() {
                    // reset();
                    show_filter()
                },

                detectMob() {
                    const toMatch = [
                        /Android/i,
                        /webOS/i,
                        /iPhone/i,
                        /iPad/i,
                        /iPod/i,
                        /BlackBerry/i,
                        /Windows Phone/i
                    ];

                    return toMatch.some((toMatchItem) => {
                        return navigator.userAgent.match(toMatchItem);
                    });
                },


            }
        }

        function searchModal(type) {
            const text = document.getElementById('searchModal' + type).value.toUpperCase();
            if (type == 'stile') {
                stili.forEach(e => {
                    let tipo = document.getElementById('stile-' + e.name).parentElement.getAttribute('data-id');
                    if (document.getElementById('stile-' + e.name).style.display = e.name.toUpperCase().includes(text)) {
                        document.getElementById('stile-' + e.name).classList.remove("hidden")
                        document.getElementById('select' + tipo).classList.add('border-yellow-400')
                        document.getElementById('select' + tipo + 'Down').classList.remove('hidden')
                        document.getElementById('select' + tipo + 'Up').classList.add('hidden')
                        document.getElementById('listStyleShow' + tipo).style.display = "block";
                    } else {
                        document.getElementById('stile-' + e.name).classList.add("hidden");
                    }
                })
            }
            if (type == 'birrificio') {
                birrifici.forEach(e => document.getElementById('birrificio-' + e.slug).style.display = e.slug.toUpperCase().includes(text)
                    ? document.getElementById('birrificio-' + e.slug).classList.remove("hidden")
                    : document.getElementById('birrificio-' + e.slug).classList.add("hidden")
                )
            }
            //   birrifici.forEach(e => document.getElementById(e.slug).style.display = e.slug.toUpperCase().includes(text) ? 'block' : 'none');
        }

        function customCheckBox(inputID, input = false) {
            let target = document.getElementById(inputID);
            if (!input) {
                target.checked = !target.checked;
            }

            this.addParam(target);
            this.search();
            if (filtriApplicati()) {
                document.getElementById("showDeleteFilter").classList.remove("hidden");
                document.getElementById("titleFiltriApplicati").classList.remove("hidden");
                document.getElementById("showDeleteFilterMobile").classList.remove("hidden");
                document.getElementById("titleFiltriMobileApplicati").classList.remove("hidden");
            } else {
                document.getElementById("showDeleteFilter").classList.add("hidden");
                document.getElementById("titleFiltriApplicati").classList.add("hidden");
                document.getElementById("showDeleteFilterMobile").classList.add("hidden");
                document.getElementById("titleFiltriMobileApplicati").classList.add("hidden");
            }
        }

        function search() {
            if (!loading) {
                setLoading();
                let xhttp = new XMLHttpRequest();

                let queryString = `${url.pathname}?ajax=1&`;

                for (const [key, item] of Object.entries(filtriSelezionati)) {

                    if (filtriSelezionati[key] instanceof Array) {
                        for (let i = 0; i < item.length; i++)
                            queryString += `${key}[]=${item[i]}&`;
                    } else if (filtriSelezionati[key] !== "") {

                        queryString += `${key}=${item}&`;
                    }
                }
                queryString = queryString.slice(0, -1);

                xhttp.open("GET", queryString, true);
                xhttp.onreadystatechange = function () {
                    if (this.readyState == 4 && this.status == 200) {
                        setLoading();
                        const {html, pagination, total_product} = JSON.parse(this.responseText);
                        document.getElementById("displayProduct").innerHTML = html;
                        document.getElementById("paginazione").innerHTML = pagination;
                        document.getElementById("total_product").innerHTML = total_product;
                    }
                };
                xhttp.send();
            }
        }

        function deleteAllFilter() {
            this.deleteFilter(['stile', 'formato', 'regione', 'birrificio', 'tipologia', 'ordina', 'gluten_free', 'natale', 'priming', 'stile', 'nome', 'colore', 'alcol', 'amaro', 'aroma', 'equilibrio', 'disponibile']);
            // reset();
            this.modal.open = true;
            this.modal.open = false;
            this.reload++;
        }

        function deleteFilterSingle(name) {
            deleteParam(`${name}`, false);
            filtriSelezionati[name] = "";
            document.getElementById(`titleFiltri${name}`).innerHTML = '';
            document.getElementById(`filtriPer${name}`).innerHTML = '';
            document.getElementById(`titleFiltriMobile${name}`).innerHTML = '';
            document.getElementById(`filtriPerMobile${name}`).innerHTML = '';
            this.search();
        }

        function deleteFilter(names) {
            names.forEach(name => {
                if (filtriSelezionati[name] instanceof Array) {
                    for (let item in filtriSelezionati[name])
                        deleteParam(`${name}[]`, false);
                    filtriSelezionati[name].length = 0;
                } else {
                    deleteParam(`${name}`, false);
                    filtriSelezionati[name] = "";
                }
            })
            this.search();
        }

        function addParam(target) {
            deleteParam('page', false);

            if (filtriSelezionati[target.name] instanceof Array) {
                let index = filtriSelezionati[target.name].indexOf(target.value);
                index === -1 ? filtriSelezionati[target.name].push(target.value) : filtriSelezionati[target.name].splice(index, 1);
                deleteParam(`${target.name}[]`, false);
                // document.getElementById(`filtraPer${target.id}`).innerHTML = ""
                filtriSelezionati[target.name].forEach(el => appendParam(`${target.name}[]`, el, false));
                moveUpCheckbox(target);

            } else {
                const name = target.name ? target.name : target.getAttribute("name");

                const value = target.value ? target.value : target.getAttribute("value");
                filtriSelezionati[name] = value ? value : "";
                if (value && value.length > 0) {
                    insertParam(name, filtriSelezionati[name])
                } else {
                    deleteParam(`${name}`, false)
                }
                this.search();

                this.reload++;
            }
        }

        function moveUpCheckbox(e) {

            if (e.checked && !e.id.includes("mobile")) {

                if (e.name == 'tipologia') {
                    let parentElement = document.getElementById("listTipologia")
                    parentElement.insertBefore(e.parentElement, parentElement.children[0]);

                }
                if (e.name == 'formato') {
                    let parentElement = document.getElementById("listFORMATO")
                    parentElement.insertBefore(e.parentElement, parentElement.children[0]);

                }
                if (e.name == 'birrificio') {
                    let parentElement = document.getElementById("listBIRRIFICIO")
                    parentElement.insertBefore(e.parentElement, parentElement.children[0]);
                }
                if (e.name == 'regione') {
                    let parentElement = document.getElementById("listREGIONE")
                    parentElement.insertBefore(e.parentElement, parentElement.children[0]);
                }

            }

        }

        function customInputName(targetID) {
            let target = document.getElementById(targetID);
            this.addParam(target);
        }

        function customOrdina(targetID) {
            let target = document.getElementById(targetID);
            let targetValue = (target.getAttribute('value') + 1) % 3;
            console.log(target, targetValue)
            target.setAttribute("value", targetValue);
            if (targetValue === 0) {
                this.deleteFilter(["ordina"]);
                document.getElementById("showDeleteFilter").classList.add("hidden");
                document.getElementById("showDeleteFilterMobile").classList.add("hidden");
            } else {
                this.addParam(target);
                document.getElementById("showDeleteFilter").classList.remove("hidden");
                document.getElementById("showDeleteFilterMobile").classList.add("hidden");

            }
        }

        function customCheckBoxTrueFalseOFF(target) {
            target.value = target.checked;
            if (target.checked) {
                this.addParam(target);
                // document.getElementById("showDeleteFilter").classList.remove("hidden");
                // document.getElementById("showDeleteFilterMobile").classList.remove("hidden");
                document.getElementById("titleFiltriApplicati").classList.remove("hidden");
                document.getElementById("titleFiltriMobileApplicati").classList.remove("hidden");
            } else {
                filtriSelezionati[target.name] = target.value;
                deleteParam(target.name);
                this.search();
                // document.getElementById("showDeleteFilter").classList.add("hidden");
                // document.getElementById("showDeleteFilterMobile").classList.add("hidden");
                document.getElementById("titleFiltriApplicati").classList.add("hidden");
                document.getElementById("titleFiltriMobileApplicati").classList.add("hidden");
            }
        }


        function showHideTipologiaProdotto(e) {
            if (e == 'producttypologylistaButtondown') {
                const elements = document.getElementsByClassName('producttypologylistadiv');
                for (const element of elements)
                    element.classList.remove("hidden")

                let elems = document.querySelectorAll(".producttypologylistaButtondown");
                [].forEach.call(elems, function (el) {
                    el.classList.add("hidden");
                });
                elems = document.querySelectorAll(".producttypologylistaButtonup");
                [].forEach.call(elems, function (el) {
                    el.classList.remove("hidden");
                });
            }
            if (e == 'producttypologylistaButtonup') {

                const elements = document.getElementsByClassName('producttypologylistadiv');
                var count = 0;
                for (const element of elements) {
                    count++;
                    if (count <= 5)
                        continue;
                    element.classList.add("hidden")
                }
                let elems = document.querySelectorAll(".producttypologylistaButtondown");
                [].forEach.call(elems, function (el) {
                    el.classList.remove("hidden");
                });
                elems = document.querySelectorAll(".producttypologylistaButtonup");
                [].forEach.call(elems, function (el) {
                    el.classList.add("hidden");
                });
            }
        }

        function showHideRegione(e) {
            if (e == 'regionelistaButtondown') {
                const elements = document.getElementsByClassName('regionelistadiv');
                for (const element of elements)
                    element.classList.remove("hidden")

                let elems = document.querySelectorAll(".regionelistaButtondown");
                [].forEach.call(elems, function (el) {
                    el.classList.add("hidden");
                });
                elems = document.querySelectorAll(".regionelistaButtonup");
                [].forEach.call(elems, function (el) {
                    el.classList.remove("hidden");
                });
            }
            if (e == 'regionelistaButtonup') {

                const elements = document.getElementsByClassName('regionelistadiv');
                var count = 0;
                for (const element of elements) {
                    count++;
                    if (count <= 5)
                        continue;
                    element.classList.add("hidden")
                }
                let elems = document.querySelectorAll(".regionelistaButtondown");
                [].forEach.call(elems, function (el) {
                    el.classList.remove("hidden");
                });
                elems = document.querySelectorAll(".regionelistaButtonup");
                [].forEach.call(elems, function (el) {
                    el.classList.add("hidden");
                });
            }
        }

