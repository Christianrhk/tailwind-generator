        function forEach(array, el) {
            for (let i = 0; i < array.length; i++)
                if (array[i] == el)
                    return true;
            return false;
        }

        function filtriApplicati() {
            countFiltri();
            let is_filter = 0;

            for (const [key, item] of Object.entries(filtriSelezionati)) {
                if (key != 'priming' && key != 'ordina' && key != 'gluten_free' && key != 'disponibile' && key != 'colore'
                    && key != 'amaro'
                    && key != 'alcol'
                    && key != 'aroma'
                    && key != 'equilibrio'
                ) {
                    document.getElementById(`titleFiltri${key}`).innerHTML = '';
                    document.getElementById(`filtriPer${key}`).innerHTML = '';
                    document.getElementById(`titleFiltriMobile${key}`).innerHTML = '';
                    document.getElementById(`filtriPerMobile${key}`).innerHTML = '';
                    if (filtriSelezionati[key].length > 0 && key != 'nome') {
                        let count_filter = 0;
                        item.forEach(el => {
                            let filter = document.getElementById(`filtriPer${key}`)
                            let filterMobile = document.getElementById(`filtriPerMobile${key}`)
                            let id = key + '-' + el;
                            let e = document.getElementById(id)
                            let value = e.parentElement.getElementsByTagName("input")[0].value
                            let idStile = e.parentElement.getElementsByTagName("input")[0].id

                            filter.innerHTML += '<div class="flex flex-row items-center" id="filtrPer' + idStile + '"> ' +
                                '   <div class="font-bold">' + e.parentElement.getElementsByTagName("label")[0].innerText + '</div>' +
                                '  <div><div  onclick="customCheckBox(\'' + key + '-' + value + '\')" class="ml-2 px-1 h-5 items-center leading-3 cursor-pointer font-bold  inline-flex hover:opacity-25 text-yellowPrimary  border-2 border-yellow-400 rounded" style="align-items: baseline">x</div></div>' +
                                '</div>';
                            filterMobile.innerHTML += '<div class="flex flex-row items-center" id="filtrPer' + idStile + '"> ' +
                                '   <div class="font-bold">' + e.parentElement.getElementsByTagName("label")[0].innerText + '</div>' +
                                '  <div><div  onclick="customCheckBox(\'mobile' + key + '-' + value + '\')" class="ml-2 px-1 h-5 items-center leading-3 cursor-pointer font-bold  inline-flex hover:opacity-25 text-yellowPrimary  border-2 border-yellow-400 rounded" style="align-items: baseline">x</div></div>' +
                                '</div>';
                            let title = document.getElementById(`titleFiltri${key}`)
                            let titleMobile = document.getElementById(`titleFiltriMobile${key}`)
                            count_filter++
                            if (count_filter == 1) {
                                title.innerHTML += `<div class="text-left uppercase text-lg text-gray-400">${key}:</div>`;
                                titleMobile.innerHTML += `<div class="text-left uppercase text-lg text-gray-400">${key}:</div>`;
                            }
                        })
                        is_filter = 1;
                    }
                    if (key == 'nome' && filtriSelezionati[key] != '') {

                        let filter = document.getElementById(`filtriPer${key}`)
                        let filterMobile = document.getElementById(`filtriPerMobile${key}`)
                        filter.innerHTML += '<div class="flex flex-row items-center" id="filtrPernome"> ' +
                            '   <div class="font-bold">' + filtriSelezionati[key] + '</div>' +
                            '  <div><div onclick="deleteFilterSingle(`nome`);"  class="ml-2 px-1 h-5 items-center leading-3 cursor-pointer font-bold  inline-flex hover:opacity-25 text-yellowPrimary  border-2 border-yellow-400 rounded" style="align-items: baseline">x</div></div>' +
                            '</div>';
                        filterMobile.innerHTML += '<div class="flex flex-row items-center" id="filtrPernome"> ' +
                            '   <div class="font-bold">' + filtriSelezionati[key] + '</div>' +
                            '  <div><div onclick="deleteFilterSingle(`nome`);"  class="ml-2 px-1 h-5 items-center leading-3 cursor-pointer font-bold  inline-flex hover:opacity-25 text-yellowPrimary  border-2 border-yellow-400 rounded" style="align-items: baseline">x</div></div>' +
                            '</div>';
                        let title = document.getElementById(`titleFiltri${key}`)
                        let titleMobile = document.getElementById(`titleFiltriMobile${key}`)
                        title.innerHTML += `<div class="text-left uppercase text-lg text-gray-400">Parola chiave:</div>`;
                        titleMobile.innerHTML += `<div class="text-left uppercase text-lg text-gray-400">Parola chiave:</div>`;
                        document.getElementById(`cerca_desk`).value = filtriSelezionati[key];
                        document.getElementById(`cerca_mobile`).value = filtriSelezionati[key];
                        is_filter = 1;
                    }
                }
                if (filtriSelezionati["priming"] === true ||
                    filtriSelezionati["disponibile"] === true ||
                    filtriSelezionati["gluten_free"] === true ||
                    filtriSelezionati["ordina"] !== ""
                )
                    is_filter = 1;

            }
            return is_filter
        }

        function countFiltri() {
            let count = "";

            if (filtriSelezionati["stile"].length > 0) {
                count = `<div class="font-bold">(${filtriSelezionati["stile"].length})</div>`;
                document.getElementById('countStili').innerHTML = count
                document.getElementById('countStiliMobile').innerHTML = count
            }
            if (filtriSelezionati["stile"].length === 0) {
                document.getElementById('countStili').innerHTML = `<div class="font-bold"></div>`;
                document.getElementById('countStiliMobile').innerHTML = `<div class="font-bold"></div>`;
            }
            if (filtriSelezionati["tipologia"].length > 0) {
                count = `<div class="font-bold">(${filtriSelezionati["tipologia"].length})</div>`;
                document.getElementById('countTipologia').innerHTML = count
                document.getElementById('countTipologiaMobile').innerHTML = count
            }
            if (filtriSelezionati["tipologia"].length === 0) {
                document.getElementById('countTipologia').innerHTML = `<div class="font-bold"></div>`;
                document.getElementById('countTipologiaMobile').innerHTML = `<div class="font-bold"></div>`;
            }
            if (filtriSelezionati["formato"].length > 0) {
                count = `<div class="font-bold">(${filtriSelezionati["formato"].length})</div>`;
                document.getElementById('countFormato').innerHTML = count
                document.getElementById('countFormatoMobile').innerHTML = count
            }
            if (filtriSelezionati["formato"].length === 0) {
                document.getElementById('countFormato').innerHTML = `<div class="font-bold"></div>`;
                document.getElementById('countFormatoMobile').innerHTML = `<div class="font-bold"></div>`;
            }
            if (filtriSelezionati["birrificio"].length > 0) {
                count = `<div class="font-bold">(${filtriSelezionati["birrificio"].length})</div>`;
                document.getElementById('countBirrificio').innerHTML = count
                document.getElementById('countBirrificioMobile').innerHTML = count
            }
            if (filtriSelezionati["birrificio"].length === 0) {
                document.getElementById('countBirrificio').innerHTML = `<div class="font-bold"></div>`;
                document.getElementById('countBirrificioMobile').innerHTML = `<div class="font-bold"></div>`;
            }
            if (filtriSelezionati["regione"].length > 0) {
                count = `<div class="font-bold">(${filtriSelezionati["regione"].length})</div>`;
                document.getElementById('countRegione').innerHTML = count
                document.getElementById('countRegioneMobile').innerHTML = count
            }
            if (filtriSelezionati["regione"].length === 0) {
                document.getElementById('countRegione').innerHTML = `<div class="font-bold"></div>`;
                document.getElementById('countRegioneMobile').innerHTML = `<div class="font-bold"></div>`;
            }
        }

        window.onload = function (e) {
            filtriSelezionati.stile.forEach(element => moveUpCheckbox(document.getElementById(`stile-${element}`)));
            filtriSelezionati.formato.forEach(element => moveUpCheckbox(document.getElementById(`formato-${element}`)));
            filtriSelezionati.regione.forEach(element => moveUpCheckbox(document.getElementById(`regione-${element}`)));
            filtriSelezionati.birrificio.forEach(element => moveUpCheckbox(document.getElementById(`birrificio-${element}`)));
            filtriSelezionati.tipologia.forEach(element => moveUpCheckbox(document.getElementById(`tipologia-${element}`)));
            mostraDiPiu('stilelistadown');
            mostraDiPiu('stilelistaup');
            if (filtriApplicati()) {
                document.getElementById("showDeleteFilter").classList.remove("hidden");
                document.getElementById("showDeleteFilterMobile").classList.remove("hidden");
                document.getElementById("titleFiltriApplicati").classList.remove("hidden");
                document.getElementById("titleFiltriMobileApplicati").classList.remove("hidden");
            } else {
                document.getElementById("showDeleteFilter").classList.add("hidden");
                document.getElementById("showDeleteFilterMobile").classList.add("hidden");
                document.getElementById("titleFiltriApplicati").classList.add("hidden");
                document.getElementById("titleFiltriMobileApplicati").classList.add("hidden");
            }

        }


    </script>

    <script>

        /* document.addEventListener('click', function handleClick(event) {
             console.log('user clicked: ', event.target);
             event.target.classList.add('bg-yellow');
         });
 */
        let loading = false;

        const setLoading = () => {
            loading = !loading;
            showCaricamento();
        }
        const showCaricamento = () => {
            document.getElementById("caricamento").style.display = loading ? "block" : "none";
        }

        function postSlider() {
            new Glide(".posts-slider", {
                type: "carousel",
                startAt: 1,
                perView: 3,
                gap: 0,
                peek: {
                    before: 50,
                    after: 50,
                },
                breakpoints: {
                    1024: {
                        perView: 3,
                        peek: {
                            before: 20,
                            after: 20,
                        },
                    },
                    768: {
                        perView: 2,
                        peek: {
                            before: 10,
                            after: 10,
                        },
                    },
                    600: {
                        perView: 1,
                        peek: {
                            before: 0,
                            after: 0,
                        },
                    },
                },
            })
                .mount();
        }


    </script>







            
