<script>
    $(function(){
        $(".js-watch-video").click(function(){
            $(".udModal").css('display','block');
            $("#js-common-video").trigger('play');
            setTimeout(() => {
                $("#js-common-video").trigger('play');
            }, 500);
            $(this).hide;
        })
    })
    class baseAnimation {
        tween

        triggerElement

        duration

        Scene

        constructor(el) {
            this.setTriggerElement(el).setDuration()

            this
                .createTween()
                .createScene()
        }

        pin() {
            this.Scene.setPin(this.triggerElement)
            return this
        }

        setTriggerElement(el) {
            this.triggerElement = el

            return this
        }

        setDuration(duration = null) {
            if (!duration) {
                this.duration = this.triggerElement.getBoundingClientRect().height
            } else {
                this.duration = duration
            }

            return this
        }

        createTween() {}

        afterCreateScene() {}

        setTriggerHook(hook) {
            this.Scene.triggerHook(hook)
            return this
        }

        createScene() {
            this.Scene = new ScrollMagic.Scene({
                triggerElement: this.triggerElement,
                duration: this.duration
            })
            console.log(this.Scene);
            this.Scene.setTween(this.tween)

            this.afterCreateScene()
            return this
        }

        getScene() {
            return this.Scene
        }

        opacity(element, time = 0) {
            this.tween
                .fromTo(element, 1, {
                    opacity: 0
                }, {
                    opacity: 1
                }, time)

            return this
        }

        refresh() {
            this.triggerElement.style.width = '100%'
        }
    }



    class Floating extends baseAnimation {
        element

        constructor(el) {
            if (!el) {
                return false
            }
            super(el)
            this.refresh()
        }

        createTween() {
            this.setDuration(800)
            this.tween = new TimelineLite()
            const floating = this.triggerElement
            const duration = floating.dataset.duration || 1.5
            const delay = floating.dataset.delay || 0

            this.tween
                .to(floating, duration, {
                    y: 90
                }, delay)

            return this
        }

        refresh() {
            if (window.innerWidth < 768) {
                this.tween.restart()
                this.Scene.enabled(false)
                return this
            }

            this.Scene.enabled(true)
            return this
        }
    }


    class FullScreen extends baseAnimation {
        constructor(el) {
            super(el)
            this.setTriggerHook('onLeave').pin()
        }

        createTween() {
            this.tween = new TimelineLite()
            const elements = this.triggerElement.querySelectorAll('.tween')

            elements.forEach((el) => {
                const delay = Number(el.dataset.delay) || 1.2
                const duration = Number(el.dataset.duration) || 1.2
                const finalValue = Number(el.dataset.final) || 1

                this.tween.to(el, duration, {
                    opacity: finalValue,
                    ease: Linear.easeIn
                }, delay)
            })

            return this
        }

        afterCreateScene() {}
    }



    class Opacity extends baseAnimation {
        constructor(el) {
            if (!el) {
                return false
            }
            super(el)
            this.setTriggerHook('onEnter')
        }

        createTween() {
            this.setDuration(500)
            this.tween = new TimelineLite()
            const delay = this.triggerElement.dataset.delay || 1.2
            const duration = this.triggerElement.dataset.duration || 1.2
            const finalValue = this.triggerElement.dataset.final || 1

            this.tween
                .to(this.triggerElement, duration, {
                    opacity: finalValue,
                    ease: Linear.easeIn
                }, delay)
            return this
        }

        refresh() {}
    }


    class Animation {
        controller

        animations = []

        init() {
            this.controller = new ScrollMagic.Controller({
                globalSceneOptions: {
                    refreshInterval: 0
                }
            })

            this.initScenes()
            this.addResizeListener()
        }

        initScenes() {
            this.getScenes().forEach((scene) => {
                const currentAnimation = scene
                this.controller.addScene(currentAnimation.getScene())
                this.animations.push(currentAnimation)
            })
        }

        getScenes() {
            const animations = []

            document.querySelectorAll('.full-screen-container').forEach((el) => {
                animations.push(new FullScreen(el))
            })

            document.querySelectorAll('.opacity-tween').forEach((el) => {
                animations.push(new Opacity(el))
            })

            document.querySelectorAll('.floating').forEach((el) => {
                animations.push(new Floating(el))
            })

            return animations
        }

        handleResize() {
            this.animations.forEach((animation) => {
                animation.refresh()
            })
        }

        addResizeListener() {
            window.addEventListener('resize', () => {
                this.handleResize()
            })
        }
    }




    var pinned = true
    var pxTrigger = 0
    var menuHeight = 80
    addListener();

    const container = new Animation()
    container.init()

    function setPinned(p) {
        pinned = p;
    }

    function addListener() {
        handleScreen()
        window.addEventListener('resize', this.handleScreen)
        addScrollListener()
    }

    function handleScreen() {
        screenWidth = window.innerWidth
    }

    function scrollHandler() {
        var pxFromTop = window.scrollY || window.pageYOffset
        if (pxFromTop > menuHeight) {
            pinned = pxFromTop < pxTrigger
            pxTrigger = pxFromTop
            belowDelta = pxFromTop > window.innerHeight
            handleBackToTop(pxFromTop)
        } else {
            pinned = true
        }
        setPinned(pinned)
    }

    function handleBackToTop(scrollTop) {
        scrolledBottom = scrollTop + window.innerHeight === document.documentElement.scrollHeight
    }

    function addScrollListener() {
        document.addEventListener('scroll', scrollHandler)
    }

    function scrollToTop() {
        window.scrollTo({
            top: 0,
            behavior: 'smooth'
        })
    }


    function createTween() {
        this.setDuration(800)
        this.tween = new TimelineLite()
        const floating = this.triggerElement
        const duration = floating.dataset.duration || 1.5
        const delay = floating.dataset.delay || 0

        this.tween.to(floating, duration, {
            y: 90
        }, delay)

        return this
    }

    function refresh() {
        if (window.innerWidth < 768) {
            this.tween.restart()
            this.Scene.enabled(false)
            return this
        }

        this.Scene.enabled(true)
        return this
    }

</script>

    <script type="text/javascript">
$( function() {

  // 商品詳細ページ表示
  if ($('.event-view-item').length > 0) {
    gdt('track', 'VIEW_CONTENT');
  }

  // 商品一覧ページ表示
  if ($('.event-view-item-list').length > 0) {
    gdt('track', 'VIEW_CONTENT_LIST');
  }

  // カート追加(とりあえず商品区別なし)
  $('.event-add-cart').on('click', function(){
    //now only customize
    gdt('track', 'ADD_TO_CART', {
      content_ids: ['WatchCustomize'],
      content_type: 'product',
      value: 2508,
      currency: 'CNY'
    });
  });

  // カート削除
  $('.event-remove-cart').on('click', function(){
    gdt('track_custom', 'remove_from_cart');
  });

  // お届け先情報
  $('.event-add-shipping-info').on('input', function(){
    if ($(this).hasClass('event-add-shipping-info')) {
      gdt('track_custom', 'add_shipping_info');
    }
    $('.event-add-shipping-info').removeClass('event-add-shipping-info');
  });

  // 支払い方法選択
  $('.event-check-payment-method').on('input', function(){
    if ($(this).hasClass('event-check-payment-method')) {
      gdt('track_custom', 'check_payment_method');
    }
    // credit np both clear
    $('.event-check-payment-method').removeClass('event-check-payment-method');
  });

  // クレジットカード情報入力
  $('.event-add-payment-info').on('input', function(){
    if ($(this).hasClass('event-add-payment-info')) {
      gdt('track_custom', 'add_payment_info');
    }
    $(this).removeClass('event-add-payment-info');
  });

  // 個人情報同意
  $('.event-consent-cart-privacy').on('input', function(){
    if ($(this).hasClass('event-consent-cart-privacy')) {
      gdt('track_custom', 'consent_cart_privacy');
    }
    $(this).removeClass('event-consent-cart-privacy');
  });


  // チェックアウト開始
  $('.event-checkout-start').on('click', function(){
    if ($(this).hasClass('event-checkout-start')) {
      gdt('track', 'INITIATE_CHECKOUT');
    }
    $(this).removeClass('event-checkout-start');
  });

  // line@ボタン
  // $('.event-add-friend').on('click', function(){
  //   if ($(this).hasClass('event-add-friend')) {
  //     fbq('track', 'AddFriend');
  //     gtag('event', 'add_frinend');
  //   }
  //   $(this).removeClass('event-add-friend');
  // });

  // 続きを読む
  $('.event-item-read-continue').on('click', function(){
    if ($(this).hasClass('event-item-read-continue')) {
      gdt('track_custom', 'item_read_continue');
    }
    $(this).removeClass('event-item-read-continue');
  });
} );

</script>

