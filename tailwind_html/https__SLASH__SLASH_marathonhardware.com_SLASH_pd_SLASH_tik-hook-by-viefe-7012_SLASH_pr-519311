            <script>
    $(document).ready(function () {
        const imageSliderSearchFilter = 'image-slider-index-';
        $.each($(`div *[id^="${imageSliderSearchFilter}"]`), function() {
            const imageSliderId = this.id;
            const imageSliderIndex = imageSliderId.replace(imageSliderSearchFilter, '');

            $(`#image-slider-index-${imageSliderIndex}`).slick({
            dots: false,
            speed: 300,
            slidesToShow: 4,
            adaptiveHeight: true,
            responsive: [
            {
                breakpoint: 1440,
                settings: {
                slidesToShow: 3,
                slidesToScroll: 3
                }
            },
            {
                breakpoint: 960,
                settings: {
                slidesToShow: 2,
                slidesToScroll: 2
                }
            },
            {
                breakpoint: 640,
                settings: {
                slidesToShow: 1,
                slidesToScroll: 1
                }
            }
            ],
            prevArrow: $(`#image-slider-nav-prev-item-${imageSliderIndex}`),
            nextArrow: $(`#image-slider-nav-next-item-${imageSliderIndex}`)
        });
        });
    });
</script>
    <script>
    $(document).ready(function () {
        let counter = 0;
        $.each($('.product-slider-div'), function() {
            $(this).find('.nav-prev').attr('id', 'product-slider-nav-prev-item-' + counter);
            $(this).find('.nav-next').attr('id', 'product-slider-nav-next-item-' + counter);
            $(this).find('.inner-product-slider-div').attr('id', 'product-slider-index-' + counter);

            $('#product-slider-index-' + counter).slick({
                dots: false,
                speed: 300,
                slidesToShow: 4,
                adaptiveHeight: true,
                responsive: [
                    {
                        breakpoint: 1440,
                        settings: {
                            slidesToShow: 3,
                            slidesToScroll: 3
                        }
                    },
                    {
                        breakpoint: 960,
                        settings: {
                            slidesToShow: 2,
                            slidesToScroll: 2
                        }
                    },
                    {
                        breakpoint: 640,
                        settings: {
                            slidesToShow: 1,
                            slidesToScroll: 1
                        }
                    }
                ],
                prevArrow: $('#product-slider-nav-prev-item-' + counter),
                nextArrow: $('#product-slider-nav-next-item-' + counter)
            });
            counter++;
        });
    });
</script>
        <script>
        
        $('.readmore-link').click(function(){
            var p = $(this).closest('.readmore').find('.readmore-text');
            if(p.hasClass('expanded')){
                $(p).removeClass('expanded');
                $(this).text('View More');
            }else{
                $(p).addClass('expanded');
                $(this).text('View less');
            }
        });

                        
        
                
