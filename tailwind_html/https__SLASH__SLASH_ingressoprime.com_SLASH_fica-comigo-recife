            <!-- FIM AFILIADO -->

            <script type="text/javascript">
              $("#afiliado_search").keypress(function(e) {
                var regex = new RegExp("^[a-zA-Z0-9]+$");
                var key = String.fromCharCode(!e.charCode ? e.which : e.charCode);
                if (!regex.test(key)) {
                  e.preventDefault();
                  return false;
                }

                if ($(this).val().length > 0) {
                  $("#afiliado_button").show();
                } else {
                  $("#afiliado_button").hide();
                }

              }).keydown(function(e) {
                if (e.keyCode == 8) {

                  if ($(this).val().length > 1) {
                    $("#afiliado_button").show();
                  } else {
                    $("#afiliado_button").hide();
                  }

                }
              });

              $("#afiliado_button").click(function(e) {

                let href = '/afiliadoscheck.php?afiliado_search=' + $("#afiliado_search").val();




                e.preventDefault();

                $.ajax({
                  url: href,
                  data: {
                    format: 'json'
                  },
                  error: function() {
                    $("#alert_afiliado_search").show().html('<i class="glyphicon glyphicon-chevron-up"></i>N&atilde;o foi poss&iacute;vel buscar o afiliado.');
                  },
                  success: function(data) {

                    console.log(JSON.stringify(data));

                    console.log(data.afiliado_id);

                    if (data.afiliado_id != null) {
                      $("#afiliado_search").hide();
                      $("#afiliado_button").hide();
                      $("#alert_afiliado_search").hide();
                      $("#afiliado_nome").show().html(data.afiliado_nome);
                      $("#afiliado_slug").show().html($("#afiliado_search").val());
                      $("#afiliado_id").val(data.afiliado_id);
                    } else {
                      $("#alert_afiliado_search").show().html('<i class="glyphicon glyphicon-chevron-up"></i> Afiliado n&atilde;o encontrado');
                    }

                  }
                });

              });
            </script>




