<script>
    (function () {
        var searchBox = document.querySelector('.faqStickySearch .search-box-wrapper')
        var searchBoxInput = document.querySelector('.faqStickySearch .search-box');

        document.addEventListener('scroll', function () {
            if (searchBox && searchBoxInput && window.innerWidth < 768) {
                var searchBoxPosition = searchBox.getBoundingClientRect();

                if (searchBoxPosition.top >= 0 && searchBoxPosition.bottom <= (window.innerHeight || document.documentElement.clientHeight)) {
                    searchBoxInput.classList.remove('is-sticky')
                } else {
                    searchBoxInput.classList.add('is-sticky');
                }
            }
        });
    })()
</script>

