    $(function() {
        // Insert Responsive Navigation Icon, Close Icon, and Overlay
        // If you have access to your HTML, you should put this directly into your markup.
        $('<div class="responsive-nav-icon -mt-px" />').appendTo('.top-bar > .container');
        $('<div class="responsive-nav-close z-10" />').appendTo('ul.menubar');
        $('<div id="sidebar-backdrop" class="hidden fixed w-full h-full backdrop-filter backdrop-sepia backdrop-blur-sm backdrop-contrast-200 z-10" />').insertBefore('.top-bar');

        // Navigation Slide In
        $('.responsive-nav-icon').click(function() {
            $('div.sidebar').addClass('w-72').removeClass('w-0');
            $('html').css("overflow", "hidden");
            $('#sidebar-backdrop').show();
            return false;
        });

        // Navigation Slide Out
        $('#sidebar-backdrop, .responsive-nav-close').click(function() {
            $('div.sidebar').removeClass('w-72').addClass('w-0');
            $('html').css("overflow", "auto");
            $('#sidebar-backdrop').hide();
            return false;
        });


        });
</script>
