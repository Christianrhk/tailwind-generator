<script>
  $('.ms1907-sp-search-block p.ttl.-maker').on('click', function () {
    $(this).toggleClass('-opened');
    $('.sp-maker-chose').slideToggle('normal');
  });
  if ( inputParams['category_id'] ) {
    $('#sp-param-category-form').val( inputParams['category_id'] ).prop('selected', true);
  }
  if ( inputParams['status'] ) {
    $('.sp-param-status-box').val( inputParams['status'].split(',') );
  }
  if ( inputParams['date_type'] ) {
    $('#sp-param-period-form input[name="sp-param-period-form"]').val( [ inputParams['date_type'] ] );
  }
  if ( inputParams['date_span'] ) {
    var dateSpan = inputParams['date_span'].match('([0-9]{4}-[0-9]{2}-[0-9]{2})?-([0-9]{4}-[0-9]{2}-[0-9]{2})?');
    $('#sp-param-period-from-form').val( dateSpan[1] );
    $('#sp-param-period-to-form').val( dateSpan[2] );
  }
  if ( inputParams['maker_id'] ) {
    $('.sp-param-maker-box').val( inputParams['maker_id'].split(',') );
    inputParams['maker_id'].split(',').forEach(function ( makerId ) {
      $('#sp-maker-list').append('<li>' + $('.sp-param-maker-box[value=' + makerId + ']').parent('label').text() + '</li>');
    });
  }
  if ( inputParams['sale'] ) {
    $('#sp-param-sale-product-filter').prop('checked', true);
  }
</script>
<script><!--//
  $(function(){
    if ( !window.location.pathname.match('log(in|out)') ) {
