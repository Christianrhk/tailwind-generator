      <script>
        var mobileMenuOpened = false;

        var toggleMobileMenu = function () {
          return !mobileMenuOpened ? openMobileMenu() : closeMobileMenu();
        };

        var openMobileMenu = function () {
          let mobileMenuWrapper = document.getElementById(
            "header-mobile-menu-wrapper"
          );

          let mobileMenuButtonOpen = document.getElementById(
            "header-mobile-menu-button-open"
          );

          let mobileMenuButtonClose = document.getElementById(
            "header-mobile-menu-button-close"
          );

          mobileMenuWrapper.style.display = "block";
          mobileMenuButtonOpen.style.display = "none";
          mobileMenuButtonClose.style.display = "block";
          mobileMenuOpened = true;
        };

        var closeMobileMenu = function () {
          let mobileMenuWrapper = document.getElementById(
            "header-mobile-menu-wrapper"
          );

          let mobileMenuButtonOpen = document.getElementById(
            "header-mobile-menu-button-open"
          );

          let mobileMenuButtonClose = document.getElementById(
            "header-mobile-menu-button-close"
          );

          mobileMenuWrapper.style.display = "none";
          mobileMenuButtonOpen.style.display = "block";
          mobileMenuButtonClose.style.display = "none";
          mobileMenuOpened = false;
        };
      </script>
      

