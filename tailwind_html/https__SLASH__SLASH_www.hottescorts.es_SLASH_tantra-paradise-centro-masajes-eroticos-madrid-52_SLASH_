    <script type="text/javascript">
    var dCookiesWOK = $(document);
    var miCookiesWOK = {};
    //miCookiesWOK.openCookiesWOK = false;
    miCookiesWOK.aCookies = false;
    miCookiesWOK.edicionposterior = "/tantra-paradise-centro-masajes-eroticos-madrid-52/";
    miCookiesWOK.Inicializar = function () {
        miCookiesWOK.interceptarButtonTitle();
        miCookiesWOK.interceptarCookiesWOK();

    };

    miCookiesWOK.interceptarButtonTitle = function () {
        $('.button-title').click(function (j) {
            var dropDown = $(this).closest('.acc__card').find('.acc__panel');
            $(this).closest('.acc').find('.acc__panel').not(dropDown).slideUp();
            if ($(this).hasClass('active')) {
                $(this).removeClass('active');
            } else {
                $(this).closest('.acc').find('.button-title.active').removeClass('active');
                $(this).addClass('active');
            }
            dropDown.stop(false, true).slideToggle({
                duration: 200,
                easing: "easeOutQuad",
                start: function () {
                    $(this).css('display', 'flex');
                }
            }
            );

            j.preventDefault();
        });
    };

    miCookiesWOK.setCookie = function (cname, cvalue, exdays) {
        var d = new Date();
        d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
        var expires = "expires=" + d.toUTCString();
        document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
    };
    miCookiesWOK.deleteCookie = function (cname) {
        var expires = "expires = Thu, 01 Jan 1970 00:00:00 GMT";
        document.cookie = cname + "= ;" + expires + ";path=/";
    };
    miCookiesWOK.getCookie = function (cname) {
        var name = cname + "=";
        var decodedCookie = decodeURIComponent(document.cookie);
        var ca = decodedCookie.split(';');
        for (var i = 0; i < ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0) == ' ') {
                c = c.substring(1);
            }
            if (c.indexOf(name) == 0) {
                return c.substring(name.length, c.length);
            }
        }
        return "";
    };

    miCookiesWOK.interceptarCookiesWOK = function () {
        var xCookiesAceptadas = miCookiesWOK.getCookie("cookies_aceptadas");
        if (xCookiesAceptadas === "1") {
            $(".j_cookies_configurador").addClass("hide");
            $(".cookies-fixed").hide();
        } else {
            $(".cookies-fixed").show();
        }


        miCookiesWOK.aCookies ={"propias":"Y","googleanalytics":"Y"};
        console.log("Valores aCookies From Load");
        console.log(miCookiesWOK.aCookies);
        var jsonGdpr = miCookiesWOK.getCookie("cookieswok");
        // var cookies = $.cookie();

        if (jsonGdpr !== "") {
            console.log("Valores aCookies From Cookies");
            console.log(JSON.parse(jsonGdpr));
            $.each(JSON.parse(jsonGdpr), function (i, row) {
                miCookiesWOK.aCookies[i] = row;
                var sIdentificador = i;
                var sEleccion = row;
                $("#j_btn_" + sIdentificador + "_n").removeClass("active").addClass("disable");
                $("#j_btn_" + sIdentificador + "_y").removeClass("active").addClass("disable");
                if (sEleccion === "Y") {
                    $("#j_btn_" + sIdentificador + "_y").removeClass("disable");
                } else {
                    $("#j_btn_" + sIdentificador + "_n").removeClass("disable");
                }

            });
        }
        console.log("Valores aCookies Updated?");
        console.log(miCookiesWOK.aCookies);
        $(".j_cookies_config").on("click", function () {
            console.log(".j_cookies_config");
            $(".j_cookies_configurador").removeClass("hide").addClass("show");
        });
        $(".j_cookies_config").on("click", function () {
            $(".j_cookies_configurador").removeClass("hide").addClass("show");
        });
        $(".j_cookies_confirm").on("click", function () {
            miCookiesWOK.setCookie("cookies_aceptadas", "1", 30);
            $(".j_cookies_configurador").addClass("hide");
            $(".cookies-fixed").hide();

            location.href = "/tantra-paradise-centro-masajes-eroticos-madrid-52/";
        });
        $(".j_cookies_config_close").on("click", function () {
            $(".j_cookies_configurador").removeClass("show").addClass("hide");
            miCookiesWOK.setCookie("cookieswok", JSON.stringify(miCookiesWOK.aCookies), 30);
            if (miCookiesWOK.edicionposterior === "/informacion/politica-de-cookies-5/") {
                location.href = "/tantra-paradise-centro-masajes-eroticos-madrid-52/";
            }

        });
        $(".j_cookies_btn_seleccion").on("click", function () {
            var sIdentificador = $(this).data("identificador");
            var sEleccion = $(this).data("eleccion");
            miCookiesWOK.aCookies[sIdentificador] = sEleccion;
            $("#j_btn_" + sIdentificador + "_n").removeClass("active").addClass("disable");
            $("#j_btn_" + sIdentificador + "_y").removeClass("active").addClass("disable");
            if (sEleccion === "Y") {
                $("#j_btn_" + sIdentificador + "_y").removeClass("disable");
            } else {
                $("#j_btn_" + sIdentificador + "_n").removeClass("disable");
            }
        });
    };


    dCookiesWOK.ready(miCookiesWOK.Inicializar);

</script> 
