<script>
    $(document).ready(function(){


        $('.notooltip').tooltip('disable');

        $('#prd-entry-link').tooltip('disable');

        $("a").tooltip();

        $(function() {
            $('[data-toggle="tooltip"]').tooltip()
        })

        $(".navbar-toggler").click(function(){
            $('body').addClass("fixedPosition");
        });

        $("#closeMobileMenu").click(function(){
            $('.navbar-collapse').collapse('hide');
            $('body').removeClass("fixedPosition");
        });

        $('#navbarsExampleDefault').click(function(e){
            if(e.target == this){
                $('.navbar-collapse').collapse('hide');
                $('body').removeClass("fixedPosition");
            }
        });

        //case study same height
    });
</script>
<script>
    $(document).ready(function(){
        $("#mainNavigation").sticky({topSpacing:0});
    });

    $(window).scroll(function() {
        var width = $(window).width();

        if($("#mainNavigation-sticky-wrapper").hasClass('is-sticky'))
        {
            if(width > 992)
            {
                $('.logo-img').attr('src', '/images/themes/lion/logo.svg?v=1');
            }

            if($("#topBarTrigger").hasClass("closed")){
                $(".order-now-main-navigation").removeClass('moved');
            }

        }else{
            $('.logo-img').attr('src', '/images/themes/lion/logo.svg?v=1');

            if($("#topBarTrigger").hasClass("closed")){
                $(".order-now-main-navigation").addClass('moved');
            }

        }
    });
</script>

