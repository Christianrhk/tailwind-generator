        $(".ffModalBtn").on('click',function(e){
          e.preventDefault();
            var vall = $(this).attr('data-val');
            if(vall == ''){
                return false;
            }
            var fdata = new FormData();
            fdata.append('action','getAchievementUsers');
            fdata.append('vall',vall);
            fdata.append('challengeId',challengeId);
            sendajaxrequest('getAchievementUsers',fdata,'challenges');
            return false;
        })
