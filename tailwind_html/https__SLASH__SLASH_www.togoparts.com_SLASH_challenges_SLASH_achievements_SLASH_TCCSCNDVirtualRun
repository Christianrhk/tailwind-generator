<script>
    $(document).ready(function(){
        $(".rvoucherCtr").tooltip();
        var challengeId = 74;

        $(".ffModalBtn").on('click',function(){
            var vall = $(this).attr('data-val');
            if(vall == ''){
                return false;
            }
            var fdata = new FormData();
            fdata.append('action','getAchievementUsers');
            fdata.append('vall',vall);
            fdata.append('challengeId',challengeId);
            sendajaxrequest('getAchievementUsers',fdata,'challenges');
            return false;
        });

        $(document).scroll(function(){
            var lb_cnt_h = $('html').height();
            $("body").css("height", lb_cnt_h + 'px');
        });

        $(document).on('click','.langSel',function(e){
            e.preventDefault();
            if($(this).hasClass('selected')){
                return;
            }
            var vall = $(this).attr('data-value');
            $("#lang").val(vall);
            $("form[name='langForm']").submit();
            return false;
        });
        $(document).on('click','.AchievementdetailsPreview',function(e){
            var achid = $(this).attr('data-achid');
            var fdata = new FormData();
            fdata.append('ach_id',achid);
            fdata.append('action','achievementPreviewDetails');
            $.ajax({
                type : "POST",
                cache : false,
                contentType : false,
                processData : false,
                url : 'https://www.togoparts.com/challenges/ajax/ajaxController.php',
                action : 'achievementPreviewDetails',
                data : fdata,
                success : function(response){
                    var obj = JSON.parse(response);
                    console.log(obj);
                    console.log(obj.data[0].modal_img)
                    // console.log(obj.achievement[0]);
                    // console.log(obj.challenge[0]);
                    // var challengeName=obj.challenge[0].name;
                    var achievementName=obj.data[0].achievement_name;
                    var achievementDesc=obj.data[0].achievement_description;
                    var achievementModalSubTitle=obj.data[0].modal_sub_title;
                    var achievementModalDesc=obj.data[0].modal_description;
                    var modalImg=obj.data[0].modal_img;

                    // console.log(challengeName);
                    // console.log(achievementName);
                    // console.log(achievementDesc);
                    // console.log(achievementIcon);
                    //
                    $('.achName').html(achievementName);
                    // $('.chName').html(challengeName);
                    $('.achDesc').html(achievementDesc);
                    $('.achModalsubTitle').html(achievementModalSubTitle);
                    $('.achModalDesc').html(achievementModalDesc);
                    $('.modimg').attr("src", 'https://www.togoparts.com/'+modalImg);
                    $("#p_PreveLnk1").css("background-image", "url('https://www.togoparts.com/"+modalImg+"')");
                    $("#p_PreveLnk1").attr("href",  'https://www.togoparts.com/'+modalImg);
                    // $('.achicon').attr("src", '/'+modalImg);
                    $("#eventAchievementPreviewMdl").modal('show');

                }
            });

        });
    });
</script>
