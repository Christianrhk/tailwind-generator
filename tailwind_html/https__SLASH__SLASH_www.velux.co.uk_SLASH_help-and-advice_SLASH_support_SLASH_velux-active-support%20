                <i width="13" height="27" class="fas fa-share-alt"></i>               
            </label>
            <div class="popup-container" role="dialog">
                <label for="close2-4492A5BE-DA69-4B7E-8977-2BE3C444C2EF" class="overlay"></label>
                <input class="ui-control" type="radio" id="close2-4492A5BE-DA69-4B7E-8977-2BE3C444C2EF" name="overlay-4492A5BE-DA69-4B7E-8977-2BE3C444C2EF" />

                <div class="popup">
                    <div class="popup__wrapper">
                        <label class="close popup__close" for="close-4492A5BE-DA69-4B7E-8977-2BE3C444C2EF">
