                <i width="13" height="27" class="fas fa-share-alt"></i>               
            </label>
            <div class="popup-container" role="dialog">
                <label for="close2-33E5B9C3-2812-45B1-B57A-146A1EC3FFF9" class="overlay"></label>
                <input class="ui-control" type="radio" id="close2-33E5B9C3-2812-45B1-B57A-146A1EC3FFF9" name="overlay-33E5B9C3-2812-45B1-B57A-146A1EC3FFF9" />

                <div class="popup">
                    <div class="popup__wrapper">
                        <label class="close popup__close" for="close-33E5B9C3-2812-45B1-B57A-146A1EC3FFF9">
                            <input class="ui-control" type="radio" id="close-33E5B9C3-2812-45B1-B57A-146A1EC3FFF9" name="overlay-33E5B9C3-2812-45B1-B57A-146A1EC3FFF9" /> <span>&#x2715;</span>
