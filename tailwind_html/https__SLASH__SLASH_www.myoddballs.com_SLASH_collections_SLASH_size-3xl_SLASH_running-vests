              .nav-bar > .link-inks.shop-by-prints {
                background: #009fe2;
                color: #fff !important;
              }

              .nav-bar > .link-inks.shop-by-prints > a {
                color: #fff !important;
              }


	
	
	
	
	
	
	
	
	
	
	


              #nav .nav-bar > li.subscriptions > a:after {
                background: #F00F8e;
              }

              .nav-bar > .link-inks.subscriptions {
                background: #F00F8e;
                color: #fff !important;
              }

              .nav-bar > .link-inks.subscriptions > a {
                color: #fff !important;
              }


	
	
	
	
	
	

