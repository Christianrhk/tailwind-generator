    <script src="https://static.neshan.org/sdk/leaflet/1.4.0/leaflet.js" type="text/javascript"></script>
    <script>
        function addClick(ibutton) {
            var count = parseInt($(".count").html());
            var basket = document.getElementById("mini-basket");

            var id = ibutton.getAttribute('data-id');
            var CurrentButton = $("#btn_" + id);

            if (CurrentButton.hasClass('added')) {
                CurrentButton.toggleClass('added');
                CurrentButton.removeClass('success-cart');
            }            
            else {
                var amount = $(".amount-" + id).val();
                //.........................................................
                $.post("/add/cart/ajax", { Id: id, Amount: amount })
                    .done(function (data, status) {
                        var obj = jQuery.parseJSON(JSON.stringify(data));
                        if (obj.status) {
                            CurrentButton.toggleClass('added');
                            if ($("p").hasClass("total")) {
                                var total = parseInt($(".total").html().replace(/\D/g, ''));
                                $(".count").html(count + parseInt(amount));
                                if (amount > 1) {
                                    for (let i = 1; i <= amount; i++) {
                                        basket.innerHTML = "<li><a href =\"/product?t=" + ibutton.getAttribute('data-t') + "\" ><span>" + ibutton.getAttribute('data-title') + "</span></a ><p><span class=\"price\">(" + formatter.format(parseInt(ibutton.getAttribute('data-price'))).replace("$", "") + " تومان)</span><a class=\"text-danger btn-delete left\" title=\"حدف از سبد خرید\" href=\"/cart/delete/item?id=" + obj.data + "\"><i class=\"ion-ios-close-outline\"></i></a></p></li >" + basket.innerHTML;
                                    }
                                    $(".total").html(formatter.format((parseInt(ibutton.getAttribute('data-price')) * amount) + total).replace("$", "") + " تومان");
                                }
                                else {
                                    basket.innerHTML = "<li><a href =\"/product?t=" + ibutton.getAttribute('data-t') + "\" ><span>" + ibutton.getAttribute('data-title') + "</span></a ><p><span class=\"price\">(" + formatter.format(parseInt(ibutton.getAttribute('data-price'))).replace("$", "") + " تومان)</span><a class=\"text-danger btn-delete left\" title=\"حدف از سبد خرید\" href=\"/cart/delete/item?id=" + obj.data + "\"><i class=\"ion-ios-close-outline\"></i></a></p></li >" + basket.innerHTML;
                                    $(".total").html(formatter.format(parseInt(ibutton.getAttribute('data-price')) + total).replace("$", "") + " تومان");
                                }
                            }
                            else {
                                $(".count").html(count + parseInt(amount));
                                basket.innerHTML = "<li><a href =\"/product?t=" + ibutton.getAttribute('data-t') + "\" ><span>" + ibutton.getAttribute('data-title') + "</span></a ><p><span class=\"price\">(" + formatter.format(parseInt(ibutton.getAttribute('data-price'))).replace("$", "") + " تومان)</span><a class=\"text-danger btn-delete left\" title=\"حدف از سبد خرید\" href=\"/cart/delete/item?id=" + obj.data + "\"><i class=\"ion-ios-close-outline\"></i></a></p></li >";

                                if (amount > 1) {
                                    for (let i = 2; i <= amount; i++) {
                                        basket.innerHTML = "<li><a href =\"/product?t=" + ibutton.getAttribute('data-t') + "\" ><span>" + ibutton.getAttribute('data-title') + "</span></a ><p><span class=\"price\">(" + formatter.format(parseInt(ibutton.getAttribute('data-price'))).replace("$", "") + " تومان)</span><a class=\"text-danger btn-delete left\" title=\"حدف از سبد خرید\" href=\"/cart/delete/item?id=" + obj.data + "\"><i class=\"ion-ios-close-outline\"></i></a></p></li >" + basket.innerHTML;
                                    }
                                    basket.innerHTML = basket.innerHTML + "<li><p class='total'>" + formatter.format(parseInt(ibutton.getAttribute('data-price')) * amount).replace("$", "") + " تومان </p><div class=\"mini-cart-buttons\"><a class=\"btn btn-vira\" style=\"width:100%\" href=\"/cart\"><i class=\"ion-ios-cart-outline flip-horizontal\"></i>سبد خرید</a></div></li >";
                                }
                                else {
                                    basket.innerHTML = basket.innerHTML + "<li><p class='total'>" + formatter.format(parseInt(ibutton.getAttribute('data-price'))).replace("$", "") + " تومان </p><div class=\"mini-cart-buttons\"><a class=\"btn btn-vira\" style=\"width:100%\" href=\"/cart\"><i class=\"ion-ios-cart-outline flip-horizontal\"></i>سبد خرید</a></div></li >";
                                }
                            }
                            CurrentButton.addClass("success-cart");

                            //setTimeout(function () {
                            //    button.classList.toggle('added');
                            //    button.classList.remove("success-cart");
                            //}, 6000);
                        }
                        else {
                            Swal.fire({ title: 'توجه', text: obj.message, type: 'warning', confirmButtonText: "باشه", });
                        }
                    })//---END DONE
                    .fail(function (jqXHR, textStatus, errorThrown) {
                        alert('Error:' + jqXHR.responseText);
                        Swal.fire({ title: 'توجه', text: jqXHR.responseText + ' ,' + textStatus + ' ,' + errorThrown, type: 'warning', confirmButtonText: "باشه", });
                    });
                //.........................................................
            }
        }
        //****************************************************************************************************************

        function NegReport() {
            var AdID = $("#NegVID").html();
            var UserId = $("#VUserID").val();
            var Text = $("#NegVText").val();
            if (AdID !== '' && UserId !== undefined && UserId !== '' && Text !== '') {
                $.post("/SubVReport", { VAdID: AdID, VUserId: UserId, VText: Text , VType: 'NEG' }, function (data, status) {
                    var obj = jQuery.parseJSON(JSON.stringify(data));
                    if (obj.status) {
                        Swal.fire('نتیجه پردازش', obj.message, 'success');
                        $("#NegVText").val('');
                        $('#NegReportModal').modal('hide');
                    }
                });
            }
            else
                Swal.fire('خطا در ثبت گلایه/شکایت!', 'اطلاعات برای ثبت گلایه کافی نیست / باید ابتدا وارد سایت شوید', 'warning');
        }
        function PosReport() {
            var AdID = $("#PosVID").html();
            var UserId = $("#VUserID").val();
            var Text = $("#PosVText").val();
            if (AdID !== '' && UserId !== undefined && UserId !== '' && Text !== '') {
                $.post("/SubVReport", { VAdID: AdID, VUserId: UserId, VText: Text, VType: 'POS' }, function (data, status) {
                    var obj = jQuery.parseJSON(JSON.stringify(data));
                    if (obj.status) {
                        Swal.fire('نتیجه پردازش', obj.message, 'success');
                        $("#PosVText").val('');
                        $('#PosReportModal').modal('hide');
                    }
                });
            }
            else
                Swal.fire('خطا در ثبت گلایه/شکایت!', 'اطلاعات برای ثبت گلایه کافی نیست / باید ابتدا وارد سایت شوید', 'warning');
        }
        //...............................................................
        $(document).ready(function () {
            var formatter = new Intl.NumberFormat('en-US', {
                style: 'currency',
                currency: 'USD',
                //minimumFractionDigits: 0, // (this suffices for whole numbers, but will print 2500.10 as $2,500.1)
                maximumFractionDigits: 0, // (causes 2500.99 to be printed as $2,501)
            });

            $(function () {
                $('.rating').rating();

                $('.ratingEvent').rating({ rateEnd: function (v) { $('#result').text(v); } });
            });
            //.................................

            $("#bookmark_a").click(function () {
                window.location.href = "/bookmark" + "?Id=" + 276;
            });

            $('#adContainer').bind('cut copy paste', function (e) {
                e.preventDefault();
            });

            $("#adContainer").on("contextmenu", function (e) {
                return false;
            });


            //$(".add-to-cart").click(function () {
            //    var id = $(this).data("id");
            //    var amount = $(".amount-" + id).val();
            //    $.post("/add/cart/ajax", { Id: id, Amount: amount }, function (data, status) {
            //        var obj = jQuery.parseJSON(JSON.stringify(data));
            //        if (obj.status) {
            //            alert("yes");
            //            $(this).toggleClass('added');
            //        }
            //        else {
            //            Swal.fire('توجه', obj.message, 'warning');
            //        }
            //    });
            //});

            //document.querySelectorAll('.add-to-cart').forEach(button => {

            //});

            var map = new L.Map('map', {
                key: 'web.Mcm3i2Obp6M4d4ePhfqw0qSRfIDsdodwR5rduzD5',
                maptype: 'dreamy',
            poi: true,
            traffic: false,
            center: [37.55051, 45.06546],
            zoom: 14,
            gestureHandling: 'cooperative'
            });
            map.dragging.disable();
            map.touchZoom.disable();
            map.doubleClickZoom.disable();
            map.scrollWheelZoom.disable();
            map.boxZoom.disable();
            map.keyboard.disable();
            map.removeControl(map.zoomControl);
            if (map.tap) map.tap.disable();
            const btnLoadMap = $('#btn-load-map');

            btnLoadMap.on('click', function () {
                $(this).hide();
                map.dragging.enable();
                map.touchZoom.enable();
                map.doubleClickZoom.enable();
                map.scrollWheelZoom.enable();
                map.boxZoom.enable();
                map.keyboard.enable();
                map.addControl(map.zoomControl);
                if (map.tap) map.tap.enable();
            })
            var greenIcon = L.icon({
                iconUrl: '/main/images/marker.png',
                iconSize: [38, 63],
                iconAnchor: [19, 61]
            });
        //.................................................................................................
        if ($('#Lat').val() != '' && $('#Long').val() != '') {
            Marker = new L.marker([$('#Lat').val(), $('#Long').val()],{ icon: greenIcon }).addTo(map);
               map.setView([$('#Lat').val(), $('#Long').val()], 14);
            lat = $('#Lat').val();
               lon = $('#Long').val();
            $.ajaxSetup({
                headers: { 'Api-Key': 'service.ehpaQEHn91ZedRUVP5UG6WHeVYFHhT672do2mOkR' }
               });
            $.ajax({
               type: 'get',
                url: 'https://api.neshan.org/v2/reverse?lat=' + lat + '&lng=' + lon,
                cache: false,
                beforeSend: function () {
                   },
                success: function (response) {
                    $('.address-neshan').html(response.formatted_address);
                   },
                error: function () {

                   }
               });
        }
        });
        $('.collapse').on('shown.bs.collapse', function () {
            $(this).parent().find(".ion-chevron-down").removeClass("ion-chevron-down").addClass("ion-chevron-up");
        }).on('hidden.bs.collapse', function () {
            $(this).parent().find(".ion-chevron-up").removeClass("ion-chevron-up").addClass("ion-chevron-down");
        });
    </script>
