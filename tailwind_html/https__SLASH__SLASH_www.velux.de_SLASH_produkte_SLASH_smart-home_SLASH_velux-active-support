<script>
function pushDataLayer(event, category, action, label, eventnoninteraction) {
    //check if the GTM dataLayer is there
    window.dataLayer = window.dataLayer || [];
    //push the object to the dataLayer
    window.dataLayer.push({
        'event': event,
        'eventcategory': category,
        'eventaction': action,
        'eventlabel': label,
        'eventnoninteraction': eventnoninteraction
    });
}

function getCategoryTitle(e) {
    var categorySection = e.closest("section").parentElement.closest("section").previousElementSibling;
    var title = categorySection.querySelector(".article-wrapper h1").innerText;
    return title;
}


(function () {

    var containers = document.querySelectorAll('.faq-spot .faq-item');
    
    if (containers.length) {
    for (var i = 0; i < containers.length; ++i) {
        containers[i].addEventListener('click', function (e) {
            if (!this.classList.contains("open")){ //need to check it this way because class open is added before this function is fired
                return;
            }
            var categoryTitle = getCategoryTitle(this);
            var questionTitle = this.querySelectorAll('.faq-question .faq-question-body')[0].innerText
            pushDataLayer("VELUX Active support", "VELUX Active support", categoryTitle+" - "+ questionTitle, "End-user", false);
    
            }, false);
        }
    }

    //search results
    var searchTerm = document.querySelector('.faqSearchSpot .search-box .search-field input').value.toLowerCase();
    if (searchTerm != "") {
        var resultsCount = document.querySelectorAll('.faqSearchSpot .search-results .search-faq-result').length;
        pushDataLayer("VELUX Active support", "VELUX Active support search", searchTerm, resultsCount.toString(), false);
    
    }
    
    
})();
</script>


