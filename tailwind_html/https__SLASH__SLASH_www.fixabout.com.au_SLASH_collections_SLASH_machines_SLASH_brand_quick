<script>
function startTimer(duration, display) {
    var timer = duration, minutes, seconds;
    setInterval(function () {
        hours = parseInt(timer / 3600, 10);
        minutes = parseInt(timer % 3600 / 60, 10);
        seconds = parseInt(timer % 60, 10);

        hours   = hours   < 10 ? "0" + hours : hours;
        minutes = minutes < 10 ? "0" + minutes : minutes;
        seconds = seconds < 10 ? "0" + seconds : seconds;

        display.textContent = hours + ":" + minutes + ":" + seconds;

        if (--timer < 0) {
            display.textContent = "Missed today"
            timer = 0;
        }
    }, 1000);
}

var midDay = new Date(),
    display = document.querySelector('#time');
midDay.setUTCHours(1,30,0,0)
counter = (midDay - new Date()) / 1000
if (counter < 0) {
    display.textContent = "Missed today"
} else {
    startTimer(counter, display);
}

var app = new Vue({
    el: '#floatingButtonHolder',
    data: {
        showCutoffTable: false
    },
    methods: {
        toggleTable: function(status) {
            this.showCutoffTable = status
        }
    }
})
</script>

