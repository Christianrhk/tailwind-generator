<script>
    let emailInput = document.querySelector('#email')
    let validEmail = /^[a-zA-Z0-9.!#$%&'*+/=?^_`{|}~-]+@[a-zA-Z0-9-]+(?:\.[a-zA-Z0-9-]+)*$/;
    function checkEmail() {
         if (emailInput.value.match(validEmail)) {
            emailInput.style.border ="1px solid #aaa"
            document.querySelector('#email-match').style.display = "none"
         } else {
             emailInput.style.border = "1px solid red"
             document.querySelector('#email-match').style.display = "block"
         }
    }

   function checkPassword(e) {
       let confirmPasswordInput = document.querySelector('#confirm-password')
       let passwordValue = document.querySelector('#password').value
       if(passwordValue === e.target.value) {
           confirmPasswordInput.style.border = "1px solid #aaa"
            document.querySelector('#password-match').style.display = "none"
            document.querySelector('#register-button').style.backgroundColor = "black"
           document.querySelector('#register-button').style.cursor = "pointer"
       }else  {
            confirmPasswordInput.style.border = "1px solid red"
            document.querySelector('#password-match').style.display = "block"
            document.querySelector('#register-button').style.backgroundColor = "#aaa"
            document.querySelector('#register-button').style.cursor = "not-allowed"
       }
   }
</script>

<script>
    function changeType(e) {
        let passwordInput = e.target.previousElementSibling
        if(passwordInput.type === "password") {
            passwordInput.type = "text"
        } else {
            passwordInput.type = "password"
        }
    }
</script>

<!-- TABS BEHAVIOUR -->
<script>
    const contentLogin = document.querySelector("#Login")
    const contentRegister = document.querySelector("#Register")
    const tabLogin = document.querySelector("#tab__login")
    const tabRegister = document.querySelector("#tab__register")

    const registerLink = document.querySelector("#register-link")
    const loginLink = document.querySelector("#login-link")

    registerLink.addEventListener("click", () => {
            contentLogin.classList.remove("content__active")
            contentRegister.classList.add("content__active")
            tabLogin.classList.remove("tab__active");
            tabRegister.classList.add("tab__active")
        });
    loginLink.addEventListener("click", () => {
            contentLogin.classList.add("content__active")
            contentRegister.classList.remove("content__active")
            tabLogin.classList.add("tab__active");
            tabRegister.classList.remove("tab__active")
        });
    tabRegister.addEventListener("click", () => {
            contentLogin.classList.remove("content__active")
            contentRegister.classList.add("content__active")
            tabLogin.classList.remove("tab__active");
            tabRegister.classList.add("tab__active")
        });
    tabLogin.addEventListener("click", () => {
        contentLogin.classList.add("content__active")
        contentRegister.classList.remove("content__active")
        tabLogin.classList.add("tab__active");
        tabRegister.classList.remove("tab__active")
    });
</script>

<script type="text/javascript">
    if (window.location.hash == '#recover') {
        showRecoverPasswordForm();
    } else {
        hideRecoverPasswordForm();
    }
    function showRecoverPasswordForm() {
        $('.recover-password').show();
        $('.login').hide();
        window.location.hash = '#recover';
        return false;
    }
    function hideRecoverPasswordForm() {
        $('.recover-password').hide();
        $('.login').show();
        window.location.hash = '';
        return false;
    }
</script>
