<script>
    $(document).ready(function () {
        var hours = 24;	// Reset when storage is more than 24hours
        var now = new Date().getTime();
        var setupTime = localStorage.getItem('setupTime');
        if (setupTime == null) {
            localStorage.setItem('setupTime', now)
        } else {
            if (now - setupTime > hours * 60 * 60 * 1000) {
                localStorage.clear()
                localStorage.setItem('setupTime', now);
                localStorage.setItem('first_visit', '');
                localStorage.setItem('curr_china', 'No');
            }
        }
        // localStorage.setItem('curr_china','');

        var my_loc = '';
        var getls = '';
        var curr = '';
        var currls = 'No';
        currls = localStorage.getItem('curr_china');
        $.ajax({
            url: '//get.geojs.io/v1/ip/country.json',
            type: 'GET',
            dataType: 'json',
            success: function (location) {
                my_loc = location.country_3;
                if ($('.china--popup__main').length > 0) {
                    $('.china--popup__main').hide();
                    if (my_loc == 'CHN' || currls == 'Yes') {
                        getls = localStorage.getItem('first_visit');
                        if (getls == null || getls == '') {
                            $('.china--popup__main').removeClass('hide');
                            $('.china--popup__main').show();
                            /* Changed to china currency */
                            $('.china--popup__main .popup--close').click(function () {
                                $('.china--popup__main').hide();
                                $('.china--popup__main').addClass('hide');
                                localStorage.setItem('first_visit', 'No');
                            });
                        }
                    }
                }
            }
        });

        $('.header-content .user-links .shopify-currency-form select').change(function () {
            curr = $(this).val();
            if (curr == 'CNY') {
                localStorage.setItem('first_visit', '');
                localStorage.setItem('curr_china', 'Yes');
            }
            else {
                localStorage.setItem('curr_china', 'No');
            }
        });

    });
</script>


<script>
    window.addEventListener('globoFilterRenderCompleted', function () {

        var ttl = document.querySelector('.collection__title');
        var sctn = document.querySelector('.collection-countdown--section');
        
        if (ttl != null) {
            document.querySelector('#gf-grid').appendChild(ttl)
        }

        setTimeout(function () {
            var countdownPlace = document.querySelector('.collection-countdown--placement')
            if (sctn != null) {
                countdownPlace.replaceWith(sctn)
                sctn.style.display = 'block'
        }
        }, 600)

        setTimeout(function () {

            var clear = document.querySelector('.clear-refinements');

            if (clear != null) {
                document.querySelector('body').classList.add('hasClear')
            } else {

                document.querySelector('body').classList.remove('hasClear')
            }

        }, 600)
    })

</script>
