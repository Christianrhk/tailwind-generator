    var onSubmitCallback = function(event) {

        var captchaTokenInputElement = document.getElementById('fxb_a2a0de23-f5b0-461c-9089-f69ed50c3672_Fields_16f3c785-780a-49fa-8620-bb00b6038eb5__Value');
        var errorSpanElement = document.getElementById('captchaError-' + 'fxb_a2a0de23-f5b0-461c-9089-f69ed50c3672_Fields_16f3c785-780a-49fa-8620-bb00b6038eb5__Value');

        
        if (!captchaTokenInputElement || !captchaTokenInputElement.value || captchaTokenInputElement.value.trim() === "") {

            var errorMessage = captchaTokenInputElement.dataset.valRegexValue;
            
            errorSpanElement.innerText = errorMessage;
            errorSpanElement.style.display = "block";

            event.preventDefault();
            return false;
        }

        errorSpanElement.innerText = "";
        errorSpanElement.style.display = "none";
        return true;
    }

    // attach the eventlistener to the form submit
    var formElement =document.getElementsByTagName("form")[0];
    if (formElement.addEventListener)
    {
        formElement.addEventListener("submit", onSubmitCallback, false);  //Modern browsers
    }
</script>

