<!-- pagetop -->
<script>
    $(function() {
        var topBtn = $('#footer__pagetop');
        topBtn.hide();
        $(window).scroll(function () {
            if (window.matchMedia('(min-width:768px)').matches) {
                if ($(this).scrollTop() > 100) {
                    topBtn.fadeIn();
                } else {
                    topBtn.fadeOut();
                }
            }
        });
    });
</script>

