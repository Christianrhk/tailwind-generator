	<script type="text/javascript" language="javascript">
        function createQryString(page,results) {
			selCat = document.getElementById('eventCategory').value;
			selTF = document.getElementById('timeframe').value;
            qryString = "&page=" + page + "&nr=" + results + "&ec=" + selCat + "&tf=" + selTF; 
		
            srchType = document.getElementById('searchType').value;
            searchText = document.getElementById('searchTxt').value;
            if (searchText != "") { qryString += "&st=" + srchType + "&srch=" + encodeURIComponent(searchText); }
 		
          location.href = "eventListings.php?nm=626" + qryString;
        }
		
		function setPageNum(pageNum) { 
			createQryString(pageNum,"50");
		}
		function setNumRows(numResults) {
			createQryString(1,numResults);
		}
		
        function doSearch(method) {
 		
          	if (method == "clr") {
                document.getElementById('searchType').value = "";
                document.getElementById('searchTxt').value = "";
				createQryString(1,"50");
            } else {
				createQryString(1,"50");
            }
		
        }
    
        $(document).ready(
            function() { 
				$("#searchFromDate").keyup(function(event) { if (event.which == 13) { doSearch('srch'); } } );
				$("#searchToDate").keyup(function(event) { if (event.which == 13) { doSearch('srch'); } } );
                $("#searchTxt").keyup(function(event) { if (event.which == 13) { doSearch('srch'); } } );
            }
        );
		
	
    </script>

	

