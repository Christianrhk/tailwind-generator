      alert('Cart value cannot be zero!');
      return
    }

    let discount = $('input.discount-field.add-discount-now').val();

    if (discount && discount != '') {

      location.href = `/checkout?discount=${discount}`;

    }
    else {
      location.href = '/checkout';

    }

  };


