<script>

  $(document).ready(function () {

    $("#xsort_by").val("manual");

    $('.non_clickable').click(function (e) {
      e.preventDefault();
    })

    

    var quantity = 1;
    $('#product_qty_s').val(quantity);

    $(document).on("click", ".minus_wrap", function () {
      if (quantity > 1) {
        quantity--;
        $('#product_qty_s').val(quantity);

        // input.value = quantity;
      }
    });

    $(document).on("click", ".plus_wrap", function () {
      quantity++;
      $('#product_qty_s').val(quantity);
    });

    

  })

</script>


<script type="text/javascript">

  /* github.com/trix/nano - v1 - MIT license */function nano(e, t) { return e.replace(/\{([\w\.]*)\}/g, function (e, n) { var r = n.split("."), i = t[r.shift()]; for (var s = 0, o = r.length; s < o; s++)i = i[r[s]]; return typeof i !== "undefined" && i !== null ? i : "" }) }

  let Xtemplate = `<div class="wish-product-wrapper" >
    <div class="wish-product-image" >
        <img class="block" loading="lazy" src="{p.iu}" alt="">
    </div>
    <div class="wish-product-title">
        <div class="variant-title-wrapper">
        <a href="{p.du}"><h5>{p.dt}</h5></a>
        <p>$ {p.pr}</p> 
        </div> 
        <div class="add-to-cart-wrapper"> 
            <button class="wish-add-to-cart wishlist-variant-add-to-cart" data-pid="{p.epi}" > Add </button>
        </div>  

    </div>


  
</div>`;

  if (!window.SwymCallbacks) { window.SwymCallbacks = []; }
  function dowishlistLoadTopBar() {
    window.SwymCallbacks.push(function () {
      _swat.fetchWrtEventTypeET(async function (products) {

        $('.loading-text').hide();
        $('.no-product-found').remove();
        $('.wishlist-products .wish-product-wrapper').remove();
        if (products && products.length > 0) {
          for (let p of products) {
            console.log(p);

            let newTemplate = nano(Xtemplate, { p });
            $('.wishlist-products').append(newTemplate);
          }
        } 
        else {
          $('.wishlist-products').append(`<p class="no-product-found">No products in wishlist</p>`);
        } 
        // render products - array of objects with same structure as productData
        // 
        // products count - products.length
        // 
      }, _swat.EventTypes.addToWishlist);
    });

  }
  dowishlistLoadTopBar();
</script>


<script>
  let reAlignPopup = function () {

    let iconOffset = $('.wishlist_icon_wrap').offset();
    if(iconOffset && iconOffset.left){ 
    $(`.wish-list-popup-wrapper`).attr('style', `right:unset;left:calc(${iconOffset.left}px - 170px);`)
    }  
  };
  $(document).ready(function (params) {
    reAlignPopup();
    $(document).on('click', '.removeProduct-wisthlist', function (e) {
      e.preventDefault();
      e.stopPropagation();
      let pid = $(this).attr('data-pid');
      let productCard = $(this).closest('.single-product-card');
      _swat.deleteEvent(function callback() {
        // Successfully deleted, re-render page
        productCard.remove();

      }, pid);

    })

    $(`.wishlist_icon_wrap`).hover(function () {
      
      $('.popup-wishlist').show();
      
      dowishlistLoadTopBar();
      setTimeout(() => {
        $(this).addClass("active");
      }, 50);


    }, function () { 
      setTimeout(() => { 
        if (!$(this).is(":hover") && !$(`.wish-list-popup-wrapper`).is(':hover')) {
          $(this).removeClass("active");
          setTimeout(() => {
            $('.popup-wishlist').hide();

          }, 50); 
        } 
      }, 2000);  
    });  

    $(document).on('click', '.wishlist-variant-add-to-cart', function (el) {
      let id = $(this).attr('data-pid');
      let eEL = $(this);
      $(this).text('Adding...')
      // Note the empty object as the third argument, representing no line item properties.
      CartJS.addItem(
        id,
        1,
        {},
        {
          // Define a success callback to display a success message.
          success(data, textStatus, jqXHR) {
            const addToCartText = eEL.html();
            eEL.html('Added...');
            eEL.removeClass('active');
            $('.toggleSideCart').first().click();
            setTimeout(() => {
              eEL.html('Add');
            }, 1000);
            data.addedQuantity = 1;
            jQuery(document).trigger('MOXIE_ADD_TO_CART', data);
          },

          // Define an error callback to display an error message.
          error(jqXHR, textStatus, errorThrown) {
            console.log(errorThrown);
            let error = jqXHR.responseJSON;
            eEL.html('Adding...')
            // eslint-disable-next-line no-alert
            alert(`${error.message}: ${error.description}`);
            eEL.removeClass('loading');
          }
        }
      );
    });

  });
  $(window).resize(function () {

    reAlignPopup();

  })





</script>



