<script src="//cdn.shopify.com/s/files/1/2569/3918/t/69/assets/moxie-config.js?v=10758076764225040161640688073" type="text/javascript"></script>
 

<script>



$(document).on('MOXIE_VIEW_ITEM', function (event, { variant, product }) {
  console.log('TRIGGERED MOXIE_VIEW_ITEM', variant.id);

  window.execFuncs.view_item({
    variant_id: variant.id,
    variant_title: `${product.title} - ${variant.title}`,
    product_type: product.type,
    price: variant.price/100, 
    currency: 'USD',
    product_id: product.id,
  });
});

// MOXIE ADD TO CART

// MOXIE VIEW ITEM ;
$(document).on('MOXIE_ADD_TO_CART', function (event, cart_item) {
  console.log('TRIGGERED MOXIE_ADD_TO_CART', cart_item.id);

  window.execFuncs.addToCart({
    variant_id: cart_item.variant_id,
    variant_title: cart_item.title,
    product_type: cart_item.product_type,
    price: cart_item.price/100, 
    currency: 'USD', 
    quantity: cart_item.addedQuantity,
    total_quantity: cart_item.quantity,
    cart_token: window.__cart && window.__cart.token, 
    product_id: cart_item.product_id,
  });
});

// MOXIE PAGE VIEW ;
$(document).on('MOXIE_PAGE_VIEW', function (event) {
  console.log('TRIGGERED MOXIE_PAGE_VIEW', location.pathname);

  window.execFuncs.page_view({
    page_type: window.current_page_type,
    path_name: location.pathname,
  });
});
// MOXIE CHECKOUT NAV ;
$(document).on('MOXIE_CHECKOUT_NAV', async function (event, { currentStep, destinationStep }) {
  console.log('TRIGGERED MOXIE CHECKOUT_NAV ', { currentStep, destinationStep });
  // Shopify.Checkout.step == 'contact_information' ||
  // Shopify.Checkout.step == 'shipping_method' ||
  // Shopify.Checkout.step == 'payment_method'
  let action = 'following';
  let cart = await $.get('/cart.json');
  cart.step = Shopify.Checkout.step;

  // JUST FOR FIRST TIME CHECKOUT LOAD
  if (currentStep == '') {
    cart.step_number = 1;
    // SHOULD INIATE CHECKOUT;
    $(document).trigger('MOXIE_CHECKOUT_START', cart);
  }

  if (
    currentStep == 'contact_information' &&
    (destinationStep == 'shipping_method' || destinationStep == 'payment_method')
  ) {
    action = 'following';
  } else if (currentStep == 'shipping_method' && destinationStep == 'payment_method') {
    action = 'following';
  } else if (
    currentStep == 'payment_method' &&
    (destinationStep == 'shipping_method' || destinationStep == 'contact_information')
  ) {
    action = 'previous';
  } else if (currentStep == 'shipping_method' && destinationStep == 'contact_information') {
    action = 'previous';
  } else if (currentStep == destinationStep) {
    action = 'reload';
  }

  // Only Trigger the event when going forward.
  if (action == 'following') {
    switch (destinationStep) {
      case 'shipping_method':
        cart.step_number = 2;
        // After Contact Information is Filled
        $(document).trigger('MOXIE_CHECKOUT_CONTACT_INFO', cart);
        break;
      case 'payment_method':
        cart.step_number = 3; 
        // After Shipping Information is Filled
        $(document).trigger('MOXIE_CHECKOUT_SHIPPING_INFO', cart);
        break;
      case 'processing':
        cart.step_number = 4;
        // After Payment is Filled
        $(document).trigger('MOXIE_CHECKOUT_PAYMENT_INFO', cart);
        break;
      default:
        break;
    }
  }

  window.execFuncs.checkoutNav({
    currentStep,
    destinationStep,
    action,
  });

  localStorage.setItem('currentCheckoutStep', destinationStep);
});

// MOXIE CHECKOUT STEP 1 - CHECKOUT START
$(document).on('MOXIE_CHECKOUT_START', function (event, cart) {
  console.log('TRIGGERED MOXIE_CHECKOUT_START', cart);

  let items = cart.items.map(({ variant_id, product_id, price, quantity }) => {
    return { variant_id, product_id, price:price/100, quantity };
  });

  window.execFuncs.checkoutInitiate({
    items,
    item_count: cart.item_count,
    total_price: cart.total_price/100,
    currency: 'USD',
  });
});

// MOXIE CHECKOUT STEP 2 - CONTACT INFORMATION FILLED;
$(document).on('MOXIE_CHECKOUT_CONTACT_INFO', function (event, cart) {
  console.log('TRIGGERED MOXIE_CHECKOUT_CONTACT_INFO COMPLETED', cart);

  let items = cart.items.map(({ variant_id, product_id, price, quantity }) => {
    return { variant_id, product_id, price:price/100, quantity };
  });

  window.execFuncs.checkoutInformation({
    items,
    item_count: cart.item_count,
    total_price: cart.total_price/100,
    currency: 'USD',
  });
});

// MOXIE CHECKOUT STEP 3 - SHIPPING INFORMATION ;
$(document).on('MOXIE_CHECKOUT_SHIPPING_INFO', function (event, cart) {
  console.log('TRIGGERED MOXIE_CHECKOUT_SHIPPING_INFO', cart);

  let items = cart.items.map(({ variant_id, product_id, price, quantity }) => {
    return { variant_id, product_id, price:price/100, quantity };
  });

  window.execFuncs.checkoutShipping({
    items,
    item_count: cart.item_count,
    total_price: cart.total_price/100,
    currency: 'USD',
  });
});

// MOXIE CHECKOUT STEP 4 - PAYMENT INFORMATION ;
$(document).on('MOXIE_CHECKOUT_PAYMENT_INFO', function (event, cart) {
  console.log('TRIGGERED MOXIE_CHECKOUT_PAYMENT_INFO', cart);

  let items = cart.items.map(({ variant_id, product_id, price, quantity }) => {
    return { variant_id, product_id, price:price/100, quantity };
  });

  window.execFuncs.checkoutPaymentInfo({
    items,
    item_count: cart.item_count,
    total_price: cart.total_price/100,
    currency: 'USD',
  });
});
 
</script> 

