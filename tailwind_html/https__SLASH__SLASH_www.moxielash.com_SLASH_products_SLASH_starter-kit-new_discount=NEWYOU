<script> 
      let SupeZoomer = setInterval(function () {
      if (window.islazyload) {
        clearInterval(SupeZoomer);

        $(".do-zoom").each(function () {
          let imageurl = $(this).data("src");
          if(window.innerWidth>=768){ 
            if($(this).zoom)
          $(this).zoom({ magnify: 0.7, on: "click", url: imageurl });
          
          }else{ 
            $('.zoom-icon.zoom-in').click(function(){

$('.slide-wrapper-for-zoom').addClass('active');
$('.do-zoom').trigger('zoom.destroy');
if($('.do-zoom.slick-slide.slick-current.slick-active').zoom)
$('.do-zoom.slick-slide.slick-current.slick-active').zoom();


});
$('.zoom-icon.zoom-out').click(function(){

$('.slide-wrapper-for-zoom').removeClass('active');
$('.do-zoom').trigger('zoom.destroy');

});
          } 
        });
      }
    }, 100);

</script>
<script>
     
    let lashImages= {

"Baby": 'https://cdn.shopify.com/s/files/1/2569/3918/products/BabyOverheadLash_5MAG_1200x1200_crop_center.jpg?v=1604679566',
"Classy": 'https://cdn.shopify.com/s/files/1/2569/3918/products/ClassyOverheadLash_10MAG_1200x1200_crop_center.jpg?v=1604613394',
"Money": 'https://cdn.shopify.com/s/files/1/2569/3918/products/MoneyOverheadLash_10MAG_1200x1200_crop_center.jpg?v=1605114403',
"Sassy": 'https://cdn.shopify.com/s/files/1/2569/3918/products/SassyOverheadLash_10MAG_db2335be-186e-4595-9e90-ae5b841ce284_1200x1200_crop_center.jpg?v=1604733442',

}


    $(document).ready(function(){


        $('.variant-product-new').change(function(e){

e.preventDefault();
let index = $(this).data('index');
let lash = $(this).val();

let imagesrc= lashImages[lash];
let image = $(this).closest('.__scope_wrapper_moxie').find('.swiper-wrapper img')
 $(`single-lash-option[data-option-index="option${index}"][data-value*="${lash}"]`).click();;
    if(imagesrc){
         image.attr('src',imagesrc);
         image.parent().attr('href',imagesrc);
    }

    let option1 = $('.js-product-option-select[data-index=1]').val();
    let option2 = $('.js-product-option-select[data-index=2]').val();

    let variants = window.jsProduct.variants.filter((variant) => {
        return (
          variant.option1.includes(option1) &&
          variant.option2.includes(option2)
        );
      });

    if (variants.first()) {
        $('.product-add-to-cart').data('pid', variants.first().id);
    }
});

$('.variant-product-new').change()

 });
</script> 
    </main>

    
