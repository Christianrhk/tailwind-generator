<script>
   document.onreadystatechange = function () {
    if (document.readyState == "interactive") {

      let date = $('.countdown-dec').data('date');
      if (date && date != '') {

      }
      else {
        return;
      }
      var countDownDate = new Date(date).getTime();

      // Update the count down every 1 second
      var x = setInterval(function () {

        // Get todays date and time

        var currentTime = new Date(Date.now() + (new Date().getTimezoneOffset() * 60000)).getTime();
        var pSTTime = currentTime - 28800000;
        var difference = countDownDate - pSTTime;

        // Find the distance between now and the count down date


        // Time calculations for days, hours, minutes and seconds
        var hours = Math.floor(difference / (1000 * 60 * 60));
        var minutes = Math.floor((difference % (1000 * 60 * 60)) / (1000 * 60));
        var seconds = Math.floor((difference % (1000 * 60)) / 1000);

        if (hours < 10) hours = '0' + hours;
        if (minutes < 10) minutes = '0' + minutes;
        if (seconds < 10) seconds = '0' + seconds;
        //console.log("hours", hours);
        if(hours <0 && minutes <0 && seconds <0){
          $('.countdown-dec').html("");
          return;
        }

        if (countDownDate) {
          let counterr = hours + ":"
            + minutes + ":" + seconds + "";;

          $('[data-time] [data-hours]').text(hours);
          $('[data-time] [data-minutes]').text(minutes);
          $('[data-time] [data-seconds]').text(seconds);

          //  document.getElementById("countdown-blackfriday").innerText = hours + ":" + minutes + ":" + seconds + "";

          $('.countdown-dec').html(counterr);
        }
        else {
          //do nothing
        }

      }, 1000);
    }
  }
  $(document).ready(function(){

    $('.top-bar-slider').slick({
    dots:false,
    arrows:false,
    slidesToShow:1,
    slidesToScroll:1,
    swipe:false,
    autoplay: true,
    autoplaySpeed: 3000,
    });
  });

  var closePopup = document.querySelector(".moxie-popup-close"),
  showPopupButton = document.querySelector(".moxie-popup-button"),
  popup = document.querySelector(".moxie-popup"),
  popupBody = document.querySelector(".moxie-popup-body");

//show button
if (showPopupButton) {
  showPopupButton.onclick = function () {
    popup.classList.add("active");
  };
}

//hide button
if (closePopup) {
  closePopup.onclick = function () {
    popup.classList.remove("active");
  };
}

//hide on click parent
if (popup) {
  popup.onclick = function () {
    popup.classList.remove("active");
  };
}

//stop propagation from child
if (popupBody) {
  popupBody.onclick = function (e) {
    e.stopPropagation();
  };
}

</script>
  
  
