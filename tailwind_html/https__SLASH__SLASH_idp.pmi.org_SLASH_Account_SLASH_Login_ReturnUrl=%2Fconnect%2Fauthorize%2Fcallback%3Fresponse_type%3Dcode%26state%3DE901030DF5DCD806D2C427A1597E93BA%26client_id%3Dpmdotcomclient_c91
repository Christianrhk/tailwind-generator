        <script type="text/javascript">
            function logAnalyticsEvent(event) {
                if (!event) { return }

                if (typeof window.raiseAnalyticsTracking === "function") {
                    window.raiseAnalyticsTracking("pmi-add-tracking", event);
                } else {
                    console.warn("window.raiseAnalyticsTracking not found");
                }
            }

            $(function () {
                // Catch Summary Validation Errors
                var $summaryErrorListItems = $("[data-valmsg-summary=true] > ul").children("li");
                var summaryErrorList = [];
                $.each($summaryErrorListItems, function (index, item) {
                    summaryErrorList.push(item.textContent);
                });

                if (summaryErrorList.length > 0) {
                    var eventLoginSummaryErrorsDataBlock = {
                        "event": "sso-form-interaction",
                        "eventInfo": "sso-form-interaction",
                        "form": {
                            "action": "form-error",
                            "formName": "login",
                            "formError": summaryErrorList.join(),
                            "ssoplatform": ""
                        }
                    };

                    setTimeout(function () {
                        logAnalyticsEvent(eventLoginSummaryErrorsDataBlock);
                    }, 500);
                }

                // Send Form Start
                var loginFornElementFocused = false;

                $('form#LoginForm :input').focus(function (e) {
                    if (loginFornElementFocused) return;

                    if (typeof window.raiseAnalyticsTracking !== "function") {
                        console.warn("window.raiseAnalyticsTracking not found");
                        return;
                    }

                    var eventLoginFormDataBlock = {
                        "event": "sso-form-interaction",
                        "eventInfo": "sso-form-interaction",
                        "form": {
                            "action": "form-start",
                            "formName": "login",
                            "formError": "",
                            "ssoplatform": ""
                        }
                    };

                    logAnalyticsEvent(eventLoginFormDataBlock);
                    loginFornElementFocused = true;
                });

                // Catch On Submit Errors
                $("#LoginForm").on("submit", function () {
                    var isFormValid = $("#LoginForm").valid();
                    if (!isFormValid) {
                        var validator = $("#LoginForm").validate();
                        var errorList = validator.errorList;

                        var errorsToSend = [];
                        $.each(errorList, function (index, err) {
                            errorsToSend.push(err.element.id);
                        });

                        var eventLoginFormErrorDataBlock = {
                            "event": "sso-form-interaction",
                            "eventInfo": "sso-form-interaction",
                            "form": {
                                "action": "form-error",
                                "formName": "login",
                                "formError": errorsToSend.join(),
                                "ssoplatform": ""
                            }
                        };

                        logAnalyticsEvent(eventLoginFormErrorDataBlock);
                    }
                });
            });

            function logSocialClick(el) {
                var customSocialLoginLinkData = {
                    "event": "sso-form-interaction",
                    "eventInfo": "sso-form-interaction",
                    "form": {
                        "action": "form-start",
                        "formName": "login",
                        "formError": "",
                        "ssoplatform": el.getAttribute('id')
                    }
                }

                logAnalyticsEvent(customSocialLoginLinkData);
            };
        </script>


