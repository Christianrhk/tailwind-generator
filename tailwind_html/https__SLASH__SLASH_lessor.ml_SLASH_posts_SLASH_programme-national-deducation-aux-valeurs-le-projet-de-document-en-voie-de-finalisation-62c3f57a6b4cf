    // Enable pusher logging - don't include this in production
    //Pusher.logToConsole = true;

    var pusher = new Pusher('b81fbb32178b275552e3', {
        cluster: 'eu'
    });

    var channel = pusher.subscribe('post-channel');
    channel.bind('post-event', function(event) {
        const Toast = Swal.mixin({
            toast: true,
            position: 'top',
            showConfirmButton: true,
            // timer: 5000
        });

        Toast.fire({
            type: "info",
            title: "Nouvel Article: " + event.post
        });
    });
</script>
