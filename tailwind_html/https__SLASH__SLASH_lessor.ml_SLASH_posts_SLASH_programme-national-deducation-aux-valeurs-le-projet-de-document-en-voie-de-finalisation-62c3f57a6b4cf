<script>
document.onkeydown = function(e) {
if (e.ctrlKey &&
(e.keyCode === 67 ||
e.keyCode === 86 ||
e.keyCode === 85 ||
e.keyCode === 117)) {
return false;
} else {
return true;
}
};
$(document).keypress("u",function(e) {
if(e.ctrlKey)
{
return false;
}
else
{
return true;
}
});

document.addEventListener('contextmenu', function(e) {
e.preventDefault();
});

</script>
  <script>

    Pusher.logToConsole = true;
    var pusher = new Pusher('b81fbb32178b275552e3', {
      cluster: 'eu'
    });

    var channel = pusher.subscribe('my-channel');
    channel.bind('my-event', function(data) {
      alert(JSON.stringify(data));
    });
  </script>
    <script>
    let max_width = window.matchMedia(' (max-width: 769px) ')
    let encontinu = document.getElementById('js-home-tab-continu')
    let back_home = document.getElementById('home')

    // console.log(encontinu, back_home)

    encontinu.addEventListener('click', function(e) {
      if (max_width.matches) {
        // e.preventDefault();
        window.location.href = "https://lessor.ml/en_continu"
      } 
    })

    back_home.addEventListener('click', function(e) {
      if (max_width.matches) {
        // e.preventDefault();
        window.location.href = "https://lessor.ml"
      } 
    })
  </script>
  
