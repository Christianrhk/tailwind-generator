  <script type="text/x-template" id="address-search-template">
    <div class="relative pt-6 mx-auto w-full md:w-1/2">
      <div class="flex">
        <input type="text" class="bg-white focus:outline-none focus:shadow-outline border border-gray-300 rounded-lg py-2 px-4 w-32 mr-6 appearance-none leading-normal" :placeholder="unitPlaceHolderText" v-model="unitNumber" @keyup="!addressSearchProgress ? onKeyUp(unitNumber,keywordSearch) : ''"/>
        <input type="text" class="bg-white focus:outline-none focus:shadow-outline border border-gray-300 rounded-lg py-2 px-4 block w-1/2 md:w-full appearance-none leading-normal" :placeholder="placeHolderText" v-model="keywordSearch" :class="{ 'loading-circle' : (keywordSearch.length > 2), 'hide-loading-circle': resultItems.length > 0 || resultItems.length == 0 && !addressSearchProgress  }" @keyup="!addressSearchProgress ? onKeyUp(unitNumber,keywordSearch) : ''"/>
      </div>
      <ul style="max-height: 12rem;" class="overflow-auto absolute bg-gray-200 border border-gray-300 mt-1 w-full rounded-lg z-50" v-if="resultItems.length > 0">
        <li class="border border-gray-400 p-2 hover:bg-gray-400 hover:font-semibold cursor-pointer" v-for="(item,i) in resultItems" :key="i" @click="onSelected(item[itemId], item[itemText])">
          {{ item[itemText] }}
        </li>
      </ul>
      <template v-if="addressSearchError">
      <section class="h-32 bg-red-400 mt-4">
        <p>We're sorry, we're not able to retrieve this information at the moment, please try back later!</p>
      </section>
      </template>
    </div>
  </script>
