          <span class="font-medium text-gray-900">
            {{ question }}
          </span>

          <span class="ml-6 h-7 flex items-center">
            <template v-if="brand === 'lolow'">
              <svg  class="h-6 w-6" :class="isOpen ? 'text-orange-400 hover:text-orange-500 transform -rotate-180' : 'text-orange-700 hover:text-orange-800'" stroke="currentColor" fill="none" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 9l-7 7-7-7"/>
              </svg>
            </template>
            <template v-else-if="brand === 'launtel'">
              <svg class="h-6 w-6" :class="isOpen ? 'text-blue-400 hover:text-blue-500 transform -rotate-180' : 'text-blue-700 hover:text-blue-800'" stroke="currentColor" fill="none" viewBox="0 0 24 24">
