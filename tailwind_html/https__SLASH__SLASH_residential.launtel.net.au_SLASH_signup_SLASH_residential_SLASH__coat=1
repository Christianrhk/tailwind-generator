			<select class="bg-white focus:outline-none focus:shadow-outline border border-gray-300 rounded-lg py-2 px-4 block w-1/2 md:w-full leading-normal" v-model="state" @change="stateOnSelected(state)" v-bind:class="{ 'border-red-500 bg-red-200': stateError }">
				<option value="" selected="selected" disabled>Select</option>
			
				<option value="TAS">Tasmania</option>
			
