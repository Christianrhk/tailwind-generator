        function filterPeople(fname, lname) {
            let jqForm = $('#search-form');
            jqForm.find('input').each(function (x) {
                if (jqForm.find('input').eq(x).attr('name') != 'csrfmiddlewaretoken')
                    jqForm.find('input').eq(x).val('')
            })
            if (fname !== 'None')
                $('#p-first-name-filter').val(fname);
            if (lname !== 'None')
                $('#p-last-name-filter').val(lname);
            jqForm.submit();
        }

