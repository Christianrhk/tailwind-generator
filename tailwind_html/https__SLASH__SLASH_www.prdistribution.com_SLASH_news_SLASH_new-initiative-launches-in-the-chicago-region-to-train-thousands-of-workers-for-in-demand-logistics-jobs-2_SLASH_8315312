$(function() {
    if (window.location.hash) {
        var hash = window.location.hash;
        $(hash).modal('toggle');
    }

    $('.pop').on('click', function() {
        $('.imagepreview').attr('src', $(this).find('img').attr('src'));
        $('#jetNewsImage').modal('show');
    });
});
