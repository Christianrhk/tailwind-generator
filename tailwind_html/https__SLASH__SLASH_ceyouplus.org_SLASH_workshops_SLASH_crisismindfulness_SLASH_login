            var mailingListForm = $('mailingListForm');

            var hideMailingList = Cookie.read("hideMailingList");
            if (!hideMailingList){
                var showMailingListForm = function() {
                    if (window.scrollY > 0) mailingListForm.addClass("slideUp");
                    else mailingListForm.removeClass("slideUp");
                };
                window.addEvent('scroll',showMailingListForm);

                mailingListForm.addEvent('click:relay(.closeForm)',function(){
                    this.getParent().setStyle('display',"none");
                    window.removeEvent('scroll',showMailingListForm);
                    Cookie.write("hideMailingList",true);
                });
            }

            mailingListForm.addEvent('submit',function(e){
                e.stop();

                this.addClass('loading').send();
            }).set('send',{url:mailingListForm.action,onSuccess:function(response){
                mailingListForm.removeClass('loading');
                switch (response) {
	                case "failed":
	                    return resultBoxPopup.create({content: "An unknown error has occurred. Please try again later", type:"error"});
	                case "invalid":
	                    return resultBoxPopup.create({content: "Please enter a valid email address", type:"error"});
	                case "duplicate":
	                    return resultBoxPopup.create({content: "Looks like you're already on the mailing list", type:"error"});
                }

                mailingListForm.addClass('success');
                (function(){
                    mailingListForm.removeClass('success').setStyle('display',"none").reset();
                }).delay(1500);
            }});
		</script>
