$( document ).ready(function() {
    productSwiper = $('#productswiper').swiper({
        mode: 'horizontal',
		lazyloading:true,
        slidesPerView: 1,
        pagination: '.swiper-pagination',
        loop:true,
		preventClicks:false
		
    });
	productSwiper.on('click', function () {
		$('.thumbnails .image-additional:nth-child('+(productSwiper.activeIndex)+') a.thumbnail').trigger('click');
	});
});
