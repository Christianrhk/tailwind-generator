    <script>

        $(document).ready(function () {

            $('#webcomment').submit(function () {
                var res = true;
                // here I am checking for textFields, password fields, and any
                // drop down you may have in the form
                $("input[type='text'],textarea", this).each(function () {
                    if ($(this).val().trim() == "") {
                        Swal.fire('ثبت نظر!', 'پر کردن نام، نظر و امتیاز دهی ضروری است', 'error');
                        res = false;
                    }
                })
                return res; // returning false will prevent the form from submitting.
            });


            $('body').bind('cut copy paste', function (e) {
                e.preventDefault();
            });

            $("body").on("contextmenu", function (e) {
                return false;
            });

            $(function () {
                $('.rating').rating();

                $('.ratingEvent').rating({ rateEnd: function (v) { $('#result').text(v); } });
            });
    });
    </script>
