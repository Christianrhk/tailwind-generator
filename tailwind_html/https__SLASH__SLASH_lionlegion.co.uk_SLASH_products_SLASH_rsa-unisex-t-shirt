<script>

  (function(){
    async function fetchProductType(t){
      let url = t.dataset.target
      if(!url || t.disabled) return
      window.location = url
    }

    function linkButtons(){
      let typeButtons = document.querySelectorAll('type-button[data-target]')
      typeButtons.forEach((el)=>el.addEventListener('click',()=>fetchProductType(el)))
    }

    function pageReset(){
      window.scrollTo(0,0)
      document.querySelectorAll('main script').forEach(s=>{
                let newScript = document.createElement('script')
                newScript.innerHTML = s.innerHTML
                s.replaceWith(newScript)
              })
      StampedFn.init()
      StampedFn.reloadUGC()
    }

    queueMicrotask(linkButtons)

  })()

  $(document).ready(function(){





  if(getComputedStyle(document.body).scrollBehavior === 'smooth'){
  } else {
  // Add smooth scrolling to all links
  $("a").on('click', function(event) {

    console.log('Link Detected');

    // Make sure this.hash has a value before overriding default behavior
    if (this.hash !== "") {

      console.log('Hash Detected');
      // Prevent default anchor click behavior
      event.preventDefault();

      console.log('Current Target:' + location.hash);

      // Store hash
      var hash = this.hash;

      console.log('New Target:' + hash);


      // Establish Offset from Screen Width to account for sticky Header
              var width = $(window).width();
        if (width < 430){
            var scrollOffset = 270;
        }else if(width < 770 && width > 430){
            var scrollOffset = 270;}else{
              var scrollOffset = 270;};

      console.log('Screen Width:' + width + ', Scroll Offset:' + scrollOffset);

      // Using jQuery's animate() method to add smooth page scroll
      // The optional number (800) specifies the number of milliseconds it takes to scroll to the specified area
      $('html, body').animate({
        scrollTop: $(hash).offset().top - scrollOffset
      }, 800, function(){

        // Add hash (#) to URL when done scrolling (default click behavior)
        history.replaceState(undefined, undefined, hash)
      });
    } // End if
  });
};
});
