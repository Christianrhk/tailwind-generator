<script>
    function cartFocus(open) {
      console.log(open);
    let cart = document.querySelector("#cart")

    let elems = cart.querySelectorAll('a, [tabindex="0"], button:not([disabled]), textarea:not([disabled]), input[type="text"]:not([disabled]), input[type="radio"]:not([disabled]), input[type="checkbox"]:not([disabled]), select:not([disabled])');
    if (open) {
        for (let i = 0; i < elems.length; i++) {
            elems[i].setAttribute("tabindex", "0");
        }
    } else {
        for (let i = 0; i < elems.length; i++) {
            elems[i].setAttribute("tabindex", "-1");
        }
    }
    }
  </script>



