<script type="text/javascript">
    function text(label) {
        var input = document.getElementById(label.htmlFor);
        if (input.type === "password") {
            input.type = "text";
            label.innerHTML = "(Hide)";
        } else {
            input.type = "password";
            label.innerHTML = "(Show)";
        }
    }
</script>
