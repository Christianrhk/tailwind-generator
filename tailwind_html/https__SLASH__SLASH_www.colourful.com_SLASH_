                $('#playerSlideContent').animate({ height: '-90px' }, 1000); //-65px
                $(this).css('backgroundPosition', 'top left');
                $(this).find(".playerArrow").toggleClass('fa fa-caret-down fa-1x fa fa-caret-up fa-1x');
                open = false;
            }

        });
        //  end activate from drawer button

        //  activate from any button with the same .playerListenButton
        $(document).on('click', '.playerListenButton', function () {
            if (open === false) {
                $('#playerSlideContent').animate({ height: '180px' }, 1000);  // 135px
                $(this).css('backgroundPosition', 'bottom left');
                $(this).find(".listenArrow").toggleClass('fa fa-caret-up fa-1x fa fa-caret-down fa-1x');
                open = true;
            }
            else {
                $('#playerSlideContent').animate({ height: '-90px' }, 1000); //-65px
                $(this).css('backgroundPosition', 'top left');
                $(this).find(".listenArrow").toggleClass('fa fa-caret-down fa-1x fa fa-caret-up fa-1x');
                open = false;
            }

        });
        // end  activate from

    });

    // playerSlide Drawer
</script>




<!-- the forms master -->
<script type="text/javascript">
    function ajaxFormsGuru(args) {
        $(args.form).off('submit');

        $(args.form).on('submit', function (e) {
            e.preventDefault();

            var errorsNum = 0;

            $(args.form).addClass('disabled-loading');
            $(args.msgBeen).removeClass('mx-success-msg');

            args.requireds.map(function (item) {
                if (!$(item).val()) errorsNum++;
            });

            if (args.recaptcha === true) {
                if (!grecaptcha.getResponse()) errorsNum++;
            }

            if (errorsNum === 0) {
                $.ajax({
                    type: $(args.form).attr('method'),
                    url: $(args.form).attr('action'),
                    data: $(args.form).serialize()
                }).done(function () {
                    $(args.msgBeen).addClass('mx-success-msg').html(args.msgSuccess);
                    $(args.form).removeClass('disabled-loading');
                    $(args.form).find('input, textarea, select').val('');
                    if (args.recaptcha === true) {
                        grecaptcha.reset();
                    }
                }).fail(function () {
                    $(args.msgBeen).html('Oops, there was an error!');
                    $(args.form).removeClass('disabled-loading');
                });
            } else {
                $(args.msgBeen).html('Please fill fields carefully!');
                $(args.form).removeClass('disabled-loading');
            }

            return false;
        });
    }
</script>
