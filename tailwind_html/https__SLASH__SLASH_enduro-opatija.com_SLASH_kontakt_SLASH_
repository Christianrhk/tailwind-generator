<script>
    window.addEventListener('DOMContentLoaded', (event) => {
        const homeSlider = document.querySelector('.home-page-slider');
        if (homeSlider) {
            // vanilla JS window width and height
            var w = window,
                d = document,
                e = d.documentElement,
                g = d.getElementsByTagName('body')[0],
                x = w.innerWidth || e.clientWidth || g.clientWidth,
                y = w.innerHeight || e.clientHeight || g.clientHeight;

            // Resize function
            const heightOutput = document.querySelector('#height');
            const widthOutput = document.querySelector('#width');

            function reportWindowSize() {
                var w = window,
                    d = document,
                    e = d.documentElement,
                    g = d.getElementsByTagName('body')[0],
                    x = w.innerWidth || e.clientWidth || g.clientWidth,
                    y = w.innerHeight || e.clientHeight || g.clientHeight;
                homeSlider.style.height = y - 20 + 'px';
            }

            window.onresize = reportWindowSize;


            homeSlider.style.height = y + 'px';
        }

        //Top search
        const topSearchIcon = document.querySelector('.top-search-icon');
        const topSearch = document.querySelector('.top-search');
        if (topSearchIcon) {
            topSearchIcon.addEventListener('click', function(event) {
                topSearch.classList.toggle('display-it');
            });
        }

        /* Accordion */
        var acc = document.getElementsByClassName("accordion");
        var i;

        for (i = 0; i < acc.length; i++) {
            acc[i].addEventListener("click", function() {
                /* Toggle between adding and removing the "active" class,
                to highlight the button that controls the panel */
                this.classList.toggle("active");

                /* Toggle between hiding and showing the active panel */
                var panel = this.nextElementSibling;
                if (panel.classList.contains('display-it')) {
                    panel.classList.remove('display-it');
                } else {
                    panel.classList.add('display-it');
                }
            });
        }

    });



    //Slick slider
    jQuery(function($) {
        $('.home-featured-content-inner1').slick({
            dots: false,
            infinite: false,
            speed: 300,
            slidesToShow: 3,
            slidesToScroll: 3,
            responsive: [{
                    breakpoint: 1024,
                    settings: {
                        slidesToShow: 2,
                        slidesToScroll: 2,
                        infinite: true,
                        dots: true
                    }
                },
                {
                    breakpoint: 600,
