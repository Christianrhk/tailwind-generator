    <div id="daily-single-serve-capsules-pods-distribution-chart-container" style="height: 400px;"></div>
    
<script type="text/javascript" defer>
    
$(function(){
    Highcharts.theme = Highcharts.themes.white;
    Highcharts.setOptions(Highcharts.theme);
    window.chart = new Highcharts.Chart({
    "chart": {
        "animation": {
            "duration": 0
        },
