            $(function () {
                // Owl Carousel
                var owl = $(".owl-carousel");
                owl.owlCarousel({
                    items: 1,
                    margin: 10,
                    loop: true,
                    nav: true,
                    dots: false,
                    navText: [
                        '<i class="fa fa-angle-left" aria-hidden="true"></i>',
                        '<i class="fa fa-angle-right" aria-hidden="true"></i>'
                    ],
                    responsive: {
                        500: {
                            items: 2,
                            nav: true
                        },
                        1000: {
                            items: 3,
                            nav: true
                        },
                        1200: {
                            items: 4,
                            nav: true,
                            loop: false
                        }
                    }
                });
            });

