				<template x-if="results === 'Missing post types'">
					<h4 class="text-2xl" style="margin:0;">Valitse sisältötyyppi tai suorita haku pelkällä hakusanalla.</h4>
				</template>
				<template x-if="results === 'Ei tuloksia'">
					<h4 class="text-2xl" style="margin:0;">Ei osumia, koita uudelleen.</h4>
				</template>
				<template x-if="results !== 'Ei tuloksia' && results !== 'Missing post types'">
					<div class="w-full flex flex-wrap md:flex-no-wrap">	
						<h4 class="text-2xl w-full md:w-1/2">Etsitkö jotakin näistä?</h4>
						<div class="w-full md:w-1/2 pt-4">
							<template x-for="type, index in Object.entries(results);">
								<div>
									<template x-if="type[1] !== null && type[1].length > 0">
										<div>
											<p class="font-bold text-lg pb-4" x-text="type[0]"></p>
											<template x-for="post in type[1]">
												<div class="flex flex-row">
													<i class="fa fa-chevron-right mr-3 mt-1" style="color:#4a7f28"></i><a class="block w-full overflow-hidden" :href="post.link"><p x-text="post.title"></p></a>
												</div>
											</template>
										</div>
									</template>
								</div>	
							</template>
						</div>
					</div>
				</template>
			</div>
		</div>
    </div>
