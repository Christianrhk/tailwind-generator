<script>

    $("#lnkSeeMoreSchools").click(function () {               
        $('#ShowMoreSchoolPartial').addClass('show');
    });


        //$('#lnkSeeMoreSchools').click(function () {
        //    $(this).hide();
        //    $('#schoollList-1').addClass('hidden');
        //    $('#schoollList-img').addClass('hidden');
        //    $('#schoollList-2').toggleClass('hidden');
        //});
        function setSchoolCode(schCode, schName) {
            // alert(schCode, schName);
            //alert(schCode);
           // alert(encodeURIComponent(schCode));
            $.ajax({
                type: "POST",
                url: "/Pages/SetSchoolCookies?Schoolcode=" + schCode + " &Schoolname=" + schName,
                datatype: 'json',

                success: function (result, status, xhr) {
                    //location.reload(true);
                },
                error: function (error, status, xhr) {
                    console.log(error);
                },
                error: function () {
                    console.log("Result: " + status + " " + error + " " + xhr.status + " " + xhr.statusText);
                }
            });
        }
        function schoolSelectFunction() {
            'use strict';
            // Declare variables
            var input, filter, ul, li, a, i;
            input = document.getElementById('schoolSelect');
            //alert(input);
            filter = input.value.toUpperCase();
            //alert(filter);
            ul = document.getElementById("schoolSelectUL");
            li = ul.getElementsByClassName('school-details-wrap');

            // Loop through all list items, and hide those who don't match the search query
            for (i = 0; i < li.length; i++) {
                a = li[i].getElementsByTagName("a")[0];
                if (a.innerHTML.toUpperCase().indexOf(filter) > -1) {
                    li[i].style.display = "";
                } else {
                    li[i].style.display = "none";
                }
            }
        }

