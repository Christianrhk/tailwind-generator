<script>
    //$('.btn btn-red text-white inline-block').on('click', function () {

    //});


     function AddToCartV2(InstrumentCode, PriceScheduleId) {
         $('#instrumentCode').val(InstrumentCode);
         $('#priceScheduleId').val(PriceScheduleId);
         var instcode=$('#instrumentCode').val();
         var priceschedule=$('#priceScheduleId').val();
         var schoolcode, browser, urlrent ;
        schoolcode = "";
         urlrent = "rents/cart";

        if (/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent)) {
           var browser = "M";
        } else {
           var  browser = "D";
         }
        var ModelCloseFlag = ('; ' + document.cookie).split(`; ModelCloseFlag=`).pop().split(';')[0];
        if (ModelCloseFlag == "") {
            ModelCloseFlag = 0;
         }

         if (schoolcode != null && schoolcode != "") {
             $.ajax({
                 type: "POST",
                 url: "/Pages/AddToCart?instrumentCode=" + InstrumentCode + " &priceScheduleId=" + PriceScheduleId + " &browser=" + browser,
                 datatype: 'html',

                 success: function (result, status, xhr) {
                     console.log(result);
                     //if (result == urlrent) {
                     //    window.location.href = result;
                     //}
                     if (result.includes(urlrent)) {
                        window.location.href = result;
                     }
                     else {
                         $("#showmessage").html(result);
                         $('#_ShowMessagePartial').addClass('show');
                         document.cookie = 'SchoolOpenFlag' + '=; Path=/; Expires=Thu, 01 Jan 1970 00:00:01 GMT;';
                         document.cookie = 'ModelCloseFlag' + '=; Path=/; Expires=Thu, 01 Jan 1970 00:00:01 GMT;';

                     }

                 },
                 error: function (error, status, xhr) {
                     /*  $("#MsgModal").html(error);*/
                     $("#showmessage").html(error);

                 }
             });
         }
         else {
             if (51> 0) {
                 $('#ShowMoreSchoolPartial').addClass('show');
                 document.cookie = "SchoolOpenFlag=1";
             }
             else {
                 $.ajax({
                     type: "POST",
                     url: "/Pages/AddToCart?instrumentCode=" + InstrumentCode + " &priceScheduleId=" + PriceScheduleId + " &browser=" + browser,
                     datatype: 'html',

                     success: function (result, status, xhr) {
                         console.log(result);
                         //if (result == urlrent) {
                         //    window.location.href = result;
                         //}
                         if (result.includes(urlrent)) {
                             window.location.href = result;
                         }
                         else {
                             $("#showmessage").html(result);
                             $('#_ShowMessagePartial').addClass('show');
                             document.cookie = 'SchoolOpenFlag' + '=; Path=/; Expires=Thu, 01 Jan 1970 00:00:01 GMT;';
                             document.cookie = 'ModelCloseFlag' + '=; Path=/; Expires=Thu, 01 Jan 1970 00:00:01 GMT;';

                         }

                     },
                     error: function (error, status, xhr) {
                         /*  $("#MsgModal").html(error);*/
                         $("#showmessage").html(error);

                     }
                 });
             }

         }
    }


