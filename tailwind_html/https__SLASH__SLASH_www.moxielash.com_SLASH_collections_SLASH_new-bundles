    $(document).on('click', '.side-cart__addon-add', function (e) {
      e.preventDefault();
      let id = $(this).data('pid');
      let context = $(this);
      if (id) {
        $(this).attr('disabled', 'disabled').html('<div class="dcbLoader">ADDING...</div>');
        CartJS.addItem(
          id,
          1,
          {},
          {
            success: function (data) {
              context.text('ADD').removeAttr('disabled');
              SuperCart.getCart();
              data.addedQuantity = 1;
              jQuery(document).trigger('MOXIE_ADD_TO_CART', data);
            },
          }
        );
      }
    });
    $(document).on('click', '.cart-coupon .accordion-header', function () {
      $('.cart-coupon').toggleClass('active');
      let coupon = Cookies.get('discount_code');
      let currentVal = $('.add-discount-now').val();

      if (coupon && coupon != '' && currentVal == '') {
        $('.add-discount-now').val(coupon);
      }
    });

    $(document).on('change', 'input.discount-field', function () {
      let value = $(this).val();
      Cookies.set('discount_code', value);
    });
    $(document).on('click', '.discount_apply', function (e) {
      e.preventDefault();
      let discount = $('input.discount-field').val();

      if (discount != '') {
        $(this).val('Applying.');
        location.href = '/checkout?discount=' + discount;
      } else {
        alert('Please input discount code');
      }
    });
    $(document).on('click', '.add-liner-ajax', function (e) {
      e.preventDefault();
      let context = $(this).closest('.code-block-slide-pop');
      let id = context.find('.variant-selector-liner').val();
      if (id) {
        $(this).attr('disabled', 'disabled').html('<div class="dcbLoader">ADDING...</div>');
        CartJS.addItem(
          id,
          1,
          {},
          {
            success: function (data) {
              context.find('.add-liner-ajax').text('ADD').removeAttr('disabled');
              SuperCart.getCart();
              data.addedQuantity = 1;
              jQuery(document).trigger('MOXIE_ADD_TO_CART', data);
            },
          }
        );
      }
    });
    let discount = getParameterByName('discount');

    if (discount && discount != '') {
      Cookies.set('discount_code', discount);
    }

    function showLinerPopup(status) {
      window.ShowingMagneticPopup = status;


      if (status) {
        $('.ss-shopify_cart .magnetic-liner .liner-slide-pop').removeClass('hidden');

        let showPopup = setInterval(() => {
          if ($('.ss-shopify_cart .magnetic-liner .liner-slide-pop.hidden').length > 0) {
            $('.ss-shopify_cart .magnetic-liner .liner-slide-pop')
              .removeClass('hidden');
            clearInterval(showPopup);
          }
        }, 100);
        //  $('.ss-shopify_cart .congrats-message-popup').addClass('hidden');
      } else {
        $('.ss-shopify_cart .magnetic-liner .liner-slide-pop').addClass('hidden');
        //  $('.ss-shopify_cart .congrats-message-popup').removeClass('hidden');
      }
    }
    function showLashiesLinerPopup(status) {
      window.ShowingLashiesPopup = status;


      if (status && !window.ShowingMagneticPopup) {
        $('.ss-shopify_cart .lashies-liner .liner-slide-pop')
          .removeClass('hidden');
        let showPopup = setInterval(() => {
          if ($('.ss-shopify_cart .lashies-liner .liner-slide-pop.hidden').length > 0) {
            $('.ss-shopify_cart .lashies-liner .liner-slide-pop')
              .removeClass('hidden');
            clearInterval(showPopup);
          }
        }, 100);

      } else {
        $('.ss-shopify_cart .lashies-liner .liner-slide-pop')
          .addClass('hidden');
      }
    }
    function ShouldShowLinerPopup(cart) {


      try {
        if ($('.ajaxcart__inner.ajaxcart__inner--has-fixed-footer.liner-slide-pop').length == 0) {

          if (!$('.ajaxcart__inner--has-fixed-footer').hasClass('liner-popup-added')) {
            setTimeout(() => {
              // $('.ajaxcart__inner--has-fixed-footer .code-block-slide-pop').remove();
              
              //  $('.ajaxcart__inner--has-fixed-footer').addClass('liner-popup-added');
            }, 0);
          }

        }

        //test
        let lashSKUs = [
          'L10-RT',
          'L10-3PK',
          'BL9',
          'BL9-3PK',
          'L2-NEW',
          'L2-3PK',
          'BL7',
          'BL7-3PK',
          'L13-NEW',
          'L13-3PK',
          'BL8',
          'BL8-3PK',
          'L3-NEW',
          'L3-3PK',
          'L19-NEW',
          'L19-NEW-3PK',
          'L15-NEW',
          'L15-NEW-3PK',
          'L12-NEW',
          'L12-NEW-3PK',
          'L1-NEW',
          'L1-NEW-3PK',
          'L17-RT',
          'L17-NEW-3PK',
          'L5-NEW',
          'L5-NEW-3PK',
          'L18-RT',
          'L18-NEW-3PK',
          'L6-RT',
          'L6-RT-3PK',
          'SIL4',
          'SIL4-3PK',
          'SIL7',
          'SIL7-3PK',
          'FS3',
          'FS3-3PK',
          'L16-NEW',
          'L16-NEW-3PK',
          'L7-NEW',
          'L7-NEW-3PK',
          'SIL2',
          'SIL2-3PK',
          'SIL8',
          'SIL8-3PK',
          'SIL3',
          'SIL3-3PK',
          'FS1-TST',
          'FS1-TST-3PK',
          'FM8',
          'FM8-3PK',
          'SIL5',
          'SIL5-3PK',
          'L9-NEW',
          'L9-NEW-3PK',
          'SIL6',
          'SIL6-3PK',
          'FS4-TST',
          'FS4-TST-3PK',
          'FM3',
          'FM3-3PK',
          'SL9-TST',
        ];

        let lashiesSKU = [
          'SL13',
          'SL1',
          'SL9',
          'SL11',
          'SL6',
          'SL8',
          'SL7',
          'SL4',
          'SL10',
          'SL12',
          'SL3',
        ];

        if (!cart) {

        }

        else {
          let showPopup = false;

          let LashesAdded = false;
          let LinerAdded = false;
          let LasheesAdded = false;
          let LasheesLinerAdded = false;

          let LashesQTY = 0,
            LinerQty = 0;
          let LashiesQTY = 0,
            LashiesLinerQty = 0;
          if (cart.item_count > 0) {
            for (let i of cart.items) {
              if (lashSKUs.includes(i.sku)) {
                LashesAdded = true;
                LashesQTY += i.quantity;
              }
              if (i.handle == 'magnetic-liquid-eyeliner' || i.handle == 'mini-liquid-magnetic-eyeliner') {
                LinerAdded = true;
                LinerQty += i.quantity;
              }


              if (lashiesSKU.includes(i.sku)) {
                LasheesAdded = true;
                LashiesQTY += i.quantity;
              }
              if (i.handle.includes('lashies-liner')) {
                LasheesLinerAdded = true;
                LashiesLinerQty += i.quantity;
              }
            }
          }
          console.log(`OverRided`);
          showLinerPopup(false);

          if (LashesAdded && !LinerAdded) {
            showLinerPopup(true);
            // SuperCart.updateProduct(null,'32612419338286',LashesQTY)
          } else if (LashesAdded && LinerAdded) {
            // SuperCart.updateProduct(null,'32612419338286',LashesQTY)
            showLinerPopup(false);
          } else if (!LashesAdded && LinerAdded) {
            showLinerPopup(false);
            // SuperCart.updateProduct(null,'32612419338286',0)
          }

          if (LasheesAdded && !LasheesLinerAdded) {
            showLashiesLinerPopup(true);
            // SuperCart.updateProduct(null,'32612419338286',LashesQTY)
          } else if (LasheesAdded && LasheesLinerAdded) {
            // SuperCart.updateProduct(null,'32612419338286',LashesQTY)
            showLashiesLinerPopup(false);
          } else if (!LasheesAdded && LasheesLinerAdded) {
            showLashiesLinerPopup(false);
            // SuperCart.updateProduct(null,'32612419338286',0)
          }


        }

      }
      catch (ex) {

        console.log(ex);
      }

    }

    $(document).on('ss-cart:add-quantity', function (e) {

      try {
        let { variant, product, quantity, currentquantity } = e.detail;

        let cart_item = {
          id: variant.id,
          variant_id: variant.id,
          title: `${product.title} - ${variant.title}`,
          product_type: product.product_type,
          price: variant.price,
          addedQuantity: quantity,
          token: __cart && __cart.token,
          quantity: variant.currentquantity + variant.quantity,
          product_id: product.id,
        };


        jQuery(document).trigger('MOXIE_ADD_TO_CART', cart_item);
      }
      catch (ex) {

      }
    });
</script>

    
