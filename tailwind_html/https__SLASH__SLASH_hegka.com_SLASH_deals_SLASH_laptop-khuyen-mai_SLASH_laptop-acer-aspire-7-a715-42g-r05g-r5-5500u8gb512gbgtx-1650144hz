    <script>
        $(document).ready(function () {
            window.livewire.on('confirmCloseAddReviewDealModal', (e) => {
                var close = e.close;
                if (close != 1 || checkEditorChange > 0) {
                    Livewire.emitTo('user.confirm-close-modal', 'setModal', 'modal-add-deal-review')
                    $('#confirm-close').modal('show');
                } else {
                    $('#modal-add-deal-review').modal('hide')
                }
            })
        });
    </script>
    <script>
        $(document).ready(function () {
            window.livewire.on('ConfirmCloseModal', (e) => {
                $('#' + e.id).modal('hide');
            })
        })
    </script>

    <script src="https://hegka.com/js/jquery.countdown.min.js"></script>
<script>
    function countDown() {
        $('span.countdown-timer').each(function (item) {
            var $this = $(this), finalDate = $(this).data('countdown');
            $this.countdown(finalDate, function (event) {
                $this.html(event.strftime('%D Ngày %H:%M:%S'));
            });
        })
    }
    $(document).ready(function() {
        countDown();
    })
</script>
    <script src="https://www.google.com/recaptcha/api.js" async defer></script>
