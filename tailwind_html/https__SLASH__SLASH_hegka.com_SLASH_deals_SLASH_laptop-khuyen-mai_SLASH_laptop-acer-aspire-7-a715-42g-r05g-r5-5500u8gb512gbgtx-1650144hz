    $(document).ready(function () {
        window.livewire.on('updatePostModalHide', (e) => {
            $('#post_modal_update').modal('hide');
        })
        window.livewire.on('createPostModalHide', (e) => {
            $('#author-editor').modal('hide');
            $('#mobile_create_post').modal('hide');
        })
        window.livewire.on('modalUpdatePost', (e) => {
            $('#post_modal_update #createPost .new-post-video').addClass('d-none');
            $('#post_modal_update #createPost .new-post-image').addClass('d-none');
            $('#post_modal_update #createPost .edit-post-content').html('')
            $('#post_modal_update #createPost .new-post-image.image-input-wrapper').css("background-image", '');
            $('#post_modal_update #createPost .new-post-video #video-edit').html('');
            var image = e.image;
            var extension = e.extension;
            var content = e.content;
            if ( content != null && content.length > 0) {
                $('#post_modal_update #createPost .edit-post-content').html(content);
            } else {
                $('#post_modal_update #createPost .edit-post-content').html('<span class="placeholder text-gray-500 ">Bạn đang nghĩ gì thế?</span>');
            }
            if ( e.image !== null && e.image.length > 0) {
                const extensions = ['mp4', 'ogx', 'oga', 'ogv', 'ogg', 'webm'];
                if(extensions.includes(extension)) {
                    $('#post_modal_update #createPost .new-post-video #video-edit').append('<source src="' + image + '">');
                    const video = document.getElementById('video-edit');
                    video.load();
                    // video.play();
                    $('#post_modal_update #createPost .new-post-video').removeClass('d-none');
                } else {
                    $('#post_modal_update #createPost .new-post-image').removeClass('d-none');
                    $('#post_modal_update #createPost .new-post-image .image-input-wrapper').css("background-image", 'url(' + image + ')');
                }
            }
        });
    });
    $(document).on('change', '#post_modal_update #createPost #image-edit-post', function () {
        Livewire.emitTo('user.articles.edit', 'resetVideo')
        document.getElementById('video-edit').pause();
        $('#post_modal_update #createPost .new-post-video').addClass('d-none');
        $('#post_modal_update #createPost #video-edit').html('');
