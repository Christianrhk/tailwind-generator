<script type="text/javascript">
     var dFunction = "oLoad" + Math.floor(Math.random() * 1000);

        window[dFunction] = function (iid) {
            var script = document.getElementById(iid);
            var herobannerconfig = script.getAttribute('data-herobannerconfig');
            var uid = script.getAttribute('data-uid');
            HeroBannerInitVue(uid, herobannerconfig);
        }
    if (window.personalizationManager) {
        window.personalizationManager.addPersonalizedContentLoaded(function () {
            var scriptpresent = document.getElementById('f45df581-9bcd-4d1d-aafa-06de81140031');
            if (scriptpresent) {
                return;
            }
            var pScript = document.createElement('script');
            pScript.setAttribute('src', "/assets/js/dist/herobanner/herobanner-min.js");
            pScript.setAttribute('onload', dFunction + "('f45df581-9bcd-4d1d-aafa-06de81140031')");
            pScript.setAttribute('id', 'f45df581-9bcd-4d1d-aafa-06de81140031');
            pScript.setAttribute('data-herobannerconfig', '{"Title":"Healthy Living","HeadingTag":"h1","VideoModalTitle":null,"Subtitle":"Get tips and information about weight, pain, heart and more.","ImageUrl":"","ImageHeight":0,"ImageWidth":0,"VideoImageUrl":"","VideoImageHeight":0,"VideoImageWidth":0,"VideoUrl":null,"LinkToExternalUrl":null,"LinkToUrl":"","Uid":"f45df581-9bcd-4d1d-aafa-06de81140031","mediaSelector":"Image","pageSelector":"Internal","DisplayCTA":false,"CTAButtonText":null,"AlignWithBreadCrumbs":false,"AlignWithNonOverLapping":false,"HeroVideoVideoBackgroundColor":null}');
            pScript.setAttribute('data-uid', 'f45df581-9bcd-4d1d-aafa-06de81140031');
            document.body.appendChild(pScript);
        });
    }

</script>



