<script>
  var articles = [];
  var idx = 0;
	$(document).ready(function(){
  	var el = document.querySelector('.page');
		el.innerHTML = el.innerHTML.replace(/&nbsp;/g,'');
  	let sectionPath = 'https://experts.6ya.com/api/v2/help_center/en-us/sections/';
    $(".prev,.next").attr("disabled", false);		
  	$.ajax({
   		context: this,
  		url:sectionPath+'1500000338301'+'/articles',
  		success:(res)=>{
  			articles = res.articles;
  		
  			if(articles.length >1){
            $('.bottom-navigation').css('display','flex')                   
        }
  			idx = articles.findIndex(a=>{ return a.id == '1500002421082'})
  			if(idx<=0){
           $('.prev').attr("disabled", true);	        
        }
        if(idx>=articles.length-1){
  				$('.next').attr("disabled", true);	  
  			}
  		}
  	})
  })
  function onPrev(index){
  	if(index < 0){
      	index = 0;
        this.idx = index;
    }
    let art = articles[index];
    window.location.href = art.html_url;               
  }
  function onNext(index){
    if(index > articles.length-1){
  			index = articles.length-1;
  			this.idx = index;
  	}
  	let art = articles[index];
    window.location.href = art.html_url;                            
  }
</script>
