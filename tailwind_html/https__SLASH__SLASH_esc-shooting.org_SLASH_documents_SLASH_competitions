        <script>
            document.querySelector('.print_btn').addEventListener('click', (event) => {
                event.preventDefault();
                window.print();
            });

            document.querySelector('li > a.bg_tw').addEventListener('click', function(event) {
                event.preventDefault();
                url = `https://twitter.com/share?url=${encodeURIComponent(window.location)}&text=${document.title}`;
                window.open(url);
            }, false);

            document.querySelector('li > a.bg_fb').addEventListener('click', function(event) {
                event.preventDefault();
                url = `https://www.facebook.com/sharer/sharer.php?u=${window.location}&text=${document.title}`;
                window.open(url);
            }, false);

            document.querySelector('li > a.bg_vk').addEventListener('click', function(event) {
                event.preventDefault();
                url = `https://vk.com/share.php?title=${document.title}&url=${window.location}`;
                window.open(url);
            }, false);

        </script>
