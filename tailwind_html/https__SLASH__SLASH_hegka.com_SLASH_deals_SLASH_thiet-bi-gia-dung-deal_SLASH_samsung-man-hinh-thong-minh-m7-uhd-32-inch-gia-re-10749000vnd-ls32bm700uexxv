<script>
    $(document).ready(function () {
        // $('#post-comment-text').emojioneArea({
        //     pickerPosition: "bottom",
        //     tonesStyle: "bullet",
        //     filtersPosition: "bottom",
        //     inline: true,
        //     shortnames: true,
