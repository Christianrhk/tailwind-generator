        // Zoom khi tạo bài viết
        $(document).on('click', '#author-editor .ck.ck-toolbar__items .editor-zoom-in', function () {
            $('#author-editor').toggleClass('full-screen');
            $(this).remove();
            $('#author-editor .ck.ck-toolbar__items').append('<button class="ck ck-button ck-off editor-zoom-out" type="button" tabindex="-1">' +
