        })

        $(document).on('click', '#author-editor .ck.ck-toolbar__items .editor-zoom-out', function () {
            $('#author-editor').toggleClass('full-screen');
            $('.editor-zoom-out').remove();
            $('.editor-zoom-in').remove();
            $('#author-editor .ck.ck-toolbar__items').append('<button class="ck ck-button ck-off editor-zoom-in" type="button" tabindex="-1">' +
                '<!--begin::Svg Icon | path: assets/media/icons/duotone/Design/ZoomMinus.svg-->\n' +
