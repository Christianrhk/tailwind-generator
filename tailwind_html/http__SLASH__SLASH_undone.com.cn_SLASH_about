    <!-- Pages Script Code -->
    <!-- <script src="http://maps.google.com/maps/api/js?key={my_key}&language=ja"></script>
<script src="/assets/undone/js/pages.js" type="text/javascript"></script> -->
    <script type="text/javascript">
$( function() {

  // 商品詳細ページ表示
  if ($('.event-view-item').length > 0) {
    gdt('track', 'VIEW_CONTENT');
  }

  // 商品一覧ページ表示
  if ($('.event-view-item-list').length > 0) {
    gdt('track', 'VIEW_CONTENT_LIST');
  }

  // カート追加(とりあえず商品区別なし)
  $('.event-add-cart').on('click', function(){
    //now only customize
    gdt('track', 'ADD_TO_CART', {
      content_ids: ['WatchCustomize'],
      content_type: 'product',
      value: 2508,
      currency: 'CNY'
    });
  });

  // カート削除
  $('.event-remove-cart').on('click', function(){
    gdt('track_custom', 'remove_from_cart');
  });

  // お届け先情報
  $('.event-add-shipping-info').on('input', function(){
    if ($(this).hasClass('event-add-shipping-info')) {
      gdt('track_custom', 'add_shipping_info');
    }
    $('.event-add-shipping-info').removeClass('event-add-shipping-info');
  });

  // 支払い方法選択
  $('.event-check-payment-method').on('input', function(){
    if ($(this).hasClass('event-check-payment-method')) {
      gdt('track_custom', 'check_payment_method');
    }
    // credit np both clear
    $('.event-check-payment-method').removeClass('event-check-payment-method');
  });

  // クレジットカード情報入力
  $('.event-add-payment-info').on('input', function(){
    if ($(this).hasClass('event-add-payment-info')) {
      gdt('track_custom', 'add_payment_info');
    }
    $(this).removeClass('event-add-payment-info');
  });

  // 個人情報同意
  $('.event-consent-cart-privacy').on('input', function(){
    if ($(this).hasClass('event-consent-cart-privacy')) {
      gdt('track_custom', 'consent_cart_privacy');
    }
    $(this).removeClass('event-consent-cart-privacy');
  });


  // チェックアウト開始
  $('.event-checkout-start').on('click', function(){
    if ($(this).hasClass('event-checkout-start')) {
      gdt('track', 'INITIATE_CHECKOUT');
    }
    $(this).removeClass('event-checkout-start');
  });

  // line@ボタン
  // $('.event-add-friend').on('click', function(){
  //   if ($(this).hasClass('event-add-friend')) {
  //     fbq('track', 'AddFriend');
  //     gtag('event', 'add_frinend');
  //   }
  //   $(this).removeClass('event-add-friend');
  // });

  // 続きを読む
  $('.event-item-read-continue').on('click', function(){
    if ($(this).hasClass('event-item-read-continue')) {
      gdt('track_custom', 'item_read_continue');
    }
    $(this).removeClass('event-item-read-continue');
  });
} );

</script>

