    <input  placeholder="" class="bx-def-font-inputs bx-form-input-hidden bx-form-input-location-lat" type="hidden" name="location_lat" value="" data-frmt-24h="on" data-frmt-date="F j, Y" data-frmt-datetime="F j, Y H:i" /><input  placeholder="" class="bx-def-font-inputs bx-form-input-hidden bx-form-input-location-lng" type="hidden" name="location_lng" value="" data-frmt-24h="on" data-frmt-date="F j, Y" data-frmt-datetime="F j, Y H:i" /><input  placeholder="Number" class="bx-def-font-inputs bx-form-input-text bx-form-input-location-street_number" type="text" name="location_street_number" value="" data-frmt-24h="on" data-frmt-date="F j, Y" data-frmt-datetime="F j, Y H:i" /><input  placeholder="Street" class="bx-def-font-inputs bx-form-input-text bx-form-input-location-street" type="text" name="location_street" value="" data-frmt-24h="on" data-frmt-date="F j, Y" data-frmt-datetime="F j, Y H:i" /><input  placeholder="City / District" class="bx-def-font-inputs bx-form-input-text bx-form-input-location-city" type="text" name="location_city" value="" data-frmt-24h="on" data-frmt-date="F j, Y" data-frmt-datetime="F j, Y H:i" /><input  placeholder="State / Province" class="bx-def-font-inputs bx-form-input-text bx-form-input-location-state" type="text" name="location_state" value="" data-frmt-24h="on" data-frmt-date="F j, Y" data-frmt-datetime="F j, Y H:i" /><input  placeholder="Postal Code" class="bx-def-font-inputs bx-form-input-text bx-form-input-location-zip" type="text" name="location_zip" value="" data-frmt-24h="on" data-frmt-date="F j, Y" data-frmt-datetime="F j, Y H:i" /><select  placeholder="" class="bx-def-font-inputs bx-form-input-select bx-form-input-location-country" name="location_country">
    
        
        
