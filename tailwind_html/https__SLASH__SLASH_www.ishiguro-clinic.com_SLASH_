<script>
$(function() {
    var pagetop = $('#gotop');
    $(window).scroll(function() {
        if ($(this).scrollTop() > 100) {
            pagetop.fadeIn();
        } else {
            pagetop.fadeOut();
        }
    });
    pagetop.click(function() {
        $('body, html').animate({
            scrollTop: 0
        }, 500);
        return false;
    });
    $('a[href^="#"]').click(function(){
      let speed = 500;
      let href= $(this).attr("href");
      let target = $(href == "#" || href == "" ? 'html' : href);
      let position = target.offset().top;
      $("html, body").animate({scrollTop:position}, speed, "swing");
      return false;
    });
});
</script>
<script>

  $('#topnews_btn button').on('click', function() {
    $('#topnews_btn button').removeClass('bg-green-500 text-white');
    $('#topnews_btn button').addClass('bg-green-100 text-green-500');
    
    $(this).removeClass('bg-green-100 text-green-500');
    $(this).addClass('bg-green-500 text-white');
    
    var cat = $(this).data('cat');
    
    if(cat != 0) {
      var cls = '.cat-' + cat;
      $('#topnews li').hide();
      $(cls).fadeIn();
    } else {
      $('#topnews li').fadeIn();
    }

  });
</script>
