  <script>
  (() => {
  document.getElementById("menuOpen").onclick = (e) => {
    document.getElementById("navMenu").classList.remove('tw-hidden');
    document.getElementsByTagName('body')[0].classList.add('no-scroll');
  };
  document.getElementById("menuClose").onclick = (e) => {
    document.getElementById("navMenu").classList.add('tw-hidden');
    document.getElementsByTagName('body')[0].classList.remove('no-scroll');
  };
})();
</script>
  
