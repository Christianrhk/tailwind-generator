                          	<div id="size-chart-destination" style="position: absolute; top: 15px; right: 10px;">
							</div>
                        </div>
						<style>
                          .no-js {
                            
                          }
                          .selector-wrapper {
                            	display:none;
                          }
                          .selector-wrapper {
                            	display:none;
                            
                          }
                          .js .product-single__variants {
                            	display:none;
                          }
                          .no-js div[data-option-index="0"] {
                            	display:none;
                          }
                          .no-js div[data-option-index="1"] {
                            	display:none;
                          }
                          .no-js div[data-option-index="2"] {
                            	display:none;
                          }
						</style>
                        
                        <script src="//cdn.shopify.com/shopifycloud/shopify/assets/themes_support/option_selection-fe6b72c2bbdd3369ac0bfefe8648e3c889efca213baefd4cfb0dd9363563831f.js" type="text/javascript"></script>
                        <script>
                        $( document ).ready(function() {
                        	function process_price_display($formObj) {
                        		try {
                                  	$formObj = typeof $formObj !== 'undefined' ? $formObj : null;
                                  	//alert($formObj.find('[name="id"]').attr('id'));
                                  	if ($formObj != null){
                                      	if ($formObj.find('[name="id"]').find('option:selected').length) {
                                          	
                                            var $selectedOptionObj = $formObj.find('[name="id"]').find('option:selected');
                                          //alert($selectedOptionObj.val());
                                            if ($selectedOptionObj.is('[data-compareatprice_money]')){
                                              	//alert($selectedOptionObj.attr('data-compareatprice_money'));
                                                $formObj.find('[js-compareatprice_wrap]').hide();
                                                if ($formObj.find('[js-compareatprice]').length){
                                                    $formObj.find('[js-compareatprice]').html($selectedOptionObj.attr('data-compareatprice_money'));
                                                  	$formObj.find('[js-compareatprice_wrap]').show();
                                                }
                                            }
                                          	if ($selectedOptionObj.is('[data-price_money]')){
                                              	//alert($selectedOptionObj.attr('data-price_money'));
                                                if ($formObj.find('[js-price]').length){
                                                    $formObj.find('[js-price]').html($selectedOptionObj.attr('data-price_money'));
                                                }
                                            }
                                        }
                                    }
                              	} catch (process_price_display) {
                                	console.log('ERROR: '+process_price_display.message);
                              	}
                          	}
                          
                          	var selectCallback = function(variant, selector) {
                                // BEGIN SWATCHES
                                if (variant) {
                                  try {
                                      var form = $('#' + selector.domIdPrefix).closest('form');
                                      var $priceObj = form.find('[data-price_wrap]');
                                      for (var i=0,length=variant.options.length; i<length; i++) {
                                        var radioButton = form.find('.swatch[data-option-index="' + i + '"] :radio[value="' + variant.options[i] +'"]');
                                        console.log('.swatch[data-option-index="' + i + '"] :radio[value="' + variant.options[i] +'"]');
                                        if (radioButton.size()) {
                                           console.log('-----------------radioButton--------------');
                                          console.log(radioButton);
                                          radioButton.get(0).checked = true;
                                            $('button[type="submit"]').prop("disabled", false);
                                            $('button[active-text]').html($('button[active-text]').attr('active-text'));
                                            $('.swatch').removeClass('tw-invisible');
                                            $('#productSelect').removeClass('tw-invisible');
                                          	process_price_display(form,$priceObj);
                                        }
                                      }

                                  } catch (selectCallbackerror) {
                                    console.log('ERROR: '+selectCallbackerror.message);
                                  }

                                }
