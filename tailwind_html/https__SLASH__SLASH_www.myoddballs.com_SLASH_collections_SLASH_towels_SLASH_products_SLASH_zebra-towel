						<div class="tw-justify-between tw-border-b tw-border-gray-300 ">

                            
                            <select name="id" id="productSelect" class="tw-invisible product-single__variants">
                              
                                

                                  
                                  <option  selected="selected"  data-price="1200" data-price_money="£12.00" data-compareatprice="1500" data-compareatprice_money="£15.00" data-sku="Zebra Towel - Regular" value="30113144963133">Regular - £12.00</option>

                                
                              
