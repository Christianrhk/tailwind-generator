<script text="text/javascript">
   var _learnq = _learnq || [];  
    
function set_details_and_birthday(fname,lname,email,day,month,year) {
      	//alert(day +'/'+month+'/'+year);
  
          _learnq.push(['identify', {
            '$email': email,
            '$first_name': fname,
            '$last_name': lname 
          }]);
      
        _learnq.push(['identify', {
          'Birthday' : day +'/'+month+'/'+year
    
		}]);
}
  
//set_birthday();
  
$( document ).ready(function() {
    
    $(document).on('submit', 'form[data-cf-form="0ktYLd"]', function(e){ 
  		var day = $('form[data-cf-form="0ktYLd"] .cf-select-wrap [placeholder="Day"]').val();
      	var month = $('form[data-cf-form="0ktYLd"] .cf-select-wrap [placeholder="Month"]').val();
      	var year = $('form[data-cf-form="0ktYLd"] .cf-select-wrap [placeholder="Year"]').val();
  		var email = $('form[data-cf-form="0ktYLd"] [type="email"]').val();
      	var fname = $('form[data-cf-form="0ktYLd"] [type="first_name"]').val();
      	var lname = $('form[data-cf-form="0ktYLd"] [type="last_name"]').val();
       	set_details_and_birthday(fname,lname,email,day,month,year);
    });
                      
});
</script>
          
          
		</div>
				</div>
			</div> <!-- cd-popup-container -->
		</div> <!-- cd-popup -->
