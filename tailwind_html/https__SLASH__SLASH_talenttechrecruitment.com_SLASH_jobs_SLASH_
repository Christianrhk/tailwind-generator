    jQuery(document).ready(function() {
      if (jQuery("#videoplay").length) {
        document.getElementById('videoplay').play();
        jQuery('.slider').on('afterChange', function(event, slick,
          currentSlide) {
          var vid = $(slick.$slides[currentSlide]).find('video');
          if (vid.length > 0) {
            $(vid).get(0).play();
          }
        });
      }

      jQuery(".slider").slick({
        dots: false,
        slide: '.slide',
        arrows: false,
        autoplay: true,
        draggable: true,
        autoplaySpeed: 8000,
        speed: 1000,
        pauseOnHover: false,
      });

      jQuery('#next').click(function(e) {
        e.preventDefault();
        jQuery(".slider").slick('slickPrev');
      });

      jQuery('#prev').click(function(e) {
        e.preventDefault();
        jQuery(".slider").slick('slickNext');
      });
    });
