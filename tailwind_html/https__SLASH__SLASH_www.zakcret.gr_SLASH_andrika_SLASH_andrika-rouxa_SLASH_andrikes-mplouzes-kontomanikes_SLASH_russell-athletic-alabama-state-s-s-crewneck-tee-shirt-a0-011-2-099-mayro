<link href="//zakcretcdn.r.worldssl.net/catalog/view/theme/bootstrap4_basic/stylesheet/vue/cart.css?v=7" rel="stylesheet" type="text/css" />
<div class="modal fade in" id="addtocartmodal" tabindex="-1" role="dialog" aria-hidden="true" data-backdrop="true" >
  <div class="modal-dialog" role="document">
    <div class="modal-content container-fluid"></div>
  </div>
</div>
<script>
$(document).ready(function() {
	//transfer el to body
	$('#addtocartmodal').appendTo("body");
});

//modalcartpopup function
function modalcartpopup( $product_id = false ){
	
	var pageWidth = $(window).width();
	//hide popup on mobile
	if (pageWidth >= 728) {
		$.ajax({
			url: 'index.php?route=vue/cart/infomodalcart&product_id='+$product_id,
			type: 'get',
			success: function(data) {
				//console.log(data);
				$('#addtocartmodal .modal-content').html(data);
				$('#addtocartmodal').modal('show');
			},
			error: function(xhr, ajaxOptions, thrownError) {
				alert(thrownError + "\r\n" + xhr.statusText + "\r\n" + xhr.responseText);
			}
		});
	}
	
}
</script>
<script>
$(document).ready(function(){
	
   	var userScreen = window.screen.width;
	if(userScreen <= 768){
		//mobile
	}else{
		//not mobile
		var mySwiper = new Swiper('.productthumbs',{
			onSlideChangeStart: function(swiper){
			   //setSwiperHeight();
			},
			lazyloading:true,
			direction: "vertical",
            observer: true,
            loop: false,
			slidesPerView: 5,
			spaceBetween: 65,
			nextButton: '.productthumbs .swiper-button-next',
			prevButton: '.productthumbs .swiper-button-prev',
		});
		
		$(document).delegate('.products-related .product-thumb img.img-fluid', 'mouseover', function(e) {
			var img = $(this);
			var img_hover = img.attr('data-image-hover');
			var img_loaded = img.attr('data-image-loaded');

			if ( typeof img_hover !== "undefined" ){
				if ( typeof img_loaded === "undefined" ){ //not set yet 1t time
					$.ajax({
						url:img_hover,
						type:'HEAD',
						error:
							function(){
								//concsole.log('img loading error');
							},
						success:
						function(){
							img.attr( "data-image-loaded", "loaded" );
							img.attr( "src", img_hover );
						}
					});
				}else if( img_loaded === "loaded"){ //now is loaded
					img.attr( "src", img_hover );
				}
			}
		});
		$(document).delegate('.products-related .product-thumb img.img-fluid', 'mouseout', function(e) {
			var img_src =  $(this).attr('data-image-main');
			if (typeof img_src !== "undefined" && this.src !== img_src ){
				this.src= img_src;
			}
		});
	}
	
	 $('.productcolors').swiper({
		lazyloading:true,
		slidesPerView: 4,
		spaceBetween: 5,
		pagination: '.swiper-pagination-colors',
		paginationClickable: true,
		paginationType: 'progressbar',
		nextButton: '.swiper-button-next',
		prevButton: '.swiper-button-prev',
	});

	$('.products-related').swiper({
		mode: 'horizontal',
		slidesPerView: 4,
		spaceBetween: 2,
		breakpoints: {
			1024: {
				slidesPerView: 4,
			},
			768: {
			  slidesPerView: 3,
			},
			640: {
			  slidesPerView: 2,
			},
			380: {
			  slidesPerView: 1,
			}
		  },
		nextButton: '.products-related .swiper-button-next',
		prevButton: '.products-related .swiper-button-prev',
	});
	
	//popover available sizes
	$('.product-thumb').mouseout(function () {
		$('.popover').popover("hide");
	});
	
	$('*[data-url]').click(function() {
		var e = $(this);
		e.off('click');
		
		// show this LOADING popover
		$.get(e.data('url'), function(d) {
			e.popover({
				content: d,
				html: true,
				delay: { "show": 500, "hide": 100 },
				placement: "top",
				container: 'body',
				trigger: 'click'
			}).popover('show');
		});
	});
});

$('select[name=\'recurring_id\'], input[name="quantity"]').change(function(){
	$.ajax({
		url: 'index.php?route=product/product/getRecurringDescription',
		type: 'post',
		data: $('input[name=\'product_id\'], input[name=\'quantity\'], select[name=\'recurring_id\']'),
		dataType: 'json',
		beforeSend: function() {
			$('#recurring-description').html('');
		},
		success: function(json) {
			$('.alert-dismissible, .text-danger').remove();
			if (json['success']) {
				$('#recurring-description').html(json['success']);
			}
		}
	});
});

$('#button-cart').on('click', function() {
	//check for multiple options
	
	$.ajax({
		url: 'index.php?route=checkout/cart/add',
		type: 'post',
		data: $('#product input[type=\'text\'], #product input[type=\'hidden\'], #product input[type=\'radio\']:checked, #product input[type=\'checkbox\']:checked, #product select, #product textarea'),
		dataType: 'json',
		beforeSend: function() {
			$('#button-cart').button('loading');
		},
		complete: function() {
			$('#button-cart').button('reset');
		},
		success: function(json) {
			$('.alert-dismissible, .text-danger').remove();
			$('.form-group').removeClass('has-error');
			if (json['error']) {
				if (json['error']['option']) {
					for (i in json['error']['option']) {
						var element = $('#input-option' + i.replace('_', '-'));

						if (element.parent().hasClass('input-group')) {
							element.parent().after('<div class="text-danger">' + json['error']['option'][i] + '</div>');
						} else {
							element.after('<div class="text-danger">' + json['error']['option'][i] + '</div>');
						}
					}
				}
				if (json['error']['recurring']) {
					$('select[name=\'recurring_id\']').after('<div class="text-danger">' + json['error']['recurring'] + '</div>');
				}
				if (json['error']['voucher']) {
					$('#button-cart').before('<div class="text-danger">' + json['error']['voucher'] + '</div>');
				}
				// Highlight any found errors
				$('.text-danger').parent().addClass('has-error');
			}

			if (json['success']) {
				
			$.ajax({
					url: 'index.php?route=extension/feed/googletags/AddToCart',
					type: 'post',
					data: $('#product input[type=\'radio\']:checked,#product input[type=\'text\'], #product input[type=\'hidden\'], #product .selectsize'),
					dataType: 'json',
					success: function(json) {
						
						if (json['success']) {
							var price= parseFloat(json['AddToCart']['price']).toFixed(2) ;
							if (json['AddToCart']['special'] != null)
							{
								price = parseFloat(json['AddToCart']['special']).toFixed(2) ;
							}
							console.log('googletags AddToCart '+ json['AddToCart']['item_id'],price);
							dataLayer.push({
									'event': 'add_to_cart',
									'value': ''+price+'',
									'items' : [{
											'id': ''+json['AddToCart']['item_id']+'',
											'google_business_vertical': 'retail'
											}]
							});
							
							
						}else{
							console.log('googletag not track');
						}
					},
					error: function(xhr, ajaxOptions, thrownError) {
						alert(thrownError +  xhr.statusText + xhr.responseText);
					}
				});$('.breadcrumb').after('<div class="alert alert-success alert-dismissible product-message">' + json['success'] + '<button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">×</span></button>');
				$('.breadcrumb').after('<div class="callout d-md-none"><div class="callout-header">'+ json['text_success_mobile']+'</div><span class="closebtn" onclick="this.parentElement.style.display=\'none\';">x</span><div class="callout-container"><a href="index.php?route=vue/cart">'+ json['text_show_checkout']+'</a></div></div>');
				$('#cart .dropdown > button').html('<span id="cart-total"><img src="image/catalog/svg2022/zakcret-basket.svg" loading="lazy" title="cart" alt="cart" class="img-svg" />' + json['total_products'] + '</span>');
				$('.cart-products-count').html('<span class="cart-total-for-mobile rounded-circle">' + json['total_products'] + '</span>');
				//show modal popup
									modalcartpopup( 118157 );
					$('html, body').animate({ scrollTop: 0 }, 'slow');
					$('#cart .dropdown > ul').load('index.php?route=common/cart/info ul li');
								$('#cart.btn-group.btn-block').addClass('open'); /*facebook tracking loading and glammy*/
			}
		},
        error: function(xhr, ajaxOptions, thrownError) {
            alert(thrownError + "\r\n" + xhr.statusText + "\r\n" + xhr.responseText);
        }
	});
});

$('#review').delegate('.pagination a', 'click', function(e) {
    e.preventDefault();
    $('#review').fadeOut('slow');
    $('#review').load(this.href);
    $('#review').fadeIn('slow');
});
$('#review').load('index.php?route=product/product/review&product_id=118157');
$('#button-review').on('click', function() {
	$.ajax({
		url: 'index.php?route=product/product/write&product_id=118157',
		type: 'post',
		dataType: 'json',
		data: $("#form-review").serialize(),
		beforeSend: function() {
			$('#button-review').button('loading');
		},
		complete: function() {
			$('#button-review').button('reset');
		},
		success: function(json) {
			$('.alert-dismissible').remove();
			if (json['error']) {
				$('#review').after('<div class="alert alert-danger alert-dismissible rounded-0"><i class="fa fa-exclamation-circle"></i> ' + json['error'] + '</div>');
			}
			if (json['success']) {
				$('#review').after('<div class="alert alert-success alert-dismissible rounded-0"><i class="fa fa-check-circle"></i> ' + json['success'] + '</div>');
				$('input[name=\'name\']').val('');
				$('textarea[name=\'text\']').val('');
				$('input[name=\'rating\']:checked').prop('checked', false);
			}
		}
	});
});

$(document).ready(function() {
	
	//pause video after popup close
	$(document).delegate('.mfp-close', 'click', function(e) {
		$('video').each(function(){
			$(this).get(0).pause();
		});
	});

	$('.thumbnails').magnificPopup({
		type:'image',
		delegate: 'a',
		gallery: {
			enabled: true,
			navigateByImgClick: true,
			preload: [0,1] // Will preload 0 - before current, and 1 after the current image
		},
		callbacks: {
			elementParse: function(item) {
				console.log(item.el[0].className);
				if(item.el[0].className == 'video d-block') {
					console.log('edw', item.el[0].hash); //#html5-video
					var typem='iframe';
					if (item.el[0].hash == '#html5-video'){
						typem='inline';
					}
						item.type = typem,
						item.iframe = {
							patterns: {
								youtube: {
								index: 'youtube.com/', // String that detects type of video (in this case YouTube). Simply via url.indexOf(index).
								id: 'v=', // String that splits URL in a two parts, second part should be %id%
								src: '//www.youtube.com/embed/%id%?autoplay=1' // URL that will be set as a source for iframe. 
								},
								vimeo: {
								index: 'vimeo.com/',
								id: '/',
								src: '//player.vimeo.com/video/%id%?autoplay=1'
								},
								gmaps: {
								index: '//maps.google.',
								src: '%id%&output=embed'
								}
							}
						}
				} else {
					item.type = 'image',
					item.tLoading = 'Loading image #%curr%...',
					item.mainClass = 'mfp-img-mobile',
					item.image = {
					tError: '<a href="%url%">The image #%curr%</a> could not be loaded.'
					}
				}
			}
		}
	});
});
</script>
