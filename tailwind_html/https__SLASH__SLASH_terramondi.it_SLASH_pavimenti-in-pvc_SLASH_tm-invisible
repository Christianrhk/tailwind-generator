if (!getCookie('exitOverlay')) {
let popup = document.getElementById('popup');

function openPopUp() {
  popup.style.display = "block";
  document.body.classList.toggle('overflow-hidden');
  dataLayer.push({'event': 'Exit Intent'});
}

function closePopUp() {
  popup.style.display = "none";
  document.body.classList.toggle('overflow-hidden');
  setCookie('exitOverlay', true, 1)
}

if (mobileAndTabletCheck()) {
  var scrollDownObserver = new DialogTrigger(completeUp, {
    trigger: 'scrollDown',
    percentDown: 40,
    scrollInterval: 1000
  });

  function completeDown() {
    var scrollUpObserver = new DialogTrigger(completeUp, {
      trigger: 'scrollUp',
      percentUp: 25,
      scrollInterval: 1000
    });
  }

  function completeUp() {
    openPopUp()
  }

} else {
  function onMouseOut(event) {
    if (event.clientY < 50 && event.relatedTarget == null && event.target.nodeName.toLowerCase() !== "select") {
      // Remove this event listener
      document.removeEventListener("mouseout", onMouseOut);
      // Show the popup
      openPopUp()
    }
  }
  document.addEventListener("mouseout", onMouseOut);
}

popup.addEventListener("click", function(e) {
  closePopUp();
});

document.querySelector('.exit-button').addEventListener("click", function(e) {
  closePopUp();
});

document.querySelector('.popup__container').addEventListener('click', e => e.stopPropagation());
}

function DialogTrigger(callback, options) {
  // Becomes this.options
  this.complete = false; // Let's us know if the popup has been triggered

  this.callback = callback;
  this.timer = null;
  this.interval = null;

  this.options = options;

  this.init = function() {
    if (this.options.trigger == 'scrollDown') {
      // Let the user scroll down by some significant amount
      var scrollStart = window.pageYOffset;
      var pageHeight = window.screen.height;

      var parentThis = this;

      // Only check the scroll position every few seconds, to avoid bogging the UI
      this.interval = setInterval(function() {
        var scrollAmount = window.pageYOffset - scrollStart;
        if (scrollAmount < 0) {
          scrollAmount = 0;
          scrollStart = window.pageYOffset;
        }
        var downScrollPercent = parseFloat(scrollAmount) / parseFloat(pageHeight);

        if (downScrollPercent > parseFloat(parentThis.options.percentDown) / 100) {
          clearInterval(parentThis.interval);
          parentThis.interval = null;
          if (!parentThis.complete) {
            parentThis.callback();
            parentThis.complete = true;
          }
        }

      }, this.options.scrollInterval);

    } else if (this.options.trigger == 'scrollUp') {
      // Let the user scroll down by some significant amount
      var scrollStart = window.pageYOffset;
      var pageHeight = window.pageYOffset;

      var parentThis = this;

      if (pageHeight > 0) {
        // Only check the scroll position every few seconds, to avoid bogging the UI
        this.interval = setInterval(function() {
          var scrollAmount = scrollStart - window.pageYOffset;
          if (scrollAmount < 0) {
            scrollAmount = 0;
            scrollStart = window.pageYOffset;
          }
          var upScrollPercent = parseFloat(scrollAmount) / parseFloat(pageHeight);

          if (upScrollPercent > parseFloat(parentThis.options.percentUp) / 100) {
            clearInterval(parentThis.interval);
            parentThis.interval = null;

            if (!parentThis.complete) {
              parentThis.callback();
              parentThis.complete = true;
            }
          }

        }, this.options.scrollInterval);
      }

    } else if (this.options.trigger == 'timeout') {
      this.timer = setTimeout(this.callback, this.options.timeout);
    }

  };
  this.init();
}

function setCookie(name, value, days) {
  var expires = "";
  if (days) {
    var date = new Date();
    date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
    expires = "; expires=" + date.toUTCString();
  }
  document.cookie = name + "=" + (
    value || ""
  ) + expires + "; path=/";
}

function getCookie(name) {
  var nameEQ = name + "=";
  var ca = document.cookie.split(';');
  for (var i = 0; i < ca.length; i++) {
    var c = ca[i];
    while (c.charAt(0) == ' ')
      c = c.substring(1, c.length);
    if (c.indexOf(nameEQ) == 0)
      return c.substring(nameEQ.length, c.length);
    }
  return null;
}
