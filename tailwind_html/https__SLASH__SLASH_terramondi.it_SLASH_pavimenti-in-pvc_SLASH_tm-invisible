// Add to Cart
$('form.add-to-cart-form').submit(function(e) {
  e.preventDefault();
  var purchasable = $(this).find('#purchasableId').val();
  if (purchasable == null) {
    var purchasable = 1011;
  }

  var quantityItem = $('#qty-input').val();
  $.ajax({
    type: "POST",
    dataType: 'json',
    headers: {
      "X-CSRF-Token": 'ykKmdPoQgI4nfYH9F1crg3vblcHSXwND_KB-XenEoc108S7hDW_k4ad10CPJKezrY0vSyEhuYfERmd_5nT5vL7uQOQijotSqQLQYr2A3iKA=',
    },
    url: '',
    data: {
      'action': 'commerce/cart/update-cart',
      'purchasableId': purchasable,
      'qty': quantityItem,
      'note': 'from ajax'
    },
    success: function(data) {
      if (data.success) {
        console.log('Sucess added to Cart');
        window.location.href = document.location.origin + '/cart/added' + '?id=' + purchasable + '&qm=' + quantityItem;
        //Sendet Event für Cart Update
         $.event.trigger({
          type: "addedtoCart",
        });
      }
    }
  });
});
