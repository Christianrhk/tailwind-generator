    })

    $(document).on('click', '.reply-comment-cancel', function () {
        action = $(this).data("action");
        comment = $(this).data("comment");
        resetCommentReply(action, comment)
    })
    $(document).on('click', '.btn-reply-comment-image-remove', function () {
        action = $(this).data("action");
        comment = $(this).data("comment");
