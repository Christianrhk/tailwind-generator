    window.addEventListener('alert', event => {
        let time = 3000

        $.notify({
            // options
            message: event.detail.message ?? 'Thông báo',
        }, {
            // settings
            element: 'body',
            position: null,
            allow_dismiss: true,
            newest_on_top: false,
            showProgressbar: false,
            placement: {
                from: "top",
                align: "right"
            },
            offset: 20,
            spacing: 10,
            z_index: 1031,
            type: event.detail.type,
            timer: time,
            delay: time,
            animate: {
                enter: 'animated fadeInDown',
                exit: 'animated fadeOutUp'
            },
            icon_type: 'class',
        });
    });
