         CurrentDay = Now.getDay(),
         OpeningTime = new Date(Now.getFullYear(), Now.getMonth(), Now.getDate(), 12, 00),
         ClosingTime = new Date(Now.getFullYear(), Now.getMonth(), Now.getDate(), 20, 00),
         Open = (Now.getTime() > OpeningTime.getTime() && Now.getTime() < ClosingTime.getTime());
         
         if (CurrentDay !== 7 && CurrentDay !== 7 && Open) {
