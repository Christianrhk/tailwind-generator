      }" class="relative w-full rounded bg-whitesmoke px-[16px] py-[12px] text-[14px] font-500 leading-[24px] tracking-[0.1px] text-title placeholder:text-title focus:outline-none hover:cursor-pointer col-span-2"><div x-on:click="formDataModel.selectedPlan.isActive = !formDataModel.selectedPlan.isActive" class="flex items-center justify-between"><span x-text="formDataModel.selectedPlan.placeholder || 'Preisplan'"></span> <img src="/svg/arrow-down-black.svg" alt="arrow down"></div> <ul x-cloak x-on:click.outside="formDataModel.selectedPlan.isActive = false" x-show="formDataModel.selectedPlan.isActive" class="absolute z-[2] left-[0px] top-[95%] w-full rounded bg-whitesmoke"><li x-bind:class="{ 'bg-light-gray text-whitesmoke': formDataModel.selectedPlan.value === 'Essential' }" x-on:click="setOption(formDataModel.selectedPlan, formDataModel.selectedPlan.options[0], 'Essential'), formDataModel.selectedPlan.isActive = false" class="px-[16px] py-[8px] hover:bg-light-gray hover:text-whitesmoke">
