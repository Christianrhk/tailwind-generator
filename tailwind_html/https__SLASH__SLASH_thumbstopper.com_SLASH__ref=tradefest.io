const stripeTokenHandler = (token, gaPay = null) => {
   // Insert the token ID into the form so it gets submitted to the server
   const form = document.getElementById('contactform');
   const hiddenInput = document.createElement('input');
   hiddenInput.setAttribute('type', 'hidden');
   hiddenInput.setAttribute('name', 'stripeToken');
   hiddenInput.setAttribute('value', token.id);
   form.appendChild(hiddenInput);

   if(gaPay) {
      jQuery("#first_name").val(gaPay.first_name);
      jQuery("#last_name").val(gaPay.last_name);
      jQuery("#email").val(gaPay.email);
      jQuery("#phone_number").val(gaPay.phone_number);
   }

   let form_data = {};

   jQuery("#contactform").serializeArray().map((data, index) => form_data[data.name] = data.value);

   form_data["classifiers"] = [];

   verticals.map(vertical => {
      if (vertical.selected) {
         form_data["classifiers"].push(vertical);
      }
   });

   // https://thatsus.atlassian.net/browse/TSD-859
   form_data["drill_down_classifiers"] = true;

   form_data["reseller_id"] = selected_vertical;
   form_data["monthlypriceInputField"] = handlePriceUpdate().price * handlePriceUpdate().value;
   form_data["package_name"] = handlePriceUpdate().package_name;
   form_data["description"] = `Best for Retailers with ${handlePriceUpdate().description}`;
   form_data["frequency"] = handlePriceUpdate().frequency;
   form_data["diff_in_months"] = handlePriceUpdate().value;
   form_data["is_hubspot"] = 0;
   form_data["is_trial"] = 0;
   form_data["service_ids"] = [9, 17];

   if(new RegExp('trial', 'g').test(window.location.href)) {
      form_data["is_trial"] = 1;
   }

  //  console.log(form_data);

   jQuery.ajax({
         type: 'POST',
         //url: 'https://app.thumbstopper.com/register', //Prod API URL
		 //url: "https://main_alt.rstanfield.laradev.thumbstopper.com/register", //Dev API URL
	   	 url: (window.location.hostname == 'thumbstopper.com') ? 'https://app.thumbstopper.com/register' : 'https://main_alt.rstanfield.laradev.thumbstopper.com/register',
         processData: false,
         data: JSON.stringify(form_data),
         contentType: 'application/json',
         success: function (result, status, xhr) {
         if (result.success === false) {
               jQuery('.ldsfjkljdsf').html(result.message);
               jQuery('.declinedmessage').show();
               let a = document.createElement('a');
               a.href = '#showdeclinedbox';
               document.body.appendChild(a);
               a.click();
               document.body.removeChild(a);
               jQuery('.PayNowbutton').html('Pay Now');
         } else {
               jQuery('.declinedmessage').hide();

               let hubspot = document.getElementById('contacthubspot');
               let $inputs = jQuery(form).find('input');

               $inputs.each(function(i, elem) {
                  let isStripeElement = Boolean(jQuery(this).closest('.ElementsApp').length);

                  if (!isStripeElement && elem.name !== 'stripeToken') {
                        let input = document.createElement('input');

                        input.name  = elem.name;
                        input.type  = elem.type;
                        input.value = elem.value;

                        hubspot.appendChild(input);
                  }
               });
               jQuery(hubspot).find('input[type="submit"]').trigger('click');
         }
      },
      error: function (xhr, status, error) {
         jQuery('.ldsfjkljdsf').html(JSON.stringify(error));
         jQuery('.declinedmessage').show();
         let a = document.createElement('a');
         a.href = '#showdeclinedbox';
         document.body.appendChild(a);
         a.click();
         document.body.removeChild(a);
      },
      complete: function(xhr, status) {
         jQuery('.PayNowbutton').removeClass('disabled');
         jQuery('.PayNowbutton').html('PAY NOW');
      }
   });
}
</script>
