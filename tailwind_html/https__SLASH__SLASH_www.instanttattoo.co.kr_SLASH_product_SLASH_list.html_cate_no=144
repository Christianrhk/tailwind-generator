<script>
    function ajaxPost(url, data, callback){
        let xhttp;
        if(window.XMLHttpRequest){
            xhttp = new XMLHttpRequest();
        }else{
            // IE5, IE6 ÏùºÎïå
            xhttp = new ActiveXObject("Microsoft.XMLHTTP");
        }
        xhttp.open('POST', url, true);
        xhttp.setRequestHeader('Content-Type', 'application/json');
        xhttp.send(JSON.stringify(data ));  
        xhttp.onreadystatechange = function(){  
          if(xhttp.readyState == 4){
            if(xhttp.status == 200){ 
              let response = JSON.parse(xhttp.responseText) 
              callback(response)
            }
            else{  
            }
          } 
        } 
    }
    
    ajaxPost('/exec/front/Product/SubCategory', '', function(categories){
        let parent_cate_set = new Set() 
        categories.map(cate=> parent_cate_set.add(cate.parent_cate_no))
        
        for(pno of parent_cate_set){  
            if(pno !=1){
                
                let pcate = categories.find(el=>el.cate_no == pno) 
                let parent = document.querySelector(`#menu_category [cate_no="${pno}"]`) 
                 
                parent.innerHTML = `<span href="#" class="flex items-center toggler" data-effect="toggle" for="collapse-${pno}">
                        <a class="flex items-center cate-title"
                            ${pno!=120? `href="/product/list.html${pcate.param}"` : '' }  
                        >${pcate.name}</a>
                    </span>
                   <ul class="toggle" id="collapse-${pno}"  style="margin-left:10px;"></ul>`
                
                 
                let found = categories.filter(el=>el.parent_cate_no == pno)
                let tags = ''
                for(child of found){   
                    
                    let href = `/product/list.html${child.param}`
                    if(child.cate_no == 123){
                        href = '/product/collection/2020spring.html'+child.param
                    }
                    if(child.cate_no == 151){
                        href = '/product/collection/2020summer.html'+child.param
                    }
                    if(child.cate_no == 145){
                        href = '/product/collection/2020postcard.html'+child.param
                    }
                    if(child.cate_no == 165){
                        href = '/product/collection/dreamforever.html'+child.param
                    }
 
                    tags += ` <li class="pt-3 flex items-center"><div class="square-circle"></div> 
                        <a href="${href}" class="flex items-center cate-title">${child.name}</a>
                    </li>`
                }
                document.getElementById(`collapse-${pno}`).innerHTML = tags
            }
        }  

        document.querySelectorAll('.cate-title').forEach(el=>{
            if(el.textContent.includes('[n]')){
                
                el.innerHTML = el.textContent.replace('[n]', '') + '<img src="/images/common/mark_n.jpg" class="square-new"></img>'
            }
        })
    }) 
</script>

