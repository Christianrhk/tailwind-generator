    const animation = (target) => {
        // 탭영역 플로팅 설정
        const io = new IntersectionObserver((entries, observer) => {
            entries.forEach(entry => {   
                if (entry.isIntersecting) {  
                    entry.target.classList.add('scroll-fade');
                    observer.disconnect();
                }
            })
        })
        io.observe(target);
    };
    // 관찰할 대상을 선언하고, 해당 속성을 관찰시킨다. 
    const elements = document.querySelectorAll('.scroll-animation')
    elements.forEach(animation)


    
    var setSwiper = new Swiper('#set-swiper', { 
        lazy:true,
        slidesPerView: 1.3,
        spaceBetween: 10,
        watchSlidesVisibility: true, 
        grabCursor: true, 
        mousewheel: true,
        loop: true, 
        observer: true,
        observeParents: true,
        breakpoints:{
            480:{
                slidesPerView : 2.3
            },
            640:{
                slidesPerView : 2.3
            }
        }
    });

    var dealSwiper = new Swiper('#deal-swiper', { 
        lazy:true,
        slidesPerView: 1.3,
        spaceBetween: 10,
        watchSlidesVisibility: true, 
        grabCursor: true, 
        mousewheel: true,
        loop: true, 
