    let isGlobal = false;
    if(window.location.hostname == "instant-tattoo.com"){ 
        isGlobal = true;
    } 
    if(window.location.hostname == "www.instant-tattoo.com"){ 
        isGlobal = true;
    } 

    var xhttp;
    if(window.XMLHttpRequest){
        xhttp = new XMLHttpRequest();
    }else{
        // IE5, IE6 일때
        xhttp = new ActiveXObject("Microsoft.XMLHTTP");
    }
    if(isGlobal){ 
        xhttp.onreadystatechange = function(){
             
            if(xhttp.readyState == 4 && xhttp.status == 200){
                let response = JSON.parse(xhttp.responseText) 
                console.log(isGlobal, response)
                if(response.country_code == "KR"){
                    location.href= 'https://instanttattoo.co.kr'
                }
                else if(response.country_code == "TW"){
                    location.href= 'https://www.instanttattoo.co/'
                }
                else if(response.country_code == "JP"){
                    location.href= 'https://instanttattoo.store'
                }
                else{
                    location.href= 'https://instant.tattoo';
                }
            }
        };
        xhttp.open('GET', 'https://api.ip.pe.kr/json/', true);
        xhttp.send();
    }
</script><!-- tailwindcss navbar https://dev.to/cryptic022/sticky-header-and-footer-with-tailwind-2oik  --><body>    
