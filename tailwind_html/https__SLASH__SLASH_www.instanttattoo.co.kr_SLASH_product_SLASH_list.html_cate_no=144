    var existsHash = new Set();
    // ÏÉÅÌíàÎ≥Ñ Ìï¥ÏãúÌÉúÍ∑∏ ÏÑ§Ï†ï Î∞è Ï†ïÎ†¨ Ï†êÏàò Ï†ÅÏö©ÌïòÍ∏∞
    document.querySelectorAll("[role='product']").forEach((el)=>{
        let hashtag = el.querySelectorAll('.hashtag span') 
        let product_name = el.querySelector('.description span').textContent
        let product_no = Number(el.getAttribute('product_no'));

        hashtag = hashtag[hashtag.length-1]
        if(hashtag){ 
            hashtag = hashtag.textContent.replace(/#/g, '').replace(/ /g,',').replace(/ÌÉÄÌà¨/g,'')
      
            el.setAttribute('data-tags', hashtag) 
            
            for(h of hashtag.split(',')){
                if(existsHash.has(h) == false){
                    existsHash.add(h)
                }
            }
        }


        // ÌíàÏ†à ÏÉÅÌíà Í¥ÄÏã¨ÏÉÅÌíà ÏïÑÏù¥ÏΩò ÏßÄÏö∞Í∏∞
        let issoldout = el.querySelector('.icon_img[alt="ÌíàÏ†à"]')
        if(issoldout){
            let wishicon = el.querySelector('.wish .icon_img')
            if(wishicon){
                wishicon.remove()
            }
        }

        // ÏÑ∏Ìä∏ Í≥®ÎùºÎã¥Í∏∞Îäî ÏàúÏÑúÏÉÅ Îß® Îí§Î°ú Î≥¥ÎÇ¥Í∏∞ÏúÑÌï¥ 
        if( product_name.includes('Í≥®ÎùºÎã¥Í∏∞') || product_name.includes('ÏÑ∏Ìä∏')){
            el.setAttribute('data-order', -500)  
        }
 

         // ÏÑ∏Ìä∏ Ìï¥ÏãúÌÉúÍ∑∏ ÌëúÏãú 
        let ohashnode = el.querySelector('.origin-hashtag')
        let hashnode = el.querySelector('.set-hashtag')
        if(ohashnode){
            for(hashtag of ohashnode.textContent.split(',')){

                hashtag = hashtag.replace(/ /g,'').replace('ÌÉÄÌà¨ÌÉÄÌà¨','ÌÉÄÌà¨')
                hashnode.appendChild( createElement(`#${hashtag}`, ['mr-1','mt-1','p-1','text-xs','text-grey','rounded', 'bg-gray'] ) ) 
            }
        } 

        setProductPrice(el.querySelector('.description > .spec'))
    }) 

 
    
    let objkeys = objicons.map(el=>el.obj) 
    let tagHash = ''
    for(hash of existsHash){
        if(objkeys.includes(hash)){
            tagHash+=`<li class="object-item-scroll uk-inline" status="off" onclick="onClickObject(this)">
                <div class="object-img object-str"> ${hash} </div> 
            </li>` 
        } 
    }
    document.querySelector('.object-list-scroll').innerHTML = tagHash

    
    docReady(function(){  
        //ÌÉ≠ ÏÑ§Ï†ï Ï†ÑÏ≤¥ÏôÄ ÏÑ∏ÏùºÏóêÏÑúÎß® ÌîÑÎ°úÎ™®ÏÖò ÌÉ≠ ÌôúÏÑ±Ìôî
        if( Number(cate_no) == 118 || Number(cate_no) == 139){  

            const imgHeight =  document.querySelector('#deal-swiper .swiper-slide').offsetWidth;  
            document.querySelectorAll('img.swiper-lazy').forEach((el)=>{
                el.style.height = (imgHeight * 0.58) + 'px'
            })  
        } 
        paramInit(); 
    })

    // Î™®Îì† Ïù¥ÎØ∏ÏßÄ ÌååÏùº ÏÑ†ÌÉù
    const images = document.querySelectorAll("img.image-lazy"); 
    const lazyLoad = (target) => {
        const io = new IntersectionObserver((entries, observer) => {
            entries.forEach((entry) => { 
                if (entry.isIntersecting) {
                    //console.log("reached!");
                    const img = entry.target; // Ïù¥ÎØ∏ÏßÄ ÏóòÎ¶¨Î©òÌä∏Î•º Í∞ÄÏ†∏ÏòµÎãàÎã§. 
      
                    img.src = img.getAttribute("data-src");
                    img.classList.add("lazy-loaded");
                    observer.disconnect();

                    const parent = img.parentElement.parentElement.parentElement
                    const el = parent.querySelector('.description > .spec')
                    //setProductPrice(el) 
                }
                else{
                    //console.log('ÌôîÎ©¥ÏóêÏÑú Ï†úÏô∏Îê®');
                }
            });
        });
        io.observe(target);
    }; 
    images.forEach(lazyLoad);

    
    //ÎîîÏä§Ïª§Î≤ÑÎ¶¨ Î∞ïÏä§ Í∞êÏãú
    const io = new IntersectionObserver((entries, observer) => {
        entries.forEach(entry => {
            // Í¥ÄÏ∞∞ ÎåÄÏÉÅÏù¥ viewport ÏïàÏóê Îì§Ïñ¥Ïò® Í≤ΩÏö∞ 'tada' ÌÅ¥ÎûòÏä§Î•º Ï∂îÍ∞Ä
            const node = document.getElementById('discovery_container')
            node.classList.remove('floated', 'sticky');
            if(entry.boundingClientRect.top < 200){
 
                if (entry.intersectionRatio > 0) {  
                    //console.log('visible')
                }
                // Í∑∏ Ïô∏Ïùò Í≤ΩÏö∞ 'tada' ÌÅ¥ÎûòÏä§ Ï†úÍ±∞
                else { 
                    node.classList.add('floated');
                    setTimeout(function(){
                        node.classList.add('sticky');
                    }, 50)
                    //console.log('hidden')
                }
            }
        })
    })
    
    // Í¥ÄÏ∞∞Ìï† ÎåÄÏÉÅÏùÑ ÏÑ†Ïñ∏ÌïòÍ≥†, Ìï¥Îãπ ÏÜçÏÑ±ÏùÑ Í¥ÄÏ∞∞ÏãúÌÇ®Îã§. 
    const boxElList = document.getElementById('discovery_container_checker')
    io.observe(boxElList); 

    


    //Ï†ïÎ†¨
    const sort_method = getUrlParameter('sort_method')
    const sort_node =  document.getElementById('sort')
    sort_node.options[sort_node.options.length] = new Option('Î™®Îì†Ï†úÌíà', `?cate_no=${cate_no}`);
    if(sort_method){
        let sortdic = {
            5 : 0, //Ïã†ÏÉÅ
            3 : 1, //ÎÇÆÏùÄÍ∞ÄÍ≤©
            4 : 2, //ÎÜíÏùÄÍ∞ÄÍ≤©
            6 : 3, //Ïù∏Í∏∞ÏÇ¨Ìíà
            7 : 4, //ÏÇ¨Ïö©ÌõÑÍ∏∞
            8 : 5, //Ï°∞ÌöåÏàò
        }
        sort_node.selectedIndex = sortdic[sort_method]
    }
    else{
        sort_node.selectedIndex = 6
    } 
    
    function onchangeSort(e){
        let selected = (e.options[e.selectedIndex])
        location.href =  window.location.origin + window.location.pathname + selected.getAttribute('value')
    }
    




    var search_object = getUrlParameter('object') 
    
    function paramInit(){     
        search_object = search_object && search_object.split(',')
          
        let flag = false
        if(search_object){ 
            for(tag of search_object){ 
                for(el of document.getElementsByClassName('object-item-scroll') ){
                    if(el.textContent.trim() == tag){
                        el.classList.add('object-active')
                        el.setAttribute('status','on')
                        
                        if(flag==false){
                            flag=true
                            const offsetLeft = el.offsetLeft 
                            const sw = screen.availWidth
                            const pw = document.querySelector('.object-list-scroll').scrollWidth
                            console.log( offsetLeft, sw, pw)
                            
                            if(offsetLeft > sw && offsetLeft < pw){  
                                console.log('scroll ')
                                $('.object-list-scroll').scrollLeft(offsetLeft - sw/4 )
                            }
                        } 
                    } 
                } 
            } 
            discovery();
        } 
    }
     

    // ÎîîÏä§Ïª§Î≤ÑÎ¶¨ Ïò§Î∏åÏ†ùÌä∏ ÌÅ¥Î¶≠
    function onClickObject(event){  
        const status = event.getAttribute('status') 
 
        if(status==='on'){
            event.classList.remove('object-active')  
            event.children[0].classList.remove('object-active') 
            event.setAttribute('status','off') 
    
        }else{
            event.classList.add('object-active') 
            event.children[0].classList.add('object-active')  
            event.setAttribute('status','on') 
        }  
        discovery();
    }

    // ÌÉêÏÉâ
    function discovery(){  
        const selected_object = []
        for(let el of document.getElementsByClassName('object-active') ){
            selected_object.push(el.textContent.trim() )
        }
 
        if(selected_object.length == 0){
            document.getElementById('sort').style.visibility = 'visible'
        }
        else{
            document.getElementById('sort').style.visibility = 'hidden' 
        }
            
 
        let count = 0
        let countword = 'Í≤ÄÏÉâ Í≤∞Í≥º'
        let nodes = document.querySelectorAll('ul.single-prd-list li[role="product"]');
        if(selected_object.length > 0){  
            nodes.forEach((el)=>{
                el.setAttribute('style','')
                
                if(el.dataset['tags']){ 
                    let tag_li = el.dataset['tags'].split(',')
    
                    let isin = false;
                    for(let tag of selected_object){
                        if(tag_li.includes(tag)){
                            isin = true;
                            break
                        }
                    }
    
                    if(isin==false){
                        el.setAttribute('style','display:none')
                    }
                    else{
                        count += 1;
                    }
                }
            })

 
            const sorted = $('ul.single-prd-list li[role="product"]').sort(function(a, b){
                return b.dataset['order'] - a.dataset['order']
                //ÎßåÏïΩÏóê Ïó≠ÏàúÏúºÎ°ú Ï†ïÎ†¨ÌïòÍ≥† Ïã∂ÏùÄ Í≤ΩÏö∞ Î∞òÎåÄÎ°ú returnÌïòÎ©¥ Îê©ÎãàÎã§. 
                //return $(a).data(dataNm) - $(b).data(dataNm);
            })
             
            $('ul.single-prd-list').html( sorted )
        }
        else{ 
            nodes.forEach((el)=>{
                el.setAttribute('style','')
                count +=1;
            });
    
            const sorted = $('ul.single-prd-list li[role="product"]').sort(function(a, b){
                //return instant_jquery(a).data('ordersort') - instant_jquery(b).data('ordersort'); 
                return b.dataset['order'] - a.dataset['order']
            }) 
            $('ul.single-prd-list').html( sorted )
 
            countword = 'Î™®Îì† Ï†úÌíà'
            if(Number(cate_no) == 139){
                countword = 'Îã®Ìíà Ìï†Ïù∏'
            }
        }
     
        document.getElementById('result_count').textContent = `(${count})`
        document.getElementById('result_count_text').textContent = countword 

    }
 

    function openTab(evt, tabName) {   
        let tabcontent = document.getElementsByClassName("tabcontent"); // Ïª®ÌÖêÏ∏†Î•º Î∂àÎü¨ÏòµÎãàÎã§. 
        for(let tab of tabcontent){ 
            tab.style.display = "none"; //Ïª®ÌÖêÏ∏†Î•º Î™®Îëê Ïà®ÍπÅÎãàÎã§. 
        } 
        let tablinks = document.getElementsByClassName("tablinks"); //ÌÉ≠ÏùÑ Î∂àÎü¨ÏòµÎãàÎã§.
        for(let tab of tablinks){
            tab.classList.remove("tab-active"); //ÌÉ≠ÏùÑ Ï¥àÍ∏∞ÌôîÏãúÌÇµÎãàÎã§. 
        }
        document.getElementById(tabName).style.display = "block"; //Ìï¥ÎãπÎêòÎäî Ïª®ÌÖêÏ∏†Îßå Î≥¥Ïó¨Ï§çÎãàÎã§. 
        evt.currentTarget.classList.add("tab-active"); //ÌÅ¥Î¶≠Ìïú ÌÉ≠ÏùÑ ÌôúÏÑ±ÌôîÏãúÌÇµÎãàÎã§. 
    }


</script> 
