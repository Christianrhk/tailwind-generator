<script type="text/javascript">
var preview_fontsize = 7;
var preview_zoom = 0.9
var preview_font = ''
var preview_fonttext = ''
var preview_lettering = 'Type Your Message' /*instant_jquery('.input-message').text();*/
var preview_linecount = 1;
var preview_lineheight = 100;
var preview_alignment_baseline_y = 0
var preview_align = 'center'
var preview_alignment_baseline = 'm'
var preview_fontweight = 400;
var preview_underline = '';
var preview_italic = '';
var preview_label = ' ' /*instant_jquery('.input-label').text();*/

let canvas_width = 90;
let canvas_height = 42.5;
let logoX = 5
let logoY = 5
let logoWidth = 20
let logoHeight = 6.3
let labelX = 30
let labelY = 5
let labelHeight = 6.3
let labelWidth = 55
let textX = 5
let textY = 12.5
let textWidth = 80
let textHeight = 25

let dpi = document.getElementById("dpi").offsetHeight
let inch = 25.4
let px = inch / dpi;

let preview_cursor_position = 0
let preview_cursor_position_direction = 0

function getRealWidth(width){
    let widthmm = width *  px;
    return widthmm
}

function mm2px(mm){
    return mm / inch * dpi
}
function setLetteringCanvas(){
    const widthmm = getRealWidth( window.screen.width)

    //console.log(widthmm, navigator.userAgent)
    //console.log(`ÎØ∏Î¶¨Î≥¥Í∏∞ ÌôîÎ©¥ÏùÄ Ïã§Ï†úÌÅ¨Í∏∞Ïùò ${preview_zoom * 100}% ÏûÖÎãàÎã§`)
    let node = document.querySelector('.preview-container')
    node.setAttribute('width', canvas_width * preview_zoom +'mm')
    node.setAttribute('height', canvas_height * preview_zoom +'mm')

    node = document.querySelector('.preview-container-boundary')
    node.setAttribute('width', canvas_width * preview_zoom +'mm')
    node.setAttribute('height', canvas_height * preview_zoom +'mm')

    node = document.querySelector('.preview-logo')
    node.setAttribute('width', logoWidth * preview_zoom +'mm')
    node.setAttribute('height',logoHeight * preview_zoom +'mm')
    node.setAttribute('x',logoX * preview_zoom +'mm')
    node.setAttribute('y',logoY * preview_zoom +'mm')

    node = document.querySelector('.preview-label-boundary')
    node.setAttribute('width', labelWidth * preview_zoom +'mm')
    node.setAttribute('height',  labelHeight * preview_zoom +'mm')
    node.setAttribute('x',  labelX * preview_zoom +'mm')
    node.setAttribute('y', labelY * preview_zoom +'mm')

    node = document.querySelector('.preview-text-boundary')
    node.setAttribute('width', textWidth * preview_zoom +'mm')
    node.setAttribute('height',  textHeight * preview_zoom +'mm')
    node.setAttribute('x',  textX * preview_zoom +'mm')
    node.setAttribute('y', textY * preview_zoom +'mm')

    document.querySelectorAll('.preview-text').forEach(node=>{
        node.setAttribute('font-size', preview_fontsize * preview_zoom + 'mm')
        node.setAttribute('font-family', preview_font)
    })

    node = document.querySelector('.preview-label')
    node.setAttribute('y', labelY * preview_zoom + 'mm')
    node.setAttribute('font-size', 3*  preview_zoom + 'mm')

    document.querySelector('.preview-label').textContent = preview_label

    applyCustomLettering();
    //checkerr()
}

function applyCustomLettering(){

    let splited = preview_lettering.split('\n').filter( (el) => el.length > 0 )
    preview_linecount = splited.length;

    let tmp_fontsize =  preview_fontsize * preview_lineheight / 100
    // ÏÑ∏Î°ú Ï†ïÎ†¨
    if(preview_alignment_baseline=='t'){
        preview_alignment_baseline_y = 1;
    }
    else if(preview_alignment_baseline=='m'){
        let tmp_startoffset = 1
        if(preview_linecount > 1){
            //ÎßàÏßÄÎßâ Ï§ÑÏùò Í≥µÎ∞± Í∞ÑÍ≤©ÏùÄ ÌïÑÏöîÏóÜÏùå
            tmp_startoffset =  (( preview_fontsize * preview_lineheight / 100 ) - preview_fontsize ) / 2
        }

        preview_alignment_baseline_y = ((textHeight/2) - (tmp_fontsize * (preview_linecount) / 2) )  + tmp_startoffset
        // console.log(tmp_fontsize, preview_linecount, tmp_startoffset, preview_alignment_baseline_y)
    }
    else if(preview_alignment_baseline == 'b'){
        preview_alignment_baseline_y = 23 - (tmp_fontsize * (preview_linecount -1))
    }


    preview_cursor_position
    textcumsum = 0
    let nodes = document.querySelectorAll('.preview-text')
    let tapadding_top = 0
    for(let idx=0; idx< nodes.length; idx++){
        node = nodes[idx]
        let ty = (textY + preview_alignment_baseline_y + tmp_fontsize * idx) * preview_zoom
        if(idx == 0){
            tapadding_top = ty
        }
        setSVGText(node, ty,  splited[idx])
    }

    nodes = document.querySelectorAll('#confirm-container .preview-text')
    for(let idx=0; idx< nodes.length; idx++){
        node = nodes[idx]
        let ty = (textY + preview_alignment_baseline_y + tmp_fontsize * idx) * preview_zoom
        if(idx == 0){
            tapadding_top = ty
        }
        setSVGText(node, ty,  splited[idx])
    }


    document.querySelector('.selected_lettering').innerHTML = ''
    for(let i=0; i< splited.length; i++){
        document.querySelector('.selected_lettering').innerHTML += `<div>${splited[i]}</div>`
    }

    document.querySelectorAll('.selected_fontsize').forEach(el=>el.textContent = preview_fontsize + 'mm')
    document.querySelectorAll('.selected_font').forEach(el=>el.textContent = preview_fonttext );
    document.querySelectorAll('.selected_lineheight').forEach(el=>el.textContent = preview_lineheight + '%');

    try{
        checkerr();
    }
    catch(error){
        console.log(error)
    }

    //setEditorBox();
}

function setSVGText(node, y, text){
    node.setAttribute('font-family', preview_font)
    node.setAttribute('font-size',  preview_fontsize * preview_zoom + 'mm')
    node.setAttribute('alignment-baseline',  'hanging')
    node.setAttribute('dominant-baseline','hanging')

    node.setAttribute('text-anchor','middle')
    node.setAttribute('x','50%')
    node.setAttribute('y',y + 'mm')
    node.textContent = text

    //node.setAttribute('font-style', preview_italic)
    //node.setAttribute('font-decoration', preview_underline)
    //node.setAttribute('font-weight', preview_fontweight)
}

function setEditorBox(){
    let editor = document.getElementById('diveditor')
    let editorbox = editor.getBoundingClientRect()
    let textnode = document.querySelector('.preview-text-boundary')
    let textbox = textnode.getBoundingClientRect()

    let h = 0;
    let w = 0;
    let base_h = 0
    let start_y = 0;
    let end_y = 0;
    let text_li = document.querySelectorAll('.preview-text');

    for(let i=0; i< text_li.length ; i++){
        subtext = text_li[i]
        bbox = subtext.getBoundingClientRect()
        //console.log(bbox)
        if(i==0){
            start_y = bbox.y;
        }
        if(bbox.height == 0){
            continue;
        }
        base_h = bbox.height;

        end_y = bbox.y
        if(w < bbox.width){
            w = bbox.width;
        }
    }
    h = end_y + base_h - start_y

    //console.log(h, start_y, end_y)
    editor.style.top = start_y + (h/2) - (editorbox.height/2)   + 'px'
}

</script>


