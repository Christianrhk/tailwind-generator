<script>
const urlParams = new URLSearchParams(window.location.search);

	
if(urlParams.get('glcid')){
	localStorage.setItem('lead-source', 'PPC Web – PPC Local RedTag');
}	
	
if(urlParams.get('utm_source')){

	if(urlParams.get('utm_source').includes('oogle') || urlParams.get('utm_source').includes('bing') ){
    	         localStorage.setItem('lead-source', 'PPC Web – PPC Local RedTag');
        } else {
		localStorage.setItem('lead-source', urlParams.get('utm_source'));
	}

} else {
	if(!localStorage.getItem('lead-source')){
	        localStorage.setItem('lead-source', 'Organic Web - Local SEO');
	}
}

if(localStorage.getItem('lead-source') == 'LOCALiQ'){
    localStorage.setItem('lead-source', 'Direct Email – LocalIQ Smart Solutions');
}


