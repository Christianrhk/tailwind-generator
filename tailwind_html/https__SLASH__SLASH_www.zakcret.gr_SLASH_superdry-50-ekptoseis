<script>
var close_info = $('.notif-info-wrapper').find('.close');
$(close_info).click(function(e) {
	var cookie_notif_info = $('.notif-info-wrapper').data('cookie');
	var hour_to_expire = 1000 * 60 * 60 * 4;//2 hours to epxire
	var notif_info_expires = new Date((new Date()).valueOf() + hour_to_expire);
	document.cookie = cookie_notif_info + "=0;expires=" + notif_info_expires.toUTCString()+'; path=/';
});
</script>
