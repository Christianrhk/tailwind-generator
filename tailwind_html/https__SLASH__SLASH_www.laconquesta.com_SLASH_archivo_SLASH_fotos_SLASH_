  <script>

    let swiper = new Swiper('#slider .swiper-container',{
        speed: 400,
        spaceBetween: 30,
        navigation: {
            prevEl: '.slider-arrow-prev',
            nextEl: '.slider-arrow-next',
            disabledClass: "hidden"
        },
        breakpoints: {
            920: {
                spaceBetween: 100
            },
        },
        observer:true,
        observeParents:true,
        observeSlideChildren:true
    });

    function cambiarFoto(pi){
        swiper.slideTo(pi);
    }

    let gmobile = new Swiper('#gmobile .swiper-container',{
        speed: 1500,
        spaceBetween: 30,
        navigation: {
            prevEl: '.gmobile-arrow-prev',
            nextEl: '.gmobile-arrow-next',
            disabledClass: "hidden"
        },
        observer:true,
        observeParents:true,
        observeSlideChildren:true
    });

