            bxSlider.reloadSlider();
        }
    });

    
     function reveal_phone(that) {
        $(that).addClass('disabled');
        $('.phone').removeClass('phone_hide');
        // increase phone view
        increasePhoneView();
     }

     $('.btn-report, .close-mark-as').click(function(event) {
         event.preventDefault();
         $('.btn-report').toggleClass('active');
         $('#dropdown-menu').toggle();
     });


         function increasePhoneView() {
         $.ajax({
            dataType: "json",
            url: "https://omomakay.com/index.php?page=ajax&action=runhook&hook=phone_reveal",
            data: {
                "id": '663',
                "CSRFName" : 'CSRF1302573438_774560281',
                "CSRFToken" : '667315cc43a7600360dd587d610e2b97eff9be1241de9e1aae4835ff78d765297860241b1399026e68979d503922fcfb0ac05ad1c298effa80fcd700a3438f28'
            },
            success: function(data){
                    if(data.error==0) {
                        console.log('NO error');
                    } else {
                        console.log('error :(');
                    }
                }
          });
     }
