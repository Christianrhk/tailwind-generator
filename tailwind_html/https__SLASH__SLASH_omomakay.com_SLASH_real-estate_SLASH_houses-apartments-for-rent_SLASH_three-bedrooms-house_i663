        e.preventDefault();
        $(this).prop('disabled', true);
        $('#mask_as_form').submit();
    });

    $('#itemTabs a').click(function (e) {
        e.preventDefault();
        $(this).tab('show');
        if($(this).attr('href')=="#map") {
