    <section id="bubbleModal" class="modal-bubble hidden fixed flex-col sm:items-center sm:justify-center z-50 py-6 sm:px-16 lg:px-6 w-full h-full max-h-screen bg-velux-white sm:bg-velux-grey-500 sm:bg-opacity-50 top-0 left-0">
        <div class="relative modal-bubble__wrapper px-4 sm:px-8 sm:pt-5 sm:pb-12 sm:bg-velux-white max-h-full flex flex-col w-auto">
            <div class="flex items-center justify-between sm:justify-end pb-4 sticky sm:static top-0 bg-velux-white">
                <div>
                    <a class="js-control-prev text-2xl text-velux-grey-400 hover:text-velux-grey-400 mr-8">
                        <span class="cursor-pointer">
