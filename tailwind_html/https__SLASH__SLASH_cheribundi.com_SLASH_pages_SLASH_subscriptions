<script> 
//   jQuery('.min-cart-wrap').mCustomScrollbar({
//         scrollButtons:{enable:false}, theme:"light-thick", scrollbarPosition:"outside", updateOnContentResize: true,
//     });
  
  function sucss(t) {

       
    var e = $(t),
        i = $("<div>").append(e),
        n = i.find(".count").text();
    console.log(i), $(".count").html(n), $(".cart-btn").html("<span class='fa fa-shopping-bag'></span><span>" + n + "</span>");
    var o = i.find(".side_cart").html();
  var cov = i.find(".g-covid-bar").html();
  $(".g-covid-bar").html(cov),
    $(".side_cart").html(o),
         $('.side_cart').addClass('active');
       $('.side-cart-close').click(function(){
    $('.side_cart').removeClass('active');
});
     $(function () {
	$('.triggerita').magnificPopup({
		type: 'inline'
		
	});
	
});
        
      jQuery('.min-cart-wrap').mCustomScrollbar({
        scrollButtons:{enable:false}, theme:"light-thick", scrollbarPosition:"outside", updateOnContentResize: true,
    });
   
   $('.c-info_top').matchHeight();
   $('.c-info_content').matchHeight();
  $('body').addClass('over-hid');
 

$('.side-cart-close').click(function(){
    $('.side_cart').removeClass('active');
  $('body').removeClass('over-hid');
})
  function addItem(t) {
            $.ajax({ type: "POST", url: "/cart/add.js", dataType: "json", data: $("#" + t).serialize(), success: Shopify.onSuccess, error: Shopify.onError });
        }
   
  
  
    $(".jkalpqw").each(function(){
   $(this).click(function(e) {
     
  e.preventDefault();

   
      
   
      var id=$(this).parents('form').attr('id');
      
      addItem(id);
   });
         });
       $(".rem").on('click', function (e) {
      e.preventDefault();
      
   
      var line = $(this).parents('.m-pro-row').attr("data-line");
           $(this).parents('.m-pro-row').addClass("is-loading");
           
 $.ajax({
        type: 'POST',
        url: '/cart/change.js',
        dataType: 'json',
         data: 'quantity='+0+'&line='+line,
       success: Shopify.onSuccess,
        error: Shopify.onError
      });
      
      

   });
    
     $(".u-qtyc .QuantitySelector__Button").on("click", function() {

        var $button = $(this);
        var oldValue = $button.parent('.u-qtyc').find("input").val();

        if ($button.hasClass('u-plus')) {
            var newVal = parseFloat(oldValue) + 1;
        } else {
            // Don't allow decrementing below zero
            if (oldValue > 0) {
                var newVal = parseFloat(oldValue) - 1;
            } else {
                newVal = 1;
            }
        }

        $button.parent().find("input").val(newVal);

    });
     $(".u-qtyc .QuantitySelector__Button").on('click', function (e) {
      e.preventDefault();
      var qty = $(this).parent('.u-qtyc').find("input").val();
      var line = $(this).parents('.m-pro-row').attr("data-line");
      $(this).parents('.m-pro-row').addClass("is-loading");
           
 $.ajax({
        type: 'POST',
        url: '/cart/change.js',
        dataType: 'json',
         data: 'quantity='+qty+'&line='+line,
       success: Shopify.onSuccess,
        error: Shopify.onError
      });
      
      

   });
      
     $('.ajazradi input[type=radio]').change(function() {
       var valuea =  $(this).val();
    var line = $(this).parents('.m-pro-row').attr("data-line");
//        line = parseInt(line) + 1;
          var vari = $(this).parents('.m-pro-row').attr("data-vari");
      var qty = $(this).parents('.m-pro-row').attr("data-quantity");
           $(this).parents('.m-pro-row').addClass("is-loading");
    if (this.value == '') {
       
     

   $.ajax({
        type: 'POST',
        url: '/cart/change.js',
        dataType: 'json',
         data: 'quantity='+0+'&id='+vari
      });
     

     
   setTimeout(function(){
              var data = {
      "id": vari,
      "quantity": qty,
                selling_plan: ''
    };
        $.ajax({
        type: 'POST',
        url: '/cart/add.js',
        dataType: 'json',
         data: data,
       success: Shopify.onSuccess,
        error: Shopify.onError
      });
     
      }, 1000);
 
    }
    else if (this.value == valuea) {
                 
   
       $.ajax({
        type: 'POST',
        url: '/cart/change.js',
        dataType: 'json',
         data: 'quantity='+0+'&id='+vari 
      });
     

     
   setTimeout(function(){
              var data = {
      "id": vari,
      "quantity": qty,
                selling_plan: valuea
    };
        $.ajax({
        type: 'POST',
        url: '/cart/add.js',
        dataType: 'json',
         data: data,
       success: Shopify.onSuccess,
        error: Shopify.onError
      });
     
      }, 1000);
    }
});
    
          
}
 
    $(".rem").on('click', function (e) {
      e.preventDefault();
      
      var line = $(this).parents('.m-pro-row').attr("data-line");
           $(this).parents('.m-pro-row').addClass("is-loading");
           
 $.ajax({
        type: 'POST',
        url: '/cart/change.js',
        dataType: 'json',
         data: 'quantity='+0+'&line='+line,
       success: Shopify.onSuccess,
        error: Shopify.onError
      });
      
      

   });
     $(".u-qtyc .QuantitySelector__Button").on("click", function() {

        var $button = $(this);
        var oldValue = $button.parent('.u-qtyc').find("input").val();

        if ($button.hasClass('u-plus')) {
            var newVal = parseFloat(oldValue) + 1;
        } else {
            // Don't allow decrementing below zero
            if (oldValue > 0) {
                var newVal = parseFloat(oldValue) - 1;
            } else {
                newVal = 1;
            }
        }

        $button.parent().find("input").val(newVal);

    });
     $(".u-qtyc .QuantitySelector__Button").on('click', function (e) {
      e.preventDefault();
      var qty = $(this).parent('.u-qtyc').find("input").val();
      var line = $(this).parents('.m-pro-row').attr("data-line");
      $(this).parents('.m-pro-row').addClass("is-loading");
           
 $.ajax({
        type: 'POST',
        url: '/cart/change.js',
        dataType: 'json',
         data: 'quantity='+qty+'&line='+line,
       success: Shopify.onSuccess,
        error: Shopify.onError
      });
      
      

   });
$(window).bind("load", function() { 
        function addItem(t) {
            $.ajax({ type: "POST", url: "/cart/add.js", dataType: "json", data: $("#" + t).serialize(), success: Shopify.onSuccess, error: Shopify.onError });
        }
        
              $(".eachatc").each(function(){
   $(this).click(function(e) {
     
  e.preventDefault();

   
      
   
      var id=$(this).parents('form').attr('id');
      
      addItem(id);
   });
         });
   $(".eachatcpro").each(function(){
   $(this).click(function(e) {
     
  e.preventDefault();

   
      
   
      var id=$(this).parents('form').attr('data-id');
      
      addItem(id);
   });
         });
           $("#AddToCart").click(function (t) {
            t.preventDefault(), $(this).prop("disabled", !0), addItem($(this).parents("form").attr("id"));
        }),
          $("#button2").click(function (t) {
            t.preventDefault(), $(this).prop("disabled", !0), addItem($(this).parents("form").attr("id"));
        }),
          
          
    
         
            (Shopify.onSuccess = function () {
                $("#AddToCart").removeAttr("disabled");
           $("#button2").removeAttr("disabled");
                 
                $.ajax({
                    type: "GET",
                    url: "/cart",
                    dataType: "html",
                    success: function (t) {
                       
                        sucss(t);
                    },
                });
            }),
            (Shopify.onError = function (XMLHttpRequest, textStatus) {
                console.log("errorerror");
                var data = eval("(" + XMLHttpRequest.responseText + ")");
                data.message ? alert(data.message + "(" + data.status + "): " + data.description) : alert("Error : " + Shopify.fullMessagesFromErrors(data).join("; ") + "."), $(".addtocart").removeAttr("disabled");
            }),
            (Shopify.fullMessagesFromErrors = function (t) {
                var n = [];
                return (
                    jQuery.each(t, function (i, t) {
                        jQuery.each(t, function (t, e) {
                            n.push(i + " " + e);
                        });
                    }),
                    n
                );
            });
    });
     
     $('.ajazradi input[type=radio]').change(function() {
        var valuea =  $(this).val();
    var line = $(this).parents('.m-pro-row').attr("data-line");
//        line = parseInt(line) + 1;
          var vari = $(this).parents('.m-pro-row').attr("data-vari");
      var qty = $(this).parents('.m-pro-row').attr("data-quantity");
           $(this).parents('.m-pro-row').addClass("is-loading");
    if (this.value == '') {
       
     

   $.ajax({
        type: 'POST',
        url: '/cart/change.js',
        dataType: 'json',
         data: 'quantity='+0+'&id='+vari 
      });
     

     
   setTimeout(function(){
              var data = {
      "id": vari,
      "quantity": qty,
                selling_plan: ''
    };
        $.ajax({
        type: 'POST',
        url: '/cart/add.js',
        dataType: 'json',
         data: data,
       success: Shopify.onSuccess,
        error: Shopify.onError
      });
     
      }, 1000);
 
    }
    else if (this.value == valuea) {
                 
   
       $.ajax({
        type: 'POST',
        url: '/cart/change.js',
        dataType: 'json',
         data: 'quantity='+0+'&id='+vari 
      });
     

     
   setTimeout(function(){
              var data = {
      "id": vari,
      "quantity": qty,
                selling_plan: valuea
    };
        $.ajax({
        type: 'POST',
        url: '/cart/add.js',
        dataType: 'json',
         data: data,
       success: Shopify.onSuccess,
        error: Shopify.onError
      });
     
      }, 1000);
    }
});
     
  
   $(document.body).on('change',".chim",function (e) {
                                   
             var imgurl = $(this).find(':selected').attr("data-image");
     $('.chanitone').attr('src',imgurl);
     
   });
             
</script>
