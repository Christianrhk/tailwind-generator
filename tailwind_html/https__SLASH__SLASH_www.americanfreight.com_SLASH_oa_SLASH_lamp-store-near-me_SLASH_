<style>
  .flyout {
    display: none !important;
    position: absolute;
    top: 99%;
    width: 100%;
    min-width: 800px;
    min-height: 250px;
    background: rgb(255, 255, 255);
    left: 0px;
    padding: 8px;
    padding-top: 5px;
    z-index: 9999;
  }

  #nav span:hover .flyout {
    display: block !important;
  }
</style>


<script type="text/javascript" src="https://www.americanfreight.com/oa/wp-content/themes/af-outlet-advisor-theme/library/components/nav.js"></script>

<script>
  function buildDeptLink(v) {
    return "<a role='menuitem' tabindex='-1' class='pr-6' href='" + v.url + "'>" + v.title + "&nbsp;&nbsp;<i class='fal fa-angle-down text-gray-500' aria-hidden='true'></i>&nbsp;&nbsp;</a>";
  }

  function buildCatLink(c) {
    return "<div><div class='whitespace-nowrap p-2 bg-gray-100 mb-3'><a role='menuitem' tabindex='-1' class='block whitespace-nowrap' href='" + c.url + "'>" + c.title + "<br /><small>Shop All</small></a></div></div>";
  }

  function buildSubCatLink(s) {
      return "<a role='menuitem' tabindex='-1' class='block whitespace-nowrap pl-2 text-md pb-2' href='https://americanfreight.com/br/plp/" + s.deptName + "/" + s.dept_name.replace(/[^a-zA-Z0-9]/g, '-').replace(/\--/g, '').toLowerCase() + "/" + s.category_name.replace(/[^a-zA-Z0-9]/g, '-').replace(/\--/g, '-').toLowerCase() + "/" + s.category_id + "?pno=1&sortBy=1&psize=25'>" + s.category_name + "</a>";
  }

  var deptHtml = '';

  for (var n = 0; n < nav.vertical.length; n++) {
    // build the depts
    deptHtml = deptHtml + '<span class="inline-block">';
    deptHtml = deptHtml + buildDeptLink(nav.vertical[n]); // set the main depts

    deptHtml = deptHtml + '<div class="flyout shadow-lg w-screen container pt-6"><div class="grid grid-rows-2 grid-flow-col gap-2">';
    // build the cats
    for (var c = 0; c < nav.vertical[n].dept.length; c++) {
      deptHtml = deptHtml + '<div>';
      deptHtml = deptHtml + buildCatLink(nav.vertical[n].dept[c]);

      // build the subcats
      for (var s = 0; s < nav.vertical[n].dept[c].cat.length; s++) {
        deptHtml = deptHtml + buildSubCatLink(nav.vertical[n].dept[c].cat[s]);
      }
      deptHtml = deptHtml + '</div>';
    }

    deptHtml = deptHtml + '</div></div>';
    deptHtml = deptHtml + '</span>';
  }

  setTimeout(() => {
    document.getElementById('nav').innerHTML = deptHtml;
  }, 1000);
</script>












<script>
  function toggleNav() {
    var mask = document.getElementById('mask');
    var nav = document.getElementById('mobileNav');

    mask.classList.toggle('hidden');
    nav.classList.toggle('hidden');
  }

  function buildMobileCatLinks(i) {
    var catHtml = '';

    for (var s = 0; s < nav.vertical[i].dept.length; s++) {
      catHtml = catHtml + "<div class='p-2'>" + nav.vertical[n].dept[s].title + buildMobileSubCatLinks(i, s) + "</div>";
    }

    return catHtml;
  };

  function buildMobileSubCatLinks(i, s) {
    return "<h1>&nbsp;&nbsp;&nbsp;&nbsp;HI</h1>";
  }

  function toggleSubcat(el, index) {
    el.querySelector('.mobileFlyout').classList.toggle('hidden');
  }

  function buildMobileDeptLink(v, index) {
    return "<button onclick='toggleSubcat(this)' class='p-4 block text-xl pt-2 pb-2 border-b w-full text-left'>" + v.title + "&nbsp;&nbsp;<i class='fal fa-angle-right float-right text-black text-3xl pt-2' aria-hidden='true'></i>&nbsp;&nbsp;<div class='mobileFlyout hidden'><a class='bg-gray-200 block p-2' href='https://americanfreight.com" + v.url + "'>Shop All " + v.title + "</a>" + buildMobileCatLinks(index) + "</div></button>";
  }

  var mobileDeptHtml = '';

  for (var n = 0; n < nav.vertical.length; n++) {
    mobileDeptHtml = mobileDeptHtml + buildMobileDeptLink(nav.vertical[n], n); // set the main depts
  }

  setTimeout(() => {
    document.getElementById('mobileNavLinks').innerHTML = mobileDeptHtml;
  }, 1000);
</script>
