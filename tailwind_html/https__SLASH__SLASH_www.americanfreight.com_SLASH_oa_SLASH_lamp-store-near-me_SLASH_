<script>
  function toggleLinks(id) {
    document.getElementById(id + 'Links').classList.toggle('hidden');
    document.getElementById(id + 'Close').classList.toggle('hidden');
    document.getElementById(id + 'Open').classList.toggle('hidden');
  }
</script>






