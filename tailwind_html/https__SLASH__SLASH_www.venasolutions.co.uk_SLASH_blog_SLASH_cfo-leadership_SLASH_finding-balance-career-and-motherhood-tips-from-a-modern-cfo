<script>
  
  let blog_post_formatter = function(blogposts) {
    
    let format = `<div class="max-w-screen-xl mx-auto flex flex-wrap px-4 lg:pb-24 pb-16">`;
    
    const options = { weekday: 'long', year: 'numeric', month: 'long', day: 'numeric' };
    
    for (var i = 0; i < blogposts.length; i++) {
      
      var blogpost = blogposts[i];
      
      format += `<div class="md:w-1/3 h-auto w-full md:px-4 lg:mb-0 mb-8 flex lg:mb-16 mb-12">`;
      
        format += `<a href="${blogpost.url}" class="newsroom-post h-full w-full flex flex-col shadow-lg block rounded border border-gray-200">`;
      
        // Freatured Image

        format += `<div class="block w-full flex-shrink-0">`;

          format += `<img src="${blogpost.featuredImage}" loading="lazy" alt="${blogpost.featuredImageAltText}" class="w-full object-cover" />`

        format += `</div>`;
      
        // Title + Body
        
        format += `<div class="lg:py-8 py-6 lg:px-6 px-4 flex flex-col w-full h-full">`;
      
          format += `<h2 class="text-2xl font-serif leading-tight text-gray-800 mb-4 font-bold">${blogpost.name}</h2>`;
      
          format += `<div class="reading-container text-gray-600 font-sm font-semibold mb-6 flex items-center">${new Date(blogpost.publishDate).toLocaleDateString('en-CA', options)}</div>`;
      
          format += `<p class="cta_button text-sm mt-auto">Read More</p>`
      
        format += `</div>`;

        format += `</a>`;
      
      format += `</div>`;
      }
      return format;
  }
</script>

<!-- End partial --> 


  <span id="hs_cos_wrapper_" class="hs_cos_wrapper hs_cos_wrapper_widget hs_cos_wrapper_type_related_blog_posts" style="" data-hs-cos-general-type="widget" data-hs-cos-type="related_blog_posts"><div id="related-blog-posts-8281801659896695283">
</div>
<script type="text/javascript">
  const xhttp_8281801659896695283 = new XMLHttpRequest();
  xhttp_8281801659896695283.open("GET", '/_hcms/related-blog-posts-callback?blogPostId=52136650002&blogId=50762079079&targetBlogPostIds=52136650002&macro=default&startDate=1577836800000&limit=3&templatePath=Website%2FNewsroom%2FBlog+Single++UK.html&contentId=52136650002&requestPath=%2Fblog%2Fcfo-leadership%2Ffinding-balance-career-and-motherhood-tips-from-a-modern-cfo&hs-expires=1691432695&hs-version=1&hs-signature=AM6Wq0a21AM-XYKmzU49bJZOYTEyTKMsog');
  xhttp_8281801659896695283.onreadystatechange = function() {
    if (this.readyState == XMLHttpRequest.DONE) {
      if (this.status == 200) {
        var id = setInterval(function() {
          var formattingFunction = blog_post_formatter;
          if (formattingFunction !== undefined) {
            clearTimeout(id);
            document.getElementById('related-blog-posts-8281801659896695283').outerHTML = formattingFunction(JSON.parse(xhttp_8281801659896695283.responseText));
          }
        }, 50);

        setTimeout(function() {
          clearTimeout(id);
        }, 10000);
      } else {
        console.error('Error: ', this.statusText);
      }
    }
  };
  xhttp_8281801659896695283.send();
</script>
