<script>
    var w;

    $( document ).ready(function() {
        changeFooter();
    });
    $( window ).resize(
        changeFooter
    )

    function changeFooter(){
        w = $(window).width();
        if (w > 1200) {
            $("#main-footer-section").show();
            $("#sub-footer-section").hide();
        } else {
            $("#main-footer-section").hide();
            $("#sub-footer-section").show();
        }
    }
    
    
    $(document).ready(function (e) {
        checkHash();
    });
    
    function checkHash() {
      var hash = location.hash;
      if (hash != '') {
        var anchor = hash.replace('#', '');
        var header_adjustment = 118;
        if (window.innerWidth < 1200) {
          header_adjustment = 72;
        }
        if ($("#anchor-"+anchor).length > 0) {
          $('html, body').animate({
              scrollTop: $("#anchor-"+anchor).offset().top - header_adjustment
          }, 1000);
        }
      }
    }
</script>
