    <script>
      $(document).on('submit', '#regForm', function(e) {
        e.preventDefault();
        var form = $(this);
        var url = form.attr('action');
        var hasError = false;
        $('.text-danger').hide();
        form.find('input.validate-required').each( function() {
          if ($.trim($(this).val()) == '') {
            $(this).parent().find('.text-danger').fadeIn();
            hasError = true;
          }
        });
        
        form.find('select.validate-select-required').each( function() {
          if ($(this).val() == null || $.trim($(this).val()) == '') {
            $(this).parent().find('.text-danger').fadeIn();
            hasError = true;
          }
        });
        
        form.find('input.validate-email').each( function() {
          var regex = /^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
          if (!regex.test($(this).val())) {
            $(this).parent().find('.error-email').fadeIn();
            hasError = true;
          }
        });
        
        form.find('input.validate-tel').each( function() {
          if ($(this).val().slice(0,1) == '2' || 
              $(this).val().slice(0,1) == '3' ||
              $(this).val().slice(0,1) == '5' ||
              $(this).val().slice(0,1) == '6' ||
              $(this).val().slice(0,1) == '8' ||
              $(this).val().slice(0,1) == '9') {
            var regex = /^\d{8}$/;
            if (!regex.test($(this).val())) {
              $(this).parent().find('.error-tel').fadeIn();
              hasError = true;
            }
          } else if ($(this).val().slice(0,1) == '1') {
            var regex = /^\d{11}$/;
            if (!regex.test($(this).val())) {
              $(this).parent().find('.error-tel').fadeIn();
              hasError = true;
            }
          } else {
            $(this).parent().find('.error-tel').fadeIn();
            hasError = true;
          }
        });
        
        form.find('input.validate-check-required').each( function() {
          if (!$(this).is(':checked')) {
            $(this).parent().next('.text-danger').fadeIn();
            hasError = true;
          }
        });
        
        form.find('.validate-radio-required').each( function() {
          if ($(this).find('input[type="radio"]:checked').length == 0) {
            $(this).find('.text-danger').fadeIn();
            hasError = true;
          }
        });
        
        if (!hasError) {
          $.ajax({
            url: url,
            type : 'POST',
            timeout: 10000,
            data: form.serialize(),
            success: function(data) {
              if (data == 'DONE') {
                window.location = '/chi/thankyou';
              }
            },
            beforeSend: function() {
              form.find('.error-message').remove();
            },
            error: function(xhr, textStatus, errorThrown) {
              if (textStatus === "timeout") {
                form.append('<div class="error-message text-danger">Connection timed out. Please try again.</div>');
              } else {
                form.append('<div class="error-message text-danger">Unknown error occurred. Please try again later.</div>');
              }
              form.find('.error-message').fadeIn();
            },
          });
        }
        return false;
      });
    </script>

