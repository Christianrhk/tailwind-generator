      $("#about-button").toggleClass("text-gray-500");
      $("#about-button").toggleClass("text-gray-900");
      $("#about-menu").toggleClass("opacity-0 -translate-y-1 hidden");
      $("#about-menu").toggleClass("opacity-100 translate-y-1 z-50");
