<script>
    $(document).on("click", function(event){
        if(!$(event.target).closest(".header").length) {
            header_close_all();
        }
    });

    $(document).keyup(function(e) {
        if (e.which == 27) {
            header_close_all();
        }
    });

    function header_close_all(ignore) {
        if (ignore != 'menu') {
            $('.toggle_menu').removeClass('open');
            $('.menu').hide();
        }
        
        if (ignore != 'categories') {
            $('.toggle_categorias').removeClass('open');
            $('.header .categorias').hide();
        }
        
        if (ignore != 'search') {
            $('.search').removeClass('open');
        }
    }

    $(function(){
        $('.toggle_menu').bind('click', function() {
            header_close_all('menu');
            
            $('.menu').animate({ height: [ "toggle", "swing" ] }, 300, "linear", function() {
                if ($('.toggle_menu').hasClass('open')) {
                    $('.toggle_menu').removeClass('open')
                } else {
                    $('.toggle_menu').addClass('open');
                }
            });
        });
        
        $('.toggle_categorias').bind('click', function() {
            header_close_all('categories');
            
            $('.header .categorias').animate({ height: [ "toggle", "swing" ] }, 300, "linear", function() {
                if ($('.toggle_categorias').hasClass('open')) {
                    $('.toggle_categorias').removeClass('open')
                } else {
                    $('.toggle_categorias').addClass('open');
                }
            });
        });

        $('.toggle_search').bind('click', function() {
            header_close_all('search');

            if ($('.header .search').hasClass('open')) {
                $('.header .search').removeClass('open')
            } else {
                $('.header .search').addClass('open')
            }
            $('.header .search input').focus();
        });
    })
</script>

