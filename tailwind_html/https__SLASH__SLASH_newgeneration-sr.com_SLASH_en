        <script>
            /********* Blueprints variables **********/
            let slides = []

                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        
                                                                                                                                            
                slides.push({"title":"<p>We are certified <strong>ISO 9001 \/ 2022<\/strong> &amp; <strong>ISO 27001 \/ 2022 Certification<\/strong><\/p>","button_text":null,"button_link":"#","color":"#2664ae","overlay":false,"image":null,"directions":["bottom","top"]})
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        
                                                                                                                                            
                slides.push({"title":"<p>New visual identity.<\/p>\n<p>As the year begins, NEW GEN is proud to announce the launch of its new logo.<\/p>","button_text":null,"button_link":"#","color":"#2749b0","overlay":false,"image":"\/user\/pages\/01.home\/PublicationInstagram10.jpg","directions":["bottom","top"]})
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        
                                                                                                                                            
                slides.push({"title":"<p>Optimize your critical infrastructures, reduce your costs and generate new earnings<\/p>","button_text":"See more","button_link":"\/products\/nuveaconsulting","color":"#2e3b44","overlay":true,"image":"\/user\/pages\/01.home\/pexels-lukas.jpg","directions":["bottom","top"]})
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        
                                                                                                                                            
                slides.push({"title":"<p>We are EIP labeled by the Systematic competitiveness cluster and a member of the IA &amp; DS HUB<\/p>","button_text":null,"button_link":"#","color":"#2749b0","overlay":false,"image":"\/user\/pages\/01.home\/fingers-g9a4834b13_1920.jpg","directions":["bottom","top"]})
            
            let timeout = 4000
            let autoplay = 1
            let transition_duration = 500
            let current_index = 0
            let display_dots = 1

            /********* Locals variables **********/
            let interval;
            let viewport;
            let dots;

            function NGCarousel(autoplay, timeout, var_dots){
                viewport = document.getElementById('slide-viewport')

                if(var_dots) {
                    dots = document.getElementById('dots')
                    dots.addEventListener('mouseenter', () => clearInterval(interval))
                    dots.addEventListener('mouseleave', () => { if(autoplay) play(timeout) })
                    initDots()
                }

                initASlide(current_index, true)

                if(autoplay) play(timeout)
            }

            function initASlide(i, first, direction){
                let text_div = document.createElement('div')

                text_div = `<div>${slides[i].title}</div>`
                if (slides[i].button_text)
                    text_div +=`<a href="${slides[i].button_link}" class="button-style mt-10">${slides[i].button_text}</a>`

                if(dots) dots.children[i].firstElementChild.firstElementChild.style.backgroundColor = 'white'

                if(first){
                    viewport.firstElementChild.classList.add('visible', 'visible-slide', 'transition')
                    viewport.firstElementChild.firstElementChild.style[slides[i].directions[0]] = 0
                    viewport.firstElementChild.firstElementChild.style.transitionDuration = transition_duration + 'ms'
                    viewport.firstElementChild.firstElementChild.style.backgroundColor = slides[i].color
                    viewport.firstElementChild.firstElementChild.firstElementChild.innerHTML = text_div

                    viewport.firstElementChild.lastElementChild.style[slides[i].directions[1]] = 0
                    viewport.firstElementChild.lastElementChild.style.transitionDuration = transition_duration + 'ms'
                    viewport.firstElementChild.lastElementChild.style.backgroundImage = `url(${slides[i].image})`
                    viewport.firstElementChild.lastElementChild.style.opacity = slides[i].overlay ? 0.3 : 1
                    viewport.firstElementChild.lastElementChild.style.backgroundPosition = slides[i].image_position

                    console.log(slides[i].overlay)
                }
                else {
                    for(child in viewport.children){
                        if(viewport.children[child].classList && !viewport.children[child].classList.contains('visible')){
                            viewport.children[child].classList.remove('transition')

                            viewport.children[child].firstElementChild.style.transitionDuration = ''
                            viewport.children[child].firstElementChild.style[slides[i].directions[0]] = direction ? '100%':'-100%'
                            viewport.children[child].firstElementChild.style.backgroundColor = slides[i].color
                            viewport.children[child].firstElementChild.firstElementChild.innerHTML = text_div

                            viewport.children[child].lastElementChild.style.transitionDuration = ''
                            viewport.children[child].lastElementChild.style[slides[i].directions[1]] = direction ? '100%':'-100%'
                            viewport.children[child].lastElementChild.style.backgroundImage = `url(${slides[i].image})`
                            viewport.children[child].lastElementChild.style.opacity = slides[i].overlay ? 0.3 : 1
                            viewport.children[child].lastElementChild.style.backgroundPosition = slides[i].image_position

                            console.log(slides[i].overlay)
                        }
                    }
                }
            }

            function hideActiveSlide(direction){
                let active;

                for(child in viewport.children){
                    if(viewport.children[child].classList && viewport.children[child].classList.contains('visible')){
                        active = viewport.children[child]

                        active.firstElementChild.style[slides[current_index].directions[0]] = direction ? '-100%':'100%'
                        active.lastElementChild.style[slides[current_index].directions[1]] = direction ? '-100%':'100%'
                        active.classList.remove('visible-slide')

                        setTimeout(() => {
                            active.classList.remove('visible')
                        }, transition_duration)
                    }
                }
            }

            function showInactiveSlide(){
                let active;

                for(child in viewport.children){
                    if(viewport.children[child].classList && !viewport.children[child].classList.contains('visible')){
                        active = viewport.children[child]

                        active.classList.add('transition')
                        active.firstElementChild.style.transitionDuration = transition_duration + 'ms'
                        active.firstElementChild.style[slides[current_index].directions[0]] = 0

                        active.lastElementChild.style.transitionDuration = transition_duration + 'ms'
                        active.lastElementChild.style[slides[current_index].directions[1]] = 0

                        setTimeout(() => {
                            active.classList.add('visible-slide')
                        }, transition_duration / 1.6)

                        setTimeout(() => {
                            active.classList.add('visible')
                        }, transition_duration)
                    }
                }
            }

            function goTo(index, click){
                if(index === current_index && click) return

                let prev_slide_index = click ? current_index : index - 1 < 0 ? slides.length - 1 : index - 1
                let direction = index - current_index < 0

                initASlide(index, false, direction)

                if(dots) {
                    dots.children[prev_slide_index].firstElementChild.firstElementChild.style.backgroundColor = ''
                    dots.children[index].firstElementChild.firstElementChild.style.backgroundColor = 'white'
                }

                setTimeout(() => {
                    hideActiveSlide(direction)
                    showInactiveSlide()
                }, 0)

                current_index = index
            }

            function initDots(){
                for(let i in slides){
                    let div = document.createElement('div')
                    
                    div.innerHTML = '<div style="margin: 1rem 0"><div style="width: 1.1rem; height: 1.1rem; border-radius: 50%; border: 0.1rem solid white; transition: all 0.2s ease-out 0s; cursor: pointer"></div></div>'
                    div.firstElementChild.addEventListener('click', () => goTo(parseInt(i), true))

                    dots.appendChild(div)
                }
            }

            function play(timeout){
                interval = setInterval(() => {
                    if(current_index + 1 < slides.length) current_index++
                    else current_index = 0
                    goTo(current_index)
                }, timeout)
            }

            NGCarousel(autoplay, timeout, display_dots)
