<script>
    $(document).ready(function () {
        $("#open-tiles-41560 .P04__tile--description").FontAwesomeUnorderedList("far fa-arrow-right");

        if (typeof document.fonts === 'undefined') {
            OTSetHeights();
        } else {
            document.fonts.ready.then(function() {
                OTSetHeights();
            });
        }

        $(window).on('resized', function () {
            OTSetHeights();
        });

        function OTSetHeights() {

            if ($(window).innerWidth() > 768) {
                $("#open-tiles-41560 .P04__tile--description").EqualizeHeights();
                $("#open-tiles-41560 .P04__tile--title").EqualizeHeights();
            } else {
                // Clear the css
                $("#open-tiles-41560 .P04__tile--description").css('min-height', '');
                $("#open-tiles-41560 .P04__tile--title").css('min-height', '');
            }
        }
    });
</script>

