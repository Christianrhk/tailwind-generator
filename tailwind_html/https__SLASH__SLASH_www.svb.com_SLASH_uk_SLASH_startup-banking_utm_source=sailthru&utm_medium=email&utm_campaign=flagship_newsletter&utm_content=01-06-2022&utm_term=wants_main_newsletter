<script>
    $(document).ready(function () {
        if (typeof document.fonts === 'undefined') {
            CTSetHeights();
        } else {
            document.fonts.ready.then(function() {
                CTSetHeights();
            });
        }

        $(window).on('resized', function () {
            CTSetHeights();
        });

        if (!hasSupportToPolygon) {
            $("#closed-tiles-95634 .P05__background").addClass("nosupport");
        }

        new SimpleBar($("#closed-tiles-95634 .box-scroll-area")[0], {
                forceVisible: true
        });

        function CTSetHeights() {
            if ($(window).innerWidth() > 768) {
                $("#closed-tiles-95634 .P04__tile").EqualizeHeights();

                var bheight = $("#closed-tiles-95634 .P04__tile").first().outerHeight();
                $("#closed-tiles-95634 .box").height(bheight);
                $("#closed-tiles-95634 .box-scroll-area").height(bheight + 40);
            } else {
                // Clear the css
                $("#closed-tiles-95634 .P04__tile").css('min-height', '');
                $("#closed-tiles-95634 .box").css('height', '');
                $("#closed-tiles-95634 .box-scroll-area").css('height', '');
            }
            SetDiagonalSize();
        }

        function SetDiagonalSize() {
            var height = 0;
            if ($(window).innerWidth() > 768) {
                height = $("#closed-tiles-95634 .P05__container").height();
            } else {
                height = $("#closed-tiles-95634 .P05__tile-content").height() + 100;
            }
                $("#closed-tiles-95634 .P05__background").height(height).width(height);
        }
    });
</script>

