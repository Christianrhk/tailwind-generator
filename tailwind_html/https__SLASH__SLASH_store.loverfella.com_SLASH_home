<div x-data x-show="$store.modals.isLoading" class="absolute top-0 left-0 h-screen w-screen bg-black bg-opacity-70 z-50">
    <div class="lds-heart absolute top-1/2 left-1/2 transform -translate-x-1/2 -translate-y-1/2"><div></div></div>
