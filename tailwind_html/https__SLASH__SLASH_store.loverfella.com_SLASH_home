<script src="//unpkg.com/alpinejs" defer></script>
<script src="//cdn.jsdelivr.net/npm/sweetalert2@11" defer></script>
<script>
document.addEventListener('alpine:init', () => {
    let currentPage = 'CATEGORIES';
    let currentCategory = null;
    const path = window.location.pathname.toLowerCase();
    const categoriesRegex = /(\/store)(\/)([a-z\-]*)(\/?)$/;
    const subcategoriesRegex = /(\/store)(\/)([a-z\-]*)(\/)([a-z\-]*)(\/?)$/;
    if (categoriesRegex.test(path)) {
        currentCategory = categoriesRegex.exec(path)[3];
        currentPage = 'SUBCATEGORIES';
    }
    if (subcategoriesRegex.test(path)) {
        currentCategory = subcategoriesRegex.exec(path)[5];
        currentPage = 'PRODUCTS';
    }
    Alpine.store('spa', {
        currentPage,
        currentCategory,
        breadcrumbs: [
            {
                text: 'Store',
                link: '/'
            }
        ]
    });
    Alpine.store('api', {
        core: getStoreDataFromEl(document),
        categories: null,
        products: null,
    });
    Alpine.store('modals', {
        isLoading: false,
        showBasket: false,
        showMobileNav:false,
        openProductModalId: '',
        openArbitraryProductModel: false,
        arbitrayProduct: {},
        setLoading(v) {
            this.isLoading = v;
        },
        toggleBasket() {
            this.showBasket =! this.showBasket;
        },
        closeBasket() {
            this.showBasket = false;
        },
        openProductModal(id) {
            this.openProductModalId = id;
        },
        closeMobileNav() {
            this.showMobileNav = false;
        }
    })
    Alpine.store('discord', {
        discordLink:'',
        discordMembers:0,
        setDiscordStats(discordStats) {
            this.discordLink = discordStats.instant_invite || `https://discord.com/invite/loverfam`;
            this.discordMembers = discordStats.presence_count;
        }
    })
    function reqListener() {
         var response = this.responseText;
         const discordStats = JSON.parse(response)
         Alpine.store('discord').setDiscordStats(discordStats);
    }
    
    if (currentPage === 'SUBCATEGORIES') {
        playerlands_LoadCategoriesAsync();
    }
    if (currentPage === 'PRODUCTS') {
        playerlands_LoadProductsAsync();
    }
    calculateBreadcrumbs();
   
   var oReq = new XMLHttpRequest();
   oReq.addEventListener("load", reqListener);
   oReq.open("GET", "https://discord.com/api/guilds/293801273734725632/widget.json");
   oReq.send();
});

function calculateBreadcrumbs() {
    const path = window.location.pathname.toLowerCase();
    const categoriesRegex = /(\/store)(\/)([a-z\-]*)(\/?)$/;
    const subcategoriesRegex = /(\/store)(\/)([a-z\-]*)(\/)([a-z\-]*)(\/?)$/;
    if (categoriesRegex.test(path)) {
        const currentCategory = categoriesRegex.exec(path)[3];
        
        Alpine.store('spa').breadcrumbs = [
            {
                text: 'Store',
                link: '/'
            },
            {
                text: currentCategory,
                link: '/' + currentCategory
            }
        ];
    } else if (subcategoriesRegex.test(path)) {
        const matchedRegex = subcategoriesRegex.exec(path);
        Alpine.store('spa').breadcrumbs = [
            {
                text: 'Store',
                link: '/'
            },
            {
                text: matchedRegex[3],
                link: '/' + matchedRegex[3]
            },
            {
                text: matchedRegex[5],
                link: '/' + matchedRegex[3] + '/' + matchedRegex[5]
            }
        ];
    } else {
        Alpine.store('spa').breadcrumbs = [
            {
                text: 'Store',
                link: '/'
            }
        ];
    }
}

function getDataFromEl(el) {
    const vapidata = el.getElementsByTagName('pre')[0];
    const jsonData = vapidata.innerText;
    const data = JSON.parse(jsonData);
    return data;
}

function getStoreDataFromEl(el) {
    const data = getDataFromEl(el);
    
    return {
        ...data,
        getProductCount(productId) {
            const lines = data.basket.lineItems.filter((x) => x.productId === productId);
            const total = lines.reduce((prev, current) => prev + current.quantity, 0);
            return total;
        },
        isProductInBasket(productId) {
            const product = data.basket?.lineItems.find(x => x.productId.toString() === productId);
            return !!product;
        }
    };
}
    
function updateApiData(el) {
    Alpine.store('api').core = getStoreDataFromEl(el);
}

function playerlands_AsyncPostback(data, isBasketUpdate, title) {
    Alpine.store('modals').setLoading(true); 
    let postbackUrl = 'https://' + window.location.host + '/~/api/data';
    if(window.location.host.indexOf('localhost') > -1) {
      postbackUrl = 'http://' + window.location.host + '/~/api/data';
    }
    var xhr = new XMLHttpRequest();
    xhr.open("POST", postbackUrl, true);
    xhr.setRequestHeader('Content-Type', 'application/json');
    xhr.send(JSON.stringify(data));
    xhr.onreadystatechange = () => {
        if (xhr.readyState === 4 && xhr.status === 200) {
            const response = JSON.parse(xhr.response);
            if(!response.url) {
              return;
            }
            let getUrl = 'https://' + window.location.host + response.url;
            if(window.location.host.indexOf('localhost') > -1) {
                getUrl = 'http://' + window.location.host + response.url;
            }
            const xhrGet = new XMLHttpRequest();
            xhrGet.open("GET", getUrl, true);
            xhrGet.send();
            xhrGet.onreadystatechange = () => {
                if (xhrGet.readyState === 4 && xhrGet.status === 200) {
                  const vdom = document.createElement( 'html' );
                  vdom.innerHTML = xhrGet.response;
                  updateApiData(vdom);
                  Alpine.store('modals').setLoading(false);
                  
                  if (isBasketUpdate) {
                      const title = (data.quantity > 0 || data.action === "ADD_TO_BASKET") ? 'Added to Cart' : 'Removed from Cart';
                      const svg =  (data.quantity > 0 || data.action === "ADD_TO_BASKET") ? '<svg class="w-6 h-6 self-center" fill="none" stroke="currentColor" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7"></path></svg>' : '<svg class="w-6 h-6 self-center" fill="none" stroke="currentColor" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M6 18L18 6M6 6l12 12"></path></svg>'
                      const bgColor = (data.quantity > 0 || data.action === "ADD_TO_BASKET") ? '#3fad46' : '#d9534f';
                      
                      const basketQuantity = Alpine.store('api').core.totalBasketItems;
                      const basketTotal = (Alpine.store('api').core.basket.totalIncludingTax / 100 ).toFixed(2);

                      
                      Swal.fire({
                          position: 'bottom-end',
                          background: bgColor,
                          color:'white',
                          backdrop:false,
                          showConfirmButton: false,
                          width: 400,
                          timer: 2500,
                          timerProgressBar: true,
                          html: `<div class="flex gap-2 font-semibold">
                          ${svg}
                          <div class="flex flex-col self-start">
                                  <span class="flex flex-wrap text-md border-b border-white">
                                    ${title}
                                  </span>
                                  <span class="text-sm text-left">Your cart now has ${basketQuantity} item(s) totaling ${basketTotal} USD.</span>
                              </div>
                          </div>`
                      });
                  }
                }
            }
        }
    };
}
                              
function playerlands_AsyncLogin(game, username) {
    playerlands_AsyncPostback({
        action: "LOGIN",
        username,
        game,
    });
}

function playerlands_AsyncLogout() {
    playerlands_AsyncPostback({
        action: "LOGOUT",
    });
    Alpine.store('modals').closeBasket();
}

function playerlands_AsyncAddToBasket(productId, serverId, giftedPlayerName) {
    playerlands_AsyncPostback(
        {
            action: "ADD_TO_BASKET",
            productId: productId,
            serverId: serverId,
            giftedPlayerName,
        },
        true,
    );
    Alpine.store('modals').openProductModal('');
    if(!Alpine.store('modals').showBasket) {
      Alpine.store('modals').toggleBasket();
    }

}

function playerlands_AsyncAddGiftCardToBasket(email, amount) {
    if(!email || !amount) {
        email = document.getElementById("playerlands_giftCardEmail").value;
        amount = parseFloat(document.getElementById("playerlands_giftCardAmount").value) * 100;
    }
    
    if(!amount){
        alert('Invalid Gift Card Amount')
        return;
    }
    
    playerlands_AsyncPostback({
        action: "ADD_GIFTCARD",
        recipientEmail: email,
        balance: amount,
    });
}

function playerlands_AsyncRemoveGiftCardFromBasket(email, amount) {
    playerlands_AsyncPostback({
        action: "REMOVE_GIFTCARD",
        recipientEmail: email,
        balance: amount,
    });
}

function playerlands_AsyncApplyGiftCardToBasket(reference) {
    playerlands_AsyncPostback({
        action: "APPLY_GIFTCARD_TO_BASKET",
        reference,
    });
}

function playerlands_AsyncRemoveAppliedGiftCardFromBasket() {
    playerlands_AsyncPostback({
        action: "REMOVE_APPLIED_GIFTCARD_FROM_BASKET",
    });
}

function playerlands_AsyncIncrementQuantity(productId, serverId, quantity, giftedPlayerName) {
    playerlands_AsyncPostback({
        action: "INCREMENT_QUANTITY",
        productId: productId,
        serverId: serverId,
        quantity: quantity,
        giftedPlayerName,
    }, true);
}

function playerlands_OpenArbitraryProductModal(productId) {
    console.log(productId)
    const getUrl = `https://${window.location.host}/api/products/${productId}`;
    var xhr = new XMLHttpRequest();
    xhr.open("GET", getUrl, true);
    xhr.setRequestHeader('Content-Type', 'application/json');
    xhr.send();
    xhr.onreadystatechange = () => {
        if (xhr.readyState === 4 && xhr.status === 200) {
            const product = JSON.parse(xhr.response);
            Alpine.store('modals').arbitrayProduct = product;
            Alpine.store('modals').openArbitraryProductModel = true;
        }
    };
}

function playerlands_LoadCategoriesAsync () {
    Alpine.store('modals').setLoading(true);
    const getUrl = `https://${window.location.host}/~/api/data/categories`;
    var xhr = new XMLHttpRequest();
    xhr.open("GET", getUrl, true);
    xhr.setRequestHeader('Content-Type', 'application/json');
    xhr.send();
    xhr.onreadystatechange = () => {
        if (xhr.readyState === 4 && xhr.status === 200) {
            const vdom = document.createElement( 'html' );
            vdom.innerHTML = xhr.response;
            const categoriesApiData = getDataFromEl(vdom);
            Alpine.store('api').categories = categoriesApiData.categories;
            Alpine.store('modals').setLoading(false);
            Alpine.store('spa').currentPage = 'SUBCATEGORIES';
        }
    };
}

function playerlands_LoadProductsAsync () {
    Alpine.store('modals').setLoading(true);
    const getUrl = `https://${window.location.host}/~/api/data/categories/${Alpine.store('spa').currentCategory}/products`;
    var xhr = new XMLHttpRequest();
    xhr.open("GET", getUrl, true);
    xhr.setRequestHeader('Content-Type', 'application/json');
    xhr.send();
    xhr.onreadystatechange = () => {
        if (xhr.readyState === 4 && xhr.status === 200) {
            const freg = new RegExp('(<pre id="api-data-products" hidden>)((.|\n)*)(</pre>)');
            const regExec = freg.exec(xhr.response.toString());
            const productsApiData = JSON.parse(regExec[2])
            Alpine.store('api').products = productsApiData.products;
            Alpine.store('modals').setLoading(false);
            Alpine.store('spa').currentPage = 'PRODUCTS';
        }
    };
}

function playerlands_GoToStoreHomeAsync () {
    window.history.pushState({}, 'LoverFella Store', '/store');
    Alpine.store('spa').currentCategory = null;
    Alpine.store('api').categories = null;
    Alpine.store('api').products = null;
    Alpine.store('spa').currentPage = 'CATEGORIES';
    calculateBreadcrumbs();
}

function playerlands_GoToSubcategoriesAsync (category) {
    window.history.pushState({}, 'LoverFella Store', window.location.pathname + '/' + category);
    calculateBreadcrumbs();
    Alpine.store('spa').currentCategory = category;
    Alpine.store('api').categories = null;
    Alpine.store('api').products = null;
    playerlands_LoadCategoriesAsync();
}

function playerlands_GoToProductsAsync (category) {
    window.history.pushState({}, 'LoverFella Store', window.location.pathname + '/' + category);
    calculateBreadcrumbs();
    Alpine.store('spa').currentCategory = category;
    Alpine.store('api').categories = null;
    Alpine.store('api').products = null;
    playerlands_LoadProductsAsync();
}

const clickToCopy = (text) => {
    const copyText = text;
    navigator.clipboard.writeText(copyText);
    Swal.fire({
      title: 'Server IP Copied!',
      text: 'We hope to see you online soon!',
      icon: 'success',
      confirmButtonColor: '#ff004f',
      timer: 3000,
      timerProgressBar: true,
    })
  }
  
 window.onpopstate = () => {
    window.location.href = window.location.href;
 };
</script>




