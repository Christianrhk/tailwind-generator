<script type="text/javascript">
function adjustLink() {
	try {
      var as = /företag/gi.test('Övrigt') ? '&as=corporate': '';
      var useLatestVersion = '&v=latest';
		window.location = "https://secure.sbab.se/logga-in?rev=" + new Date().getTime() + as + useLatestVersion;
		return false;
	} catch(ex) {}
	// if reach this point use default <a "href" /> value
}
</script>

