        $jq(document).ready(function () {
            var fixButtons = function (buttonClass) {
                $jq('span.' + buttonClass + ' a').addClass(buttonClass + ' btn');
                $jq('span.' + buttonClass + ' a')
                    .parent()
                    .removeClass(buttonClass);
            };

            fixButtons('btn-primary');
            fixButtons('btn-secondary');
            fixButtons('btn-tertiary');
        });
    </script>
