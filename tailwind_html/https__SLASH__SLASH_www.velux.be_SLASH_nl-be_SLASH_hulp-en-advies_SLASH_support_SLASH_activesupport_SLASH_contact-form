    }

    var onSubmitCallback = function(event) {

        var captchaTokenInputElement = document.getElementById('fxb_99651e40-89dd-4d1a-96ef-1f95786784de_Fields_087a4781-5256-4981-b1d0-08332d72a105__Value');
