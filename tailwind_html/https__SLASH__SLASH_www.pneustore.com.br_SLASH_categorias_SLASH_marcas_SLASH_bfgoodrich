	addEventListener("DOMContentLoaded", learnMore)
    
    function learnMore() {
        verifyCardBodySize();
        btn = !btn
        if ( btn === true ) {
            document.getElementById('card-body').style.height = 'auto'
            document.getElementById('btn').innerHTML = 'Ocultar'
        } else {
            document.getElementById('card-body').style.height = '100px'
            document.getElementById('card-body').style.overflow = 'hidden'
            document.getElementById('btn').innerHTML = 'Mostrar mais'
        }
    }

    function verifyCardBodySize() {
        const card = document.getElementById('card-body');
        const btn = document.getElementById('btn');
        card.offsetHeight < 100? btn.style.display = 'none' : btn.style.display = 'block'
    }

