	<script>
		var dsn = "";
		var ravenOptions = {
			ignoreErrors: [
				/Blocked a frame with origin/,
				/Request failed with status code 401/,
				/Can't find variable: Vue/,
			]
		};

		Raven.config(dsn, ravenOptions).install();
	</script>
