		<script type="text/javascript">
		$(window).scroll(function(){ 
			var a = 240;
			var pos = $(window).scrollTop();
			if(pos > a) {
				$("nav").addClass("fixedMenu"); 
				$("#menu-logo").show();
			}
			else {
				$("nav").removeClass("fixedMenu");
				$("#menu-logo").hide(); 
			}
		});
		</script>

