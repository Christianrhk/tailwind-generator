<script>
	var id = "page_contact15965";
	formOptList[id] = {"pWPFormKey":"15965","blockId":"page_contact15965","className":"","pContent":true,"pName":true,"pKana":true,"pTel":true,"pMail":true,"pAddr":true,"pQuestion":true,"pContentReq":true,"pNameReq":true,"pMailReq":true,"pQuestionReq":true};
	formList[id] = getFormValid(id);
    document.addEventListener("DOMContentLoaded", function(event) {
        $(document).on("change",'#page_contact15965 [type=file]',function(){
            //画像選択
            var file = $(this).prop('files')[0];
            $('#page_contact15965 .file-select .filename.'+$(this).attr('name')).html(file.name);
            $('#page_contact15965 .file-select [data-file-delete='+$(this).attr('name')+']').css('visibility','visible');
            $('#page_contact15965 .check-att-image').html(getImageNamespage_contact15965());
        });
        $(document).on("click", "#page_contact15965 .file-select .mwform-file-delete", function(){
            //選択削除
            var target = $(this).data('file-delete');
            $('#page_contact15965  .mw_wp_form.mw_wp_form_input form [data-mwform-file-delete='+target+']').click();
            $('#page_contact15965 .file-select .filename.'+target).html('');
            $(this).css('visibility','');
            $('#page_contact15965 .check-att-image').html(getImageNamespage_contact15965());
        });
        $('#page_contact15965 .file-select button').on('click',function(){
            var target = $(this).data('target');
            $('#page_contact15965 [type=file][name='+target+']').click();
        });
    });
    function getImageNamespage_contact15965() {
        var img = '';
        $('#page_contact15965 .file-select .filename').each(function(){
            if($(this).text() != '') {
                img += $(this).text()+'<br>';
            }
        });
		if(img == '') img = 'なし';
        return img;
    }
</script>
