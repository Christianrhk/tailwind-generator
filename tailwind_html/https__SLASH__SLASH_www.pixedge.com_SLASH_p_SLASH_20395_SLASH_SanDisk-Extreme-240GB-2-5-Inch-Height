      borderColor: 'rgb(108,178,235)',
      borderWidth: 1,
      data: [82, 0, 6, 1, 1, 5, 21, 59, 147, 327, 122],
    }]
  },
  options: {
    title: { display: false },
    legend: { display: false },
    tooltips: {
      callbacks: {
        title: function(tooltipItems, data) {
          return '# Products with Rating of ' + tooltipItems[0].xLabel;
        },
        label: function(tooltipItems, data) {
          return tooltipItems.yLabel;
        }
      }
    },
    scales: {
      yAxes: [{
        scaleLabel: {
          display: true,
          labelString: 'Number of SanDisk Products'
        }
      }],
      xAxes: [{
        scaleLabel: {
          display: true,
          labelString: 'Product Rating'
        },
      }]
    }
  }
});
</script>



    </div>

