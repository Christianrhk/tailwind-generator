<script>
  // collapsing mobile nav
    (function ($) {
      function setup_collapsible_submenus() {
            var FirstLevel = $('#MobileNav .first-level > a');

            FirstLevel.off('click').click(function (e) {
                e.preventDefault();
                $(this).parent().children().children().toggleClass('reveal-items');
                $(this).toggleClass('icon-switch');
            });
        }
      	function toggle_all_submenus() {
            $('#MobileNav .first-level > a').parent().children().children().toggleClass('reveal-items');
            $('#MobileNav .first-level > a').toggleClass('icon-switch');
        }
        /*$(window).load(function () {
            setTimeout(function () {
                setup_collapsible_submenus();
            }, 700);
        });*/
        $(document).ready(function() {
          setup_collapsible_submenus();
        }); 
    })(jQuery);

</script>

