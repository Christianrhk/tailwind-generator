<script>
$( document ).ready(function() {  
    $(document).on('mouseenter', ".site-nav__dropdown-li-a", function() { /*click*/
      var handleurl = $(this).attr("handleurl");
      var tophandleurl = $(this).attr("tophandleurl");
      $('.site-nav__dropdown-data').hide();
      $('.site-nav__dropdown-data[handleurl="'+handleurl+'"]').show(); 
      $('.site-nav__dropdown-data[tophandleurl!="'+tophandleurl+'"].default').show();
    });
    $(document).on('click', ".site-nav__dropdown-btn", function() {
      window.location = "/collections/all-products";
    });  
    //$(document).on('click', ".site-nav__dropdown-l", function() {
    //  var handlelink = $(this).closest(".site-nav__dropdown-n").attr("handlelink");
    //  window.location = ""+handlelink;
    //}); 
  
    $(document).on('mouseenter', ".site-nav--has-new-dropdown a.site-nav__link", function() {
      var tthis = $(this).parent();      
      if ($('.site-nav--has-new-dropdown.active').length > 0) {
        tthis.addClass('active');
        tthis.find('.site-nav__dropdown-panel').removeClass('site-nav-hide-panel-contents').removeClass('site-nav-fadeout-panel-contents').addClass('site-nav-fadein-panel-contents');
        tthis.find('.slide-down-panel').removeClass('site-nav-slideup-panel-background').removeClass('site-nav-slidedown-panel-background').removeClass('site-nav-hide-panel-background').addClass('site-nav-show-panel-background').removeClass('transition');
      }
      else {      
        setTimeout(function(){
          if ($(tthis).find('a.site-nav__link').is(':hover')) {
            tthis.addClass('active');
            tthis.find('.site-nav__dropdown-panel').removeClass('site-nav-hide-panel-contents').removeClass('site-nav-fadeout-panel-contents').addClass('site-nav-fadein-panel-contents');
            tthis.find('.slide-down-panel').removeClass('site-nav-slideup-panel-background').removeClass('site-nav-show-panel-background').removeClass('site-nav-hide-panel-background').addClass('site-nav-slidedown-panel-background').removeClass('transition');  
          }
          else {
            console.log("Menu load cancelled");
          }
        }, 50);
      } 
    });    
    $(document).on('mouseleave', ".site-nav--has-new-dropdown", function() {
      var tthis = $(this);
      setTimeout(function(){
        tthis.removeClass('active');
      }, 200); 
      if ($('.site-nav').find(".site-nav--has-new-dropdown:hover").length > 0) {
        //console.log("hover");
        $('.site-nav').find(".site-nav--has-new-dropdown:hover .slide-down-panel").addClass('transition');
        tthis.find('.site-nav__dropdown-panel').removeClass('site-nav-fadein-panel-contents').addClass('site-nav-hide-panel-contents');
        tthis.find('.slide-down-panel').removeClass('site-nav-slidedown-panel-background').removeClass('site-nav-slideup-panel-background').removeClass('site-nav-show-panel-background').addClass('site-nav-hide-panel-background');
      }
      else {
        //console.log("nohover");
        tthis.find('.site-nav__dropdown-panel').removeClass('site-nav-fadein-panel-contents').addClass('site-nav-fadeout-panel-contents');
        tthis.find('.slide-down-panel').removeClass('site-nav-slidedown-panel-background').removeClass('site-nav-hide-panel-background').removeClass('site-nav-show-panel-background').addClass('site-nav-slideup-panel-background');        
      }
    }); 
});
</script>  

