<!-- ----------------------------------------------------------------------------------------- -->
<script>
function geoWorldRedirect(r) {
	try {
      var ip_con = "";
      if (r.continent_code) {
          ip_con = r.continent_code;
          if (ip_con.length > 0) {
          	js_createCookie('ipcontinentcode', ip_con, 7);
          }
      }
      var ip_cou = "";
      if (r.country_code) {
          ip_cou = r.country_code;
          if (ip_cou.length > 0) {
          	js_createCookie('ipcountrycode', ip_cou, 7);
          }
      }
      var countrystore = js_readCookie('ipcountryddstore');
      var countryflag = js_readCookie('ipcountryddflag');
      var countrycurr = js_readCookie('ipcountryddcurrency');
      var countryname = js_readCookie('ipcountryddname');
 
      if ((countrystore == null) || (countrystore == "") || (countryflag == null) || (countryflag == "")) {  
        switch (ip_con) {
          case "NA":
          case "SA":
            console.log("Read geoWorldRedirect default LLC");
            countryname = "United States";
            countrystore = "llc";
            countryflag = "flag-united_states"; 
            countrycurr = "USD";
            break;             
          default: 
            console.log("Read geoWorldRedirect default World");
            countryname = "United Kingdom";
            countrystore = "world";
            countryflag = "flag-united_kingdom";
            countrycurr = "GBP";
            break;   
        }  
 
        js_createCookie('ipcountryddname', countryname, 365);
        js_createCookie('ipcountryddflag', countryflag, 365);
        js_createCookie('ipcountryddstore', countrystore, 365);
        js_createCookie('ipcountryddcurrency', countrycurr, 365);        
      }
      
      geoWorld(ip_cou, ip_con, countrystore, countryflag, false);
	} catch (errf) {console.log("ERROR: "+errf.message);}  
} 
function addFlags(url, currency) {
	try {
      /*if (url.indexOf("?") > -1) {
        url = url + "&fl="+flag.replace("flag-", "");
      }
      else {
        url = url + "?fl="+flag.replace("flag-", "");
      }*/
      url = url.replace(/\?currency=[a-zA-Z]{3,3}/gi, '?').replace(/&currency=[a-zA-Z]{3,3}/gi, '').replace('?&', '?').replace(/[\?&]$/, "");
      if ((currency) && (currency.length = 3)) {
        if (url.indexOf("?") > -1) {
          url = url + "&currency="+currency;
        }
        else {
          url = url + "?currency="+currency;
        }
      }
      return url;
	} catch (errf) {console.log("ERROR: "+errf.message);}  
}
</script>

  
  
  
  
  
