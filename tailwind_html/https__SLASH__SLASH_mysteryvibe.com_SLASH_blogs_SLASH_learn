<script>
function getParentUrl() {
    var isInIframe = (parent !== window),
        parentUrl = null;

    if (isInIframe) {
        parentUrl = document.referrer;
    }
    console.log(parentUrl);
    return parentUrl;
}
function geoWorld(ip_cou, ip_con, ip_store, ip_flag, ip_curr) {
	try { 
      var thisurl, newurl;
      var host = window.location.protocol + "//" + window.location.hostname;
      var path = window.location.href.replace(host, "");
      var ip_red = js_readCookie('ipnocountryredirect');
      if (ip_red == null && !getParentUrl()) {        
  		if (ip_con.length > 0) {
          	console.log("geoWorldRedirect LLC: "+ip_con+" || Store: "+ip_store);          
            if ((ip_store != null) && (ip_store != "")) {
              switch (ip_store) {
                 case "llc":
                      thisurl = "https://mysteryvibe.com" + path;
                      newurl = addFlags(thisurl, ip_curr);
                      if (newurl != thisurl) {
                        window.location = newurl;
                      }
                      else {
                        return null;
                      }
                    break;
                 case "world":
                    if (path.indexOf('/blogs/') > -1) {
                         newurl = addFlags("https://worldblog.mysteryvibe.com" + path, ip_curr);
                         console.log("REDIRECT TO "+newurl);
                         window.location = newurl; 
                    }
                  	else if ((path.indexOf('/search') > -1) && (path.indexOf('/article') > -1)) {
                         newurl = addFlags("https://worldblog.mysteryvibe.com" + path, ip_curr);
                         console.log("REDIRECT TO "+newurl);
                         window.location = newurl; 
                    }
                    else {
                         newurl = addFlags("https://world.mysteryvibe.com" + path, ip_curr);
                         console.log("REDIRECT TO "+newurl);
                         window.location = newurl;
                    }
                    break;      
              }
            }
            /*else {
              switch (ip_con) {
                 case "NA":
                 case "SA":
                     return null;
            		 break;                      
                 default: 
                     if (path.indexOf('/blogs/') > -1) {
                          window.location = "https://worldblog.mysteryvibe.com" + path; 
                     }
                     else {
                          window.location = "https://world.mysteryvibe.com" + path;
                     }
                     break;   
              }
            }*/
        }     
      }
      else {
		console.log("geoWorldRedirect: noredirect");
      }
	} catch (errf) {console.log("ERROR: "+errf.message);}  
}
</script>
