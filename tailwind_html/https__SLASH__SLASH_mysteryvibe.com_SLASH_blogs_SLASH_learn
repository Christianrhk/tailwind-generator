<script>
  $(".blog_border a").click(function(e){      
      e.preventDefault();
      $(".blog-article-item").removeClass("blog-special");
        $(this).parent().hide();
        $(".pagination-custom").css("display", "inline-block");
    });  
    
    $(".mobile-b-h1").click(
      function(){
        $(".mobile-b-menu ul").slideToggle();
    });
  
    var cur_url = $(location).attr('href');
    if(cur_url.includes('tagged')){      
      //$(".blog_border").hide();
      $(".pagination-custom").css("display", "inline-block");
      $(".blog-page-wrap").addClass("hide");
      $(".blog-page-wrap.default-wrap").removeClass("hide");
    }
</script>

<!-- /******************------Stick Menu -------*************/ -->

<script>
  var position = 350;   

  $(window).scroll(function() {
      var scroll = $(window).scrollTop();
      if(scroll > position) {
        $(".blog-header").addClass('b-header-fixed');
        $(".mobile-blog-menu").addClass('b-menu-fixed');        
      } else {
        $(".blog-header").removeClass('b-header-fixed');
        $(".mobile-blog-menu").removeClass('b-menu-fixed');                
      }
  });
</script>  
       
<!-- /******************------ Search Function -------*************/ -->

<script>  
  
  $("form[role='search'] .btn").click(function(e){
    var search_value = $("input[name='q']").val();
    if(search_value == ''){
      e.preventDefault();
    }
  });
  
  $("input[name='q']").keypress(function(e){
    var search_value = $(this).val();
    if(keycode == '13'){
      if(search_value == ''){
        e.preventDefault();
      }
    }
  });
  
</script>  






