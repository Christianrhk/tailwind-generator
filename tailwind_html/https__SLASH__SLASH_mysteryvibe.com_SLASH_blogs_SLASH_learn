    var customBanner = $('.announcement');

    customBanner.flexslider({
      animation: 'slide',
      direction: 'vertical',
      controlNav: false,
      directionNav: false,
      animationLoop: true,
      slideshowSpeed: 5000,
    });

    if (!Cookies.get('hide_banner')) {
      customBanner.show();
      Cookies.set('banner_seen', 'true');
    }

    $('.announcement .icon-x').on('click', function () {
      customBanner.hide();
      Cookies.set('hide_banner', 'true');
    });

    if (Cookies.get('banner_seen')) {
      customBanner.removeClass('purple');
    } else {
      setTimeout(function () {
        customBanner.removeClass('purple');
      }, 3000);
    }
  });
