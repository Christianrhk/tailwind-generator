<script>
    // Exit intent
    function addEvent(obj, evt, fn) {
        if (obj.addEventListener) {
            obj.addEventListener(evt, fn, false);
        }
        else if (obj.attachEvent) {
            obj.attachEvent("on" + evt, fn);
        }
    }

    // Exit intent trigger
    addEvent(document, 'mouseout', function (evt) {
      try {
        if (evt.toElement == null && evt.relatedTarget == null && !Cookies.get('exit_intent')) {
            $.fancybox.open([
                {
                    src: '#offer-popup',
                },
            ]);
            Cookies.set('exit_intent', 'true');
        };
      } catch (err) {console.log("EXIT INTENT ERROR: "+err.message);}
    });
    /*setTimeout(function() {
      try {
        if (!Cookies.get('exit_intent')) {
          $.fancybox.open([
            {
              src: '#offer-popup',
            },
          ]);
          Cookies.set('exit_intent', 'true');
        }
      } catch (err) {console.log("Email popup error: "+err.message);}
    }, 5000);*/
</script>


