<script type="text/javascript">
    // added for simple functionality
    function collapse(group) {
        let options = group.nextElementSibling
        options.classList.toggle('opacity-0');
        options.classList.toggle('h-0');
        if([...options.classList].includes('opacity-0')){
            group.children[1].innerHTML = "+"
        }else{
            group.children[1].innerHTML = "-"
        }
    }
</script>
