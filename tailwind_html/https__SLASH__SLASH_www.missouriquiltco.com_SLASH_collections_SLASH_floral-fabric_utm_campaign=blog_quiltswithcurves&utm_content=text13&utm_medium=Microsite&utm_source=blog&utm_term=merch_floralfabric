        mutations_list.forEach(function(mutation) {
          mutation.addedNodes.forEach(function(added_node) {
            var acsbTrigger = added_node.getAttribute('class')
            if(acsbTrigger.includes('acsb-trigger')) {
              $('div[data-acsb="trigger"]').removeClass('acsb-trigger-hidden');
              triggerObserver.disconnect();
            }
          
