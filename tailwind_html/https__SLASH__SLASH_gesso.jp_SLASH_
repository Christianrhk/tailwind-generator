<script>
jQuery(document).ready(function($){
    /*----------------------------------------------------
     pagetop
    ----------------------------------------------------*/
    $(window).on('load scroll', function() {
        var value = $(this).scrollTop();
        if ( value > 0 ) {
            $('body').addClass('scroll');
        } else {
            $('body').removeClass('scroll');
        }
    });
});
</script>

<script>
jQuery(document).ready(function($){
    /*----------------------------------------------------
     gmenu
    ----------------------------------------------------*/
    $('.gmenu-toggle').on('click', function() {
        $(this).parents('.gmenu').toggleClass('open');
    });

    $(".gmenu-main li a").click(function(){
        $('.gmenu').removeClass('open');
    })
});
</script>

<script>
jQuery(document).ready(function($){
    /*----------------------------------------------------
     トップページローディング
    ----------------------------------------------------*/
    if ($.cookie('loading')) {
        // ローダー隠す＆アニメーションロゴを表示
        window.setTimeout(function() {
            // アニメーションロゴを表示
            var date = new Date();
            var timestamp = date.getTime();
            $('.logo-main').css('background-image', 'url(//gesso.jp/wp-content/themes/gesso/img/gesso_white.gif?'+timestamp+')');
        }, 0);
        // アニメーションロゴが終わったあたりでフェイドアウト※目には見えない
        window.setTimeout(function() {
            $('.logo-main').css('opacity', 0);
        }, 3500);
        // 静止ロゴをフェイドイン
        window.setTimeout(function() {
            $('.logo-main').css('background-image', 'url(//gesso.jp/wp-content/themes/gesso/img/gesso_white-fin.png)');
            $('.logo-main').css('opacity', 1);
        }, 5500);
        // メニューをフェイドイン
        window.setTimeout(function() {
            $('.gmenu').css('opacity', 1);
            $('.cc-window').css('opacity', 1);
        }, 6500);
    } else {
        // 初読み込み
        // 左側の数字をローリング＆バー1段階目
        $('.loader').show();
        window.setTimeout(function() {
            var rand = Math.floor( Math.random()*10 );
            $('.loader-number-each:nth-child(1) > div').addClass('pos' + String(rand));
            $('.loader-bar').addClass('loader-bar-1');
        }, 2000);
        // 右側の数字をローリング＆バー2段階目＆ローダーをフェイドアウト
        window.setTimeout(function() {
            var rand = Math.floor( Math.random()*10 );
            $('.loader-number-each:nth-child(2) > div').addClass('pos' + String(rand));
            $('.loader-bar').addClass('loader-bar-2');
            $('.loader').css('opacity', 0);
        }, 3000);
        // バー3段階目
        window.setTimeout(function() {
            $('.loader-bar').addClass('loader-bar-3');
        }, 3300);
        // ローダー隠す＆アニメーションロゴを表示
        window.setTimeout(function() {
            // ローダー隠す
            $('.loader').hide();
            // アニメーションロゴを表示
            var date = new Date();
            var timestamp = date.getTime();
            $('.logo-main').css('background-image', 'url(//gesso.jp/wp-content/themes/gesso/img/gesso_white.gif?'+timestamp+')');
        }, 5500);
        // アニメーションロゴが終わったあたりでフェイドアウト※目には見えない
        window.setTimeout(function() {
            $('.logo-main').css('opacity', 0);
        }, 9000);
        // 静止ロゴをフェイドイン
        window.setTimeout(function() {
            $('.logo-main').css('background-image', 'url(//gesso.jp/wp-content/themes/gesso/img/gesso_white-fin.png)');
            $('.logo-main').css('opacity', 1);
        }, 11000);
        // メニューをフェイドイン
        window.setTimeout(function() {
            $('.gmenu').css('opacity', 1);
            $('.cc-window').css('opacity', 1);
        }, 12000);
        $.cookie('loading', 'done', { expires: 7 });
    }

});
</script>

