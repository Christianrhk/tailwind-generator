    el: '#app',
    data: {
        cart: {"id":1740557439122643,"attributes":[],"original_total_price":0,"total_discount":0,"total_price":0,"tax_price":0,"tax_rate":2500,"total_weight":0,"item_count":0,"items":[],"requires_shipping":true,"has_free_shipping":false,"is_recurring":false,"voucher_code":""},
        mobileMenu: false,
        subMenu: false,
        stylesToggleSubMenuClosed: {
            top: '-30px',
            opacity: 0,
            zIndex: -1
        },
        stylesToggleSubMenuOpened: {
            top: 0,
            opacity: 1,
            zIndex: 30
        },
        stylesToggleSubMenu: {
            top: '-30px',
            opacity: 0,
            zIndex: -1
        },
        accordion:  null,
        showModal: false,
        submitting: false,
        shippingLoading: true,

        shippingMethods: [],

        country: '348',
        zip: '',
        address: '',
        terms: '',
        paymentMethod: '',
        shipping_method: '',
        shipping_pickup_point: '',

    	money: moneyMethod,

		streetName: '',
        streetNumber: '',

        retailerBussinessOp1: 'Szalonok',
        retailerBussinessOp2: 'Kiskereskedők',
        retailerBussinessOp3: 'Terjesztés',
        retailerBussinessOp4: 'Más',
        retailerBussinessType: 'Szalonok',
        retailerTelephone: '',
        retailerCompanyName: '',
    },
    computed: {
        streetFull: function () {
            this.address = this.streetName + ' ' + this.streetNumber;
        	return this.address;
        },
        shippingMethodModel: function () {
        	return _.find (this.shippingMethods, (object) => {
            	return object.id === this.shipping_method;
            });
        },
        validZip: function () {
        	return this.zip.length >= 4;
        },
        totalPrice() {
        	let subtotal = this.cart.total_price;
            
            if (! this.shippingMethodModel) {
            	return subtotal;
            }

			return subtotal + this.shippingMethodModel.price
        },
        filteredShippingMethods: function () {
        	return _.filter(this.shippingMethods, (shippingMethod) => {

				if (this.cart.total_weight > 2000 && shippingMethod.handle === 'postnord-dk') {
                	return false;
                }

				// Check minimum price
            	if (shippingMethod.min_order_subtotal !== null && shippingMethod.min_order_subtotal > this.cart.total_price) {
                	return false;
                }

				// Check maximum price
            	if (shippingMethod.max_order_subtotal !== null && shippingMethod.max_order_subtotal < this.cart.total_price) {
                	return false;
                }
                // ">" syntax fix

				// Check Country
                let hasCountry = _.find (shippingMethod.countries, (country) => {
                    return country.id == this.country;
                }) !== undefined;
                if (! hasCountry) {
                	return false;
                }

            	return true;
            });
        },
    },
    watch: {
        shipping_method: function (value) {
        	this.resetPickupPoint()
        },
        country: function () {
    		this.updateByZip()
        },
        filteredShippingMethods: function (methods) {
            var currentMethod = _.find(methods, function(method) {
            	return method.id == this.shipping_method
            });

			if (! currentMethod) {
            	this.shipping_method = methods[0].id;
                return;
            }
        },
    },
    methods: {
        toggleSubMenu(subMenu) {
            // close mobileMenu if open and trying to open country menu
            if ('country' == subMenu) {
                this.mobileMenu = false
            }
            if (this.subMenu == subMenu) {
                this.subMenu = false;
                this.stylesToggleSubMenu.top = this.stylesToggleSubMenuClosed.top;
                this.stylesToggleSubMenu.opacity = this.stylesToggleSubMenuClosed.opacity;
                this.stylesToggleSubMenu.zIndex = this.stylesToggleSubMenuClosed.zIndex;
            } else {
                this.subMenu = subMenu
                this.stylesToggleSubMenu.top = this.stylesToggleSubMenuOpened.top;
                this.stylesToggleSubMenu.opacity = this.stylesToggleSubMenuOpened.opacity;
                this.stylesToggleSubMenu.zIndex = this.stylesToggleSubMenuOpened.zIndex;
            }
        },
        toggleMobileMenu() {
            // close mobileMenu if open and trying to open country menu
            if ('country' == this.subMenu) {
                this.subMenu = false;
                this.stylesToggleSubMenu.top = this.stylesToggleSubMenuClosed.top;
                this.stylesToggleSubMenu.opacity = this.stylesToggleSubMenuClosed.opacity;
                this.stylesToggleSubMenu.zIndex = this.stylesToggleSubMenuClosed.zIndex;
            }
            this.mobileMenu = ! this.mobileMenu
        },
        toggleAccordion(accordion) {
            if (accordion == this.accordion) {
                this.accordion = null
                return
            }
            this.accordion = accordion
        },
        accept(accordion) {
            this.showModal = false
            this.terms = 1
        },
        updateByZip() {
        	this.shippingLoading = true;
            axios.post('/checkout/shipping.js', {
                country: this.country,
                zip: this.zip,
                address:this.address
            })
            .then(response => {
                this.shippingMethods = response.data;
                this.resetPickupPoint();
        		this.shippingLoading = false;
            })
            .catch(e => {
                console.log(e)
            })

        },
        resetPickupPoint() {
            if (! this.shippingMethodModel) {
                return;
            }

            let store = _.head(this.shippingMethodModel.stores);

            this.shipping_pickup_point = store ? store.id : '';
        },
        cartHasProduct(productId) {
        	let item = _.find(this.cart.items, (item) => {
            	return item.product_id == productId;
            });

			return item !== undefined;
        },
        updateCartRow(item, type) {
			var quantity = item.quantity
            
            
			if (type == 'minus') {
            	quantity--
            }
            
            if (type == 'plus') {
            	quantity++
            }

            axios.post('/cart/update.js', {
                id: item.id,
                quantity: quantity,
            })
            .then(response => {
            	this.cart = response.data
            })
            .catch(e => {
                console.log(e)
            })
        },
        addToCart(variantId) {
        	var vm = this;
            axios.post('/cart/add.js', {
                id: variantId,
                quantity: 1,
            })
            .then(function (response) {
                vm.refreshCart();
            })
            .catch(e => {
                console.log(e)
            })
        },
        refreshCart() {
        	var vm = this;
            axios.get('/cart.js')
            .then(function (response) {
            	vm.cart = response.data;
            })
            .catch(e => {
                console.log(e)
            })
        },
        getExtrafieldsInfo() {
            let retailerTelephone = this.retailerTelephone;
            if (this.retailerTelephone == '') {
                retailerTelephone = 'empty';
            }
            let retailerCompanyName = this.retailerCompanyName;
            if (this.retailerCompanyName == '') {
                retailerCompanyName = 'empty';
            }
            return 'Company name: ' + retailerCompanyName +', Telephone: ' + retailerTelephone + ", Bussiness type: "+ this.retailerBussinessType;
        },
        debounceZip: _.debounce(function (e) {
            if (this.country == '208' && this.zip.length === 4) {
    			this.updateByZip();
            }
        	if (this.country == '752' && this.zip.length === 5) {
