/* Custom styles ************************/
html {
    height: 100%;
}
body {
    min-height: 100%;
    font-family: proxima-nova, sans-serif;
}
a {
    color: inherit;
    text-decoration: none;
}

.rte a {
    color: #e27769;
    text-decoration: underline;
}
.font-serif {
	font-family: 'Playfair Display', serif;
}

.btn-is-disabled{
	pointer-events: none; 
}

.fancy-checkbox input[type="checkbox"],
.fancy-checkbox .checked {
    display: none;
}
.fancy-checkbox input[type="checkbox"]:checked ~ .checked {
    display: inline-block;
}
.fancy-checkbox input[type="checkbox"]:checked ~ .unchecked {
    display: none;
} 

/* Hiren Style Start */

.text-handwriting{
	font-family: 'Pinyon Script', cursive;
}

:focus{
outline:0 none;
}

.container{
	max-width:1280px!important;
    width:100%;
}

.custom-select.nice-select {
    float: none;
    height: auto;
    line-height: normal;
    font-size: inherit;
    border: 2px solid #e8e8e8;
    border-radius:0;
}

.nice-select .list{
	width:100%;
}
.custom-select.nice-select.open, .custom-select.nice-select:active, .custom-select.nice-select:focus{
	border-color: #e8e8e8;
}

.custom-select.nice-select .list{
	border-radius:0;
}

.custom-select.nice-select .option.focus, .custom-select.nice-select .option.selected.focus, .custom-select.nice-select .option:hover{
	background-color:#fff;
}

.custom-select.nice-select .option{
	padding-top: 15px;
	padding-bottom: 15px;
    line-height:normal;
    font-weight: normal;
    border-bottom: 1px solid #ccc;
}

.transition{
	transition:.5s;
}

.product-tags span + span:before{
	width: 6px;
    height: 6px;
    border-radius: 50%;
    background: #000;
    display: inline-block;
    content: "";
    margin-left: 15px;
    margin-right: 15px;
    margin-bottom: 2px;
}

.richtext-box > *:not(:last-child){
	margin-bottom:1.75rem;
}

.list-with-check-bullet ul, .multi-list-style ul{
	padding-left:0;
    list-style:none;
}

.list-with-check-bullet li{
	position:relative;
    margin-bottom:6px;
    padding-left:30px;
}

.list-with-check-bullet li svg{
	position:absolute;
    left:0;
    top:4px;
}

.product-fancy-layout-img-overlay{
	margin-top:-180px;
    width:250px;
    height:250px;
    object-fit: cover;
    object-position: center;
}

.insta-cell{
    background-size:cover;
    background-position:center;
}

.insta-cell:before{
	content:"";
    width:100%;
    padding-top:100%;
    float:left;
}

.insta-cell:after{
	content:"";
    display:block;
    clear:both;
}

.multi-list-style ul li{
	border-top:1px solid #f5f5f5;
    padding:14px 10px 14px 30px;
    position:relative;
}

.multi-list-style ul li:last-child{
	border-bottom:1px solid #f5f5f5;
}

.multi-list-style ul li svg{
	position:absolute;
    top:18px;
    left:0;
}

.ingredient-box{
	padding-top:100%;
}

.ingredient-box > div{
	transition:.5s;
}

.ingredient-box > div:hover{
	opacity:0;
}

.ingredients-accordion-content{
	top:100%;
    z-index:10;
}


@media(max-width:1279px){
	.mobile-slider{
    	flex-wrap:nowrap;
        overflow:auto;
    }
	.mobile-slider .mobile-slider-item{
        flex-basis: 300px;
        flex-grow:1;
        flex-shrink:0;
    }
}

@media(max-width:767px){
	.product-fancy-layout-img-overlay{
    	margin-top: -100px;
        width: 150px;
        height: 150px;
        transform:translatex(1rem);
    }
}

@media(min-width:1280px){
	.product-fancy-layout-third-img{
        margin-top:-150px;
    }
}


.description {
    transition: max-height .5s ease-in-out;
    overflow: hidden;
}

.description .gradient {
    opacity: 0;
    transition: all .5s ease-in-out;
}

.description.minimized .gradient {
    opacity: 1;
}

/* Hiren Style End */


@media (min-width: 992px) {
    .form-box {
        width: 500px;
    }
}
.max-h-md {
    max-height: 40rem;
}
.cart-count {
    position: absolute;
    top: -3px;
    right: -2px;
    font-size: .75rem;
    min-width: 1rem;
    text-align: center;
    border-radius: 9999px;
}
.justify-evenly {
    justify-content: space-around;
}
@media (min-width: 768px) {
    .main-menu {
        display: block !important;
    }
    .desktop-first{
    	order:-1;
    }
}













.index-banner {
	background-image: url('https://betacdn.codefort.com/pearlwax-beta/files/index-main-banner-sm.jpg');
}

@media (min-width: 768px) {
    .index-banner {
        background-image: url('https://betacdn.codefort.com/pearlwax-beta/files/index-main-banner-lg.jpg');
    }
    .md\:block-important {
    	display: block !important;
    }
}






















/* How to page Styles Start */
.main .bg-right {
    width:80vw;
    transform: translateX(-50%);
}
.guide .js-question,
.faqs .js-question {
    background-color: #E4D1C9;
}
.tips-tricks .text-content {
    bottom: -200px;
}

.steps .step-1 .arrow {
    bottom: -20px;
    left: 88px;
}
.steps .step-2 .arrow {
    left: 90px;
    bottom: -15px;
}
.steps .step-3 .arrow {
    left: 82px;
    top: -55px;
    transform: rotate(-45deg);
}
.steps .step-1 .product-img {
    max-height: 300px;
    bottom: -21px;
}
.steps .step-2 .product-img {
    max-height: 300px;
    width: auto;
}
.steps .step-3 .product-img {
    max-height: 400px;
    width: auto;
}

@media(max-width:1500px){
	.main .bg-right {
        width:110vw;
    }
}
@media(max-width:1220px){
	.main .bg-right {
        width:120vw;
    }
}
@media(max-width:767px){
    .main .bg-right {
        width:100vw;
    }

    .tips-tricks .text-content {
        bottom: 0px;
        top: -20px;
        margin-top: -100px;
    }
    .steps .step-1 .arrow {
        bottom: 18px;
        left: 45px;
    }
    .steps .step-1 .product-img {
        max-height: auto;
        bottom: 0;
    }
    .steps .step-1 .product-img img {
        max-height: 300px;
    }
    .steps .step-3 .arrow {
        left: 82px;
        top: -22px;
        transform: rotate(-45deg);
    }
    .steps .step-3 .product-img {
        max-height: 200px;
    }
}

