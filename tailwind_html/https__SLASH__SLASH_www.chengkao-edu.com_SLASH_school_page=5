    <script type="text/javascript">

        $("#searchForm img").click(function(){
             $("#searchForm").submit();
        });

        $("#applyForm").submit(function (event) {
            $.ajax({
                type: 'post',
                url: $("#applyForm").attr('action'),
                data: $("#applyForm").serialize(),
                dataType: "json",
                success: function (data) {
                    if (data.status == 'success') {
                        layer.msg(data.message, {
                            icon: 1,
                            time: 2000
                        }, function () {
                            window.location.href = $("#applyForm").attr('target-url');
                        });
                    } else {
                        var msg = '';
                        if(data.message instanceof Array){
                            msg = data.message.join(',');
                        }else{
                            msg = data.message;
                        }
                        console.log(msg);
                        layer.open({
                            icon: 2,
                            title: '请检查表单数据',
                            content: msg,
                            offset: 'auto',
                        })
                    }
                },
                error: function (err) {
                    console.log(err.statusText);
                    console.log('异常');
                }
            });
            event.preventDefault();
            return false;

        });
