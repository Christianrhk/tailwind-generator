		  $('.cat_4356').click(function(e) {
	  var evtobj=window.event? event : e;
	     var go_to_url ="https://www.phonandroid.com/cat/actu-generaliste";
	     
	     if(evtobj.ctrlKey ||   evtobj.metaKey ||  evtobj.which == 2){
	     			//e.preventDefault();
	     			//$(this).target = "_blank";
	     			window.open(go_to_url);
	     			return false;
	     	} 
	     else{
	         document.location.href = go_to_url;
	          return false;
	      } 
	     	  	
	   });
	  
