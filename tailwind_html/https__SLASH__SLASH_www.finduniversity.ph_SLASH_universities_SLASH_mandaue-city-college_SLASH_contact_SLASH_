<script type="text/javascript">
    function navigateToFbAuth() {
        var cbPolicyConfirmation = document.getElementById("cbPolicyConfirmation");
        var txtPolicyConfirmation = document.getElementById("txtPolicyConfirmation");

        if (cbPolicyConfirmation.checked === true) {
            window.location.href = "https://www.edukasyon.ph/user/signup";
        }
        else {
            txtPolicyConfirmation.style.display = "block";
        }
    }

    function navigateToGoogleAuth() {
        var cbPolicyConfirmation = document.getElementById("cbPolicyConfirmation");
        var txtPolicyConfirmation = document.getElementById("txtPolicyConfirmation");

        if (cbPolicyConfirmation.checked === true) {
            window.location.href = "https://www.edukasyon.ph/user/signup";
        }
        else {
            txtPolicyConfirmation.style.display = "block";
        }
    }

    function closeModal() {
        var x = document.getElementById("registerModal");
        x.style.display = "none";
    }
</script>

