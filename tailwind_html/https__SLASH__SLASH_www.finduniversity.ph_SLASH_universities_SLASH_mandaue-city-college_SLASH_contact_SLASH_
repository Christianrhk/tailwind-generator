</footer><!-- /.footer -->


<script type="text/javascript">
    /*
     * Top nav bar is being cloned and injected to the mobile menu while it opens and that makes the nav duplicate.
     * It can be noticed when the window resizes from mobile to tablet.
     * Close the mobile menu on window resize to prevent this.
    */
    var resizeId;

    $(window).on("resize", function () {
        clearTimeout(resizeId);
        resizeId = setTimeout(doneResizing, 250);
    });

    function doneResizing(){
        if ($('.top-nav-mobile').length) {
            if ($('.top-nav-mobile').css('display') === "block") {
                $(".nav-trigger").trigger("click");
            }
        }
    }
</script>
        </div>
