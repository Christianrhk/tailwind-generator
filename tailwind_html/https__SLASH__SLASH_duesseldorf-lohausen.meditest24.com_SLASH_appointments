            const confirmIntendDiv = $('#confirm_intend_div');

            const reasonSelect = $('#frontpage_book_appointment_testReason');


            // reasonSelection.hide();

                            const paymentMethodWrapper = $('#payment_method_wrapper');
                const paymentMethodInput = $('#frontpage_book_appointment_paymentMethod');
                const offlineRadioButton = $('#js-payment-offline');
                const onlineRadioButton = $('#js-payment-online');

                                    onlineRadioButton.change(function () {
                        if (onlineRadioButton.is(':checked')) {
                            paymentMethodInput.val('online');
                            paymentMethodInput.change();
                        }
                    })
                
                offlineRadioButton.change(function () {
                    if (offlineRadioButton.is(':checked')) {
                        paymentMethodInput.val('offline');
                        paymentMethodInput.change();
                    }
                })

                if(paymentMethodInput.val() === 'online') {
                                        onlineRadioButton.prop("checked", true);
                    onlineRadioButton.change();
                                    } else {
                    offlineRadioButton.prop("checked", true);
                    offlineRadioButton.change();
                }
            
            submitButton.click(function (event) {
                event.preventDefault();
                submitButton.prop('disabled', true);
                submitButton.html('<div class="spinner-border text-light" role="status"><span class="sr-only"></span></div>')
                $("#frontpage_book_appointment_book").click();
            });

            timeSlotErrorSpan.hide();
            agbDiv.hide();
            confirmIntendDiv.hide();
            invoiceAddressContainer.hide();
            passwordRow.hide();

            $("#day_select option").each(function () {
                const val = $(this).val();
                if (availableSlots[val].length === 0) {
                    $(this).remove();
                }
            });
            let carryOverTime = -1;
            let lastSelectedDay = null;
            if (!invoiceIsSameCheckbox.is(':checked')) {
                invoiceAddressForm.hide();
            } else {
                invoiceAddressForm.show();
            }

            if (!onlyVisitingCheck.is(':checked')) {
                justVisitingFields.hide();
            } else {
                justVisitingFields.show();
            }

            if (!createAccountCheckbox.is(':checked')) {
                passwordRow.hide();
            } else {
                passwordRow.show();
            }

            createAccountCheckbox.change(function () {
                if (!createAccountCheckbox.is(':checked')) {
                    passwordRow.hide();
                } else {
                    passwordRow.show();
                }
            });
            invoiceIsSameCheckbox.change(function () {
                if (!invoiceIsSameCheckbox.is(':checked')) {
                    invoiceAddressForm.hide();
                } else {
                    invoiceAddressForm.show();
                }
            })
            onlyVisitingCheck.change(function () {
                if (!onlyVisitingCheck.is(':checked')) {
                    justVisitingFields.hide();
                } else {
                    justVisitingFields.show();
                }
            })

            if (currentStreetLabel && currentStreetLabel.children().length > 0) {
                if (!onlyVisitingCheck.is(':checked')) {
                    onlyVisitingCheck.attr("checked", "checked");
                    justVisitingFields.show();
                }
            }

            if (productHiddenVal.val()) {
                $(`input[name='testType'][value='${productHiddenVal.val()}']`).attr("checked", "checked")
            }

            if (appointmentDayHiddenVal.val()) {
                daySelect.val(appointmentDayHiddenVal.val());
            }

            if (appointmentSlotHiddenVal.val() && parseInt(appointmentSlotHiddenVal.val()) !== -1) {
                const val = appointmentSlotHiddenVal.val();
                if (!isNaN(val)) {
                    setTimeout(function () {
                        hourSelect.val(parseInt(Math.floor(val / 60))).change();
                        setTimeout(function () {
                            slotSelect.val(val).change();
                        })
                    }, 100);
                }
            }

            
            const eyeButton = $('#eye_button');
            const passwordField = $('#frontpage_book_appointment_password');
            eyeButton.on('click', function (e) {
                e.preventDefault();
                if(passwordField.attr("type") === 'text') {
                    eyeButton.html(feather.icons['eye-off'].toSvg());
                    passwordField.attr('type', 'password');
                } else {
                    eyeButton.html(feather.icons['eye'].toSvg());
                    passwordField.attr('type', 'text');
                }
            });

            reasonSelect.change(function() {
                updateCheckout();
            }).change();

            productRadio.click(function () {
                updateCheckout();
                const selectedDay = $("#day_select option:selected").val();
                // LOOK HERE IF THEY CHANGE THE DATES AGAIN!
                const isAfterTestVChange = getMomentForKey(selectedDay).isSameOrAfter('2022-07-01');
                //const isAfterFreeAgain = getMomentForKey(selectedDay).isSameOrAfter('2021-11-13');
                if (isAfterTestVChange) {
                    calulateCheckoutButtonState();
                    checkBirthday();
                    const prodId = $(`input[name='testType']:checked`).val();

                        if ('-1' === prodId) {
                            reasonSelection.show();
                        } else {
                            reasonSelection.hide();
                        }

                    // if (!isAfterFreeAgain) {
                    //     if ('-1' === prodId) {
                    //         reasonSelection.show();
                    //     } else {
                    //         reasonSelection.hide();
                    //     }
                    // } else {
                    //     reasonSelection.hide();
                    // }
                }
            });

            function calulateCheckoutButtonState() {
                const selectedDay = $("#day_select option:selected").val();
                // LOOK HERE IF THEY CHANGE THE DATES AGAIN!
                const isAfterTestVChange = getMomentForKey(selectedDay).isSameOrAfter('2022-07-01');
                const isAfterFreeAgain = getMomentForKey(selectedDay).isSameOrAfter('2021-11-13');

                const checkAgb = function () {
                    const prodId = $(`input[name='testType']:checked`).val();
                    if ($(`input[name='testType']:checked`).data('netPrice') === 0) {
                        // if (!isAfterTestVChange) {
                        //     submitButton.prop('disabled', false);
                        // } else {
                        //     submitButton.prop('disabled', !(isFree() || reasonSelected()));
                        // }
                        submitButton.prop('disabled', false);
                    } else {
                        if (!agbCheck.is(':checked')) {
                            submitButton.prop('disabled', true);
                        } else {
                            if (prodId === '-1') {
                                submitButton.prop('disabled', !reasonSelected());
                            } else {
                                submitButton.prop('disabled', false);
                            }
                        }
                    }
                }

                if (isAfterTestVChange /* && !isAfterFreeAgain*/) {
                    const prodId = $(`input[name='testType']:checked`).val();
                    if ('-1' === prodId) {
                        if (isFree()) {
                            submitButton.prop('disabled', false);
                        } else {
                            submitButton.prop('disabled', true);
                            checkAgb();
                        }
                    } else {
                        checkAgb();
                    }
                } else {
                    checkAgb();
                }
            }

            function updateCheckout() {
                const selected = $(`input[name='testType']:checked`);
                productHiddenVal.val(selected.val());

                let subTotalText = selected.data('netPriceFormatted');
                let totalText = selected.data('netPriceFormatted');
                let netPrice = selected.data('netPrice');

                if (isFree() && selected.val() === '-1') {
                    subTotalText = '0,00 €';
                    totalText = subTotalText;
                    netPrice = 0;
                }

                subtotal.text(subTotalText)
                total.text(totalText)
                if (selected.data('cwaEnabled') === true) {
                    cwaWrapper.show();
                } else {
                    cwaWrapper.hide();
                }
                if (selected.val() === '-1') {
                    confirmIntendDiv.show();
                } else {
                    confirmIntendDiv.hide();
                }

                if (netPrice === 0) {
                    submitButton.text('Termin buchen')
                    agbDiv.hide();
                    invoiceAddressContainer.hide();
                                        paymentMethodWrapper.hide();
                                    } else {
                    submitButton.text('Kostenpflichtig bestellen')
                    agbDiv.show();
                    invoiceAddressContainer.show();
                                        paymentMethodWrapper.show();
                                    }
                calulateCheckoutButtonState();
            }

            agbCheck.change(function () {
               if (!$(this).is(':checked'))  {
                   submitButton.prop('disabled', true);
               } else {
                   submitButton.prop('disabled', false);
               }
            });

            firstNameInput.change(function () {
                if (!invoiceIsSameCheckbox.is(':checked')) {
                    firstNameHiddenInput.val($(this).val());
                }
            });

            birthdayInput.change(function () {
                checkBirthday();
            });

            birthdayInput.on('hide', function () {
                checkBirthday();
            })

            function checkBirthday() {
                const selectedDay = $("#day_select option:selected").val();
                // LOOK HERE IF THEY CHANGE THE DATES AGAIN!
                const isAfterTestVChange = getMomentForKey(selectedDay).isSameOrAfter('2022-07-01');
                const isAfterFreeAgain = getMomentForKey(selectedDay).isSameOrAfter('2021-11-13');
                if (isAfterTestVChange/* && !isAfterFreeAgain*/) {
                    const prodId = $(`input[name='testType']:checked`).val();
                    if (!isFreeBasedOnBirthday() && '-1' === prodId) {
                        reasonSelection.show();
                    } else {
                        reasonSelection.hide();
                    }
                }
                updateCheckout();
            }

            lastNameInput.change(function () {
                if (!invoiceIsSameCheckbox.is(':checked')) {
                    lastNameHiddenInput.val($(this).val());
                }
            });

            streetInput.change(function () {
                if (!invoiceIsSameCheckbox.is(':checked')) {
                    streetHiddenInput.val($(this).val());
                }
            });

            zipInput.change(function () {
                if (!invoiceIsSameCheckbox.is(':checked')) {
                    zipHiddenInput.val($(this).val());
                }
            });

            cityInput.change(function () {
                if (!invoiceIsSameCheckbox.is(':checked')) {
                    cityHiddenInput.val($(this).val());
                }
            });

            invoiceFirstName.change(function () {
                if (invoiceIsSameCheckbox.is(':checked')) {
                    firstNameHiddenInput.val($(this).val());
                }
            });

            invoiceLastName.change(function () {
                if (invoiceIsSameCheckbox.is(':checked')) {
                    lastNameHiddenInput.val($(this).val());
                }
            });

            invoiceStreet.change(function () {
                if (invoiceIsSameCheckbox.is(':checked')) {
                    streetHiddenInput.val($(this).val());
                }
            });

            invoiceZip.change(function () {
                if (invoiceIsSameCheckbox.is(':checked')) {
                    zipHiddenInput.val($(this).val());
                }
            });

            invoiceCity.change(function () {
                if (invoiceIsSameCheckbox.is(':checked')) {
                    cityHiddenInput.val($(this).val());
                }
            });

            invoiceCountry.change(function () {
                if (invoiceIsSameCheckbox.is(':checked')) {
                    countryHiddenInput.val($(this).val());
                }
            }).change();

            countryInput.change(function () {
                if (!invoiceIsSameCheckbox.is(':checked')) {
                    countryHiddenInput.val($(this).val());
                }
            }).change();

            daySelect.change(function () {

                calulateCheckoutButtonState();
                hours = [];
                day = $("#day_select option:selected").val();
                const isAfterTestVChange = getMomentForKey(day).isSameOrAfter('2022-07-01');
                // LOOK HERE IF THEY CHANGE THE DATES AGAIN!
                const isAfterFreeAgain = getMomentForKey(day).isSameOrAfter('2021-11-13');
                const schnelltestPrice = $('#schnelltest_price');
                if (isAfterTestVChange) {
                    const prodId = $(`input[name='testType']:checked`).val();
                    if ('-1' === prodId) {
                        reasonSelection.show();
                    } else {
                        reasonSelection.hide();
                    }
                } else {
                    $(`input[name='testType']:checked`).data('netPrice', 0);
                    $(`input[name='testType']:checked`).data('netPriceFormatted', '0,00 €');
                    schnelltestPrice.text('(Kostenlos / Bürgertest)');
                    reasonSelection.hide();
                }
                updateCheckout();

                // if (isAfterTestVChange && !isAfterFreeAgain) {
                //     const prodId = $(`input[name='testType']:checked`).val();
                //     if ('-1' === prodId) {
                //         reasonSelection.show();
                //     } else {
                //         reasonSelection.hide();
                //     }
                // } else {
                //     reasonSelection.hide();
                // }
                appointmentDayHiddenVal.val(day);
                hourSelect.find('option')
                    .remove()
                    .end();
                availableSlots[day].forEach(function (slot) {
                    const hour = parseInt(Math.floor(slot / 60));
                    if (!hours.includes(hour)) {
                        hours.push(hour);
                    }
                })
                hours.forEach(function (h) {
                    hourSelect.append(new Option(h, h));
                    // If available hours contains carry over time set select value
                    if (carryOverTime > -1 && Math.floor(carryOverTime / 60) === h) {
                        hourSelect.val(h);
                    }
                })
                if (isWeekend(day)) {
                    const testType = $(`input[name='testType']:checked`).data("testType");
                    if (testType !== 'antigen') {
                        $(`input[name='testType'][value='-1']`).attr("checked","checked")
                        updateCheckout();
                    }
                    $(`input[name='testType']`).each((key, val) => {
                        const prodVal = $(val);
                        // if (prodVal.data('testType') !== 'antigen') {
                        //     prodVal.prop('disabled', true);
                        //     prodVal.prop('checked', false);
                        // }
                    })
                } else {
                    $(`input[name='testType']`).each((key, val) => {
                        const prodVal = $(val);
                        // if (prodVal.data('testType') !== 'antigen') {
                        //     prodVal.prop('disabled', false);
                        // }
                    })
                }
                hourSelect.change(function () {
                    slotSelect.find('option')
                        .remove()
                        .end();
                    const hour = parseInt($(this).val());
                    const availableForDay = availableSlots[day];
                    //Add place holder slot to options
                    slotSelect.append(new Option("- Bitte auswählen -", -1));
                    appointmentSlotHiddenVal.val(-1);
                    slotSelect.val(-1);
                    let couldCarryOverSlot = false;
                    if (availableForDay) {
                        const min = hour * 60;
                        const max = min + 59;
                        availableForDay.forEach(function (slot) {
                            if (slot >= min && slot < max) {
                                slotSelect.append(new Option(formatSlot(slot), slot));
                                if (carryOverTime === slot) {
                                    slotSelect.val(slot);
                                    appointmentSlotHiddenVal.val(parseInt(slot));
                                    couldCarryOverSlot = true;
                                }
                            }
                        });
                        //Show warning only when switching between days
                        if (!couldCarryOverSlot && carryOverTime > -1 && lastSelectedDay && lastSelectedDay !== day) {
                            slotSelect.val(-1);
                            timeSlotErrorSpan.show();
                        }
                    }
                }).change();
                slotSelect.change(function () {
                    appointmentSlotHiddenVal.val(parseInt($(this).val()));
                    carryOverTime = parseInt($(this).val());
                    //Hide warning when new valid value is selected
                    if (parseInt($(this).val()) > -1) {
                        timeSlotErrorSpan.hide();
                    }
                }).change();
                // Needs to be set last in order to perform checking logic above
                lastSelectedDay = day;
            }).change()
            updateCheckout();
            calulateCheckoutButtonState();
            checkBirthday();

            function reasonSelected() {
                const selectedReason = $('#frontpage_book_appointment_testReason option:selected').val();

                return selectedReason === 'indoor_activity'  || selectedReason === 'visits_vulnerable_person'  || selectedReason === 'red_cwa' ;
            }

            function isFree() {
                const selectedReason = $('#frontpage_book_appointment_testReason option:selected').val();
                return isFreeBasedOnBirthday() ||
                selectedReason === 'can_not_vaccinate'  || selectedReason === 'in_vaccine_study'  || selectedReason === 'has_covid'  || selectedReason === 'infection_in_household'  || selectedReason === 'visits_hospital'  || selectedReason === 'in_ambulant_care'  || selectedReason === 'provides_care_p19_sgb_xi'  || selectedReason === 'budget_p29_sgb_ix' ;
            }

            function isFreeBasedOnBirthday() {
                const selectedDay = $("#day_select option:selected").val();
                let birthday = getMomentForBirthday(birthdayInput.val());
                const presetBirthday = $('#frontpage_book_appointment_bookingPreset option:selected');
                if (presetBirthday && presetBirthday.data('birthday') &&presetBirthday.data('birthday') !== 'nan.') {
                    birthday = moment(presetBirthday.data('birthday'));
                }
                let age = 5; // 18 years
                return getMomentForKey(selectedDay).diff(birthday,'years', true) < age
            }
        });

        function isWeekend(day) {
            const date = getMomentForKey(day);
            return date.day() === 0 || date.day() === 6;
        }

        function getMomentForKey(key) {
            const dayParts = key.split('-');
            return moment(`${dayParts[2]}${dayParts[1]}${dayParts[0]}`);
        }

        function getMomentForBirthday(key) {
            const dayParts = key.split('.');
            return moment(`${dayParts[2]}${dayParts[1]}${dayParts[0]}`);
        }

        function formatSlot(slot) {
            return `${parseInt(Math.floor(slot / 60)).toString().padStart(2, '0')}:${(slot % 60).toString().padStart(2, '0')} - ${parseInt(Math.floor((slot + slotInterval) / 60)).toString().padStart(2, '0')}:${((slot + slotInterval) % 60).toString().padStart(2, '0')}`
        }
    </script>

</head>

