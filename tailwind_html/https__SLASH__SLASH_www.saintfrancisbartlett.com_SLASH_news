                    <iframe :title="VideoModalTitle"  width="100%" height="350" :src="VideoUrl"  style="border:0;" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
                </div>
                <div class="modal-footer flex justify-end pt-2">
                    <button class="inline-flex btn btn-primary hover:underline leading-normal" v-on:click="CloseYoutubeVideo($event)">Close</button>
                </div>
            </div>
        </div>
    </div>
</div>
    <!--END: modal Dialog-->
