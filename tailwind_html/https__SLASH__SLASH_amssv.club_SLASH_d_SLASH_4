<script type="text/javascript">
      loadstatus();
      function loadstatus(){
        
              jQuery.get("https://api.mc-list.xyz/amssv.club", function( data ) {
                if (data.status === "online"){
                  jQuery("#nowplayer").html("Online " + data.player.now + " / " + data.player.max);
                } else {
                  jQuery("[data-online]").hide();
                  jQuery("[data-offline]").show();
                  jQuery("#reason").html("伺服器目前為離線狀態，暫時無法加入。");
                }              
                jQuery("#ref_serverstatus").show();
              });        
                  }

      jQuery(document).on("click", "#ref_serverstatus", function() {
        jQuery("#nowplayer").html("Loading ...");
        jQuery("#ref_serverstatus").hide();
        loadstatus();
      });

      var clipboard = new ClipboardJS('#copy');
      clipboard.on('success', function(e) {
        e.clearSelection();
        change();
      });
      var id = 0;
      function change() {
        if(id > 0){
          clearTimeout(id);
        } else {
          jQuery("#copy").html("已複製連線位置");
        }
        id = setTimeout(function() {
          jQuery("#copy").html("amssv.club");
          id = 0;
        }, 3000);
      }
    </script>
