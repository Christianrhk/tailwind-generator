    });
    $("#editArticle").on("show.bs.modal", function () {
        $('#editArticle').remove('full-screen');
        $('#editArticle .ck.ck-toolbar__items .editor-zoom-in').remove();
        $('#editArticle .ck.ck-toolbar__items .editor-zoom-out').remove();
        $('#editArticle .ck.ck-toolbar__items').append('<button class="ck ck-button ck-off editor-zoom-out" type="button" tabindex="-1">' +
            '<!--begin::Svg Icon | path: assets/media/icons/duotone/Design/ZoomMinus.svg-->\n' +
