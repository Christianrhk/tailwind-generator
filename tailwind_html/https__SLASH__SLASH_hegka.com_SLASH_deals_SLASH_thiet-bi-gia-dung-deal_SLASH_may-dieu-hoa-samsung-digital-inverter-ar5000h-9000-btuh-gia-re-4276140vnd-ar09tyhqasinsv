    window.addEventListener('alert', event => {
        let time = 3000

        $.notify({
            // options
            message: event.detail.message ?? 'Thông báo',
        }, {
            // settings
            element: 'body',
            position: null,
            allow_dismiss: true,
            newest_on_top: false,
            showProgressbar: false,
            placement: {
                from: "top",
                align: "right"
            },
            offset: 20,
            spacing: 10,
            z_index: 1031,
            type: event.detail.type,
            timer: time,
            delay: time,
            animate: {
                enter: 'animated fadeInDown',
                exit: 'animated fadeOutUp'
            },
            icon_type: 'class',
        });
    });
    $(document).on('click', '#back-to-top', function () {
        document.body.scrollTop = 0;
        document.documentElement.scrollTop = 0;
    })
    $(document).on('click', '.notify-new-post .notify', function () {
        document.body.scrollTop = 0;
        document.documentElement.scrollTop = 0;
        $(this).remove()
    })
    $(document).on('click', '#author-editor .modal-body button.active-post', function () {
        $('#author-editor .modal-body button.active-article .img-nav img').attr('src', '/assets/new-article.png')
    })
    $(document).on('click', '#author-editor .modal-body button.active-article', function () {
        $('#author-editor .modal-body button.active-article .img-nav img').attr('src', '/assets/new-article-after.png')
    })
    var check = true;

    // window.onscroll = function (ev) {
    //     heightScroll = window.scrollY;
    //     if (heightScroll < 300) {
    //         $('.notify-new-post').html('');
    //     }
    // };
    // window.onscroll = function (ev) {
    //     if ((window.innerHeight + window.scrollY) >= $('.main-body').height() - 100) {
    //         setTimeout(function () {
    //             $('.article-read-more').click();
    //         }, 100);
    //     }
    // };
    $(function () {
        if ($(window).innerWidth() <= 992) {
            var lastScrollTop = 0, delta = 5;
            $(window).scroll(function () {
                var nowScrollTop = $(this).scrollTop();
                if (Math.abs(lastScrollTop - nowScrollTop) >= delta) {
                    if (nowScrollTop > lastScrollTop && nowScrollTop > 60) {
                        $('#kt_header').addClass('border-bt')
                        $('#kt_toolbar').addClass('d-none')
                        $('#visiter-page .left-section').addClass('top-5px')
                        $('#my-progress.progress-fixed').addClass('top-55px')
                    } else {
                        $('#kt_header').removeClass('border-bt')
                        $('#kt_toolbar').removeClass('d-none')
                        $('#visiter-page .left-section').removeClass('top-5px')
                        $('#my-progress.progress-fixed').removeClass('top-55px')
                    }
                    lastScrollTop = nowScrollTop;
                }
            });
        }
    });
    var widthWindows = window.innerWidth;
    // if (widthWindows > 992) {
    //     var copyrightTop = $("#rightbar").outerHeight()
    //     var outerWidth = $("#trending-tags .trending").width();
    //     $(window).scroll(function () {
    //         var eTop = $(window).scrollTop();
    //
    //         if (eTop >= (copyrightTop + 75)) {
    //             $(".copyright-sidebar-footer").addClass('position-fixed')
    //             $("#back-to-top").addClass('position-fixed')
    //             $(".copyright-sidebar-footer").removeClass('w-100')
    //             $(".copyright-sidebar-footer").css('top', 125).css('width', 270)
    //         } else {
