        <div class="container">
            
                <div class="row content-area" id="primary">


                    <aside id="secondary" class="widget-area archive-sidebar col-md-4" role="complementary">

                        


<div class="site-sidebar-archive">
	</div>

<h3>Hae ajankohtaista sisÃ¤ltÃ¶Ã¤</h3>

<div class="archive-search-container well">
	<form class="form advanced-search" method="get">
		<input type="hidden" name="search_post_type" value="post" />	
    	<div class="form-group">
        	<label class="control-label" for="title_keywords">Hakusana</label>
        	<input type="text" class="form-control" name="title_keywords" id="title_keywords" value="">
    	</div>
