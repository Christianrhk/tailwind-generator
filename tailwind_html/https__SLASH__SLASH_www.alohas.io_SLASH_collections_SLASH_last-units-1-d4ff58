              {% if product.compare_at_price > product.price %}
              {%- unless product.tags contains 'status:lst' or product.tags contains 'status:lstd' -%}
              style="color:{{settings.accent_sustainability}}"
              {%endunless%}
              {%endif%}>
              {% if product.compare_at_price > product.price %}
              {{ product.price_min | money }}
              {%else%}
              {{ product.price | money }}
              {%endif%}
              </span>
              {% if product.compare_at_price > product.price %}
                {%-unless product.tags contains 'status:lst' or product.tags contains 'status:lstd' -%}
                  <span class="preorder-tag _ml-2 _uppercase _text-sm _font-bold" style="color:{{settings.accent_sustainability}}">Pre-order</span>
                {%- endunless -%}
              {% endif %}
              
            {% else %}
              <span class="sold-out">Sold Out</span>
            {% endif %}
        </span>
      </div>
    </a>
  </div>

  <div data-color="{{-product.metafields.my_fields.color_info-}}" class="color-collection-page _flex _flex-row _items-center _justify-center {%if product.metafields.my_fields.color_info == nil %}_hidden{%endif%}">
  {% assign numberOfColors = product.metafields.my_fields.color_info.all_products.size %}
  {%assign currentColor = product.metafields.my_fields.color_info.selected_product%}
    <a class="_cursor-pointer">
      <div onclick="window.handleProductChange(event, '{{currentColor.handle}}')" id="color-swatch-collection" class="_p-2 lg:_p-1.5">
        <img data-image="{{clickImage | img_url: '300x' }}" data-hover-image="{{hoverImage | img_url: '300x' }}" alt="{{colorOption.color}}" class="_rounded-full _w-6 lg:_w-7 _border _border-black _border-solid"
          src="https://cdn.shopify.com/s/files/1/0762/7039/files/{{currentColor.color_format}}.png" />
      </div>
    </a>
  {% assign selected_color_found = false %}
  {% for colorOption in product.metafields.my_fields.color_info.all_products limit: 3 %}
    {% capture productHandle %}{{colorOption.handle}} {%endcapture%}
    {% assign productHandle = productHandle | handle %}
    {% assign waitList = colorOption.wait_li %}
    {% assign soldOut = colorOption.sold_out %}
    {% assign shownColors = 3 %}
  {%if productHandle == currentColor.handle%}
    {% assign selected_color_found = true %}
    {%continue%}
  {%else%}
    {%if selected_color_found == false and forloop.index == 3 %}
      {%break%}
    {%else%}
    {%if colorOption.color_format == ''%}
    {% assign shownColors = shownColors | minus: 1 %}
    {%else%}
      <a class="_cursor-pointer ">
        <div onclick="window.handleProductChange(event, '{{productHandle}}', '€{{amount_with_comma_separator}}', '{{waitList}}', '{{soldOut}}' )" id="color-swatch-collection" class="_p-2 lg:_p-1.5">
          <img data-image="{{clickImage | img_url: '300x' }}" data-hover-image="{{hoverImage | img_url: '300x' }}"
            alt="{{colorOption.color}}" class="_rounded-full _w-6 lg:_w-7 _border _border-black _border-solid"
            src="https://cdn.shopify.com/s/files/1/0762/7039/files/{{colorOption.color_format}}.png"/>
        </div>
      </a>
      {%endif%}
   {%endif%}
  {%endif%}
  {%endfor%}
 
   {%if numberOfColors > 3 %}
    <a class="_cursor-pointer" href="{{ product.url}}">
     <div class="_flex _items-center _justify-center _text-black _text-xl">
       <p class="_m-0 _p-0.5 _text-black _text-xl _font-medium"> <span>+</span> {{numberOfColors | minus: shownColors }}</p>
     </div>
    </a>
   {%endif%}
 </div>

</div>
  {% endif %}

