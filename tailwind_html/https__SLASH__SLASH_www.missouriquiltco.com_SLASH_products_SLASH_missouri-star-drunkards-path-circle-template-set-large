<script>
    let current_item,
        current_item_index,
        current_item_subscription,
        current_item_sku = 'NOT4568';

    let additional_product_add_to_cart_options = {};

    const product = JSON.parse(document.getElementById('ProductJson').innerHTML)

    if(typeof dd_page === 'undefined') dd_page = false;

    

    
        additional_product_add_to_cart_options._shop_product_id = 112534;
    

    
    
    

    

    document.addEventListener('DOMContentLoaded', function() {
        // Image zoom
        if (document.querySelector('img.primary_photo')) {
            new Drift(document.querySelector('img.primary_photo'),{
                paneContainer: document.querySelector('.pane-container'),
                touchDelay: 500
            });
        }

        splide_check();
        
        // update 
        current_item_index = CartJS.cart.items.findIndex(item => item.variant_id === 39280931733539);

        if(current_item_index != -1){
            current_item = CartJS.cart.items[current_item_index];
            current_item_subscription = current_item.sku.includes('SUB') || current_item.sku.startsWith('BOM');
        }

        if(current_item != undefined) {
            if(current_item.sku.startsWith('TRIM') || current_item.sku.startsWith('QB') || current_item.sku.startsWith('FBY')) {
                document.getElementById('quantity').dataset.value = current_item.quantity;
                document.getElementById('quantity').value = current_item.quantity / 4;
            } else {
                document.getElementById('quantity').dataset.value = current_item.quantity;
                document.getElementById('quantity').value = current_item.quantity;
            }

            if (dd_page === false && !current_item_subscription) {
                $('#btn_addToCart').text('Update Quantity');
            }
        }
        
        // hide arrows when lte 4 products
        $('.container').has('.splide .splide__list').each( function() {
            if ($(this).find('.splide__list').children().length <= 4) {
                $(this).find('.splide__arrows').addClass('hidden').removeClass('md:block');
            }
        });
    });

    // add product to cart
    $('#btn_addToCart').on('click', function(){
        
        let qty_value = $('#quantity').val();
        let new_qty = 0;

        if (current_item_sku.startsWith('TRIM') || current_item_sku.startsWith('QB') || current_item_sku.startsWith('FBY')) {
            if (qty_value < .5) {
                document.getElementById('quantity').dataset.value = fby_min;
            } else if (qty_value % .25 !== 0){
                document.getElementById('quantity').dataset.value = (Math.ceil(qty_value / .25) * .25) * 4;
            }
        }else{
            document.getElementById('quantity').dataset.value = Math.ceil(qty_value);
        }

        if(current_item === undefined) {
            // add item if not in cart
            if(product.type !== 'Gift Card' && !current_item_sku.startsWith('BLOCK') && !current_item_sku.includes('SUB')){
                if(CartJS.getAttributes().hasOwnProperty('_bundle_bound')) {
                    additional_product_add_to_cart_options = {...additional_product_add_to_cart_options, ...{'_bundle_bound': 'true'}};
                }
            }

            CartJS.addItem(
                $('#selected_variant').val(), 
                document.getElementById('quantity').dataset.value, 
                additional_product_add_to_cart_options, 
                {
                    "success": function(data, textStatus, jqXHR){
                        updateCartCounter();
                        
                        current_item = jqXHR.responseJSON;
                        
                        dataLayer.push({'event': 'AddToCart'});

                        notification(textStatus, $('.product_title').first().text() + ' has been added to your cart.', jqXHR.responseJSON);

                        if (dd_page === false && !jqXHR.responseJSON.sku.includes('SUB')) {
                            $('#btn_addToCart').text('Update Quantity'); // added to cart
                        }

                         //remove session variable for adding free gift back to cart from PDP
                         let keys = Object.keys(sessionStorage);
                         let selected = $('#selected_variant').val();
                     
                         let found = keys.find(key => key === `giftRemoved_${selected}`);
                         if (found) {
                             sessionStorage.removeItem(`giftRemoved_${selected}`); 
                         }
                    },
                    "error": function(jqXHR, textStatus, errorThrown){
                        notification(textStatus, jqXHR.responseJSON.description, {'image': 'https://cdn.shopify.com/s/files/1/0299/5411/3669/t/11/assets/chuck-in-cart-no-bubbles.png?v=2441228424905278583'})
                    }
                }
            );
        } else {
            CartJS.updateItemById(
                current_item.key,
                document.getElementById('quantity').dataset.value,
                additional_product_add_to_cart_options,
                {
                    "success": function(data, textStatus, jqXHR){
                        updateCartCounter();

                            //remove session variable for adding free gift back to cart from PDP
                            let keys = Object.keys(sessionStorage);
                            let selected = $('#selected_variant').val();
                        
                            let found = keys.find(key => key === `giftRemoved_${selected}`);
                            if (found) {
                                sessionStorage.removeItem(`giftRemoved_${selected}`); 
                            }

                        notification(textStatus, $('.product_title').first().text() + ' has been updated in your cart.', data.items.find(item => item.variant_id === 39280931733539))
                    },
                    "error": function(jqXHR, textStatus, errorThrown){
                        notification(textStatus, jqXHR.responseJSON.description, current_item);
                    }
                }
            );
        }

    });

    function splide_check() {
        document.querySelectorAll('.splide').forEach(carousel => new Splide( carousel, {
                // type: 'loop',
                arrowPath: 'M41.25 1C19.297 1 1.5 18.796 1.5 40.75S19.297 80.5 41.25 80.5 81 62.704 81 40.75 63.203 1 41.25 1zm22.32 39.131L42.871 62.908l-4.194-3.811 14.525-15.983-10.991-.006-5.666-.004-17.778-.01.003-5.666 17.777.01 5.667.003 10.056.006-16.952-14.63 3.778-4.225 20.388 17.61.02-.021 2.706 2.461 1.523 1.362z',
                perPage: 4,
                gap: 4,
                arrows: true,
                pagination: false,
                breakpoints: {
                    767: {perPage: 2},
                    1023: {perPage: 3}
                }
            }).mount()
        )
    }

    function updateQuantity(qty, type, min = 1){
        
        const curQty = document.getElementById('quantity');
        const max_purchase = curQty.attributes.max;

        let next_qty = parseInt(curQty.dataset.value) + qty;

        if(max_purchase && max_purchase.value < next_qty) {
            return false;
        }
        
        if(parseInt(curQty.dataset.value) + qty >= min) curQty.dataset.value = next_qty;
        
        (type && type === 'qrt') ? curQty.value = parseInt(curQty.dataset.value) / 4 : curQty.value = parseInt(curQty.dataset.value);
        
    }

    $('select.product-form__variants').change(function(e) {

        let selected_variant = product.variants.find( v => v.id === parseInt(this.value) );

        // is variant in cart ?
        let variant_in_cart = CartJS.cart.items.find(i => i.variant_id === selected_variant.id);

        if (!!variant_in_cart) {
            current_item = variant_in_cart;
            $('#btn_addToCart').text('Update Quantity');
            $('#quantity').val(variant_in_cart.quantity);
            document.getElementById('quantity').dataset.value = parseInt(variant_in_cart.quantity);
        }else{
            current_item = void 0;
            // re-label button text
            $('#btn_addToCart').text('Add to Cart');
            $('#quantity').val(1);
            document.getElementById('quantity').dataset.value = parseInt(1);
        }

        $('div[data-price]').text(Shopify.formatMoney(selected_variant.price)); // Set the display price to the current selection
        $('.product_sku').text(selected_variant.sku); // Set the display SKU to the current selection

        if (selected_variant.compare_price && selected_variant.compare_price > selected_variant.price) {
            $('div[data-compare-price]').text(Shopify.formatMoney(selected_variant.compare_price));
        }

        $('#selected_variant').val(selected_variant.id)

        var newurl = window.location.protocol + '//' + window.location.host + window.location.pathname + '?variant=' + this.value;
        window.history.replaceState({ path: newurl }, '', newurl);

    });
</script> 

