		});


		jQuery('#decSize').click(function () {
			curSizep = parseInt(jQuery('body p').css('font-size')) - 2;
			if (curSizep >= 17)
				jQuery('body p').css('font-size', curSizep);
				localStorage.setItem('fontSizep', curSizep);
				
