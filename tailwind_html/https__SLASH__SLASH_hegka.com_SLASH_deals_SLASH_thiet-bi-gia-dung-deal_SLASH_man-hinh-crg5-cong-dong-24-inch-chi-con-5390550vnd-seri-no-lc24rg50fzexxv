        })

        $(document).on('click', '#editArticle .ck.ck-toolbar__items .editor-zoom-out', function () {
            $('#editArticle').toggleClass('full-screen');
            $('.editor-zoom-out').remove();
            $('.editor-zoom-in').remove();
            $('#editArticle .ck.ck-toolbar__items').append('<button class="ck ck-button ck-off editor-zoom-in" type="button" tabindex="-1">' +
                '<!--begin::Svg Icon | path: assets/media/icons/duotone/Design/ZoomMinus.svg-->\n' +
