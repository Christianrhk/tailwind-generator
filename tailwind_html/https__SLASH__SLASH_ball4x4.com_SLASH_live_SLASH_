<div ng-app="app">
    <div ng-controller="LiveController">
        <div class="flex" ng-show="isLive">
            <div id="player">
                Player Loading...
            </div>
        </div>

        <table ng-repeat="live in lives" class="min-w-full divide-y divide-gray-200 ">
            <thead class="bg-gray-500">
            <tr>
                <th class=" px-6 py-3 flex justify-center font-medium text-white uppercase tracking-wider">
                    <img style="max-width: 50px;" ng-src="{{ live.league_logo }}">
                </th>
                <th class="px-6 font-medium text-left text-lg text-white uppercase tracking-wider" colspan="2">
                    {{ live.league_name }}
                </th>
            </tr>
            </thead>
            <tr ng-repeat="match in live.fixtures">
                <td style="width:40%;" class="bg-red-100 tracking-wider text-center border border-white ">

                    <div class="flex flex-col justify-center items-center"><img style="max-width: 50px;"  ng-src="{{ match.team_home_logo }}">{{ match.team_home_name }}</div>
                </td>
                <td class="bg-red-100 tracking-wider text-center flex flex-col border border-white">
                    <div>{{ dateM(match.event_time) }}</div>
                    <div>{{ timeM(match.event_time) }}</div>
                    <div>
                        <button class="bg-blue-500 hover:bg-blue-700 text-white font-bold py-2 px-4 rounded" ng-click="playLive(match.channel)">ดูบอลสด</button>
                    </div>
                </td>
                <td style="width:40%;" class="bg-red-100 tracking-wider text-center border border-white">
                    <div class="flex flex-col justify-center items-center"><img style="max-width: 50px;"  ng-src="{{ match.team_away_logo }}">{{ match.team_away_name }}</div>
                </td>
            </tr>

        </table>
    </div>
</div>


<script>

    angular.module('app', []).controller('LiveController', function ($scope, $http) {

        $scope.lives = [];
        $scope.LiveURL = '';
        $scope.LiveOptions = {};

        $http.get('https://ballnaka.tv/api/fixtures').then(function success(response) {
            $scope.lives = response.data;
        }, function error() {

        });
        $scope.isLive = false;

        $scope.timeM = function (text) {
            moment.locale('th');
            return moment(text, "YYYY-MM-DD h:mm:ss").format('เวลา HH:mm');
        }

        $scope.dateM = function (text) {
            moment.locale('th');
            return moment(text, "YYYY-MM-DD h:mm:ss").format('วันที่ DD/MM/YYYY');
        }

        $scope.playLive = function (chanel) {
            $scope.isLive = true;
            $scope.LiveURL = "https://r1-sn-w5nuxa-o73.googlevideocdn.com/ball4x4/" + chanel + "/playlist.m3u8";

            $scope.LiveOptions = {
                controls: true,
                width: "100%",
                height: "100%",
                aspectratio: "16:9",
                fullscreen: "true",
                primary: 'html5',
                autostart: true
            }


            var playerInstance = jwplayer('player');
            playerInstance.setup({
                sources: [{
                    "file": $scope.LiveURL
                }],
                controls: true,
                width: "100%",
                height: "100%",
                aspectratio: "16:9",
                fullscreen: "true",
                primary: 'html5',
                logo: {
                    file: "https://ball4x4.com/wp-content/themes/ball4x4/images/logoball4x4_fix.png",
                    link: "https://ball4x4.com",
                    hide: "false",
                    position: "top-right"
                },
                autostart: true
            });

        }

    })
</script>
                                    
