                    $(".modal-backdrop").css('display','none');
                    let target_top = $("#ad_click2").offset().top;
                    $("html,body").scrollTop(target_top-300);
                    }, ma2);
                }else{
                    setTimeout(function(){
                    $("#fbad").css('display','none');
                    $(".modal-backdrop").css('display','none');
                    var target_top = $("#displayarticle2").offset().top;
                    $("html,body").scrollTop(target_top-300);
                    }, ma2);
                }



            }else {
                $("#fbad").css('display','none');
                $(".modal-backdrop").css('display','none');
            }

            // setTimeout(function(){
            //     $("#fbad").css('display','none');
            //     $(".modal-backdrop").css('display','none');
            // }, ma2);


        });
            
    });
</script>


</html>