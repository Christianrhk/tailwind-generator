                            <th class="dotted_border header_row" colspan="2">220 - 230 lbs.</th>
                            <th class="dotted_border header_row" colspan="2">230 - 240 lbs.</th>
                            <th class="dotted_border header_row" colspan="2">240 - 250 lbs.</th>
                            <th class="dotted_border header_row" colspan="2">250 - 260 lbs.</th>
                            <th class="dotted_border header_row" colspan="2">260 - 270 lbs.</th>
