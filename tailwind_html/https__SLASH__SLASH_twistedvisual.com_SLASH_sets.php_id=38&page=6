"></script>
</body>
</html>
<script>

document.addEventListener('DOMContentLoaded', () => {

  let readLess = ''
  const view_more_text = "View More"
  const view_less_text = "view less"

  function shrink(){

    const modelImgHeight = document.querySelector('.model-image').offsetHeight
    const modelInfoHeight =  document.querySelector('.model-info').offsetHeight
    const modelName =  document.querySelector('.model-name').offsetHeight
    const modelBioDiv =  document.querySelector('.model-bio-text')
    const modelBioText =  document.getElementById('data-bio').innerHTML
    const modelDetails =  document.querySelector('.model-details').offsetHeight
    const modelBioSection = document.querySelector('.model-bio-section')

    let modelBioTextArr = modelBioText.split(' ')

    if(modelImgHeight < modelInfoHeight - 40){
      modelBioDiv.innerHTML = ''

      const widthOfBioText = modelImgHeight - modelName - modelDetails 

      for (let i = 0; i < modelBioTextArr.length; i++) {
        const word = modelBioTextArr[i];
        
        if(modelBioSection.offsetHeight < widthOfBioText - 40){
          document.querySelector('.model-bio-text').innerHTML += ' ' + word 
        } else {
          document.querySelector('.model-bio-text').innerHTML += `... <button class="text-xs text-accent uppercase readmore">${view_more_text}</button>`
          readLess = document.querySelector('.model-bio-text').innerHTML
          break
        }

      }

    } else {
      modelBioDiv.innerHTML = modelBioText
      document.querySelector('.model-bio-section').style.height = 'auto'
    }
  
    document.querySelector('.model-bio-text').classList.remove('invisible')

  }

  shrink()

  document.querySelector(".model-bio-text").addEventListener ("click", function(e){
    if(e.target.classList.contains("readmore")){
      const modelBioText =  document.getElementById('data-bio').innerHTML
      this.innerHTML = modelBioText 
      this.innerHTML += ` <button class="text-xs text-accent uppercase readless">${view_less_text}</button>`
      document.querySelector('.model-bio-section').style.height = 'auto'
    } else if (e.target.classList.contains("readless")) {
      this.innerHTML = readLess 
    }
  })

});

</script>