<script>
function trainingjelSubmit(){
  var text='';
  $('input[type=text],input[type=email]', '#trainingjelform').each(function() {
    var label = $(this).attr("placeholder");
    if($(this).attr("data-pluss")){
      label=label+$(this).attr("data-pluss");
    }
    text=text+label+": "+$(this).val()+"\n";
  });
  $("#trainingjeldata").val(text);
  console.log(text);
  return true;
}
</script>

