<div id="dialog-video-1533566036" class="modal-active relative bg-gray-100 font-sans leading-normal tracking-normal" aria-labelledby="dialog-video-title-1533566036"
     aria-describedby="dialog-video-desc-1533566036" role="dialog" style="display:none">
    <div id="overlay-container-1533566036" class="container mx-auto text-center pt-12 pb-32">
        <div class="modal fixed w-full h-full top-0 left-0 flex items-center justify-center">
            <div class="modal-overlay absolute w-full h-full bg-gray-100 opacity-50 top-0 left-0 cursor-pointer"></div>

            
            <div class="z-50 w-5/6 md:w-1/2 bg-white rounded-lg shadow-2xl mx-2 md:mx-0 p-4">
                <div class="modal-title flex justify-between items-center pb-2">
                    <p id="dialog-video-title-1533566036" class="text-2xl font-bold">Sign Up for Health Tips</p>
                    <span id="dialog-video-desc-1533566036" class="sr-visible"> "This is a dialog window which overlays the main content of the page and plays an embedded YouTube video. Pressing the Close Modal button at the bottom of the modal or pressing the Escape key will close the modal and bring you back to where you were on the page.</span>
                    <a id="closebutton-1533566036" href="javascript:CloseYoutubeVideo('1533566036')" class="modal-close cursor-pointer font-bold text-3xl controller-close" aria-label="Close (Press escape to close)"></a>
                </div>

                <div class="modal-body py-4 text-left">
                </div> <div class="modal-footer flex justify-end pt-2"><button class="inline-flex btn btn-primary hover:underline leading-normal" onclick="CloseYoutubeVideo('1533566036');" aria-label="Close (Press escape to close)">Close</button></div>
            </div>
        </div>
    </div>
</div>

<script type="text/javascript">
    var ShowYouTubeVideoForTenetCard = false;
    var dialogId = "dialog-video-1533566036";
    var closeButtonId = "closebutton-1533566036";
    var pvideoId = "playVideo-1533566036";
    //document.getElementById(dialogId).hidden = true;

    function CloseYoutubeVideo(id) {
        
        //event.preventDefault();
        var _playVideoId = "playVideo-" + id;
        var _dialogId = "dialog-video-" + id;
        var _iframeVideoId = "iframe-video-" + id;
        var _getVideoSrc = document.getElementById(_iframeVideoId).getAttribute('src');
        ShowYouTubeVideoForTenetCard = false;
        //$("#" + _dialogId + " iframe").attr("src", $("#" + _dialogId + " iframe").attr("src"));
        document.getElementById(_iframeVideoId).setAttribute('src', _getVideoSrc);

        //document.getElementById(_dialogId).hidden = true;
        document.getElementById(_dialogId).setAttribute('style', 'display: none !important');

        var pvideo = document.getElementById(_playVideoId);
        pvideo.focus();
    }

    function clickAction(modalDialogId) {
        //event.preventDefault();
        //if (event.target.id === "playVideo") {
        dialogId = "dialog-video-" + modalDialogId;
        closeButtonId = "closebutton-" + modalDialogId;
        pvideoId = "playVideo-" + modalDialogId;
        ShowYouTubeVideoForTenetCard = true;
        document.getElementById(dialogId).setAttribute('style', 'display: block !important');
        var closeButton = document.getElementById(closeButtonId);
        closeButton.focus();
        //document.getElementById(dialogId).hidden = false;
        //}
    }

    document.addEventListener('keyup', function (evt) {
        if (ShowYouTubeVideoForTenetCard && evt.keyCode === 27) {
            var pvideo = document.getElementById(pvideoId);
            pvideo.focus();
            ShowYouTubeVideoForTenetCard = false;
            //document.getElementById(dialogId).hidden = true;
            document.getElementById(dialogId).setAttribute('style', 'display: none !important');
        } else if (ShowYouTubeVideoForTenetCard && evt.keyCode === 9) {
            var elm = evt.target;
            var parentFound = elm.closest("div[role='dialog']");

            if (!parentFound) {
                var cbutton = document.getElementById(closeButtonId);
                cbutton.focus();
            }
        }
    });


</script>

