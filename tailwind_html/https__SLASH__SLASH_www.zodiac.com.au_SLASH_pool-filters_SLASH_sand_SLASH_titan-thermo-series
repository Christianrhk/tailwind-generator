    <script>
        zodiac.product();

$(document).ready(function(){

list = $('#product-tag-extra').children();
var found = 0;
for (var i = 0; i < list.length; i++) {
    if(list[i].className == 'owl-dots disabled')
        {
            found = 1
            break;
        }
}
if(!found)
$('#product-tag-bottom_id').addClass('product-tag-bottom-extra')

});
