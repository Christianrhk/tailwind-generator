        const createRadio = (divToAdd, name, value, label, event, checked = false) => {
            let div = document.createElement("div");
            let input = document.createElement("input");
            let span = document.createElement("span");
            div.appendChild(input);
            div.appendChild(span);
            div.className = ("roundend-full btn border border-yellow-400 p-2 m-2");
            input.classList.add("form-radio");
            input.classList.add("h-5");
            input.classList.add("w-5");
            input.classList.add("text-yellowPrimary");

            span.classList.add("ml-2");
            span.classList.add("b");
            span.classList.add("text-sm");
            span.classList.add("capitalize");
            span.classList.add("tracking-wide");
            input.setAttribute("name", name);
            input.setAttribute("value", value);
            input.setAttribute("type", "radio");
            input.setAttribute("x-on:change", event);
            checked && input.setAttribute("checked", "checked");

            span.innerText = label;

            divToAdd.appendChild(div);
        }

