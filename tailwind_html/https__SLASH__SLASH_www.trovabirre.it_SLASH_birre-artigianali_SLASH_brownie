    <script>
        function productSliderSale() {
            document
                .querySelectorAll(".product-slidersale")
                .forEach(slider => {
                    new Glide(slider, {
                        type: "carousel",
                        perView: 3,
                        autoplay: 2000,
                        breakpoints: {
                            1024: {
                                perView: 2,
                                peek: {
                                    before: 20,
                                    after: 20,
                                },
                            },
                            768: {
                                perView: 2,
                                peek: {
                                    before: 10,
                                    after: 10,
                                },
                            },
                            600: {
                                perView: 1,
                                peek: {
                                    before: 0,
                                    after: 0,
                                },
                            },
                        },
                    })
                        .mount();
                });
        }
    </script>

