        const formatPrice = (price, quantita = 1, increment = 1) => {
            if (price === null)
                return `${prodotto.minPrice} &euro; - ${prodotto.maxPrice} &euro;`;

            price = Math.round((price * quantita * increment) * 100) / 100;
            price = price.toFixed(2).toString().replace(".", ",");
            return `${price} &euro;`;
        }

        function app() {
            return {
                urlVal: {
                    tipologia: urlSearcher.get("tipologia"),
                    formato: urlSearcher.get("formato"),
                    da: urlSearcher.get("da"),
                },
                quantity: {min: 0, increment: 0, max: 0},
                prodSelected: {
                    variantID: 0,
                    hasOneVariant: false,
                    sku: '',
                    lotto: '',
                    numero: '',
                    image: '',
                    cover: '',
                    price: null,
                    is_prime: null,
                },
                show: false,
                avvisami: false,
                show_stock: false,
                constructor() {
                    for (let media of prodotto.media)
                        if (media.cover === 1)
                            this.prodSelected.cover = this.prodSelected.image = media.big;

                    this.prodSelected.hasOneVariant = prodotto.has_variants == 0;

                    for (let variant of prodotto.variants) {
                        createRadio(tipologiaDiv, "tipologia", variant.id, `${variant.name}`, "onFirstSelectChange($event.target.value)", this.prodSelected.hasOneVariant || variant.id == this.urlVal.tipologia);
                        if (this.urlVal.tipologia || this.prodSelected.hasOneVariant)
                            if (variant.id == this.urlVal.tipologia || this.prodSelected.hasOneVariant)
                                for (let formato of variant.formati) {
                                    createRadio(formatoDiv, "formato", formato.id, `${formato.name}`, "onSecondSelectChange($event.target.value)", this.prodSelected.hasOneVariant || formato.id == this.urlVal.formato);
                                    if (this.urlVal.formato || this.prodSelected.hasOneVariant)
                                        if (formato.id == this.urlVal.formato || this.prodSelected.hasOneVariant)
                                            for (let quantita of formato.quantita) {
                                                createRadio(da, "da", quantita.qty, `${quantita.qty}`, "onThirdSelectChange($event.target.value)", this.prodSelected.hasOneVariant || quantita.qty == this.urlVal.da);
                                                if ((this.urlVal.da && quantita.qty == this.urlVal.da) || this.prodSelected.hasOneVariant) {
                                                    this.show = true;

                                                    console.log(quantita);
                                                    this.quantity.min = quantita.qty;
                                                    this.quantity.increment = 1;
                                                    this.quantity.max = Math.floor(quantita.stock / quantita.qty);
                                                    if (this.quantity.max != 0) {
                                                        this.show_stock = true;
                                                        this.avvisami = false;
                                                    } else {
                                                        this.avvisami = true;
                                                        this.show_stock = false;
                                                    }
                                                    this.prodSelected.sku = quantita.sku;
                                                    this.prodSelected.lotto = quantita.lotto;
                                                    this.prodSelected.numero = quantita.numero;
                                                    this.prodSelected.variantID = quantita.id;
                                                    this.prodSelected.price = quantita.price;
                                                    this.prodSelected.is_prime = quantita.is_prime == 1;
                                                }
                                            }
                                }
                    }
                },

                onFirstSelectChange(value) {
                    insertParam("tipologia", value);
                    deleteParam("formato");
                    deleteParam("da");
                    this.show = false;
                    this.quantity.min = 0;
                    this.quantity.increment = 0;
                    this.quantity.max = 0;
                    this.show_stock = false;
                    this.avvisami = false;
                    this.urlVal.formato = null;
                    this.urlVal.da = null;
                    this.urlVal.tipologia = value;

                    this.prodSelected.price = null;
                    this.prodSelected.is_prime = null;

                    formatoDiv.innerHTML = "";
                    da.innerHTML = "";

                    for (let variant of prodotto.variants)
                        if (variant.id == this.urlVal.tipologia) {
                            this.prodSelected.tipologia = 0;
                            this.prodSelected.image = this.prodSelected.cover;
                            for (let formato of variant.formati)
                                createRadio(formatoDiv, "formato", formato.id, `${formato.name}`, "onSecondSelectChange($event.target.value)", formato.id == this.urlVal.formato);
                            return;
                        }
                },
                clickImage(e) {

                    //image_product
                    var highlightedItems = document.querySelectorAll(".image_product");

                    highlightedItems.forEach(function (userItem) {
                        userItem.classList.remove("active");
                    })
                    e.classList.add("active");
                },
                onSecondSelectChange(value) {
                    insertParam("formato", value);
                    deleteParam("da");
                    this.quantity.min = 0;
                    this.quantity.increment = 0;
                    this.quantity.max = 0;
                    this.show_stock = false;
                    this.avvisami = false;
                    this.show = false;
                    this.urlVal.da = null;
                    this.urlVal.formato = value;
                    this.prodSelected.price = null;
                    this.prodSelected.is_prime = null;

                    da.innerHTML = "";

                    for (let variant of prodotto.variants)
                        if (variant.id == this.urlVal.tipologia)
                            for (let formato of variant.formati)
                                if (formato.id == this.urlVal.formato) {
                                    for (let quantita of formato.quantita)
                                        createRadio(da, "da", quantita.qty, `${quantita.qty}`, "onThirdSelectChange($event.target.value)", quantita.qty == this.urlVal.da);
                                    return;
                                }
                },

                onThirdSelectChange(value) {
                    insertParam("da", value);
                    this.quantity.min = 0;
                    this.quantity.increment = 0;
                    this.quantity.max = 0;

                    this.urlVal.da = value;
                    this.show = true;
                    for (let variant of prodotto.variants)
                        if (variant.id == this.urlVal.tipologia)
                            for (let formato of variant.formati)
                                if (formato.id == this.urlVal.formato)
                                    for (let quantita of formato.quantita)
                                        if (quantita.qty == this.urlVal.da) {
                                            this.show = true;

                                            this.quantity.min = quantita.qty;
                                            this.quantity.increment = 1;
                                            this.quantity.max = Math.floor(quantita.stock / quantita.qty);
                                            if (this.quantity.max != 0) {
                                                this.show_stock = true;
                                                this.avvisami = false;
                                            } else {
                                                this.avvisami = true;
                                                this.show_stock = false;
                                            }

                                            this.prodSelected.lotto = quantita.lotto;
                                            this.prodSelected.numero = quantita.numero;
                                            this.prodSelected.variantID = quantita.id;
                                            this.prodSelected.price = quantita.price;
                                            this.prodSelected.sku = quantita.sku;
                                            this.prodSelected.is_prime = quantita.is_prime === 1;
                                            return;
                                        }
                },

                aggiungiProd(value) {
                    this.quantity.increment += value;
                    if (this.quantity.increment > this.quantity.max) {
                        this.quantity.increment = this.quantity.max
                        setModal(htmlAggiungiAlCarrello);
                        toggleModal();
                    }
                    this.quantity.increment <= 0 && (this.quantity.increment = 1);
                },
                avvisamiRequest() {
                    let pot = document.getElementById("pot").value !== '';
                    if (pot) {
                        return false;
                    }

                    let email = document.getElementById("email").value !== '';
                    if (!email) {
                        document.getElementById("title_modal").textContent = 'Inserisci email';
                        toggleModal()
                        return false;
                    }
                    var re = /^[a-zA-Z0-9.!#$%&'*+/=?^_`{|}~-]+@[a-zA-Z0-9-]+(?:\.[a-zA-Z0-9-]+)*$/;
                    if (!re.test(document.getElementById("email").value)) {
                        document.getElementById("title_modal").textContent = 'Email Non valida';
                        toggleModal()
                        return false;
                    }


                    let xhttp = new XMLHttpRequest();
                    xhttp.onreadystatechange = function () {
                        if (this.readyState == 4 && this.status == 200) {
                            let response = JSON.parse(this.response);
                            if (response.status) {
                                toggleModal()
                            } else {
                                alert('ERROR')
                            }
                        }
                    };
                    let data = {
                        "_token": "riqoJw2UrlObNvVewZUOZbdWgvzzSHOmGoH3UZst",
                        "email": document.getElementById('email').value,
                        "sku": this.prodSelected.sku,
                        "qty": this.quantity.increment,
                    };
                    xhttp.open("POST", "/avvisami", true);
                    xhttp.setRequestHeader("Content-type", "application/json");
                    xhttp.send(JSON.stringify(data));
                },

                History() {
                    if (length === 0)
                        document.getElementById('len')
                    else
                        document.getElementById('go')
                },


                aggiungiAlCarrello() {


                    if (this.quantity.increment > this.quantity.max) {
                        this.quantity.increment = this.quantity.max;
                    }

                    if (this.quantity.increment < 0) {
                        this.quantity.increment = 1;
                    }

                    aggiungiAlCarrello(this.prodSelected.variantID, this.prodSelected.sku, this.quantity.increment);
                }
            }
        }
    </script>







            
