let imageClass = '.roof-window-fix',
imageLocation = document.querySelector(imageClass),
imageLink = imageLocation.getAttribute('href'),
buttonLink = imageLocation.closest('.product').querySelector('.vlx-button');
buttonLink.setAttribute('href', imageLink);
</script>

