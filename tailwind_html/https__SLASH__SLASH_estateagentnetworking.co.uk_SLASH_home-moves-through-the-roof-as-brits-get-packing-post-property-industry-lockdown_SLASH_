      var(--fontScale) * var(--fontScale) * var(--fontScale) * var(--fontScale) *
      var(--fontScale) * var(--fontScale) * var(--fontScale) * var(--fontScale) *
      var(--fontScale) * var(--fontScale) * var(--fontScale) * var(--fontScale) *
      var(--fontScale)
  );
 --primaryFontFamily: 'Barlow', sans-serif;
  --secondaryFontFamily: 'Barlow', sans-serif;
  
}
@media(min-width:768px){
  :root {
    font-size: 15px;
  }
}
