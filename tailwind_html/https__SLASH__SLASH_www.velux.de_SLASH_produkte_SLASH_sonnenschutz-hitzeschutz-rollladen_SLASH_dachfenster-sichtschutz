            <article class="grid_12">
                <div class="filterenabled spot-wrapper"  >
                    
                    <div class="page-titles">
                        

<!-- GlobalLocalHeadline.ascx -->

    
        <h1 >
            <p>Sichtschutz für Dachfenster</p>
        </h1>
    
    
     


                        
                        
                    </div>
                    <ul class="menu submenu">
                        
                                <li>
                                    
    
        <div class="">
            <a href="https://www.velux.de/produkte/sonnenschutz-hitzeschutz-rollladen" target="_self" >
