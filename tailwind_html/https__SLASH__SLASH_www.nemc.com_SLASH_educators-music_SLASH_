<script>
    $(document).ready(function () {
        $(document).on('click', '.updateZIPCode', function () {
            var ele = $(this);
            if (ele.hasClass('updateZIPCodePartial')) {
                var postalCode = ele.parents('.searchBox-wrapper').find('input').val();
            } else {
                var postalCode = ele.parents('.location-update-body').find('input').val();
            }
            CheckValidZipCodeAndSetZipCode(ele,postalCode);
        });


    });
    function CheckValidZipCodeAndSetZipCode(ele, postalCode) {

        $('.loader-overlay').show();
        $.ajax({
            type: "POST",
            url: "/Teachers/CheckZipCode?SchoolZipCode=" + postalCode,
            datatype: 'json',

            success: function (result, status, xhr) {
                $('.loader-overlay').hide();
                if (result == false) {
                    if (ele.hasClass('updateZIPCodePartial')) {
                        ele.parents('.searchBox-wrapper').find('.zipcodevalidation').removeClass('hidden');
                    } else {
                        ele.prev().removeClass('hidden');
                    }

                }
                else {
                    if (ele.hasClass('updateZIPCodePartial')) {
                        ele.parents('.searchBox-wrapper').find('.zipcodevalidation').addClass('hidden');
                    } else {
                        ele.prev().addClass('hidden');
                    }
                    SetZipCode(postalCode);
                }



            },
            error: function (error, status, xhr) {

            },
            error: function () {
                console.log("Result: " + status + " " + error + " " + xhr.status + " " + xhr.statusText);
            }
        });
    }

    function SetZipCode(postalCode) {
        $('.loader-overlay').show();
        $.ajax({
            type: "POST",
            url: "/Pages/SetZipCodeCookiesFromAffiliateAndRentPage?PostalCode=" + postalCode,
            datatype: 'json',

            success: function (result, status, xhr) {
                $('.loader-overlay').hide();

                window.location.href = result;
            },
            error: function (error, status, xhr) {
                //$("#MsgModal").html(error);
                //console.log(error);
            }
        });
    }

    function geoFindMe() {
            if (!navigator.geolocation) {
                console.log("Geolocation is not supported by your browser");
                return;
            }
            function success(position) {
                var latitude = position.coords.latitude;
                var longitude = position.coords.longitude;
                $.ajax({
                    type: "POST",
                    url: "/Home/GetZipCodeFromLatAndLang?latitude=" + latitude + "&longitude=" + longitude,
                    datatype: 'json',


                    success: function (result, status, xhr) {
                        /* console.log(result);*/
                        if (result != null) {
                            if (result.status == "OK") {
                                window.location.reload();
                            }
                        }

                    },
                    error: function (error, status, xhr) {
                        /* console.log(error);*/
                    }

                });
            }
            function error() {
                if("" == "")
                {
                    alert("Your ZIP code is needed for accurate online availability and pricing.");
                     SetZipCode("08854");
                }


            }
                navigator.geolocation.getCurrentPosition(success, error);
            }

    geoFindMe();

