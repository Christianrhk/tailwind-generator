<script>
window.onload = function() {
	init();
};

// add this function, so the transition would only start running when the page has been fully loaded. This is a fallback in case when someone has turned off their JavaScript. 

function init() {
	var menu = document.getElementById("menu");
	menu.classList.add("transition-after-pageload");


}

document.getElementById("close-filter-button").addEventListener("click", displayDate);

function displayDate() {
  document.getElementById("dropdown-button").checked = false; 
}


/* FILTER */
const divs = document.querySelectorAll('.filter-checkbox');

divs.forEach(el => el.addEventListener('click', event => {
  console.log(event.target.getAttribute("title"));
}));


function myFunction(){
    const divCustomGrid = document.getElementById('custom-grid-home');
    const divGridFilter = document.getElementById('gf-grid');
    const divGridTree = document.getElementById('gf-tree');
    const divFilter = document.getElementById('collection-filter-sidebar');

    setTimeout(function(){
      var myEle = document.querySelector(".gf-block-content.globo-selected-items");
      if(myEle){        
        //const divCustomGrid = document.getElementById('custom-grid-home');
        divCustomGrid.classList.add('hidden');

        //const divGridFilter = document.getElementById('gf-grid');
        divGridFilter.style.display = "block";

        //const divGridTree = document.getElementById('gf-tree');
        divGridTree.classList.add('active-filter');
    

        //const divFilter = document.getElementById('collection-filter-sidebar');
        divFilter.classList.remove("col-span-2");
        divFilter.classList.remove("md:col-span-3");
        divFilter.classList.remove("xl:col-span-2");
        divFilter.classList.add("col-span-12");
        console.log('filtro activo');
       
      }else{
        
       // const divCustomGrid = document.getElementById('custom-grid-home');
        divCustomGrid.classList.remove('hidden');

        //const divGridFilter = document.getElementById('gf-grid');
        divGridFilter.style.display = "none";

        //const divGridTree = document.getElementById('gf-tree');
        divGridTree.classList.remove('active-filter');
    

        //const divFilter = document.getElementById('collection-filter-sidebar');
        divFilter.classList.add("col-span-2");
        divFilter.classList.add("md:col-span-3");
        divFilter.classList.add("xl:col-span-2");
        divFilter.classList.remove("col-span-12");
        console.log('no filtro activo');
       
      }
    }, 600);


    


    /*if ('URLSearchParams' in window) {
        var searchParams = new URLSearchParams(window.location.search);
        searchParams.set("foo", "bar");
        window.history.replaceState(null, null, "?gf_31673=123");
        //window.location.search = searchParams.toString();
    }*/

    //alert('click')
    
}

function clearAllCustom(){
  
  
    const divCustomGrid = document.getElementById('custom-grid-home');
    divCustomGrid.classList.remove('hidden');

    const divGridFilter = document.getElementById('gf-grid');
    divGridFilter.style.display = "none";

    const divGridTree = document.getElementById('gf-tree');
    divGridTree.classList.remove('active-filter');
 

    const divFilter = document.getElementById('collection-filter-sidebar');
    divFilter.classList.add("col-span-2");    
    divFilter.classList.add("md:col-span-3");
    divFilter.classList.add("xl:col-span-2");
    divFilter.classList.remove("col-span-12");
    divFilter.classList.remove("md:col-span-12");
    divFilter.classList.remove("xl:col-span-12");

}



