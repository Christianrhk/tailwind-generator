    <script>
      
        
        function storeCredit(){
          
          const credit = document.getElementById('Rise-account-page-button');
          credit.click()
          //console.log('Hello credit!');
        }
       
      let myDropdownAccount = document.getElementById("my-dropdown-account");
      if (myDropdownAccount != null){
        document.getElementById("my-dropdown-account").onchange = function(){
          var value = document.getElementById("my-dropdown-account").value;
          //console.log(value)
          if(value == '/balance'){
            storeCredit()
          }else{
            window.location.href= value;              
          }
         };
      }
        
   
       /* CLOSE MOBILE MENU ABOUT US ANCHOR */
       function hideMenu(){
        
        const menu = document.getElementById('summaryHeaderMenu');
        menu.click();
        //console.log('something'); 

       }
       
    </script>

