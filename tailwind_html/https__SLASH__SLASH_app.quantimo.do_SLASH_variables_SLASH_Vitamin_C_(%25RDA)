            "type": "sankey",
            "events": {
                "click": function(e){
    var p = e.point;
    if(p.url){
        showLoader();
        window.location.href = p.url
    } else if (p.linksTo[0]) {
        showLoader();
        window.location.href = p.linksTo[0].url
    } else if (p.linksFrom[0]) {
        showLoader();
        window.location.href = p.linksFrom[0].url
    } else {
        debugger
        console.error("Could not find url on event:", event);
    }
}
            },
            "visible": true
        }
    ],
    "subtitle": {
        "text": "These are the outcomes most likely influenced by Vitamin C based on our current data. The percent value indicates the typical change from baseline in the outcome on the right following above average measurements for Vitamin C (%RDA)."
    },
    "themeName": "dark",
    "title": {
        "text": "Vitamin C Outcomes Flow Chart",
        "enabled": true
    },
    "tooltip": {
        "nodeFormat": "{point.name}: <b>{point.sum}<\/b><br\/>",
        "formatter": function() {
            //debugger;
            var data = this.point.series.data;
            var i = this.point.index;
            var point = data[i];
            if(!point){
                return this.point.name;
            }
            return point.tooltip;
        },
        "followPointer": false,
        "style": {
            "pointerEvents": "auto"
        },
        "useHtml": true,
        "hideDelay": 2000
    },
    "useHighStocks": false
});
});
        
</script>

