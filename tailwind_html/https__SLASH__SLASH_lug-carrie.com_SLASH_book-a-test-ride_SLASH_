	function date_clicked(x){
		var z = jQuery("#available_slots .slot_list:nth-of-type(2)");
		////console.log(x);
		jQuery(z).remove();
		//clean_scroll_warning();
		jQuery(".keep_scrolling").remove();
		jQuery(".Calendar--table").after("<div class='keep_scrolling' style='display:block;'>Please scroll to see more available times</div>");		
		
	}
	
	function time_clicked(x){
		date_clicked(x);
	}

	jQuery(document).ready(function() {
		// remove next button 
		jQuery("#stage_2 a.goto").remove();
	});
	
		function convertTime12to24(time12h){
			  const split_string = time12h.split(' ');
			  var time=split_string[0];
			  var modifier=split_string[1];
			  const split_time = time.split(':');
			  var hours=split_time[0];
			  var minutes=split_time[1];
			  
			  if (hours === '12') {
				hours = '00';
			  }
			  if (modifier === 'PM') {
				hours = parseInt(hours, 10) + 12;
			  }
			  
			  var rtn_str = hours + ":" + minutes;
			  return rtn_str;
		}	
	
	function confirm_booking_time(button_clicked){
		

		
		// get element directly before button (elements conatins date)
		var button_prev = jQuery(button_clicked).prev();
		// get the selected time as string 
		var appoint_time_ampm = jQuery(button_prev).html();
		// convert time string to 24 hour time 
		var appoint_time=convertTime12to24(appoint_time_ampm);
		if(appoint_time.length<5){
			appoint_time = "0" + appoint_time;
		}
		// get selected radio button for date 
		var appoint_day =jQuery('.outer_content_calendar input[name=day]:checked').val();
		// get the timestamp related to attrib
		var time_stamp = jQuery(button_prev).attr('for');
		var appoint_date_au=new Date(eval(time_stamp)).toLocaleString("en-AU", {timeZone: "Australia/Sydney"});
		// appoint_date_au = "30/06/2022, 3:00:00 pm"
		// get first 10 chars 
		var appoint_date_only = appoint_date_au.substring(0, 10);
		// split to array 
		var date_array =appoint_date_only.split('/');
		date_array[2] = date_array[2].replace(",","");
		
		date_array[0] = date_array[0].replace(" ","");
		date_array[1] = date_array[1].replace(" ","");
		date_array[2] = date_array[2].replace(" ","");
		
		if(date_array[1].length==1){
			date_array[1]="0"+date_array[1];
		}
		if(date_array[0].length==1){
			date_array[0]="0"+date_array[0];
		}		
		
		
		
		// refromat the date string to "appointmentDate" : "2022-05-14" --required to make a booking (yyyy-MM-dd)
		var new_date_format = date_array[2] + '-' + date_array[1] + '-' + date_array[0];		
		// get end time for selected start time...
		var end_time;


		var id_str = "end_time_" + new_date_format + "_" + appoint_time;
		id_str = id_str.replace(":", "-");
	
		
		end_time = 	jQuery(".end_times_container #"+id_str).val();
		
		if (typeof end_time == "undefined" || end_time=="") {
			//////alert("Error #450 - end time is empty. Start Time is "+ new_date_format + "_" + appoint_time);
			if (typeof available_dates_end_times !== "undefined") {
				if (typeof available_dates_end_times[new_date_format] !== "undefined") {	
					if (typeof available_dates_end_times[new_date_format][appoint_time] !== "undefined") {
						end_time = available_dates_end_times[new_date_format][appoint_time];
					}
				}
			}
		}
		
		jQuery("#start_date").val(new_date_format);
		jQuery("#start_time").val(appoint_time);
		jQuery("#end_time").val(end_time);
		if (typeof end_time == "undefined" || end_time=="") {
			//alert("Error #464 - end time is empty");
		} else {
			// call API with details 
			form_next(3);
		}
	}
	
	
	function scrolling_message(){
		if($(".keep_scrolling").length != 1) {
			//jQuery(".Calendar--table").after("<div class='keep_scrolling' style='display:none;'>Please scroll to see more available times</div>");
		} else {
			jQuery(".keep_scrolling").hide();
			
		}
		var count_items = jQuery("#available_slots ul li").length;
		if(count_items>5){
			jQuery(".keep_scrolling").show();
			console.log("keep_scrolling show :  " +  count_items);
		} else {
			jQuery(".keep_scrolling").hide();
			console.log("keep_scrolling  hide  :  " +  count_items);
		}
	}
	
	function clean_scroll_warning(){
		jQuery(".keep_scrolling").hide();
			jQuery('.outer_content_calendar').bind('DOMSubtreeModified', function () {
				//scrolling_message();
			});		
	}
	
	jQuery('.white_btn').click(function() {
