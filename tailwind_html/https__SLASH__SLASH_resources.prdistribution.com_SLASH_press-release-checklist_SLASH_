<script>
    // Prevents menu from closing when clicked inside
    document.getElementById("dropdownMenuNews").addEventListener('click', function (event) {
        //alert("click outside");
        event.stopPropagation();
    });

    function showAllNewsInDropdownMenu()
    {
        $('#show-all-button').addClass('hidden');
        $('#show-less-button').removeClass('hidden');
        $('.more-in-news-dropdown').removeClass('hidden');
    }

    function showLessNewsInDropDownMenu()
    {
        $('#show-all-button').removeClass('hidden');
        $('#show-less-button').addClass('hidden');
        $('.more-in-news-dropdown').addClass('hidden');
    }
</script>
<script>
    $(document).ready(function(){
        $("a").tooltip();

        $(function() {
            $('[data-toggle="tooltip"]').tooltip()
        })

        $(".navbar-toggler").click(function(){
            $('body').addClass("fixedPosition");
        });

        $("#closeMobileMenu").click(function(){
            $('.navbar-collapse').collapse('hide');
            $('body').removeClass("fixedPosition");
        });

        $('#navbarsExampleDefault').click(function(e){
            if(e.target == this){
                $('.navbar-collapse').collapse('hide');
                $('body').removeClass("fixedPosition");
            }
        });

        //case study same height
    });
</script>
<script>
    $(document).ready(function(){
        $("#mainNavigation").sticky({topSpacing:0});
    });

    $(window).scroll(function() {
        var width = $(window).width();

        if($("#mainNavigation-sticky-wrapper").hasClass('is-sticky'))
        {
            if(width > 992)
            {
                $('.logo-img').attr('src', '/images/themes/lion/logo.svg');
            }

            if($("#topBarTrigger").hasClass("closed")){
                $(".order-now-main-navigation").removeClass('moved');
            }

        }else{
            $('.logo-img').attr('src', '/images/themes/lion/logo.svg');

            if($("#topBarTrigger").hasClass("closed")){
                $(".order-now-main-navigation").addClass('moved');
            }

        }
    });

    $(document).ready(function() {

        $(".search-header").click(function(){
            $(".search-box").removeClass('hidden');
            $(".right-part .featured-section").addClass("hidden");
            $(".search-header").addClass("hidden");
            $(".search-buttons").removeClass('hidden');
            $(".search-box").fadeIn();
            $("#resource-center-text-header").addClass("hidden");
        });

        $(".search-action-close").click(function(){
            $(".search-box").addClass('hidden');
            $(".right-part .featured-section").removeClass("hidden");
            $(".search-buttons").addClass("hidden");
            $(".search-header").removeClass("hidden");
            $("#resource-center-text-header").removeClass("hidden");
        });

        $(".search-action-header").click(function(){
            $('form#searchFormHeader').submit();
        });

        $(".search-action-header-mobile").click(function(){
            $('form#searchFormHeaderMobile').submit();
        });

    });


</script>
<script>
    function getCookie(name) {var cookie = document.cookie;var prefix = name + "=";var begin = cookie.indexOf("; " + prefix);if (begin == -1) {begin = cookie.indexOf(prefix);if (begin != 0) return null;} else {begin += 2;var end = document.cookie.indexOf(";", begin);if (end == -1) {end = cookie.length;}}return unescape(cookie.substring(begin + prefix.length, end));}

    $(document).ready(function(){
        if (window.location.protocol != "https:") {
            window.location.href = "https:" + window.location.href.substring(window.location.protocol.length);
        }
    })

    $(document).ready(function(){
        var topBarState = getCookie("topBarState");
    });

    $("#topBarTrigger").click(function(){

        if($(this).hasClass('open')){

            var d = new Date();
            d.setTime(d.getTime() + 1800000);
            document.cookie = 'topBarState=collapsed;path=/;expires='+d.toGMTString()+';';
            $("#topBarTrigger").removeClass('open');
            $("#topBarTrigger").addClass('closed');
            $("#topBar .wrapper").removeClass('fadein').addClass("fadeout");
            $(".order-now-main-navigation").addClass('moved');
            $("#topBar .wrapper").delay(100).slideUp('50');
        }else{
            var d = new Date();
            d.setTime(d.getTime() + 1800000);
            document.cookie = 'topBarState=expanded;path=/;expires='+d.toGMTString()+';';
            $("#topBarTrigger").removeClass('closed');
            $("#topBarTrigger").addClass('open');
            $("#topBar .wrapper").removeClass('hidden');
            $("#topBar .wrapper").slideDown('50');
            $(".order-now-main-navigation").removeClass('moved');
            $("#topBar .wrapper").delay(100).removeClass('fadeout').addClass("fadein");
        }
    });

    function launchPush(){
        window._pcq.push(['triggerOptIn',{httpWindowOnly: true}]);
    }

    function signOut() {
        var auth2 = gapi.auth2.getAuthInstance();
        auth2.signOut().then(function () {
            console.log('User signed out.');
            var random_id = Math.floor(Math.random() * 10001);
            window.location.replace("/spirit/app/logout.php?id="+random_id);
        });
    }

    function onLoad() {
        gapi.load('auth2', function() {
            gapi.auth2.init();
        });
    }

    // Prevents menu from closing when clicked inside
    document.getElementById("dropdownMenuNews").addEventListener('click', function (event) {
        //alert("click outside");
        event.stopPropagation();
    });

    function showAllNewsInDropdownMenu()
    {
        $('#show-all-button').addClass('hidden');
        $('#show-less-button').removeClass('hidden');
        $('.more-in-news-dropdown').removeClass('hidden');
    }

    function showLessNewsInDropDownMenu()
    {
        $('#show-all-button').removeClass('hidden');
        $('#show-less-button').addClass('hidden');
        $('.more-in-news-dropdown').addClass('hidden');
    }

    $("#hardRefreshClose").click(function(){
        var d = new Date();
        d.setTime(d.getTime() + 259200000);
        document.cookie = 'hardRefreshBar=hide;path=/;expires='+d.toGMTString()+';';
        $("#hardRefreshNotice").addClass("hidden");
    });
    window._peq = window._peq || []; window._peq.push(["init"]); //pushengage
    
</script>
