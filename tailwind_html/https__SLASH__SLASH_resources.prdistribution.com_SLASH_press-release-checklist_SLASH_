    background: #39b54a;
    color:#fff !important;
    border:1px solid #39b54a;
    border-radius:4px;
    transition: all 0.5s ease;
}
article.post {
    border: 1px solid rgb(191 200 204);
    border-radius: 4px;
}
article .wp-post-image {
    border: 1px solid #fff;
    border-radius: 4px;
    box-shadow: 0px 0px 20px 0px #dedede;
}
