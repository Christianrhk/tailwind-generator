   margin-bottom:20px;
}
ol ol, ul ol { 
   list-style-type: lower-latin; 
   list-style-position: inside; 
   margin-left: 15px; 
   margin-bottom:20px;
}

figure.prd-shadow {
    margin: 30px 0 !important;
    border: 1px solid #bfc8cc;
    border-radius: 4px;
    box-shadow: 0px 0px 20px 0px #dedede;
}
.prd-button a {
    background: #39b54a;
    color:#fff !important;
    border:1px solid #39b54a;
    border-radius:4px;
    transition: all 0.5s ease;
}
.prd-button a:hover {
    background: #fff;
    border:1px solid #39b54a;
    color:#39b54a !important;
    border-radius:4px;
}

.prd-button-outline a {
    background: #fff;
    border:1px solid #39b54a;
    color:#39b54a !important;
    border-radius:4px;
}
