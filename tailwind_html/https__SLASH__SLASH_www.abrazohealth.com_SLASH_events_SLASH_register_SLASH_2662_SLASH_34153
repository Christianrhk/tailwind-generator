<script type="text/javascript">
     var dFunction = "oLoad" + Math.floor(Math.random() * 1000);

        window[dFunction] = function (iid) {
            var script = document.getElementById(iid);
            var herobannerconfig = script.getAttribute('data-herobannerconfig');
            var uid = script.getAttribute('data-uid');
            HeroBannerInitVue(uid, herobannerconfig);
        }
    if (window.personalizationManager) {
        window.personalizationManager.addPersonalizedContentLoaded(function () {
            var scriptpresent = document.getElementById('3e04123f-47b5-446c-90ef-e490b27efb51');
            if (scriptpresent) {
                return;
            }
            var pScript = document.createElement('script');
            pScript.setAttribute('src', "/assets/js/dist/herobanner/herobanner-min.js");
            pScript.setAttribute('onload', dFunction + "('3e04123f-47b5-446c-90ef-e490b27efb51')");
            pScript.setAttribute('id', '3e04123f-47b5-446c-90ef-e490b27efb51');
            pScript.setAttribute('data-herobannerconfig', '{"Title":"Events","HeadingTag":"h1","VideoModalTitle":null,"Subtitle":null,"ImageUrl":"","ImageHeight":0,"ImageWidth":0,"VideoImageUrl":"","VideoImageHeight":0,"VideoImageWidth":0,"VideoUrl":null,"LinkToExternalUrl":null,"LinkToUrl":"","Uid":"3e04123f-47b5-446c-90ef-e490b27efb51","mediaSelector":"Image","pageSelector":"Internal","DisplayCTA":false,"CTAButtonText":null,"AlignWithBreadCrumbs":false,"AlignWithNonOverLapping":false,"HeroVideoVideoBackgroundColor":null}');
            pScript.setAttribute('data-uid', '3e04123f-47b5-446c-90ef-e490b27efb51');
            document.body.appendChild(pScript);
        });
    }

</script>


