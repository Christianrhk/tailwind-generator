        box-shadow: none;
    }
    .menubar li.has-sub > div > ul {
        padding: 0;
    }
    .menubar li.has-sub > div > ul > li.submenu-head {
        display: none;
    }
    .menubar li.slide-menu-profile {
        margin: -8px 0 10px;
        padding-left: 23px;
        padding-bottom: 10px !important;
    }
    .menubar li.slide-menu-profile .picture {
        margin-top: 18px;
    }
    .dropdown.menu-profile {
        position: relative !important;
        top: auto;
        right: auto;
    }
    .notice.badge {
        top: -8px;
