                    <script>
                        $(document).ready(function () {
                            if (typeof(MktoForms2) !== 'undefined') {
                                $(".error-msg").hide();
                                try {
                                        
                                        MktoForms2.setOptions({
                                            formXDPath: "/rs/610-KAK-266/images/marketo-xdframe-relative.html"
                                        });
                                        
                                    MktoForms2.whenReady(function (form) {
                                        form.addHiddenFields({
                                            'gAClientID': GetGAClientID()
                                        });
                                    });
                                    MktoForms2.loadForm("//lp.svb.com", "610-KAK-266", 2227, function (form) {
                                        UpdateMarketoForm();


	                                    // Track
                                        $("#mktoForm_2227").on("click", function () {
		                                    if (!dlFormInit ){
			                                    window.dataLayer = window.dataLayer || [];
			                                    dataLayer.push({
                                                    'Form': 'Newsroom',
                                                    'event': 'formInitialized'
                                                });
			                                    dlFormInit = true;
		                                    }
                                        });

                                        //Add an onSuccess handler
                                        form.onSuccess(function () {
                                            SetCookie("ecmFormFilled", true, 365);
                                            window.dataLayer = window.dataLayer || [];
                                            dataLayer.push({
                                                'Form': 'Newsroom',
                                                'event': 'formSubmitted'
                                            });

                                            // close form modal and show thank you
                                            $('#formModal').hide();
                                            $('body').removeClass("no-scroll");
                                            $('#thanksModal').css('display', 'block');
                                            modalTimer = setTimeout(autoCloseModal, 10000);
                                            $("#mktoForm_2227 button").prop('disabled', false);
                                            return false;
                                        });
                                    });
                                } catch (err) {
                                    LogError(err);
                                    ShowErrorMessage();
                                }
                            } else {
                                ShowErrorMessage();
                            }
                        });

                errorTrackerCreated = false;
                function LogError(err) {
                    if (!errorTrackerCreated) {
                        ga('create', 'UA-84999-4', 'auto', 'errorTracker');
                        errorTrackerCreated = true;
                    }
                    ga('errorTracker.send', {
                        hitType: 'event',
                        eventCategory: 'Newsroom',
                        eventAction: 'Javascript Error',
                        eventLabel: err
                    });
                    console.log(err);
                }
                function ShowErrorMessage() {
                    $("#mktoForm_2227").hide();
                    $(".error-msg").show();
                }
                    </script>
                    <noscript>
                        Your browser does not support JavaScript!
                    </noscript>
                    <!-- end form content -->
