<script>
    dlFormInit = false;

    $(document).ready(function () {
        // if modal popup exists, move it outside of main-block so it displays on top of nav
        if ($('#formModal') !== undefined) {
            var form = $('#formModal').detach();
            form.appendTo("body");
            form = $('#thanksModal').detach();
            form.appendTo("body");
        }

        // Adjust bottom margin on hero
        BHSetBottomMargin();

        $(window).on('resized', function () {
            BHSetBottomMargin();
        });

        function IsOverflowing(e1, e2) {
            var e1Rect = $(e1)[0].getBoundingClientRect();
            var e2Rect = $(e2)[0].getBoundingClientRect();
            return e1Rect.bottom > e2Rect.bottom;
        }

        function BHSetBottomMargin() {
            // Add/Remove padding from the bottom of the hero if the form container sticks out the bottom
            if (IsOverflowing(".P11__form-container", ".P11")) {
                $(".P11__padding").removeClass("md:mb-0").addClass("md:mb-12");
            } else {
                $(".P11__padding").removeClass("md:mb-12").addClass("md:mb-0");
            }
        }

    });
</script>

