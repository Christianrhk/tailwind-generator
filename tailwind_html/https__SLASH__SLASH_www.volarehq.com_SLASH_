      <script>
        const FreeTrialApp = {
          data() {
            return {
              name: '',
              email: '',
              venue: '',
              loading: false,
              error: null,
              submitted: false
            }
          },
          computed: {
            valid () {
              // /.+@.+/.test(this.email) || 'Your e-mail address does not look quite right'
              return !!this.name && /.+@.+/.test(this.email)
            }
          },
          methods: {
            onClick() {
              if (!this.valid) {
                return
              }
              this.loading = true
              this.error = null
              const { name, venue, email, type = 'freeTrial' } = this
              const url = `/.netlify/functions/submission`
              const payload = { name, venue, email, type }
              axios.post(url, payload).then(() => {
                this.submitted = true
                this.loading = false
              }).catch(e => {
                this.error = e.toString()
                this.loading = false
              })
            }
          }
        }

        Vue.createApp(FreeTrialApp).mount('#free-trial')
      </script>

      <script>
        const ContactUsApp = {
          data() {
            return {
              name: '',
              email: '',
              venue: '',
              message: '',
              loading: false,
              error: null,
              submitted: false
            }
          },
          computed: {
            valid () {
              return !!this.name && !!/.+@.+/.test(this.email) && !!this.message
            }
          },
          methods: {
            onClick() {
              if (!this.valid) {
                return
              }
              this.loading = true
              this.error = null
              const { name, venue, email, message, type = 'contactForm' } = this
              const url = `/.netlify/functions/submission`
              const payload = { name, venue, email, message, type }
              axios.post(url, payload).then(() => {
                this.submitted = true
                this.loading = false
              }).catch(e => {
                this.error = e.toString()
                this.loading = false
              })
            }
          }
        }

        Vue.createApp(ContactUsApp).mount('#contact-us')
      </script>

      <script>
        const URL = 'https://blog.volarehq.com/ghost/api/v3/content/posts/?key=eec4e65eb6a883b4b8048e6685&page=1&include=authors,tags&limit=3&fields=feature_image,primary_tag,published_at,custom_excerpt,url,title'
        const BlogApp = {
          created() {
            this.fetch()
          },
          data() {
            return {
              items: [],
              loading: true,
              error: null,
            }
          },
          methods: {
            formatDate(date) {
              const options = { weekday: 'long', year: 'numeric', month: 'long', day: 'numeric' }
              return (new Date(date)).toLocaleDateString('en-US', options)
            },
            fetch() {
              this.loading = true
              this.error = null
              fetch(URL)
                .then(response => response.json())
                .then(data => {
                  this.items = data.posts
                  this.loading = false
                })
                .catch(e => {
                  console.error(e)
                  this.error = e.toString()
                  this.loading = false
                })
            }
          }
        }

        Vue.createApp(BlogApp).mount('#blog')
      </script>

