<div class="text-center py-6 px-2 md:py-8 ">
<h1 class="text-2xl md:text-4xl">Welcome to wpUniverse!<sup class="text-xs md:text-base">BETA</sup></h1>
<p class="text-base">A custom search engine for Wordpress and web developers</p>
</div>
<main id="main" x-data="cseInit()" x-init="getSearchResultsWithParams" class="site-main main-home flex flex-col md:flex-row w-full">
<section class="cell search w-full sm:w-2/5 lg:w-1/5 flex-auto p-3" @keydown.enter.window="getSearchResults">
<div class="serch-area w-full mx-auto">
<p x-text="status" class="text-center text-sm sm:text-left mb-2"></p>
<form class="flex flex-wrap cse-form relative">
<input id="autoComplete" tabindex="1" class="flex-auto w-full mb-2 text-base py-3 px-2 cse-form--input autoComplete" type="text" name="theQuery" x-model.lazy="theQuery">
<button title="search" id="main-search-btn" class="flex-1 text-sm py-2 mr-1" type="submit" @click.prevent.stop.self="getSearchResults($event)">Search 🔎</button>
<button title="clear" id="clear-search-btn" class="flex-1 text-base py-2" @click.prevent.stop="clearSearch"> ✖ </button>
</form>
<template x-if="spelling">
<p class="my-2">Did you mean <a :href="'/?q=' + spelling" x-text="spelling"></a></p>
</template>
<div class="clear my-3">
<template x-if="prevPageIndex">
<button class="float-left text-sm" @click="setStartIndex('prev',$event)">Prev</button>
</template>
<template x-if="nextPageIndex">
<button class="float-right text-sm" @click="setStartIndex('next', $event)">Next</button>
</template>
</div>
<template x-if="facets">
<div class="facets flex flex-wrap">
<p class="text-sm my-1 w-full">Search "<span class="font-bold" x-html="theQuery"></span>" in specific verticals: </p>
<span class="facet m-1 text-xs md:text-sm flex-auto inline-block text-center p-1 selected" :id="facet_all" @click="$nextTick( () => { clearSelected(); $el.classList.add('selected') })">
<a class="facet__link p-1 block" @click.prevent="getFacteResults($event)" :href="all">All</a>
</span>
