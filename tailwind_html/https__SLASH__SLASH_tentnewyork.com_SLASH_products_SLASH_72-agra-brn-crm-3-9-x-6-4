    //=== COLLECTION ===
    // Run collection on all templates/pages if we're running the 5+ script
    wsgRunCollection = true;
    //=== AJAX CART ===
    
      if ( wsgAjaxSettings && (
          wsgAjaxSettings.price || wsgAjaxSettings.linePrice || wsgAjaxSettings.subtotal || wsgAjaxSettings.checkoutBtn
        )
