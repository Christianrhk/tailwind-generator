	function release(idx, el){
		var delay = 100 * idx;
		setTimeout(function(){
			jQuery(el).removeClass('beforeload');
		}, delay);
	}
	jQuery(document).ready(function(){
		jQuery('.beforeload').each(function(i, el){
			release(i, el);
		});
	});
