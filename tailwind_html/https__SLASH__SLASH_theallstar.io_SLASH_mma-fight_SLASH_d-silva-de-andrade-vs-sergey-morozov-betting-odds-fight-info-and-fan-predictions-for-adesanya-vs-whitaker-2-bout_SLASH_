		window.fanaticsApp
			.firestore()
			.collection('fight_cards')
			.where('fight_league.id', '==', 1)
			.where('status', '!=', 'Ended')
			.get()
			.then(function(cardDocs){
				var fightCards = {};
				var matchups = {};
				var byDateEt = {};
				var byDateEtCard = {};
				cardDocs.docs.forEach(function(cardDoc){
					var d = cardDoc.data();
					fightCards[cardDoc.id] = 
					d.fightsOnCard.forEach(function(fight){
						if(fight.status != 'cancelled'){
							matchups[fight.matchup.normalize('NFD').replace(/[\u0300-\u036f]/g, '')] = d.name + ': ' + d.headline_matchup;
						}
					});
					var utcSeconds = d.timestamp / 1000;
					var _d = new Date(0); 
					_d.setUTCSeconds(utcSeconds);
					var utcDay = _d.toDateString();
					byDateEtCard[utcDay] = d.name + ': ' + d.headline_matchup;
				});

				var nowMinus30 = new Date().getTime() - (1000 * 30 * 60);
				var nowMinus500 = new Date().getTime() - (1000 * 1000 * 60);

				var isFiltered = false;

				if(fightersFilter.length == 2){
					var query =  window.fanaticsApp
								.firestore()
								.collection('odds_comparison')
								.doc('us')
								.collection('odds')
								.where('tick', '>=', nowMinus500)
								.where('home_team', 'in', fightersFilter);
					isFiltered = true;
				} else {
					var query =  window.fanaticsApp
								.firestore()
								.collection('odds_comparison')
								.doc('us')
								.collection('odds')
								.where('tick', '>=', nowMinus500);
				}

				query
					.onSnapshot((documentSnapshot) => {
						var dataSet = [];

						//loop and blend.
						var allFights = {};

						documentSnapshot.docs.forEach(function(doc){
							var oddsData = doc.data();
							var passThru = (matchupFilter.length == 0);
							if(passThru || (fightersFilter.indexOf(oddsData.home_team) >= 0 && fightersFilter.indexOf(oddsData.away_team) >= 0)){
								var matchup = oddsData.home_team + ' vs ' + oddsData.away_team;

								if(!allFights[matchup]){
									allFights[matchup] = oddsData;
								} else {
									allFights[matchup].bookmakers = allFights[matchup].bookmakers.concat(oddsData.bookmakers);
								}
							}
							
						});

						allFights = Object.values(allFights);
						allFights.sort((b, a) => (a.timestamp > b.timestamp) ? 1 : ((b.timestamp > a.timestamp) ? -1 : 0))

						allFights.forEach(function(oddsData){
							//var oddsData = doc.data();
							var redCorner = {
								commence_time: oddsData.commence_time
							};
							var blueCorner = {
								commence_time: oddsData.commence_time
							};
							
							var matchupNames = [
								oddsData.home_team + ' vs ' + oddsData.away_team,
								oddsData.away_team + ' vs ' + oddsData.home_team,
							];
							var card = 'Future Events';
							var onlyDay = true;
							matchupNames.forEach(function(matchupName){
								if(matchups[matchupName]){
									card = matchups[matchupName];
									onlyDay = false;
								}
							});
							
							if(onlyDay){
								matchupNames.forEach(function(matchupName){
									Object.keys(matchups).forEach(function(matchupNameInCard){
										if(onlyDay && similarity(matchupName, matchupNameInCard) > 0.7){
											card = matchups[matchupNameInCard];
											onlyDay = false;
										}
									});
									
								});
							}

							if(onlyDay){
								var day = new Date(oddsData.timestamp).toDateString();
								if(byDateEtCard[day]){
									card = byDateEtCard[day];
									onlyDay = false;
								}

							}
							
							redCorner.fighter = oddsData.home_team;
							blueCorner.fighter = oddsData.away_team;
							bookies.forEach(function(bookie){
								redCorner[bookie] = '';
								blueCorner[bookie] = '';
							});
							var numberOfLines = 0;
							oddsData.bookmakers.forEach(function(line){
								var bookie = line.key;
								numberOfLines++;
								redCorner[bookie] = toAmerican( (line.markets[0].outcomes[0]['name'] == redCorner.fighter) ? line.markets[0].outcomes[0].price: line.markets[0].outcomes[1].price );
								blueCorner[bookie] = toAmerican( (line.markets[0].outcomes[0]['name'] != redCorner.fighter) ? line.markets[0].outcomes[0].price: line.markets[0].outcomes[1].price );


								redCorner[bookie+'_dec'] =  (line.markets[0].outcomes[0]['name'] == redCorner.fighter) ? line.markets[0].outcomes[0].price: line.markets[0].outcomes[1].price ;
								blueCorner[bookie+'_dec'] =  (line.markets[0].outcomes[0]['name'] != redCorner.fighter) ? line.markets[0].outcomes[0].price: line.markets[0].outcomes[1].price ;
							});
							if(numberOfLines > 0){
								redCorner.card = card;
								blueCorner.card = card;
								
								dataSet.push(redCorner);
								dataSet.push(blueCorner);

								var day = new Date(oddsData.timestamp).toDateString();


								if(onlyDay){
									if(!byDateEt[day]){
										byDateEt[day] = {
											title: card,
											dateTime: day,
											fights: []
										};
									}
								} else {
									if(!byDateEt[card]){
										byDateEt[card] = {
											title: card,
											dateTime: day,
											fights: []
										};
									}
									day = card;
								}


								byDateEt[day].fights.push({
									redCorner: redCorner,
									blueCorner: blueCorner
								});
							}
							
						});
						
						var $ = jQuery;
						for(var day in byDateEt){
							var card = byDateEt[day];
							var table = $('.odds-clone').clone();
							var captionTitle = (card.title == 'Future Events') ? card.dateTime: card.title + ' - ' + card.dateTime;
							var caption = $('<caption />').html('<h6 style=\'color: white; margin: 0; text-transform: none;\'>' + captionTitle + '</h6>');
							table.append(caption);

							if(isFiltered){
							}  else {
								var th = jQuery('<th />').text('Matchup');
								$(table).find('thead tr').append(th);
								bookies.forEach(function(bookie){
									var th = jQuery('<th />').text(bookiesconfig[bookie]['name']);
									$(table).find('thead tr').append(th);
								});
							}
							
							
							card.fights.forEach(function(fight){


								if(isFiltered){
									var th = jQuery('<th />', {
										'class': 'bookie-name'
									}).text('Bookmaker');
									$(table).find('thead tr').append(th);
									var th = jQuery('<th />').text(fight.redCorner.fighter);
									$(table).find('thead tr').append(th);
									var th = jQuery('<th />').text(fight.blueCorner.fighter);
									$(table).find('thead tr').append(th);

									var bestLineRed = 0;
									var bestLineBlue = 0;
									var bestBookiesRed = [];
									var bestBookiesBlue = [];

									bookies.forEach(function(bookie){
										if(fight.redCorner[bookie+'_dec'] > bestLineRed){
											bestBookiesRed = [bookie];
											bestLineRed = fight.redCorner[bookie+'_dec'];
										} else if(fight.redCorner[bookie+'_dec'] == bestLineRed){
											bestBookiesRed.push(bookie);
										}

										if(fight.blueCorner[bookie+'_dec'] > bestLineBlue){
											bestBookiesBlue = [bookie];
											bestLineBlue = fight.blueCorner[bookie+'_dec'];
										} else if(fight.blueCorner[bookie] == bestLineBlue){
											bestBookiesBlue.push(bookie);
										}
									});


									bookies.forEach(function(bookie){
										var row = $('<tr />');
										row.append(
											$('<td />', { 'class': 'bookie-name'}).append(bookiesconfig[bookie]['name'])
										);
										var redOdds = $('<div />', {
											'class': (bestBookiesRed.indexOf(bookie) >= 0) ? 'best-odds': ''
										}).html('<span><a class="odds-link" href="https://theallstar.io/visit/' + bookiesconfig[bookie].url + '">' + fight.redCorner[bookie] + '</a></span>');

										var blueOdds = $('<div />', {
											'class': (bestBookiesBlue.indexOf(bookie) >= 0) ? 'best-odds': ''
										}).html('<span><a class="odds-link" href="https://theallstar.io/visit/' + bookiesconfig[bookie].url + '">' + fight.blueCorner[bookie] + '</a></span>');

										row.append(
											$('<td />', { 'class': 'bookie-line'}).append(redOdds)
										);
										row.append(
											$('<td />', { 'class': 'bookie-line'}).append(blueOdds)
										);

										if(fight.redCorner[bookie]){
											$(table).append(row);
										}

										
									});


								} else {
									var row = $('<tr />');
									
									fight.matchup = fight.redCorner.fighter + ' vs ' + fight.blueCorner.fighter;

									var linked = false;
									if(matchupLinks[fight.redCorner.fighter + ' vs ' + fight.blueCorner.fighter]){
										var linked = $('<a />', {
											href: matchupLinks[fight.matchup]
										}).html(fight.redCorner.fighter + '<br />' + fight.blueCorner.fighter);
									} else if(matchupLinks[fight.blueCorner.fighter + ' vs ' + fight.redCorner.fighter]){
										var linked = $('<a />', {
											href: matchupLinks[fight.blueCorner.fighter + ' vs ' + fight.redCorner.fighter]
										}).html(fight.redCorner.fighter + '<br />' + fight.blueCorner.fighter);
									} else {
										for(var key in matchupLinks){
											var matchup1 = fight.redCorner.fighter + ' vs ' + fight.blueCorner.fighter;
											var matchup2 = fight.blueCorner.fighter + ' vs ' + fight.redCorner.fighter;

											if(similarity(matchup1, key) > 0.7 || similarity(matchup2, key) > 0.7){
												var linked = $('<a />', {
													href: matchupLinks[key]
												}).html(fight.redCorner.fighter + '<br />' + fight.blueCorner.fighter);
											}
										}
									}




									if(linked != false){
										row.append(
											$('<td />', { 'class': 'fighter-names'}).append(linked)
										);
									} else {


										//find a link!
										//

										row.append(
											$('<td />', { 'class': 'fighter-names'}).html(fight.redCorner.fighter + '<br />' + fight.blueCorner.fighter)
										);
									}

									var bestLineRed = 0;
									var bestLineBlue = 0;
									var bestBookiesRed = [];
									var bestBookiesBlue = [];

									bookies.forEach(function(bookie){
										if(fight.redCorner[bookie+'_dec'] > bestLineRed){
											bestBookiesRed = [bookie];
											bestLineRed = fight.redCorner[bookie+'_dec'];
										} else if(fight.redCorner[bookie+'_dec'] == bestLineRed){
											bestBookiesRed.push(bookie);
										}

										if(fight.blueCorner[bookie+'_dec'] > bestLineBlue){
											bestBookiesBlue = [bookie];
											bestLineBlue = fight.blueCorner[bookie+'_dec'];
										} else if(fight.blueCorner[bookie] == bestLineBlue){
											bestBookiesBlue.push(bookie);
										}
									});

									bookies.forEach(function(bookie){
										var redOdds = $('<div />', {
											'class': (bestBookiesRed.indexOf(bookie) >= 0) ? 'best-odds': ''
										}).html('<span><a class="odds-link" href="https://theallstar.io/visit/' + bookiesconfig[bookie].url + '">' + fight.redCorner[bookie] + '</a></span>');

										var blueOdds = $('<div />', {
											'class': (bestBookiesBlue.indexOf(bookie) >= 0) ? 'best-odds': ''
										}).html('<span><a class="odds-link" href="https://theallstar.io/visit/' + bookiesconfig[bookie].url + '">' + fight.blueCorner[bookie] + '</a></span>');

										row.append(
											$('<td />', { 'class': 'bookie-line'}).append(redOdds).append(blueOdds)
										);
									});
									$(table).append(row);
								}


								
							});
							if(captionTitle.toLowerCase().indexOf('ufc') >= 0){
								$(table).removeClass('odds-clone').prependTo('#odds-comparison');
							}
							
						}
					});
			});
</script>
