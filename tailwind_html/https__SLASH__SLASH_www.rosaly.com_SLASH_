    <script>
        var buttons = document.getElementsByClassName("js-switch-trust")

        function switchTrustArrows(e) {
            if (window.innerWidth >= 1024) {
                return
            }
            var resetArrow = function (arrow, container) {
                arrow.style.transform = "rotate(0deg)"
                arrow.firstElementChild.style.fill = "#F3F5FF"
                arrow.lastElementChild.style.stroke = "#012AFF"

                const nodesToHide = container.querySelectorAll('.js-to-hide')
                for (let node of nodesToHide) {
                    node.classList.add('hidden')
                }
                container.classList.remove('h-52')
                container.classList.add('h-20')
            }

            var deploySection = function (arrow, container) {
                if (arrow.style.transform === "rotate(180deg)") {
                    return
                }

                arrow.style.transform = "rotate(180deg)"
                arrow.firstElementChild.style.fill = "#012AFF"
                arrow.lastElementChild.style.stroke = "#FFFFFF"

                const nodesToHide = container.querySelectorAll('.js-to-hide')
                container.classList.remove('h-20')
                container.classList.add('h-52')
                for (let node of nodesToHide) {
                    node.classList.remove('hidden')
                }
            }

            var buttonTarget = e.target
            var i = 0
            while (!buttonTarget.classList.contains("js-switch-trust") && i++ < 3) {
                buttonTarget = buttonTarget.parentElement
            }

            var arrow
            for (var button of buttons) {
                if (button === buttonTarget) {
                    continue
                }
                arrow = button.children[0]
                resetArrow(arrow, button)
            }
            arrow = buttonTarget.children[0]
            deploySection(arrow, buttonTarget)
        }

        for (var button of buttons) {
            button.addEventListener('click', switchTrustArrows)
        }
        buttons[0].click()

    </script>
