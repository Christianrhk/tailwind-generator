            toBeClosed.find('> a > span').toggleClass('icon-down icon-up');
        });

        e.preventDefault();

        if (isOpen) return;

        toBeOpen.find('> a > span').toggleClass('icon-down icon-up');
        toBeOpen.find('> ul').slideDown(200);
    });
