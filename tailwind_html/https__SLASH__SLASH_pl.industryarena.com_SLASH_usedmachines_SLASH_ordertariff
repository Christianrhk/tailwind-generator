    function toggleModuleList(name, isDropdownClick = false) {
        let searchModule = document.getElementById('header-search-module'),
            moduleList = document.getElementById('moduleList'),
            inputWrapper = document.getElementById('input-wrapper');

        if (name !== null) {
            document.getElementById('current-module').innerText = list[name];
            document.getElementById('module-input').setAttribute('value', name);
        }

        if (searchModule.classList.contains('active') && !isDropdownClick) {
            searchModule.style.justifyContent = 'space-between';
            moduleList.style.display = 'none';
            inputWrapper.style.borderRadius = '20px';
            return;
        }

        if (isDropdownClick) {
            if (moduleList.style.display == 'none') {
                moduleList.style.display = 'block';
                inputWrapper.style.borderRadius = '20px 20px 0 20px';
                return;
            }
            moduleList.style.display = 'none';
            inputWrapper.style.borderRadius = '20px';
            return;
        }

        searchModule.classList.add('active');
        searchModule.style.justifyContent = 'space-between';
    }</script>                                             </div>
