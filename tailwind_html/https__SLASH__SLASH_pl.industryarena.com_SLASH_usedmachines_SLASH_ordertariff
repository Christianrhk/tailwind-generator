                    //set timezonefield if exist
                    var timezonefield = document.getElementById('timezonefield');
                    var timezonefieldmobile = document.getElementById('timezonefield-mobile');
                    var d = new Date();
                    if (timezonefield) {
                        timezonefield.value = d.getTimezoneOffset();
