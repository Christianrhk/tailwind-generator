<script>
    window.onload = () => {
        //input settings
        document.querySelectorAll('#quoteForm #next').forEach(btn => {
            btn.addEventListener('click', (event) => {
                const currQID = btn.closest('.options').dataset.question;
                const nextQuestionIndex = parseInt(currQID.split('-')[1]) + 1;
                const nextQuestionId = 'question-' + nextQuestionIndex;
                const textFieldInputName = document.querySelector('#' + currQID + ' input');
                const textFieldTextareaName = document.querySelector('#' + currQID + ' textarea');
                if (textFieldInputName == null) {
                    console.log(textFieldTextareaName.name) 
                    nextQuestion(nextQuestionId, textFieldTextareaName.name, 'textarea');
                } else {
                    console.log('textarea')
                    nextQuestion(nextQuestionId, textFieldInputName.name, 'input');
                }

            })
        });
        // Checkbox Settings
        var checkBoxes = document.querySelectorAll('.checkboxes input');
        var checkboxValueBag = [];
        checkBoxes.forEach((box) => {
            box.addEventListener('change', (e) => {
                         if (! e.target.checked) {
                    e.target.parentElement.classList.remove('selected')
                    const index = checkboxValueBag.indexOf(e.target.value);
                    checkboxValueBag.splice(index, 1);
                    console.log(checkboxValueBag)
                }
              else{
                box.parentElement.classList.add('selected')
              }
                const optionFor = box.closest('.options').dataset.question;
                const nextQuestionIndex = parseInt(optionFor.split('-')[1]) + 1;
                const nextQuestionId = 'question-' + nextQuestionIndex;
                if (box.closest('.checkboxes').classList.contains('multichoice')&& e.target.checked) {
                    if (!checkboxValueBag.includes(box.value)) {
                        checkboxValueBag.push(box.value);
                        console.log(checkboxValueBag)
                    }
                    console.log(checkboxValueBag)
                    document.querySelector('.checkboxes input[name="' + box.id + '"]').value = checkboxValueBag.toString();
                } else {
                    if (box.checked) {
                        document.querySelector('.checkboxes input[name="' + box.id + '"]').value = box.value;
                        var uniqueClass = 'class-' + Math.floor(Math.random() * 100000);
                        box.closest('.checkboxes').classList.add(uniqueClass);
                        const currCheckBoxes = document.querySelectorAll('.' + uniqueClass + ' input');
                        currCheckBoxes.forEach((secondNest) => {
                            if (secondNest != e.target) {
                                secondNest.checked = false;
                                secondNest.parentElement.classList.remove('selected')
                            }
                        })
                        document.querySelector('#' + optionFor).classList.add('answered');
                        nextQuestion(nextQuestionId, box.id, 'input');
                    } else {
                        document.querySelector('.checkboxes input[name="' + box.id + '"]').value = '';
                        box.parentElement.classList.remove('selected')
                        document.querySelector('#' + optionFor).classList.remove('answered');
                        nextQuestion(nextQuestionId, box.id, 'input');
                    }
                }
            })
        })
    }

    function nextQuestion(id, fieldName, fieldType) {
        (function($) {
            const field = $(fieldType + '[name="' + fieldName + '"]')
            const fieldValue = field.val();
            const wrapId = $(fieldType + '[name="' + fieldName + '"]').parent().parent().attr('id');
            const optionsId = field.parent().parent()[0].id;
            // textbox
            // get last question id
            const lastQuestionIndex = $('.question').length - 1;
            if (id != 'question-' + (lastQuestionIndex + 1)) {
                $('#' + id).show();
            } else {
                sendRequest();
            }
        })(jQuery);
    }

    function sendRequest() {
        (function($) {
            $.fn.isValid = function() {
                return this[0].checkValidity()
            }
            if (!$('#quoteForm').isValid()) {
                alert('Some fields (marked orange) are invalid.');
            } else {
                document.querySelector(".submit-btn").disabled = true;
                // send data logic
                var data = $('#quoteForm').serialize(); //serializing the form and turning it into a string
                var formData = new FormData(); //
                formData.append('action', 'quote_mail'); //action = enquiry
                formData.append('quote_mail', data); //enquiry = string
                $.ajax("https://bestratedvoip.com/wp-admin/admin-ajax.php", {
                    type: 'POST',
                    data: formData,
                    contentType: false,
                    processData: false,
                    success: function(response) {
                        console.log(response)
                        $('#quoteForm').hide();
                        $('.feedback-success').css('display', 'flex');
                    },
                    error: function(response) {
                        $('#quoteForm').hide();
                        $('.feedback-failed').css('display', 'flex');
                        console.log(response)
                    },
                });

                // 

            }
        })(jQuery);
    }
</script>
