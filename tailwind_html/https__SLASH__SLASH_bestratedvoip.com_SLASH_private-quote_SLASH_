<script>
    Window.onload = () =>{
var botTogglers = document.querySelectorAll('.toggle-bot-window');
botTogglers.forEach((toggler)=>{
	toggler.addEventListener('click',()=>{
		openInbox()
	})
})
}
    function flashTitle(pageTitle, newTitle) {
        if (document.title == pageTitle) {
            document.title = newTitle;
        } else {
            document.title = pageTitle;
        }
    }
    var interval;
    var allCheckboxes = document.querySelectorAll('.chatbot-wrapper input[type="checkbox"]');
    var checkedboxValues = [];
    jQuery(($) => {
        setTimeout(() => {
            $('.inbox-icon').each((index, icon) => {
                if (!icon.classList.contains('open')) {
                    interval = setInterval(flashTitle("Best Rated VoIP", '(1) new message'), 1500);
                    $('.badge-danger').show()
                    $('.popup').show()
                }
            })
        }, 9000)
        $(document).on('change', '.btn input[type="checkbox"]', (checkbox) => {

            updateCheckboxStyles()
            $('.typing').show()
            if (checkbox.checked) {
                checkedboxValues.push(checkbox.value)
                document.querySelector('.to-input').value = checkedboxValues.toString();
            }
        })
    });
    updateCheckboxStyles();

    function updateCheckboxStyles() {

        jQuery(($) => {
            $('.btn input[type="checkbox"]').each((idx, box) => {
                if (box.checked) {
                    var checkedCheckboxes = $(box).parent().removeClass('btn-outline-primary');
                    var checkedCheckboxes = $(box).parent().addClass('btn-primary');
                    $(box).attr("disabled", true);
                    document.querySelector('.to-input').value = $(box).val();
                }
                // else {
                //     var checkedCheckboxes = $(box).parent().addClass('btn-outline-primary');
                //     var checkedCheckboxes = $(box).parent().removeClass('btn-primary');
                //     // remove the existing stored value
                //     var valToBeSpliced = $(box).val();
                //     var valTbSIdx = checkedboxValues.indexOf(valToBeSpliced);
                //     if (valTbSIdx > -1) {
                //         checkedboxValues.splice(valTbSIdx, 1);
                //          }
                // }
            });
            if (document.querySelector('.to-input').value != '') {
                $('.btn-outline-primary').addClass('d-none')
                sendMessage(event)
            }
        });
    }

    function openInbox() {
        jQuery(($) => {
            $('.inbox').toggle()
            $('.inbox-icon').each((index, icon) => {
                if (!icon.classList.contains('open')) {
                    icon.style.transform = 'rotate(0deg)';
                    clearInterval(interval)
                    document.title = "Best Rated VoIP";
                    $('.badge-danger').hide()
                    $('.popup').hide()
                    setTimeout(() => {
                        icon.classList.add("open");
                    }, 200)
                } else {
                    icon.style.transform = 'rotate(360deg)';
                    setTimeout(() => {
                        icon.classList.remove("open");
                    }, 200)
                }
            })
        })
    }

    function sendMessage(e) {
        if (e) {
            e.preventDefault();
        }
        jQuery(($) => {
            // Detect question being answered
            var entryBagTextarea = $('#entryBagTextarea');
            const lastId = $('.messages').find('.from:last').attr('id');
            var questionId = lastId;
            if (lastId.indexOf('randId') > -1) {
                questionId = $($('.from')[$('.from').length - 2]).attr('id');
                console.log(questionId)
            }

            const input = $('.to-input').val();
            const toRandId = Math.floor(Math.random() * 10000);
            setTimeout(() => {
                questionProcessor(input);
            }, 1000)
            var toInput = $('.to-input').val();
            checkedboxValues = []

            function questionProcessor(input) {
                //    Create a message card for each entry
                const toMessageCard = (' <div class="my-2 to bg-primary card w-full p-2" id = "unique' + toRandId + '">  <p class = "text-white" > ' + input + ' </p> </div>')
                if (questionId == 'question-id-1') {
                    questionId1(input)
                } else if (questionId == 'question-id-1-2') {
                    entryBagTextarea.val(entryBagTextarea.val() + 'Issues described by user : ' + toInput + '\n');
                    questionsForRespose1();
                } else if (questionId == 'question-id-1-1-1') {
                    var resp = input = 1 ? 'yes' : 'no';
                    entryBagTextarea.val(entryBagTextarea.val() + 'has cloud phone system ? : ' + resp + '\n');
                    questionsForRespose1()
                } else if (questionId == 'question-id-1-1-2') {
                    var resp = input;
                    entryBagTextarea.val(entryBagTextarea.val() + 'has how many employees ? : ' + toInput + '\n');
                    questionsForRespose1()
                } else if (questionId == 'question-id-1-1-3') {
                    var resp = input;
                    entryBagTextarea.val(entryBagTextarea.val() + 'Features most important to the business ? : ' + toInput + '\n');
                    questionsForRespose1()
                } else if (questionId == 'question-id-1-1-4') {
                    var resp = input;
                    entryBagTextarea.val(entryBagTextarea.val() + 'When do you need your new phone system ? : ' + toInput + '\n');
                    questionsForRespose1()
                } else if (questionId == 'question-id-1-1-5') {
                    var resp = input;
                    entryBagTextarea.val(entryBagTextarea.val() + 'What is the name of the company needing better phone service? : ' + resp + '\n');
                    $('.messages').append(toMessageCard)
                    $('#unique' + toRandId)[0].scrollIntoView();
                    questionsForRespose1()
                } else if (questionId == 'question-id-1-1-5-1') {
                    var resp = input;
                    $('.messages').append(toMessageCard)
                    $('#unique' + toRandId)[0].scrollIntoView();
                    entryBagTextarea.val(entryBagTextarea.val() + 'Full name : ' + resp + '\n');

                    questionsForRespose1()
                } else if (questionId == 'question-id-1-1-5-2') {
                    var resp = input;
                    entryBagTextarea.val(entryBagTextarea.val() + 'Email ? : ' + resp + '\n');
                    $('.messages').append(toMessageCard)
                    $('#unique' + toRandId)[0].scrollIntoView();
                    questionsForRespose1()
                } else if (questionId == 'question-id-1-1-5-3') {
                    var resp = input;
                    entryBagTextarea.val(entryBagTextarea.val() + 'Phone number ? : ' + resp + '\n');
                    $('.messages').append(toMessageCard)
                    $('#unique' + toRandId)[0].scrollIntoView();
                    sendBotMail();
                    var response = '<p>Great news, we received your request, you will hear from our team members shortly to begin working on this project! In the mean time, feel free to browse around. Here are a few links of some of our partners:</p><p><a href="https://bestratedvoip.com/nextiva/" target="_blank">Nextiva &nbsp;</a><br/><a href="https://bestratedvoip.com/ring-central/" target="_blank">Ring Central &nbsp;</a> <br/><a href="https://bestratedvoip.com/vonage/" target="_blank">Vonage &nbsp;</a></p>'
                    appendFromCard(response, '1-1-5-l')
                    $('.send-form').hide();
                } else {
                    response = 'Sorry,  I did not understand. Let us try that again using the provided options e.g 2';
                    appendFromCard(response, 'randId-' + Math.floor(Math.random() * 100))
                }
            }
            // Process the answer

            //  answer based functions
            function questionId1(input) {
                var response = ' ';
                if (input == 1) {
                    entryBagTextarea.val(entryBagTextarea.val() + 'New startup \n');
                    response = '<p>Congrats on your new startup! I have a few questions to make sure I get you to the right team.</p>';
                    appendFromCard(response, '1-1')
                    questionsForRespose1();
                } else if (input == 2) {
                    entryBagTextarea.val(entryBagTextarea.val() + 'Issues with provider \n');
                    response = '<p>What issues are you having with your current provider ?</p><div class="options p-1 bg-light"> <label class="btn btn-md btn-outline-primary"><input type="checkbox" class="form-check-input" name="single-i" id="" value="Reliability/quality issues"> Reliability/quality issues </label><label class="btn btn-md btn-outline-primary"><input type="checkbox" class="form-check-input" name="single-i" id="" value="support is horrible"> support is horrible </label><label class="btn btn-md btn-outline-primary"><input type="checkbox" class="form-check-input" name="single-i" id="" value="promises they can not deliver"> promises they can not deliver </label></div>';
                    appendFromCard(response, '1-2')
                } else if (input == 3) {
                    response = '<p> You came to the right place! &#9995; I have a few questions to make sure I get you to the right team. \n</p>';
                    appendFromCard(response, '1-3')
                    questionsForRespose1();
                } else {
                    response = '<p>Sorry,  I did not understand. Let us try that again using the provided options e.g 2</p>';
                    appendFromCard(response, 'randId-' + Math.floor(Math.random() * 100))
                }
            }

            function questionsForRespose1() {
                if (questionId.indexOf('1-1-') == -1) {
                    response = '<p> Do you already have a cloud phone system? </p><div class="options p-1 bg-light"><label class="btn btn-md btn-outline-primary mx-3"><input type="checkbox" class="form-check-input" name="hasCloudsystem" id="" value="1">  Yes</label><label class="btn btn-md btn-outline-primary mx-3"><input type="checkbox" class="form-check-input" name="hasCloudsystem" id="" value="2"> No</label></div>'
                    appendFromCard(response, '1-1-1')
                } else if (questionId == 'question-id-1-1-1') {
                    response = ' <p> Number of employees? </p><div class="options p-1 bg-light"><p> <label class="btn btn-md btn-outline-primary mx-1"><input type="checkbox" class="form-check-input" name="hasCloudsystem" id="" value="1-10">  1-10 </label> <label class="btn btn-md btn-outline-primary mx-1"><input type="checkbox" class="form-check-input" name="hasCloudsystem" id="" value="11-20">  11-20 </label> <label class="btn btn-md btn-outline-primary mx-1"><input type="checkbox" class="form-check-input" name="hasCloudsystem" id="" value="21-50">  21-50 </label> <label class="btn btn-md btn-outline-primary mx-1"><input type="checkbox" class="form-check-input" name="hasCloudsystem" id="" value="50-100">  50-100 </label> <label class="btn btn-md btn-outline-primary mx-1"><input type="checkbox" class="form-check-input" name="hasCloudsystem" id="" value="101-150">  101-150 </label> <label class="btn btn-md btn-outline-primary mx-1"><input type="checkbox" class="form-check-input" name="hasCloudsystem" id="" value="150+">  150+ </label></p></div>'
                    appendFromCard(response, '1-1-2')
                } else if (questionId == 'question-id-1-1-2') {
                    response = ' <p> What features are most important to your business? </p><div class="options p-1 bg-light"><label class="btn btn-md btn-outline-primary mx-1"><input type="checkbox" class="form-check-input" name="hasCloudsystem" id="" value="Audio/video conferencing">  Audio/video conferencing </label><label class="btn btn-md btn-outline-primary mx-1"><input type="checkbox" class="form-check-input" name="hasCloudsystem" id="" value=" cloud hosted solution">   cloud hosted solution </label><label class="btn btn-md btn-outline-primary mx-1"><input type="checkbox" class="form-check-input" name="hasCloudsystem" id="" value=" home/mobile worker">   home/mobile worker </label> <label class="btn btn-md btn-outline-primary mx-1"><input type="checkbox" class="form-check-input" name="hasCloudsystem" id="" value=" call monitoring/recording">   call monitoring/recording </label>  <label class="btn btn-md btn-outline-primary mx-1"><input type="checkbox" class="form-check-input" name="hasCloudsystem" id="" value=" contact center">   contact center </label><label class="btn btn-md btn-outline-primary mx-1"><input type="checkbox" class="form-check-input" name="hasCloudsystem" id="" value="unsure please advise">   unsure please advise</label></div>'
                    appendFromCard(response, '1-1-3')
                } else if (questionId == 'question-id-1-1-3') {
                    response = ' <p> When do you need your new phone system? </p><div class="options p-1 bg-light"><label class="btn btn-md btn-outline-primary mx-1"><input type="checkbox" class="form-check-input" name="hasCloudsystem" id="" value="Within 1 week">  Within 1 week </label><label class="btn btn-md btn-outline-primary mx-1"><input type="checkbox" class="form-check-input" name="hasCloudsystem" id="" value=" Within 1 month">  Within 1 month </label><label class="btn btn-md btn-outline-primary mx-1"><input type="checkbox" class="form-check-input" name="hasCloudsystem" id="" value=" Within 3 months">  Within 3 months </label> <label class="btn btn-md btn-outline-primary mx-1"><input type="checkbox" class="form-check-input" name="hasCloudsystem" id="" value=" 3 - 6 months">  3 - 6 months </label> <label class="btn btn-md btn-outline-primary mx-1"><input type="checkbox" class="form-check-input" name="hasCloudsystem" id="" value="6 + months from now"> 6 + months from now </label'
                    appendFromCard(response, '1-1-4')
                } else if (questionId == 'question-id-1-1-4') {

                    response = ' <p> What is the name of the company needing better phone service? </p>'
                    appendFromCard(response, '1-1-5')
                    $('.send-form .to-input').attr('placeholder', 'Company name');
                    $('.send-form').show();
                } else if (questionId == 'question-id-1-1-5') {
                    var response = '<p>Good news! \n We have the business phone service product youre looking for.</p><p>To get started, Please provide us with your full name.</p>'
                    $('.send-form .to-input').attr('placeholder', 'Your full name');
                    appendFromCard(response, '1-1-5-1')
                } else if (questionId == 'question-id-1-1-5-1') {
                    var response = '<p>Email ?</p>'
                    $('.send-form .to-input').attr('placeholder', 'Your email...');
                    $('.send-form .to-input').attr('type', 'email');
                    appendFromCard(response, '1-1-5-2')
                } else if (questionId == 'question-id-1-1-5-2') {
                    var response = '<p>What is a good phone number where an advisor can reach you at ?</p>'
                    $('.send-form .to-input').attr('type', 'tel');
                    $('.send-form .to-input').attr('placeholder', 'Your phone number...');
                    appendFromCard(response, '1-1-5-3')
                }
            }

            function appendFromCard(response, index) {

                const fromRandId = 'question-id-' + index;
                const fromMessageCard = (' <div class=" from card w-full p-2" id="' + fromRandId + '"> ' + response + '')
                $('.typing').hide()
                $('.messages').append(fromMessageCard)
                $('#' + fromRandId)[0].scrollIntoView();
            }

            $('.to-input').val('')
            console.log(entryBagTextarea.val())
        })
    }

    function sendBotMail() {
        jQuery(($) => {
            // send bot email
            var data = $('.entryBag').serialize(); //serializing the form and turning it into a string
            var formData = new FormData(); //
            formData.append('action', 'bot_mail'); //action = enquiry
            formData.append('bot_mail', data); //enquiry = string
            $.ajax("https://bestratedvoip.com/wp-admin/admin-ajax.php", {
                type: 'POST',
                data: formData,
                contentType: false,
                processData: false,
                success: function(response) {
                    console.log('bot mail sent')
                },
                error: function(response) {
                    console.log('bot mailnot sent')
                },
            });
        });
    }
