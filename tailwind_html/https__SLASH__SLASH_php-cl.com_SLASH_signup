    <script>
        jQuery(document).ready(function() {
            /*
             * Connect search box to PHP CL API.
             */
            jQuery('input.mac-style').autocomplete({
                source: function(request, response) {
                    jQuery.ajax('/api/v1/courses', {
                        /* Disable cache breaker; potentially improves performance. */
                        cache: true,
                        /*data: {
                            client: 'amazon-search-ui',
                            mkt: 1,
                            'search-alias': 'aps',
                            q: request.term
                        },*/
                        beforeSend: function(xhr){xhr.setRequestHeader('Accept', 'application/json');},
                        error: function() {
                            /*
                             * jquery-autocomplete's documentation states that response()
                             * must be called once per autocomplete attempt, regardless
                             * of whether or not the background request completed
                             * successfully or not.
                             */
                            response([]);
                        },
                        success: function(data) {
                            /*
                             * PHP CL API returns standard JSON
                             * We quick sort on titles only (Object.title).
                             */
                            let titles = [];

                            for (let i = 0; i < data.length; i++) {
                                if (data[i].title.toUpperCase().includes(request.term.toUpperCase())
                                    || data[i].lead.toUpperCase().includes(request.term.toUpperCase())
                                    || data[i].description.toUpperCase().includes(request.term.toUpperCase())
                                ) {
                                    titles.push(data[i].title);
                                }
                            }

                            if (titles.length === 0) {
                                titles.unshift('No results');
                            } else {
                                titles.unshift('Found in...');
                            }

                            response(titles);
                        },
                        //dataType: 'jsonp'
                    });
                }
            });
        });
    </script>  
    <!-- IE10 viewport hack for Surface/desktop Windows 8 bug -->
