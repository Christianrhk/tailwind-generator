<script src="/website/plugins/slick/slick.min.js" type="text/javascript"></script>
<script type="text/javascript">
    const VNP_HOME = {
      init: () => {
        $('#lbDate').unbind('click').click(function () {
          $('#txtDate').show();
          $('#lbDate').hide();
        });
        VNP_HOME.slide();
      },
      slide: () => {
        $('.slick-slider').slick({
          dots: true,
          infinite: true,
          speed: 500,
          slidesToShow: 3,
          slidesToScroll: 1,
          arrows: true,
          responsive: [
            {
              breakpoint: VNP_COMMON.breakingPoint.sm,
              settings: {
                infinite: false,
                slidesToShow: 1.3
              }
            }, {
              breakpoint: VNP_COMMON.breakingPoint.md,
              settings: {
                slidesToShow: 2
              }
            }, {
              breakpoint: VNP_COMMON.breakingPoint.lg,
              settings: {
                slidesToShow: 3
              }
            }
          ]
        });
        $('.slick-slider.agenda').slick('slickFilter', '.agenda-1');
        $('.js-filter').on('click', function(){
          var type = $(this).data('type');

          $('.slick-slider.agenda').slick('slickUnfilter');
          $('.slick-slider.agenda').slick('slickFilter','.' + type).slick('refresh');
          $('.slick-slider.agenda').slick('slickGoTo', 0);

          $('.js-filter').removeClass('bg-color-darkblue text-white bg-gray-300');
          $('.js-filter').each(function(index, item) {
                $(item).addClass('bg-gray-300');
          });
          $(this).removeClass('bg-gray-300');
          $(this).addClass('bg-color-darkblue text-white');
      });

        $('#slick-slider').slick({
          dots: true,
          infinite: true,
          arrows: false,
          autoplaySpeed: 3000,
          /*responsive: [
            {
              breakpoint: VNP_COMMON.breakingPoint.sm,
              settings: {
                slidesToShow: 1
              }
            }, {
              breakpoint: VNP_COMMON.breakingPoint.md,
              settings: {
                slidesToShow: 2
              }
            }, {
              breakpoint: VNP_COMMON.breakingPoint.lg,
              settings: {
                slidesToShow: 3
              }
            }
          ]*/
        });
        $('.space-slider').slick({
          dots: true,
          infinite: false,
          speed: 1000,
          slidesToShow: 6.2,
          slidesToScroll: 1,
          arrows: true,
          responsive: [
            {
              breakpoint: VNP_COMMON.breakingPoint.sm,
              settings: {
                slidesToShow: 2.2
              }
            }, {
              breakpoint: VNP_COMMON.breakingPoint.md,
              settings: {
                slidesToShow: 3.2
              }
            }, {
              breakpoint: VNP_COMMON.breakingPoint.lg,
              settings: {
                slidesToShow: 4.2
              }
            }
          ]
        });
      }
    }
    $(function () {
      VNP_HOME.init();
    });
  </script>
