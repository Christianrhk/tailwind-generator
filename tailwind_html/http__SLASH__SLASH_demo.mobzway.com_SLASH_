  function get_usermessages(msg_flag)
  {
  var client_id="";
  var user_id="";
  var msg_html = '';
  var open_e = '';
  if(msg_flag != 'new')
  {
    open_e = '-open';
  }
  var api_url="https://api.bopanel.xyz/index.php/Web_service/"+"message_list"; 
  jQuery.ajax({
        type: 'POST',
        url: api_url,
        data: {
          client_id: client_id,
          user_id: user_id,
          msg_flag: msg_flag
        }, 
        dataType: 'json',
        cache: false,
        success: function(data) { 
          console.log(data);

          if(data.status == 1){
          $.each(data.data,function(index, el) {
            msg_html += '<div class="row notification_new_row" data-toggle="collapse" data-flag="'+msg_flag+'" onclick="update_msg_status('+el.msg_id+','+index+',this)" data-target="#message_accodian'+index+'"><div class="col-md-1"><i class="far fa-envelope'+open_e+'"></i></div><div class="col-md-11"><p>'+trim_msgstr(el.message, 30, '...')+'</p><small class="float-right"><span>'+el.msg_date+'</span> </small></div></div><div id="message_accodian'+index+'" class="collapse closeallmessage message_accodian'+index+'"><p>'+el.message+'</p></div><hr>';
          });
        }else{
          if(msg_flag == 'new')
          msg_html+="<div class='text-center' >No new messages</div>";
          else
          msg_html+="<div class='text-center' >No messages</div>";
        }
          if(msg_flag =='new')
          $('#newmessagepanel').html(msg_html);
          else
          $('#historymessagepanel').html(msg_html);
        
        
        },
    });

  }
  function trim_msgstr(str, length, ending)
  {
    if (length == null) {
      length = 100;
    }
    if (ending == null) {
      ending = '...';
    }
    if (str.length > length) {
      return str.substring(0, length - ending.length) + ending;
    } else {
      return str;
    }
  }

