    <div>
      <nav ref="nav" :class="cls(null)">
        <div :class="cls('container', { open }, 'wrap')">
          <ul :class="cls('menu')" @mouseleave="closeAll()">
            <li class="site-navigation__parent">
                <a href="/" class="site-navigation__parent-title site-navigation__parent-title--home">
