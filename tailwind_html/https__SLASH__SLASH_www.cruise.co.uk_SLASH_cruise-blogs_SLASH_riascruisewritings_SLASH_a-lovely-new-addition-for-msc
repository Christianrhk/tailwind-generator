                            :rel="linkRel(subItem.noIndex, subItem.noFollow)"
                            :class="subItem.customClass"
                          >
                            {{ subItem.text }}
                          </a>
                        </li>

                        <li v-if="group.footer" :class="cls('sub-menu--featured')">
                          <a
                            :href="group.footer.url"
                            :target="group.footer.target"
                            :rel="linkRel(group.noIndex, group.noFollow)"
                          >
