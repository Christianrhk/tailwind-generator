              v-for="{
                id,
                url,
                text,
                shortText = '',
                open = false,
                customClass,
                links,
                target,
                noIndex,
                noFollow
              } in filterPublished(menu)"
              :key="id"
              :class="cls('parent', { open })"
            >
              <template v-if="filterPublished(links).length === 0">
                <a
                  :id="id"
                  :href="url"
                  :target="target"
                  :rel="linkRel(noIndex, noFollow)"
                  :class="customClass"
                >
                  <span :class="cls('parent-title', { long: true })">{{ text }}</span>
                  <span :class="cls('parent-title', { short: true })">{{ shortText || text }}</span>
                </a>
              </template>
              <template v-else>
                <button
                  :class="cls('parent-trigger', customClass, 'p-0')"
                  type="button"
                  @click="toggleDropdown(id)"
                >
                  <span :class="cls('parent-title', { long: true })">{{ text }}</span>
                  <span :class="cls('parent-title', { short: true })">{{ shortText || text }}</span>
