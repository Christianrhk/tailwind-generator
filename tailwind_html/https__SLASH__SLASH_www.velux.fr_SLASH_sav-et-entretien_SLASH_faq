<script>
    // TRACKING
    // Articles click
     (function () {
        var articles = document.querySelectorAll(".faq__topics_popular_element");

        if (articles && dataLayer) {
            articles = [].slice.call(articles);

            articles.forEach(function (article) {
                article.addEventListener('click', function () {
                    var name = article.querySelector('p');

                    var trackEvent = {
                        event: "interactionevent",
                        eventcategory: "eGain FAQ",
                        eventaction: "article - click",
                        eventlabel: name ? name.innerText : ""
                    }

                    dataLayer.push(trackEvent);
                })
            });
        }
    })();
</script>


