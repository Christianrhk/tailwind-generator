<script>
    // TRACKING
    // Category click
    (function () {
        var categories = document.querySelectorAll(".faq__topics_element");

        if (categories && dataLayer) {
            categories = [].slice.call(categories);

            categories.forEach(function (cat) {
                cat.addEventListener('click', function () {
                    var name = cat.innerText;

                    var trackEvent = {
                        event: "interactionevent",
                        eventcategory: "eGain FAQ",
                        eventaction: "category - click",
                        eventlabel: name
                    }

                    dataLayer.push(trackEvent);
                })
            });
        }
    })();
</script>

