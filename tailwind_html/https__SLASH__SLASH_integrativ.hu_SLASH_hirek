    <script type="text/javascript">
      $(document).ready(function() {
        $('.js-nextpagelink').hide().attr('href', '');

        $('.js-nextpagelink').click(function(e) {
          e.preventDefault();
          var url = $(this).attr("href");
          var element = $(this);
          $.ajax({
            caller: element,
            url: url,
            type : 'POST',
            data : {mode:"ajax", itemsonly:1},
            dataType: 'json',
            success: function(data){
              $('#newsitems').append(data.content.content.value);
            }
          });
        });

      });
    </script>

            </div>

            <div class="center mt3" id="js-morelinkcontainer">
              <center><a class="fs-12 js-nextpagelink light text-dove-gray" href="">Továbbiak betöltése</a></center>
