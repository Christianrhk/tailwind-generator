					var start = fights[0].end.split("-")[0] + '-01-01';//always start of the year
					var end = fights[fights.length-1].end.split("-")[0] + "-12-31";//always end of the year
					var date1 = new Date(start);
					var date2 = new Date(end);
					var Difference_In_Time = date2.getTime() - date1.getTime();
					var Difference_In_Days = Difference_In_Time / (1000 * 3600 * 24);

					$(document).ready(function(){

					    var yearsRendered = {};
					    //TODO render one per year.



					    fights.forEach(function(fight){
					        // To calculate the time difference of two dates
					        
					        var year = fight.end.split("-")[0];
					        if(!yearsRendered[year]){
					            yearsRendered[year] = true;
					            var date1 = new Date(start);
					            var date2 = new Date(year + "-01-01");
					            var Difference_In_Time = date2.getTime() - date1.getTime();
					            var sinceStart = Difference_In_Time / (1000 * 3600 * 24);
					            var left = 100 * (sinceStart / Difference_In_Days);

					            var block = $("<div />", {
					                'class': "year-label"
					            });
					            var span = $("<span />", {
					                'class': "year"
					            }).text(year);
					            block.append(span);
					            block.css('left', left + "%");

					            $("#heatmap").append(block);
					        }

					        var date1 = new Date(start);
					        var date2 = new Date(fight.end);
					        var Difference_In_Time = date2.getTime() - date1.getTime();
					        var sinceStart = Difference_In_Time / (1000 * 3600 * 24);
					        var left = 100 * (sinceStart / Difference_In_Days);

					        var block = $("<div />", {
					            'class': "block"
					        });
					        if(!fight.method){
					        	fight.method = 'Decision';
					        }
					        var square = $("<div />", {
					            'class': "square"
					        }).text(fight.method[0].toUpperCase());
					        square.css('background-color', fight.color);
					        //square.text(fight.title);

					        block.append(
					            square
					        );

					        var fightTime = $("<div />", {
					            'class': "fightTime",
					        });

					        var bar = $("<div />", {
					            'class': "fightTime-bar",
					            'title': fight.total_fight_time
					        });

					        var fightPercentage = 100 * (fight.cage_time / (25*60));
					        bar.css('height', fightPercentage + '%');

					        fightTime.append(bar);

					        block.append(fightTime);

					        if(fight.title_fight){
					        	block.addClass("belt");
					        }

					        
					        block.css('left', left + "%");

					        var d = fight;
					        var title = '';
			            	switch(d.result){
			            		case 'Win':
			            			title = "Defeated " + d.objectName;
			            		break;
			            		case "Loss":
			            			title = "Lost to " + d.objectName;
			            		break;
			            		default:
			            			title = d.result + " &bull; " + d.objectName;
			            		break;
			            	}

			            	var bonus = "";
			            	if(d.has_bonus){
			            		bonus = [];
			            		if(d.performance_of_the_night){
			            			bonus.push("<li>Awarded performance of the night bonus.</li>");
			            		}
			            		if(d.fight_of_the_night){
			            			bonus.push("<li>Awarded fight of the night bonus.</li>");
			            		}
			            		if(d.knockout_of_the_night){
			            			bonus.push("<li>Awarded knockout of the night bonus.</li>");
			            		}
			            		if(d.submission_of_the_night){
			            			bonus.push("<li>Awarded submission of the night bonus.</li>");
			            		}
			            		if(bonus.length > 0){
			            			bonus = "<ul>" + bonus.join("") + "</ul>";
			            		}
			            	}
					        var result = '';
					        if(!d.method){
					        	d.method = '';
					        }
					        if(!d.final_round){
					        	d.final_round = 3;
					        }
					        if(!d.final_round_time){
					        	d.final_round_time = '05:00';
					        }

					        result = d.method + " R" + d.final_round + " " + d.final_round_time;
					        var billing = "";
					        if(fight.title_fight){
					        	if(fight.title_status == 'Challenger' || fight.title_status == 'Vacant'){
					        		if(d.result == 'Win'){
					        			billing = "<h4>WON " + fight.title_fight + "</h4>";
					        		} else if(d.result == "Loss"){

					        			billing = "<h4>Lost challenge for " + fight.title_fight + "</h4>";
					        		} else {
					        			billing = "<h4>Title Challenge: " + fight.title_fight + "</h4>";
					        		}
					        	} else {
					        		if(d.result == 'Win'){
					        			billing = "<h4>Defended " + fight.title_fight + "</h4>";
					        		} else if(d.result == "Loss"){

					        			billing = "<h4>Lost " + fight.title_fight + "</h4>";
					        		} else {
					        			billing = "<h4>Title Defence: " + fight.title_fight + "</h4>";
					        		}
					        	}
					        	
					        }
					        var res =  "<div class='fight-popover fight-popover-" + d.result.toLowerCase() + "'><p>" + d.event_title_and_date + "</p>" + billing +  
					        	"<h5>" + title + "</h5>" + "<h6>" + result + "</h6>" + bonus + "</div>" ;

		                    $(block).popover({
		        	            placement: 'auto top',
		        	            container: 'body',
		        	            trigger: 'hover',
		        	            html: true,
		        	            content: function() {
		        	            	return "<div class='placed-here'>" + res + "</div>";

		        	            }
		        	        });
		        	       
		        	        //$(el).popover('show');

					        $("#heatmap").append(block);

					        // $(".start-label span").text('2012');
					        // $(".end-label span").text('2021');

					    });


					});
				</script>
<style type="text/css">
