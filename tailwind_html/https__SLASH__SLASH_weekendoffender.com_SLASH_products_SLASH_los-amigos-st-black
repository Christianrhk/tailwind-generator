<script>

    // Plain Javascript
    //event listener: DOM ready
    function addLoadEvent(func) {
        var oldonload = window.onload;
        if (typeof window.onload != 'function') {
            window.onload = func;
        } else {
            window.onload = function() {
                if (oldonload) {
                    oldonload();
                }
                func();
            }
        }
    }
    //call plugin function after DOM ready
    addLoadEvent(function(){
        outdatedBrowser({
            bgColor: '#ffffff',
            color: '#1a1a1a',
            lowerThan: 'grid',
            languagePath: 'your_path/outdatedbrowser/lang/en.html'
        });
    });

    if( navigator.userAgent.match(/Android/i)
        || navigator.userAgent.match(/webOS/i)
        || navigator.userAgent.match(/iPhone/i)
        || navigator.userAgent.match(/iPad/i)
        || navigator.userAgent.match(/iPod/i)
        || navigator.userAgent.match(/BlackBerry/i)
        || navigator.userAgent.match(/Windows Phone/i)
    ){

        let megaMenuLink = document.querySelectorAll(".megamenu-item > a");

        megaMenuLink.forEach(function (value) {
            value.addEventListener("click", function(e){
                if (! value.classList.contains("hovered-item")){
                    e.preventDefault();
                    value.classList.add("hovered-item");
                    window.addEventListener("click", function(ev){
                        if (! ev.srcElement.classList.contains("hovered-item")){
                            console.log(value);
                            value.classList.remove("hovered-item");
                        }
                    });

                }
            });
        });
    }
</script>

