<script>
 
  let pattern = ['s', 'p', 'o', 'i', 'l', 'e', 'r'];
  let current = 0;
  let secretStorageId = 'secretEnabled';


  loadBackground();


  let keyHandler = function (event) {

    // If the key isn't in the pattern, or isn't the current key in the pattern, reset
    if (pattern.indexOf(event.key) < 0 || event.key !== pattern[current]) {
      current = 0;
      return;
    }

    // Update how much of the pattern is complete
    current++;
    // If complete, alert and reset
    if (pattern.length === current) {
      current = 0;
      
      let isSecret = localStorage.getItem(secretStorageId);
      console.log("Secret: " + isSecret);

      if (isSecret == 'true') {
        document.getElementsByTagName("body")[0].style.backgroundImage = "url('/images/background.png')";
        localStorage.setItem(secretStorageId, 'false');

      } else if(isSecret == 'false') {
        document.getElementsByTagName("body")[0].style.backgroundImage = "url('/images/background1.png')";
        localStorage.setItem(secretStorageId, 'true');
      }
    }
  };

  document.addEventListener('keydown', keyHandler, false);

  function loadBackground()
  {
    let backgroundImg = '';
    let isSecret = localStorage.getItem(secretStorageId);
    console.log(isSecret);
     if(isSecret === 'null' || isSecret === null)
     {
      localStorage.setItem(secretStorageId, 'false');
      isSecret = false;
     }

    if (isSecret == 'true') {
      backgroundImg = '/images/background1.png';
    } else if(isSecret == 'false') {
      backgroundImg = '/images/background.png';
    }
   
    document.getElementsByTagName("body")[0].style.backgroundImage = "url(" + backgroundImg + ")";
  }

  function randomInt(min, max) 
  { // min and max included 
    return Math.floor(Math.random() * (max - min + 1) + min)
  }

  window.onload = function () 
  {
    document.getElementById("hamburger").onclick = function toggleMenu() {
      const navToggle = document.getElementsByClassName("toggle");
      for (let i = 0; i < navToggle.length; i++) {
        navToggle.item(i).classList.toggle("hidden");
      }
    };
  }
</script>
