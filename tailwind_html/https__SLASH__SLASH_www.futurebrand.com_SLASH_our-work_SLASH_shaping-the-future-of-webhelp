    <script>
        function animateInTitles() {
            $('.animate-in').velocity("transition.slideRightIn", {
                delay: 250,
                stagger: 250
            });
        }

        $(function() {

            if ( !$('.flexslider').length ) {
                animateInTitles();
            }

            var run = false;
            $('.flexslider').flexslider({
                directionNav: true,
                slideshow: true,
                animation: "slide",
                useCSS: true,
                video: true, // false messes up text animation, but true makes poorer slide animation
                animationSpeed: 400,
                /* reload video on navigation */
                start: function(slider){
                    if (!run) {
                        $('.flex-control-paging').velocity("transition.slideRightIn", {
                            begin: function(elements) {
                                animateInTitles();
                            }
                        });
                    }
                    run = true;
                }
            });
        });
    </script>
