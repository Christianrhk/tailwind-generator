cument.getElementById("InstallerChatClose"); var bubbleIntro=bubble.getElementsByClassName("installer-bubble-intro")[0]; var bubbleContent=bubble.getElementsByClassName("installer-bubble-conversation")[0]; var bubbleText=bubbleContent.getElementsByClassName("installer-bubble-text"); var trackEvent=function (eventAction, eventLabel){eventObject={event: "interactionevent", eventcategory: "installer assistant", eventaction: eventAction, eventlabel: eventLabel ? eventLabel : ""}; try{window.dataLayer=window.dataLayer || []; window.dataLayer.push(eventObject);}catch (error){console.warn(error);}}; var openChat=function (){if (isOpen){return;}isOpen=true; chatbot.classList.add("installer-bubble-open", "installer-bubble-loading"); setTimeout(function (){bubbleText[0].classList.add("installer-bubble-text__show"); chatbot.getElementsByClassName("typing-indicator")[0].style.display="none"; bubbleContent.style.display="grid"; chatbot.classList.remove("installer-bubble-loading"); opened=true;}, opened ? 0 : 2000);}; var closeChat=function (event){isOpen=false; chatbot.classList.remove("installer-bubble-open"); avatar.style.animation="none"; trackEvent("Close chat", locationPath); event.stopPropagation();}; var clickBubble=function (){trackEvent("Click", locationPath); openChat();}; var clickAvatar=function (){if (isOpen){return;}trackEvent("Click", locationPath); openChat();}; var clickQuestion=function (index){trackEvent(installerTexts.installerQuestions[index].text, locationPath);}; bubbleIntro.addEventListener("click", clickBubble); avatar.addEventListener("click", clickAvatar); closeChatbot.addEventListener("click", closeChat); (function (){var questionElement=bubble.getElementsByClassName("installer-bubble-question"); var questionContent=installerTexts.installerQuestions; bubble.getElementsByClassName("installer-bubble-intro")[0].innerHTML=installerTexts.findMe; bubble.getElementsByClassName("installer-bubble-text")[0].innerHTML=installerTexts.installerIntro; for (var i=0; i < questionContent.length; i++){var question=document.createElement("a"); question.innerHTML=questionContent[i].text; question.href=questionContent[i].url; question.classList.add("installer-bubble-question"); bubbleContent.append(question);}var bubbleQuestion=bubbleContent.getElementsByClassName("installer-bubble-question"); for (var i=0; i < bubbleQuestion.length; i++){bubbleQuestion[i].addEventListener("click", function (event){var index=Array.prototype.indexOf.call(bubbleQuestion, event.target); clickQuestion(index);});}})();}</script><style>#InstallerChatbot{position: fixed; bottom: 16px; right: 16px; display: flex; z-index: 999; align-items: flex-end;}#InstallerChatbot #InstallerChatBubble{background: white; max-width: 66vw; line-height: 24px; font-weight: 400; box-shadow: 0px 8px 32px rgba(0, 0, 0, 0.32); animation: bounce-in-bubble 0s ease; right: -60px; transition: all 350ms cubic-bezier(0.4, 0, 0.2, 1); max-height: 90vh;}#InstallerChatbot.installer-bubble-open #InstallerChatBubble{background: #f5f5f5; max-width: initial; width: 91vw; padding: 20px; right: 0;}#InstallerChatbot #InstallerChatBubble.installer-chat-open{cursor: default;}#InstallerChatbot .installer-bubble-intro{font-size: 16px; padding: 10px 16px;}#InstallerChatbot.installer-bubble-open .installer-bubble-intro{display: none; padding: 0;}#InstallerChatbot .installer-bubble-text{margin-bottom: 0.5em; transition: all 350ms cubic-bezier(0.4, 0, 0.2, 1); justify-self: end; width: 92%;}#InstallerChatbot .installer-bubble-text.installer-bubble-text__show{display: block;}#InstallerChatbot .installer-bubble-content{justify-items: start; gap: 20px; font-size: 14px; display: none;}#InstallerChatbot.installer-bubble-open .installer-bubble-content{display: grid; transition: all 350s ease; max-height: 70vh; overflow-y: scroll;}#InstallerChatbot .installer-bubble-content p+p{margin-top: 1em;}#InstallerChatbot .installer-bubble-shape{background: white; border-radius: 24px 24px 0 24px; box-shadow: 0px 8px 16px rgba(0, 0, 0, 0.04);}#InstallerChatbot.installer-bubble-open .installer-bubble-shape{padding: 16px 20px; box-sizing: border-box;}#InstallerChatbot .installer-bubble-conversation{display: grid; gap: 20px;}#InstallerChatbot.installer-bubble-loading .installer-bubble-conversation{display: none;}#InstallerChatbot .installer-bubble-question{border-radius: 24px; background: #e00; color: white; padding: 8px 16px; font-weight: 700; max-width: 100%; text-decoration: none; justify-self: left;}#InstallerChatbot .installer-bubble-question:hover{background: #b00;}#InstallerChatbot .installer-bubble-question.installer-bubble-question__show{display: block;}#InstallerChatbot #InstallerChatAvatar{animation: bounce-in-avatar 0s ease;}#InstallerChatbot.installer-bubble-open #InstallerChatAvatar{display: none;}#InstallerChatbot #InstallerChatClose{position: absolute; top: 12px; right: 12px; cursor: pointer; z-index: 10; display: none;}#InstallerChatbot.installer-bubble-open #InstallerChatClose{display: flex;}#InstallerChatbot.installer-bubble-loading #InstallerChatClose{display: none;}#InstallerChatbot #InstallerChatClose:hover svg{animation: rotate-in 0.3s;}#InstallerChatbot #InstallerChatClose:not(:hover) svg{animation: rotate-out 0.3s;}#InstallerChatbot #InstallerChatAvatar svg{transform-origin: bottom right; transform: scale(0.45); filter: drop-shadow(0px 8px 32px rgba(0, 0, 0, 0.32));}#InstallerChatbot #InstallerChatBubble, #InstallerChatbot #InstallerChatAvatar{position: relative; bottom: 0; cursor: pointer;}#InstallerChatbot.installer-bubble-open #InstallerChatBubble, #InstallerChatbot.installer-bubble-open #InstallerChatAvatar{cursor: default;}@media screen and (min-width: 360px){#InstallerChatbot.installer-bubble-open #InstallerChatBubble{padding: 26px;}#InstallerChatbot .installer-bubble-question{max-width: 92%;}}@media screen and (min-width: 640px){#InstallerChatbot{bottom: 32px; right: 32px;}#InstallerChatbot #InstallerChatBubble{max-width: 328px;}#InstallerChatbot.installer-bubble-open #InstallerChatBubble{max-width: 328px; right: -60px; width: auto; padding: 26px;}#InstallerChatbot.installer-bubble-open #InstallerChatAvatar{display: block;}}@media screen and (min-width: 768px){#InstallerChatbot .installer-bubble-intro{font-size: 20px;}#InstallerChatbot #InstallerChatBubble{border-radius: 32px 32px 0px 32px; right: 0;}#InstallerChatbot #InstallerChatBubble .installer-bubble-intro{padding: 20px 16px;}#InstallerChatbot #InstallerChatAvatar svg{transform: scale(0.75);}#InstallerChatbot #InstallerChatBubble{max-width: 328px;}#InstallerChatbot.installer-bubble-open #InstallerChatBubble{max-width: 328px; width: auto; right: 0;}#InstallerChatbot.installer-bubble-open #InstallerChatAvatar{display: block;}#InstallerChatbot.installer-bubble-open .installer-bubble-content{overflow-y: auto;}}@keyframes bounce-in-bubble{0%{opacity: 0; transform: translateY(2000px);}25%{opacity: 0; transform: translateY(2000px);}60%{opacity: 1; transform: translateY(-30px);}80%{transform: translateY(10px);}100%{transform: translateY(0);}}@keyframes bounce-in-avatar{0%{opacity: 0; transform: translateY(2000px);}60%{opacity: 1; transform: translateY(-30px);}80%{transform: translateY(10px);}100%{transform: translateY(0);}}@keyframes rotate-in{to{transform: rotate(180deg);}}@keyframes rotate-out{to{transform: rotate(-180deg);}}.typing-indicator{background-color: #e6e7ed; width: auto; border-radius: 50px; padding: 10px; display: table; margin: 0 auto; position: relative;}.typing-indicator span{height: 10px; width: 10px; float: left; margin: 0 1px; background-color: #9e9ea1; display: block; border-radius: 50%; opacity: 0.4;}.typing-indicator span:nth-of-type(1){animation: 1s blink infinite 0.3333s;}.typing-indicator span:nth-of-type(2){animation: 1s blink infinite 0.6666s;}.typing-indicator span:nth-of-type(3){animation: 1s blink infinite 1s;}@keyframes blink{50%{opacity: 1;}}</style><style>/* Adam adjustment for the new image */ #InstallerChatAvatar{width: 80px; disp