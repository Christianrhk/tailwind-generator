<script>
    $(function () {

        $('[name^="FormData."]').on('keypress', function () {
            $('+ div', this).addClass('d-none');
        });

        $("#form_contact").on("submit", function (event) {
            let col = $('[name="FormData.MainContact"]').val();
            $('[name^="FormData."] + div').addClass('d-none');
            if ($('[name="FormData.' + col + '"]').val() === "") {
                $('[name="FormData.' + col + '"]').next('.d-none').removeClass('d-none');
                $([document.documentElement, document.body]).animate({
                    scrollTop: $('[name="FormData.' + col + '"]').offset().top
                }, 1000);
                return false;
            }
            return true;
        });

    });

    function validateForm(token) {
        $("#form_contact").trigger('submit');
    }
</script>

