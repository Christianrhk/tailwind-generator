        var chatbox = document.getElementById('fb-customer-chat');
        chatbox.setAttribute("page_id", "110962838916337"); 
        chatbox.setAttribute("attribution", "biz_inbox");
        function injectFbSdkAsync(d, s, id) {
            var js, fjs = d.getElementsByTagName(s)[0];
            if (d.getElementById(id)) return;
            js = d.createElement(s);
            js.id = id;
            js.src = 'https://connect.facebook.net/da_DK/sdk/xfbml.customerchat.js';
            fjs.parentNode.insertBefore(js, fjs);
        };
            
            var fbMessengerFakeButton = document.getElementById('fb-messenger-fake-button');
            fbMessengerFakeButton.addEventListener('click', function() {
                this.classList.add('active');
                injectFbSdkAsync(document, "script", "facebook-jssdk");
                window.fbAsyncInit = function () {
            FB.init({
                xfbml: true,
                version: 'v12.0'
            });
                };
                
            setTimeout(function(){ fbMessengerFakeButton.style.display = 'none' }, 2000);
            });
           
