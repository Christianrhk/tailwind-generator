 	<script type="text/javascript">
		
		let timer = setInterval(verificaGT, 1500)
		
		function verificaGT(){
			let gg_frame = document.querySelector(".goog-te-menu-frame")
			if(!!gg_frame){
				document.querySelector(".goog-te-menu-frame").contentWindow.document.querySelector(".goog-te-menu2-item .text").addEventListener("click", e => {
					let new_lang = document.querySelector(".goog-te-menu-frame").contentWindow.document.querySelector(".goog-te-menu2-item .text").innerHTML
					
					new_lang = new_lang == "Inglês" ? "en" : "pt";
					changeLanguage(new_lang)
				})
				console.log("ready")
				
			clearInterval(timer);
			return;
			}
		}
		


		function changeLanguage(lang){
			fetch(`/changeLanguage.php?language=${lang}`).
				then(res => {
					return res.json();
				}).
				then(res => {
					setTimeout(() => document.location.reload(true), 1250)
				})
		}

			
		

    </script></header>
