    <script>
$(document).on("click", ".image-modal-opener", function () {
    $('#image-modal').data('current-modal-index', $(this).data("modal-index"));
    updateImageModal($(this).data("image-src"), $(this).data("image-alt"), $(this).data("modal-index"));
});

$(".image-modal .post-previous-page, .image-modal .post-next-page").click(function(){
    var np = $('#image-modal').data('current-modal-index') + $(this).data('offset');
    var pid = $('.image-modal-opener[data-modal-index="' + np + '"]').data('image-src');
    updateImageModal(pid, np);
    $('#image-modal').data('current-modal-index', np);
});

function updateImageModal(imageSrc, imageAlt, currentIndex) {
    $(".image-modal .post-previous-page, .image-modal .post-next-page").css('display', 'inline-block').attr("disabled", false).removeClass("edge");
    if (currentIndex >= $('.image-modal-opener').length) {
        currentIndex = $('.image-modal-opener').length;
        $('.image-modal .post-next-page').attr("disabled", true).addClass("edge");
    }
    if (currentIndex <= 1) {
        currentIndex = 1;
        $('.image-modal .post-previous-page').attr("disabled", true).addClass("edge");
    }

    $('.image-modal .main-image').prop({src: imageSrc, alt: imageAlt});
}
</script>
</body>
</html>