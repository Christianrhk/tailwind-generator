    <script type="text/javascript">
        function hideBanner(div_id) {
            var x = document.getElementById(div_id);
            if (x.style.display === "none") {
                x.style.display = "block";
            } else {
                x.style.display = "none";
            }
        }

        var isMobile = ('ontouchstart' in document.documentElement && navigator.userAgent.match(/Mobi/));
        var websiteBanner = document.querySelectorAll('.website_banner'),
            wi = 0,
            wl = websiteBanner.length;
        var mobileBanner = document.querySelectorAll('.mobile_banner'),
            mi = 0,
            ml = mobileBanner.length;
        if (isMobile){
            for (wi; wi < wl; wi++) {
                websiteBanner[wi].style.display = 'none';
            }

            for (mi; mi < ml; mi++) {
                mobileBanner[mi].style.display = 'block';
            }
        }else{
            for (wi; wi < wl; wi++) {
                websiteBanner[wi].style.display = 'block';
            }

            for (mi; mi < ml; mi++) {
                mobileBanner[mi].style.display = 'none';
            }
        }
    </script>

