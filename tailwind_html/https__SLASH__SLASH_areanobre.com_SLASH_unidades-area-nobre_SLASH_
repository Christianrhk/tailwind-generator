<script type="text/javascript">

  $(document).ready(function(){

    $('body').on('click', '.cookie-notice .accept', function(){
      localStorage.setItem("cookie-area-nobre", 'aceito');
      $(".cookie-notice").fadeOut();
    });

    var status_cookie = localStorage.getItem('cookie-area-nobre');
    if (localStorage.getItem("cookie-area-nobre") == null) {
      $(".cookie-notice").css("display", "block");
    }
    var slider_clientes = new Swiper('#swiper-clientes', {
        loop: false,
        navigation: {
            nextEl: '.slider-clientes-swiper-button-next',
            prevEl: '.slider-clientes-swiper-button-prev',
        },
        observer: true,
        observeParents: true,
        pagination: {
          el: "#swiper-pagination-clientes",
          clickable: true,
        },
        breakpoints: {
            1290: {
                slidesPerView: 5,
                spaceBetween: 30,
            },
            300: {
                slidesPerView: 2,
                spaceBetween: 10,
            },
        },
    });
    var slider = new Swiper('#swiper-main', {
        loop: false,
        navigation: {
            nextEl: '.slider-main-swiper-button-next',
            prevEl: '.slider-main-swiper-button-prev',
        },
        autoplay: {
            delay: 5000,
            disableOnInteraction: false,
        },
        pagination: {
          el: "#swiper-pagination-slider",
          clickable: true,
        },
        observer: true,
        observeParents: true,
        breakpoints: {
            1290: {
                slidesPerView: 1,
                spaceBetween: 30,
            },
            840: {
                slidesPerView: 1,
                spaceBetween: 10,
            },
            300: {
                slidesPerView: 1,
                spaceBetween: 10,
            },
        },
    });

    var url = window.location.href;
    var parts = url.split('?');
    var lastSegment = parts.pop() || parts.pop();
    if(lastSegment.length == 2) {
        $('#svg-map a[href="#'+ lastSegment +'"]').addClass('active');
        $("#"+lastSegment).parent().addClass("active")
        $("#"+lastSegment).next().css("display", "block");
    }

    $('#svg-map').on('click', 'a', function(event){
        event.preventDefault();
        var item = $(this).attr('href');
        if ($($(item).parent()).hasClass('active')) {
            $('.listmapa li.active .sublist').slideToggle();
            $('#svg-map a, .listmapa li').removeClass('active');
        }else{
            $('.listmapa li.active .sublist').slideToggle();
            $('#svg-map a, .listmapa li').removeClass('active');
            $(this).addClass('active');
            $(item).parent().addClass('active');
            $(item).parent().find('.sublist').slideToggle();
        }
    });

    $('.listmapa li').on('click', '.city', function(event){

        event.preventDefault();
        var item = $(this).parent();
        if (!$(item).hasClass('active')) {
            $('.listmapa li.active .sublist').slideToggle();
            $('#svg-map a, .listmapa li').removeClass('active');
            $('#svg-map a[href="#'+ $(this).attr('id') +'"]').addClass('active');
        };

        item.find('.sublist').slideToggle();
        item.toggleClass('active');

    });

    $('.sublist').each(function(index, element) {
        $(this).filter(function(){
            var qtd = $(this).children("li").length*20;
            if(qtd > 300) {
                $(this).addClass("sublist-scroll");
            }
        })
    });

  });
</script>

