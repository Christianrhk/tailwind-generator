<script> sledPartsObj['about-section'] = {init: function($el) {$('.about_container.accordion', $el).each(function(){

    var $container = $(this);
    
    var $headings = $('.about_item__heading', $container);

    $headings.click(function(){
        var $heading = $(this);
        var $container = $heading.closest('.about_container.accordion');
        var $content = $(this).next();
        var opening = $heading.hasClass('closed');

        $headings.addClass('closed');
        $('.about_item__content', $container).hide();

        if(opening)
        {
            $heading.removeClass('closed');
            $content.show();
        }
    });

});
}};</script>
<script> sledPartsObj['article-listing'] = {init: function($el) {var $tableIframe = $('.live-data-table', $el);
        
$(".filter_container input[name='filter']", $el).change(function(){
    // if any tag is checked hide all articles and then show the ones with the tag
    var $activeTags = $(".filter_container input[name='filter']:checked", $el);

    var tags = '';
    $activeTags.each(function(){
        if(tags!='') tags += ',';
        tags += $(this).data("tag");
    });

    if(tags) window.location = window.location.pathname + '?tags=' + tags;
    else window.location = window.location.pathname;
});}};</script>
<script> sledPartsObj['container'] = {init: function($el) {var $collapsibleHeading = $('> .collapsible_heading', $el);
var $collapsibleContent = $('> .collapsible_content', $el);

$collapsibleHeading.click(function(){
    var opening = $collapsibleHeading.hasClass('closed');
    console.log(opening);
    
    if(opening)
    {
        $collapsibleHeading.removeClass('closed');
        $collapsibleContent.show();
    }
    else
    {
        $collapsibleHeading.addClass('closed');
        $collapsibleContent.hide();
    }
});}};</script>
<script> sledPartsObj['content'] = {init: function($el) {$(function(){
	$(window).scroll(function(){
		var scroll = $(window).scrollTop();
		$('.edit-content-btn').css('top', scroll / 0.7 );
	});

	// make the sled parts full bleed, if an element inside is full bleed so we can click anywhere to edit
	$('.sled-part .full-bleed-width').each(function(){
		$(this).closest('.sled-part').addClass('full-bleed-width');
		$(this).closest('.sled-part').css('display', 'block');
	})

	// make content height the right size when scaled down
	setInterval(function(){
		$(".sled-part[data-part='content']").each(function(){
			var $content = $(this);
			if($content.hasClass('editable'))
			{
				$content.css('margin-bottom', '-' + $content.height() * 0.3 + 'px');
			} 
			else
			{
				$content.css('margin-bottom', '0px');
			}
		});
	}, 100);
});}};</script>
<script> sledPartsObj['feature-carousel'] = {init: function($el) {$(function(){
    $(".slick__container", $el).slick({
        dots: true,
        infinite: false,
        speed: 300,
        slidesToShow: 1,
        slidesToScroll: 1,
    });
});}};</script>
<script> sledPartsObj['live-data-chart'] = {
