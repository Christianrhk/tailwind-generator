    }
};</script>
<script> sledPartsObj['live-data-table'] = {
    init: function($el) {
        var $tableIframe = $('.live-data-table', $el);
        setInterval(function(){
            if(!$tableIframe[0].contentWindow) return;
            var iframeContentBody = $tableIframe[0].contentWindow.document.body;
            if(!iframeContentBody) return;
            
            var iframeHeight = iframeContentBody.clientHeight;
            if($tableIframe.height() != iframeHeight) $tableIframe.height(iframeHeight);
        }, 500);
    }
};</script>
<script> sledPartsObj['navigation'] = {init: function($el) {$(function(){
    $('.mobile-toggle-bars').click(function(event){
        $(this).toggleClass("fa-bars");
        $(this).toggleClass("fa-times");
        $(".mobile-nav.navigation").toggleClass("hidden");
        event.stopPropagation();
    });
    
    $(window).click(function() {
        $('.mobile-nav').addClass('hidden');
    });
    
    $('.mobile-nav').click(function(event){
        event.stopPropagation();
    });

    $('.sub-menu-container .sub-menu-opener').on('click', function(){
        var $subMenuContainer = $(this).closest(".sub-menu-container");
        $("> .sub-menu", $subMenuContainer).toggleClass("is-open");
    });
})
}};</script>
<script> sledPartsObj['product-data-table'] = {init: function($el) {var $search = $('.search', $el);
var $rows = $('tr:not(:first-child)', $el);
var isCollapsed = false;

$search.on('keyup change search', function(){
    var term = $search.val().toLowerCase();
    $rows.removeClass('filtered-out');
    if(term) $rows.each(function(){
        if( $(this).text().toLowerCase().indexOf(term) === -1 ) 
            $(this).addClass('filtered-out');
    });
    if(isCollapsed) collapse();
});

/*$('table').each(function(){
    sorttable.makeSortable(this);
});*/

var $datatable = $('.datatable', $el);
var nRows = $datatable.attr('data-number-of-rows');
if(nRows=='') nRows = 0; else nRows *= 1;
if(nRows!=0) collapse();

$('.expand-btn', $datatable).click(expand);
$('.collapse-btn', $datatable).click(collapse);

function collapse()
{
    $('tr', $datatable).hide();
    $('tr:not(.filtered-out)', $datatable).slice(0, nRows+1).show();
    isCollapsed = true;
    $('.expand-btn', $datatable).show();
    $('.collapse-btn', $datatable).hide();
}

function expand()
{
    $('tr:not(.filtered-out)', $datatable).show();
    isCollapsed = false;
    $('.expand-btn', $datatable).hide();
    $('.collapse-btn', $datatable).show();
}}};</script>
