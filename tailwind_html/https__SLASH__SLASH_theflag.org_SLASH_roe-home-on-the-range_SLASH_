</div><script type="text/javascript">
function ajax_search(){

	var page = jQuery('#custom-search-paged').val();

	if ( page == 1 )
		$('.products-wrapper, .articles').addClass('loading');

	jQuery.ajax({
		url: 'https://theflag.org/wp-admin/admin-ajax.php',
		type: 'post',
		data: { _ajax_nonce: 'ba322e9307', action: 'live_search', keyword: jQuery('#custom-search-field').val(), page: jQuery('#custom-search-paged').val(), post_type: jQuery('#custom-search-post_type').val(), tax: jQuery('#custom-search-tax').val(), tax_term: jQuery('#custom-search-tax_term').val() },
		success: function(data) {

			var data = JSON.parse(data);

			if ( data.success ) {
				
				// If new search, remove existing products
				if ( page == 1 ) {
					jQuery( ".products-wrapper ul.products *, .articles *" ).remove();
					jQuery('.woocommerce-result-count').html("");
				}

				// Add data to products container
				jQuery( ".products-wrapper ul.products, .articles" ).append( data.html );

				if ( parseInt(data.post_count) > 0 ) {
					// Update result count
					var n_results = parseInt((page-1)*data.posts_per_page)+parseInt(data.post_count);
					jQuery('.woocommerce-result-count').html("Showing 1–" + n_results + " of " + data.found_posts + " results");
				}

				// If no more posts, set globals
				else
					no_more_posts = true;

			}

			else {
				// Clear posts
				jQuery( ".products-wrapper ul.products *, .articles *" ).remove();
				// Output error
				jQuery( ".products-wrapper ul.products, .articles" ).append( data.html );
				no_more_posts = true;
			}

			// Remove loading animation and update globals
			$('.products-wrapper, .articles').removeClass('loading');
			$('.site-main').removeClass('site-main-load');
			ajax_busy = false;
		}
	});

}
</script>

