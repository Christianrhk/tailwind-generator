<script src="//cdnjs.cloudflare.com/ajax/libs/list.js/1.5.0/list.min.js"></script>
<script>
	var options = {
		valueNames: [ 'nazev', 'pivovar', 'stupen' ]
	};

	var beerList = new List('piva-list', options);
	var noItems = $('<span class="grey-text" id="no-items-found">... Tomuto hledání neodpovídá žádné pivo ...</span>');

	beerList.on('updated', function(list) {
		if (list.matchingItems.length == 0) {
			$(list.list).append(noItems);
		} else {
			noItems.detach();
		}
	});
</script>
