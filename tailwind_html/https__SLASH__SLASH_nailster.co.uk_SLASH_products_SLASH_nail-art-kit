  // Add to cart message position when visible
  let baseHeaderHeight = $('.header').height()+ "px";     
  $('.added-to-cart-message').css({
    "top": baseHeaderHeight
  });


  // Prevent scroll on the full page, when a mobile sidebar it's opened
  var mobileSidebar = $('.js-mobile-sidebar-toggle');
  mobileSidebar.on('click', function (event) {
    event.preventDefault();
    $('html').toggleClass('overflow-y-hidden md:overflow-y-auto');
  });
  
  // Link to anchor with animation
    $(".anchor-link").click(function(e) {
        e.preventDefault();
        var aid = $(this).attr("href");
        $('html,body').animate({scrollTop: $(aid).offset().top-100},'slow');
    });

  // Fixed header
    $(document).ready(function() {

      let baseHeaderHeight = $('.header').height()+ "px";
      let startfixedHeader = $('.start-fixed-js').offset().top; // Default Header position
      let currentHeaderHeight = $('.content-fixed-js').height();
      headerFixedScroll();
      
      $(window).resize(function() {
          headerFixedScroll();
      });

      function headerFixedScroll() {
          $( window ).scroll(function() {
              let currentScroll = $(window).scrollTop();
              
              if (currentScroll >= startfixedHeader) {
                  $('.content-fixed-js').css({
                      position: 'fixed',
                      top: '0',
                  });
                  $('.content').css({
                      "margin-top": currentHeaderHeight + "px"
                  });
                  $('.added-to-cart-message').css({
                      "top": currentHeaderHeight + "px"
                  });
                  
              } else {
                  $('.content-fixed-js').css({
                      position: 'static'
                  });
                  $('.content').css({
                      "margin-top": "0"
                  });
                  $('.added-to-cart-message').css({
                      "top": baseHeaderHeight
                  });
              }
          });
      }
    });
</script>
  
  
