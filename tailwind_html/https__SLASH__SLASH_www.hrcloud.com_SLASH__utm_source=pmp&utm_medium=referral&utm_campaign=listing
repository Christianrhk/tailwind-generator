  function handleSlickFeaturedResources() {
			if ($(window).outerWidth() > 1074) {
				$('.featured-resources').filter('.slick-initialized').slick('unslick')
			} else {
				$('.featured-resources').not('.slick-initialized').slick({
					dots: true,
					arrows: false,
					slidesToShow: 1,
					adaptiveHeight: true,
				});
			}
		}

		handleSlickFeaturedResources()

		$(window).resize(function () {
			handleSlickFeaturedResources()
		});
</script>
