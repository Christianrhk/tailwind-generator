			$('#close-homepage-popup').click(function () {
				setCookie('popupcookie-hrc', 'close-hrc-popup', "10");
				$('.homepage-popup-container').fadeOut()
			});

			$('.homepage-mobile-overlay').click(function (e) {
				setCookie('popupcookie-hrc', 'close-hrc-popup', "10");
				$('.homepage-popup-container').fadeOut()
			});

			$('.btn-orange-action').click(function (e) {
				setCookie('popupcookie-hrc', 'close-hrc-popup', "10");
				setCookie('popupcookie-wm', 'close-wm-popup', "10");
				$('.homepage-popup-container').fadeOut()
			});

			$('.testimonials-slick').slick({
				dots: false,
				arrows: true,
				prevArrow: "<img class='cursor-pointer mr-5 transform rotate-180 hover:opacity-50' loading='lazy' src='https://549864.fs1.hubspotusercontent-na1.net/hubfs/549864/assets/request-a-demo-2022/arrow.svg' alt='arrow left'>",
				nextArrow: "<img class='cursor-pointer ml-5 hover:opacity-50' loading='lazy' src='https://549864.fs1.hubspotusercontent-na1.net/hubfs/549864/assets/request-a-demo-2022/arrow.svg' alt='arrow right'>",
				appendArrows: $('.testimonials-slick--arrows'),
				responsive: [
					{
						breakpoint: 1075,
						settings: {
							slidesToShow: 1,
							slidesToScroll: 1,
							dots: true,
							arrows: false
						}
					}
				]
			});

			$('.testimonials-slick').on('afterChange', function (event, slick, currentSlide, nextSlide) {
				$('.testimonials-slide-count').html((currentSlide + 1) + '/3')
			});

			$('.hero-images').slick({
				dots: false,
				arrows: false,
				slidesToShow: 1,
				autoplay: true,
				infinite: true,
				autoplaySpeed: 4000,
				fade: true,
				speed: 500,
				cssEase: 'linear',
			});

			function handleSlickAutomate() {
				if ($(window).outerWidth() > 1074) {
					$('.automate').filter('.slick-initialized').slick('unslick')
				} else {
					$('.automate').not('.slick-initialized').slick({
						dots: true,
						arrows: false,
						slidesToShow: 1,
						adaptiveHeight: true,
					});
				}
			}

			handleSlickAutomate()

			$(window).resize(function () {
				handleSlickAutomate()
			});
		</script>
	
