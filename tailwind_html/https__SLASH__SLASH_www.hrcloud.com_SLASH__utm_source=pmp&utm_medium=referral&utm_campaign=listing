  var levelOpened = 0

  function closeAllCommunication() {
    $('#hrplatform-link').removeClass('maxlg:hidden')
    $('#integrations-link').removeClass('maxlg:hidden')
    $('#communication-list').addClass('maxlg:hidden')
    $('#communication-list').removeClass('maxlg:grid')
