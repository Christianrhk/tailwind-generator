    const appendAddThisScript = () => {
      if(document.querySelector('#cookie-hub-script')) return;

      const  script = document.createElement('script');
      script.id = 'add-this-script'
      script.src = '//s7.addthis.com/js/300/addthis_widget.js#pubid=ra-6284d780f4ccf18a';
      document.head.appendChild(script);
    }

    const killAddThis = () => {
      const script = document.querySelector('#add-this-script');

      if (script) {
        script.remove();
      }

      const addThis = document.querySelectorAll('.addthis_inline_share_toolbox');

      console.log(addThis.length)

      addThis.forEach(el => {
        el.innerHTML = '';
      });
    }

    const handleAddThis = cookieHub => cookieHub.hasConsented('analytics')
        ? appendAddThisScript()
        : killAddThis();

    var cpm = {
      onInitialise: function(status) {
        handleAddThis(this);
      },
      onStatusChange: function(status, previousStatus)
      {
        console.log(this.hasConsented('analytics'))
        handleAddThis(this);
      }
    };
