  <script>
    const client = algoliasearch( '7SOS4PGMKI', '775f92696f47d1e347ee8f01ff1d9d68' );
    const posts = client.initIndex( 'prod_searchable_posts' );
    let suggestionCounter = 0;
    autocomplete(
        '#aa-search-input',
        {
            debug: true,
            templates: {
                dropdownMenu:
                    '<div class="aa-dataset-posts">'
            },
        },
        [
            {
                source: autocomplete.sources.hits(posts, {hitsPerPage: 20}),
                displayKey: 'post_title',
                templates: {
                    header: `<div class="aa-suggestions-category text-gray-600 text-xl mb-14">... Posts found</div>`,
                    suggestion: function(suggestion) {
                        return `<div class="search-page-results-item mb-10 container max-w-6xl mx-auto">
          <a
            class="search-page-results-item-title text-gray-600 font-bold inline-block border-b-4 border-white hover:border-yellow-400"
            href="${suggestion.permalink}">
            ${suggestion._highlightResult.post_title.value}
          </a>
          <p class="text-gray-600 text-with-elipsis mb-8">
          ${suggestion.content}
          </p>
        </div>`
                      },
                      empty: '<div class="aa-empty text-gray-600 font-bold text-3xl text-center">No results</div>'
                  }
              }
          ]
      )
    </script>
