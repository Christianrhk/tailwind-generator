window.onload = function() {
  if (typeof jQuery === 'undefined') {
    return;
  }
  
  Array.prototype.insert = function ( index, item ) {
    this.splice( index, 0, item );
  };
  
  jQuery('body').on('mw_fees', function (event, action) {    
    if (action === 'end-initialize') {  
      $.each($(".product-fees-container ul"), ( index, value ) => {
        var myValue = $(value).clone();
        if($(value).find("input")[0]) {
          
          var mainInput  = $(value).find("input");
          $(value).find("li").css("display", "none");

          var arr = myValue[0].innerHTML.split('');
          
          arr.forEach((item, key)=> {
            if(item === "." && arr[key + 3] === "$") {        	
                arr[key] = ",";
                if(!isNaN(+arr[key - 4]) && arr[key - 4] != " ") {
					arr.insert(key - 3, '.');
                }
            }
          })
//           arr[arr.length - 9] = ",";
//           if(!isNaN(+arr[arr.length - 13]) && +arr[arr.length - 13] != " ") {
//             arr.insert(arr.length - 12, '.');
//           }
          var str = arr.join("");

          $($(".product-fees-container ul")[index]).append(str);

          $($($(".product-fees-container ul")[index]).find("input")[1]).on('click', function() {
              $(mainInput).trigger("click")
          })         
        }
      });         
    }  
    
    if (action === 'RefreshSettings-done_api') {    
      //window.location = window.location;
      
      setTimeout(function(){  
        if($(".mw-hidden-fee-price ").length || $(".mw-hidden-fee-price ").html()) {
          $.each($(".mw-hidden-fee-price "), function( index, value ) {
          	//console.log(value);
            var hiddenArr = $(value).html().split('');
            hiddenArr[hiddenArr.length - 4] = ',';
            if(hiddenArr.length > 7) {
              hiddenArr.insert(hiddenArr.length - 7, '.');
            }
   			$(value).html(hiddenArr.join(''));          
          })             
        }         
        setTimeout(function(){
          
          	var subTotalArr =  $(".cart__subtotal").html().split('');
            subTotalArr[subTotalArr.length - 4] = ',';
            if(subTotalArr.length > 7) {
              subTotalArr.insert(subTotalArr.length - 7, '.');
            }
   			$(".cart__subtotal").html(subTotalArr.join(''));
        },1000)       
      },0)     	     
    } 
  });    
} 
</script>
