        <script>
            var scrollpos = window.scrollY;
            var header = document.getElementById("header");
            var navcontent = document.getElementById("nav-content");
            var navaction = document.getElementById("navAction");
            var brandname = document.getElementById("brandname");
            var logo = document.getElementById("header-logo");
            var mainnav = document.getElementById("main-nav");
            var toggle = document.getElementById("nav-toggle");
            var navbutton = document.getElementById("navButton");

            document.addEventListener("scroll", function () {
              /*Apply classes for slide in bar*/
              scrollpos = window.scrollY;

              if (scrollpos > 10) {
                header.classList.add("bg-white");
                navcontent.classList.add("text-black");
                toggle.classList.remove("text-white");
                toggle.classList.add("text-black");
                navcontent.classList.remove("text-white");
                navaction.classList.remove("bg-white");
                navaction.classList.add("bg-blue-600");
                navaction.classList.remove("text-gray-800");
                navaction.classList.add("text-white");
                header.classList.add("shadow");
                navcontent.classList.remove("bg-gray-100");
                navcontent.classList.add("bg-white");
                logo.classList.remove("h-36");
                logo.classList.add("h-12");
                mainnav.classList.remove("py-8");
                mainnav.classList.add("py-4");

                navbutton.classList.remove("text-white");
                navbutton.classList.add("text-black");
              } else {
                header.classList.remove("bg-white");
                navcontent.classList.remove("text-black");
                toggle.classList.remove("text-black");
                toggle.classList.add("text-white");
                navcontent.classList.add("text-white");
                navaction.classList.remove("bg-blue-600");
                navaction.classList.add("bg-white");
                navaction.classList.remove("text-white");
                navaction.classList.add("text-gray-800");
                header.classList.remove("shadow");
                navcontent.classList.remove("bg-white");
                navcontent.classList.add("bg-gray-100");
                logo.classList.remove("h-12");
                logo.classList.add("h-36");
                mainnav.classList.remove("py-4");
                mainnav.classList.add("py-8");

                navbutton.classList.remove("text-black");
                navbutton.classList.add("text-white");
              }
            });
        </script>
        <script>

            var navMenuDiv = document.getElementById("nav-content");
            var navMenu = document.getElementById("nav-toggle");
            var navbutton = document.getElementById("navButton");

            document.onclick = check;
            function check(e) {
              var target = (e && e.target) || (event && event.srcElement);

              //Nav Menu
              if (!checkParent(target, navMenuDiv)) {
                // click NOT on the menu
                if (checkParent(target, navMenu)) {
                  // click on the link
                  if (navMenuDiv.classList.contains("hidden")) {
                    navMenuDiv.classList.remove("hidden");
                    navbutton.classList.remove("text-white");
                    navbutton.classList.add("text-black");
                  } else {
                    navMenuDiv.classList.add("hidden");
                    navbutton.classList.remove("text-black");
                    navbutton.classList.add("text-white");
                  }
                } else {
                  // click both outside link and outside menu, hide menu
                  navMenuDiv.classList.add("hidden");
                }
              }
            }
            function checkParent(t, elm) {
              while (t.parentNode) {
                if (t == elm) {
                  return true;
                }
                t = t.parentNode;
              }
              return false;
            }
        </script>
        <script>
    function imgFunc(){
            var i = 1;
            var img ='img'+i;
            return {
            i,
            img,
            startImg(){
                var self=this;
                var myvar = window.setInterval(function(){
                    self.img = 'img'+self.i;
                    if(self.i<3){
                        self.i++;
                    }else{
                        self.i=1;
                    }
                }, 10000);
            },
            changeImg() {
                this.img = 'img'+this.i;
                if(this.i<3){
                    this.i++;
                }else{
                    this.i=1;
                }
            }

        }

    }
        </script>
