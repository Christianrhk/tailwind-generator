<script> 

  function getParameterByName(name, url = window.location.href) {
          name = name.replace(/[\[\]]/g, '\\$&');
          var regex = new RegExp('[?&]' + name + '(=([^&#]*)|&|#|$)'),
              results = regex.exec(url);
          if (!results) return null;
          if (!results[2]) return '';
          return decodeURIComponent(results[2].replace(/\+/g, ' '));
      }
  
  
      document.addEventListener("DOMContentLoaded", function(){
          //....
          const storeCredit = getParameterByName('storecredit');
          //console.log(storeCredit); 
  
          if( storeCredit ){

            setTimeout(function() {
              const credit = document.getElementById('Rise-account-page-button');
              credit.click()
              //console.log('si existe');
            }, 1700);
            
            
          }else{
            //console.log('no  existe');
          }
      });  
  </script>
  
<script> 
  var altura = 0;
  var startProductBarPos=-1;
  if ( screen.width < 768) {
	altura = 65;
  }else{	   
    altura = 140;
  }
  
  //console.log('ancho'+ screen.width + altura)
window.onscroll=function(){
  
  var bar = document.getElementById('navigation-blog');
  
  if ( bar && bar != null ) {
    //console.log('bar: ' + bar);
    if(startProductBarPos<0)startProductBarPos=findPosY(bar);

    if(pageYOffset>startProductBarPos){
      bar.style.position='fixed';
  //    bar.style.top=0;

      if ( screen.width < 368) {
            bar.style.top="64px";
          //console.log('mobile');

      }else if(screen.width < 768){	   
        bar.style.top="68px";
          //console.log('mobile'); 
      }else{
          bar.style.top="140px"; 
          //console.log('desktop');
      }

    }else{
      bar.style.position='relative';
      bar.style.top=0;
    }
  }
};

function findPosY(obj) {
  var curtop = 0;
  if ( (typeof obj == 'object') && obj != null) {
      
    if (obj.offsetParent != 'undefined' && obj.offsetParent) {
      while (obj.offsetParent) {
        curtop += obj.offsetTop;
        obj = obj.offsetParent;
      }
      curtop += obj.offsetTop;
  	} else if (obj.y) {
   		curtop += obj.y;
  	return curtop;
	}
  }
}
  
</script>

