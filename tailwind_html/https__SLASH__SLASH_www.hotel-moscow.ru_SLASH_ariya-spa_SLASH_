<!--Menu 2 -->
<script>
    var expand_menu = function () {
        $('#header-2').addClass('header-2--expanded');
        $('div[class^="x-tl-booking-widget-"]').hide();
        $('.lt-label').hide();
    }
    var collapse_menu = function () {
        $('#header-2').removeClass('header-2--expanded');
        $('div[class^="x-tl-booking-widget-"]').show();
        $('.lt-label').show();
    }
    $('.header-expander').click(expand_menu);
    $('.header-collapser').click(collapse_menu);
    window.onresize = collapse_menu;
    var moveTlWidget = function () {
        $widget = $('div[class^="x-tl-booking-widget-"]').eq(0);
        if ($widget.length > 0) {
            $widget.css({
                left: '20px',
                right: '',
                top: '',
                bottom: '20px',
            });
        } else {
            setTimeout(moveTlWidget, 1000);
        }
    }
    moveTlWidget();

</script>




