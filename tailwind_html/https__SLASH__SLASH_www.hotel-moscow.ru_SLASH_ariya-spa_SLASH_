<script>
    (function() {
        document.addEventListener("DOMContentLoaded", function(event) {
            var maxdate = new Date();
            maxdate.setDate(maxdate.getDate() + 30);
            $('#spa-reserve_date').datepicker({
                minDate: new Date(),
                maxDate: maxdate,
                dateFormat: 'dd MM',
            });
            $elements = $('.spa-reserve_form input');
            $btn = $('#spa_reserve');
            $btn.click(function(e) {
                var filled = true;
                var data = {};
                $elements.each(function(k,f) {
                    var val = f.value.trim();
                    if (! val) filled = false;
                    data[f.name] = val;
                });
                filled = filled && $('#rules').prop('checked');
                if (filled) {
                    $elements.prop('disabled', 'true').css('opacity', .75);
                    $btn.prop('disabled', 'true').addClass('loading');
                    $.post(
                        '/bitrix/templates/common/ajax/spa-reserve-add.php?lang=ru',
                        JSON.stringify(data)
                    )
                     .done(function(response) {
                        if (response.success == true) {
                            $btn.replaceWith("<b>Спасибо! Ваша заявка принята.</b>");
                        }
                     })
                     .fail(function() {
                        $elements.prop('disabled', false).css('opacity', 1);
                        $btn.prop('disabled', false).removeClass('loading');
                     })
                    ;
                }
            });
        });
    })();
</script>			</div>
		</div>
		 <script>
    $(document).ready(function(){
        $('.content-header').addClass('text-6xl mb-12');

        $('.price-slider').slick({
            adaptiveHeight: true,
            arrows: false,
        });

        $('.spa-gallery').slick({
            centerMode: true,
            centerPadding: '60px',
            slidesToShow: 3,
            prevArrow: $('#slick-prev'),
            nextArrow: $('#slick-next'),
        });

        $priceButtons = $('.price-buttons span');
        $priceButtons.eq(0).addClass('bg-green-600');

        $priceButtons.click(function(e) {
            $priceButtons.removeClass('bg-green-600');
            $(e.currentTarget).addClass('bg-green-600');
            $('.price-slider').slick(
                'slickGoTo',
                $(e.currentTarget).index()
            );
        });
    });
</script>
	</div>
