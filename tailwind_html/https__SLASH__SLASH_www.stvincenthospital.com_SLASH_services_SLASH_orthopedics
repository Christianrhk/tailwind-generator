<script type="text/javascript">
    var ShowYouTubeVideoForTenetCard = false;
    var dialogId = "dialog-video-59171467";
    var closeButtonId = "closebutton-59171467";
    var pvideoId = "playVideo-59171467";
    //document.getElementById(dialogId).hidden = true;

    function CloseYoutubeVideo(id) {
        
        //event.preventDefault();
        var _playVideoId = "playVideo-" + id;
        var _dialogId = "dialog-video-" + id;
        var _iframeVideoId = "iframe-video-" + id;
        var _getVideoSrc = document.getElementById(_iframeVideoId).getAttribute('src');
        ShowYouTubeVideoForTenetCard = false;
        //$("#" + _dialogId + " iframe").attr("src", $("#" + _dialogId + " iframe").attr("src"));
        document.getElementById(_iframeVideoId).setAttribute('src', _getVideoSrc);

        //document.getElementById(_dialogId).hidden = true;
        document.getElementById(_dialogId).setAttribute('style', 'display: none !important');

        var pvideo = document.getElementById(_playVideoId);
        pvideo.focus();
    }

    function clickAction(modalDialogId) {
        //event.preventDefault();
        //if (event.target.id === "playVideo") {
        dialogId = "dialog-video-" + modalDialogId;
        closeButtonId = "closebutton-" + modalDialogId;
        pvideoId = "playVideo-" + modalDialogId;
        ShowYouTubeVideoForTenetCard = true;
        document.getElementById(dialogId).setAttribute('style', 'display: block !important');
        var closeButton = document.getElementById(closeButtonId);
        closeButton.focus();
        //document.getElementById(dialogId).hidden = false;
        //}
    }

    document.addEventListener('keyup', function (evt) {
        if (ShowYouTubeVideoForTenetCard && evt.keyCode === 27) {
            var pvideo = document.getElementById(pvideoId);
            pvideo.focus();
            ShowYouTubeVideoForTenetCard = false;
            //document.getElementById(dialogId).hidden = true;
            document.getElementById(dialogId).setAttribute('style', 'display: none !important');
        } else if (ShowYouTubeVideoForTenetCard && evt.keyCode === 9) {
            var elm = evt.target;
            var parentFound = elm.closest("div[role='dialog']");

            if (!parentFound) {
                var cbutton = document.getElementById(closeButtonId);
                cbutton.focus();
            }
        }
    });


</script>


