<script>
if (window.caseHubInterval) {
  clearInterval(window.caseHubInterval);
}

window.caseHubInterval = setInterval(function() {
  let caseImagesToFound = document.querySelectorAll('.js-image-lazyload');
   
  if (caseImagesToFound.length > 1 ) {

    document.querySelectorAll('a[data-original]').forEach((link) => {
	   let source = link.getAttribute('data-original');

	   if (source && source.includes(" ")) {
			source.replace(/\s/g, '%20');
	   }
	    
            link.style.backgroundImage = "url('" + source + "')";

	});

    clearInterval(window.caseHubInterval);
  }
}, 100);
</script>


