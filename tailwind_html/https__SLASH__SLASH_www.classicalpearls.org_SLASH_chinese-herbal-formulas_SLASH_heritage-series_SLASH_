    <script>
        // SCRIPT LOCATED IN CATEGORY.HTML
        // GRAB THE FULL DESCRIPTION
        let categoryDescriptionFull = document.querySelector(".category-description");
        // FIND THE NTH PARAGRAPH END
        let categoryDescriptionPFirst = categoryDescriptionFull.innerHTML.indexOf('</p>') + 4;
        let categoryDescriptionPSecond = categoryDescriptionFull.innerHTML.indexOf('</p>',categoryDescriptionPFirst) + 4;

        if(categoryDescriptionPSecond > 5){
            // SPLIT THE CONTENT AT THE END OF NTH PARAGRAPH
            let categoryDescriptionPartialBefore = categoryDescriptionFull.innerHTML.slice(0, categoryDescriptionPSecond);
            let categoryDescriptionPartialAfter = categoryDescriptionFull.innerHTML.slice(categoryDescriptionPSecond);

            // BUILD AND POST TEXT WITH READ MORE BUTTON
            categoryDescriptionFull.innerHTML = categoryDescriptionPartialBefore + '<p><a onclick="expandText()" id="readMoreButton">Read More</a></p><div class="categoryDescriptionPartialAfterHidden">' + categoryDescriptionPartialAfter + "</div>";

            // SHOW HIDDEN TEXT AND REMOVE READMORE BUTTON
            function expandText() {
                document.querySelector('.categoryDescriptionPartialAfterHidden').style.display = "block";
                document.querySelector('#readMoreButton').style.display = "none";
            }
        }
    </script>


