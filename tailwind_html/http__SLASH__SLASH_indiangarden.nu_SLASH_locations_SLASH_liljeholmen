<style>


/* Style the tab */
.tab {
  overflow: hidden;
  border: 1px solid #ccc;
  background-color: #f1f1f1;
}

/* Style the buttons inside the tab */
.tab button {
  background-color: inherit;
  float: left;
  border: none;
  outline: none;
  cursor: pointer;
  padding: 14px 16px;
  transition: 0.3s;
  font-size: 17px;
}

/* Change background color of buttons on hover */
.tab button:hover {
  background-color: #ddd;
}

/* Create an active/current tablink class */
.tab button.active {
  background-color: #ccc;
}

/* Style the tab content */
.tabcontent {
  display: none;
  padding: 6px 12px;
  -webkit-animation: fadeEffect 1s;
  animation: fadeEffect 1s;
  background-size: cover;
  background-position: top right;
}

/* Fade in tabs */
@-webkit-keyframes fadeEffect {
  from {opacity: 0;}
  to {opacity: 1;}
}

@keyframes  fadeEffect {
  from {opacity: 0;}
  to {opacity: 1;}
}
</style>

<script>
function openCity(evt, cityName, resClass) {
  var i, tabcontent, tablinks;
  tabcontent = document.getElementsByClassName('res_'+resClass);
  for (i = 0; i < tabcontent.length; i++) {
    tabcontent[i].style.display = "none";
  }
  tablinks = document.getElementsByClassName('res_tab_'+resClass);
  for (i = 0; i < tablinks.length; i++) {
    tablinks[i].className = tablinks[i].className.replace(" active", "");
  }
  document.getElementById(cityName).style.display = "block";
  evt.currentTarget.className += " active";
}
</script>

   <div id="map" class="google-maps">
      
				<script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBT3yJopu36lncmRtsPTBPCMIp58hgfqsc&sensor=false&language=se-SE"></script>
			<script type="text/javascript">
			//<![CDATA[
			
			var map; // Global declaration of the map
			var lat_longs_map = new Array();
			var markers_map = new Array();
            var iw_map;
			
			iw_map = new google.maps.InfoWindow();
				
				 function initialize_map() {
				
				var myLatlng = new google.maps.LatLng(59.30788295252552, 18.028674999999996);
				var myOptions = {
			  		zoom: 16,
					center: myLatlng,
			  		mapTypeId: google.maps.MapTypeId.ROADMAP}
				map = new google.maps.Map(document.getElementById("map_canvas"), myOptions);
				
			var myLatlng = new google.maps.LatLng(59.30788295252552, 18.028674999999996);
				
			var markerOptions = {
				map: map,
				position: myLatlng,
				animation:  google.maps.Animation.DROP		
			};
			marker_0 = createMarker_map(markerOptions);
			
			marker_0.set("content", "<div class=\"map_content\"><h3>Liljeholmen</h3><p>Sjövikstorget 10, 117 60 Liljeholmen, Stockholm.</p></div>");
			
			google.maps.event.addListener(marker_0, "click", function(event) {
				iw_map.setContent(this.get("content"));
				iw_map.open(map, this);
			
			});
			
			
			}
		
		
		function createMarker_map(markerOptions) {
			var marker = new google.maps.Marker(markerOptions);
			markers_map.push(marker);
			lat_longs_map.push(marker.getPosition());
			return marker;
		}
		
			google.maps.event.addDomListener(window, "load", initialize_map);
			
			//]]>
			</script>
