<article class=" cpt dossier accroche secondaire large" id="home-economics">
    <header class="issue header dossier-header accroche-header">
                <section class="thumbnail dossier-thumbnail" style="background-image:url();"></section>

        <section class="infos dossier-infos p-6 lg:p-20">
            <a href="https://esse.ca/en/home-economics/">
                <h2 class="field-title">Home Economics</h2>
                <h3 class="field-subtitle"></h3>
            </a>
