<script>
  $(document).ready(function(){


    $('.pairs-form__controls select').change(function(){


let subval = $(this).val();

if(subval=='subscription'){

    $('[class="pww-option-input"][data-type="subscription"]').click();
}else{
    $('[class="pww-option-input"][data-type="one-time"]').click();

}
})


$('.product-subscription__button').click(function(e){
    e.preventDefault()

        $('.pww-product .moxie-atc-prod a').click();
})  

  })
</script>
