    $(document).on('click', '.side-cart__addon-add', function (e) {
      e.preventDefault();
      let id = $(this).data('pid');
      let context = $(this);
      if (id) {
        $(this).attr('disabled', 'disabled').html('<div class="dcbLoader">ADDING...</div>');
        CartJS.addItem(
          id,
          1,
          {},
          {
            success: function (data) {
              context.text('ADD').removeAttr('disabled');
              SuperCart.getCart();
              data.addedQuantity = 1;
              jQuery(document).trigger('MOXIE_ADD_TO_CART', data);
            },
          }
        );
      }
    });
    $(document).on('click', '.cart-coupon .accordion-header', function () {
      $('.cart-coupon').toggleClass('active');
      let coupon = Cookies.get('discount_code');
      let currentVal = $('.add-discount-now').val();

      if (coupon && coupon != '' && currentVal == '') {
        $('.add-discount-now').val(coupon);
      }
    });

    $(document).on('change', 'input.discount-field', function () {
      let value = $(this).val();
      Cookies.set('discount_code', value);
    });
    $(document).on('click', '.discount_apply', function (e) {
      e.preventDefault();
      let discount = $('input.discount-field').val();

      if (discount != '') {
        $(this).val('Applying.');
        location.href = '/checkout?discount=' + discount;
      } else {
        alert('Please input discount code');
      }
    });
    $(document).on('click', '.add-liner-ajax', function (e) {
      e.preventDefault();
      let context = $(this).closest('.code-block-slide-pop');
      let id = context.find('.variant-selector-liner').val();
      if (id) {
        $(this).attr('disabled', 'disabled').html('<div class="dcbLoader">ADDING...</div>');
        CartJS.addItem(
          id,
          1,
          {},
          {
            success: function (data) {
              context.find('.add-liner-ajax').text('ADD').removeAttr('disabled');
              SuperCart.getCart();
              data.addedQuantity = 1;
              jQuery(document).trigger('MOXIE_ADD_TO_CART', data);
            },
          }
        );
      }
    });
    let discount = getParameterByName('discount');

    if (discount && discount != '') {
      Cookies.set('discount_code', discount);
    }

    function showLinerPopup(status) {
      window.ShowingMagneticPopup = status;


      if (status) {
        $('.ss-shopify_cart .magnetic-liner .liner-slide-pop').removeClass('hidden');

        let showPopup = setInterval(() => {
          if ($('.ss-shopify_cart .magnetic-liner .liner-slide-pop.hidden').length > 0) {
            $('.ss-shopify_cart .magnetic-liner .liner-slide-pop')
              .removeClass('hidden');
            clearInterval(showPopup);
          }
        }, 100);
        //  $('.ss-shopify_cart .congrats-message-popup').addClass('hidden');
      } else {
        $('.ss-shopify_cart .magnetic-liner .liner-slide-pop').addClass('hidden');
        //  $('.ss-shopify_cart .congrats-message-popup').removeClass('hidden');
      }
    }
    function showLashiesLinerPopup(status) {
      window.ShowingLashiesPopup = status;


      if (status && !window.ShowingMagneticPopup) {
        $('.ss-shopify_cart .lashies-liner .liner-slide-pop')
          .removeClass('hidden');
        let showPopup = setInterval(() => {
          if ($('.ss-shopify_cart .lashies-liner .liner-slide-pop.hidden').length > 0) {
            $('.ss-shopify_cart .lashies-liner .liner-slide-pop')
              .removeClass('hidden');
            clearInterval(showPopup);
          }
        }, 100);

      } else {
        $('.ss-shopify_cart .lashies-liner .liner-slide-pop')
          .addClass('hidden');
      }
    }
    function ShouldShowLinerPopup(cart) {


      try {
        if ($('.ajaxcart__inner.ajaxcart__inner--has-fixed-footer.liner-slide-pop').length == 0) {

          if (!$('.ajaxcart__inner--has-fixed-footer').hasClass('liner-popup-added')) {
            setTimeout(() => {
              // $('.ajaxcart__inner--has-fixed-footer .code-block-slide-pop').remove();
              
              //  $('.ajaxcart__inner--has-fixed-footer').addClass('liner-popup-added');
            }, 0);
          }

        }

        //test
        let lashSKUs = [
          'L10-RT',
          'L10-3PK',
          'BL9',
          'BL9-3PK',
          'L2-NEW',
          'L2-3PK',
          'BL7',
          'BL7-3PK',
