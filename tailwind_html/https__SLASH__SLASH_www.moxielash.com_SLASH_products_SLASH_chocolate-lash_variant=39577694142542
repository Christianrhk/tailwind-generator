<script >
    //klaviyo tracking script
var _learnq = _learnq || [];

// Helper function that wraps the big "track" call above.
window.trackStartedCheckout = function (checkout) {
  _learnq.push(['track', 'Started Checkout', {
      "$value" : checkout.price,
      "ItemNames" : checkout.itemNames,
      "Items" : checkout.items,

  }]);

};

window.identifyCustomer = function (email) {

  if (email && /^.+@.+\..+$/.test(email)) {
      console.log('customer tracked - '+email);
      _learnq.push(['identify', { $email: email }]);
                     if(window.customCheckout){
      window.trackStartedCheckout(window.customCheckout);
  }
  }
};

$(document).on('change', '[name="email"]', function (e) {
  var email = $(this).val();
  identifyCustomer(email);
});


</script>

<script >
    jQuery(document).ready(function(){
        setTimeout(function(){$('body').removeClass('hidden');},600);

      jQuery(document).on('click','a[href^="#"]:not([href="#swym-wishlist"])',function(e){
        if(!$(this).hasClass('see-the-look-btn'))
        {
        e.preventDefault();  
        }
          var id= jQuery(this).attr('href');
          if($(this).hasClass('no-scroll'))
          {
            return;
          }
          try{
         $('html, body').animate({
              scrollTop: $(id).offset().top-100
          }, 1200);
        }
        catch(e){ 

        }
      });
      //TRIGGER PAGE VIEW ON EACH PAGE LOAD;
      

    });
  </script>
