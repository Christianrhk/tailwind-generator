<script type="text/javascript">
    $(document).ready(function () {
        initFormValidate();
    });
    function initFormValidate() {
        $('#form_customForm').validate({
            ignore:[],
            rules: {
                'ID': {
                    required: true,
                    number: true
                }
            }
        });
        $.validator.addMethod('phone', function (value, element) {
            if (value.replace(/\s/g, "") == "")
                return true;
            var reg = /^\(?[0]{1}[0-9]{1,3}\)?\-?(\d{1,4})\-?(\d{4})$/;
            return reg.test(value);
        }, "電話格式有錯, 格式應為 (02xx)123-4567 或 (02xx)1234567 或 02xx1234567 或 02xx123-4567");
        $.validator.addMethod('mobile', function (value, element) {
            if (value.replace(/\s/g, "") == "")
                return true;
            var rule1 = /^[0]{1}[9]{1}[0-9]{8}$/;
            var rule2 = /^09[0-9]{2}-[0-9]{6}$/;
            return rule1.test(value) || rule2.test(value);
        }, "手機格式有錯, 格式應為 0912345678 或 09xx-123456");
        $.validator.addMethod('checkdatetime', function (value, element) {
            if (value.replace(/\s/g, "") == "")
                return true;
            var regEx = /^\d{4}\/\d{2}\/\d{2}$/;
            if (!value.match(regEx)) return false;  // 格式不正確
            var d = new Date(value);
            if (Number.isNaN(d.getTime())) return false; // 日期不正確
            return true;
        }, "日期內容不正確, 格式應為 yyyy/MM/dd");
        $('[id^="txt_"]').each(function (i, elem) {
            var isRequired = $(elem).parents('.form-group').find('label').data('required') == 'True' ? true : false;
            var validateType = $(elem).data('validate');
            switch (validateType.toUpperCase()) {
                case 'TXT':
                    $(elem).rules('add', {
                        required: isRequired,
                        messages: {
                            required: "欄位必須填寫"
                        }
                    });
                    break;
                case 'NUM':
                    $(elem).rules('add', {
                        required: isRequired,
                        number: true,
                        messages: {
                            required: "欄位必須填寫",
                            number: "輸入必須為數字"
                        }
                    });
                    break;
                case 'TEL':
                    $(elem).rules('add', {
                        required: isRequired,
                        phone: true,
                        messages: {
                            required: "欄位必須填寫"
                        }
                    });
                    break;
                case 'MOB':
                    $(elem).rules('add', {
                        required: isRequired,
                        mobile: true,
                        messages: {
                            required: "欄位必須填寫"
                        }
                    });
                    break;
                case 'EML':
                    $(elem).rules('add', {
                        required: isRequired,
                        email: true,
                        messages: {
                            required: "欄位必須填寫",
                            email: "電郵格式有錯"
                        }
                    });
                    break;
                case 'DATE':
                    $(elem).renderUI('datepicker');
                    $(elem).rules('add', {
                        required: isRequired,
                        checkdatetime: true,
                        messages: {
                            required: "日期必須填寫"
                        }
                    });
                    break;
            }
        });
        $('[name^="hdnValidate_"]').each(function (i, elem) {
            var isRequired = $(elem).data('required') == 'True'? true : false;
            $(elem).rules('add', {
                required: isRequired,
                messages: {
                    required:"選項沒有選擇"
                }
            });
        });
        $('input[type="checkbox"]').on('click', function () {
            var valu = "";
            var $parent = $(this).parents('.form-group');
            $parent.find('input[type="checkbox"]').filter(':checked').each(function (i, el) {
                valu += $(el).attr('value');
            });
            $parent.find('[name^="hdnValidate_"]').val(valu);
            $parent.find('[name^="hdnValidate_"]').valid();
        });
        $('input[type="radio"]').on('click', function () {
            var valu = "";
            var $parent = $(this).parents('.form-group');
            $parent.find('input[type="radio"]').filter(':checked').each(function (i, el) {
                valu += $(el).attr('value');
            });
            $parent.find('[name^="hdnValidate_"]').val(valu);
            $parent.find('[name^="hdnValidate_"]').valid();
        });
        $('select').on('change', function () {
            var valu = $(this).val();
            var $parent = $(this).parents('.form-group');
            $parent.find('[name^="hdnValidate_"]').val(valu);
            $parent.find('[name^="hdnValidate_"]').valid();
        });
        //$('textarea').each(function (i, el) {
        //    var isRequired = $(el).parents('.form-group').find('label').data('required') == 'True'? true: false;
        //    $(el).rules('add', {
        //        required: isRequired,
        //        maxlength: 1000,
        //        messages: {
        //            required:"欄位必須填寫",
        //            maxlength:"輸入字數超過{0}字".replace("{0}", 1000)
        //        }
        //    });
        //});
    }
</script>

