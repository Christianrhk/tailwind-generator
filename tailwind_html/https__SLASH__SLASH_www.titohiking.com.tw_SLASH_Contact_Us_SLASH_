        content: "";
        position: absolute;
        display: none;
    }

    .form-check .customcheck input:checked ~ .checkmark:after {
        display: block;
    }

    .form-check .customcheck .checkmark:after {
        left: 5px;
        top: 0px;
        width: 7px;
        height: 12px;
