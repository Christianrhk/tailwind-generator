<script>
    function rearrangeFormStructure() {
        $('[id^="field_"]').each(function (i, elem) {
            var dataType = $(elem).data('type');
            switch (dataType) {
               case "TXT":
                var answer = $(elem).find('[id^="txt_"]').val();
                $(elem).find('[name="answer[]"]').val(encodeURIComponent(answer));
                break;
                case "RDO":
                var answer = [];
                $(elem).find('input[type="radio"]').filter(':checked').each(function(i, el){
                    answer.push(encodeURIComponent($(el).attr('value')));
                });
                $(elem).find('[name="answer[]"]').val(answer.join('##'));
                break;
                case "CBX":
                    var answer = [];
                    $(elem).find('input[type="checkbox"]').filter(':checked').each(function (i, el) {
                        answer.push(encodeURIComponent($(el).attr('value')));
                    });
                    $(elem).find('[name="answer[]"]').val(answer.join('##'));
                break;
                case "OPT":
                    var optValu = $(elem).find('select').val();
                    $(elem).find('[name="answer[]"]').val(encodeURIComponent(optValu));
                break;
                case "RMK":
                    var txtAreaValu = $(elem).find('textarea').val();
                    $(elem).find('[name="answer[]"]').val(encodeURIComponent(txtAreaValu));
                break;
            }
        });
    }
    $(function () {
        $("#form_customForm").on("submit", function (event) {
            if(!$(this).valid()){
                return false;
            }
            rearrangeFormStructure();
            return true;
        });
    });

    function validateForm(token) {
        $("#form_customForm").trigger('submit');
    }
</script>

