<script>
// cookieの値がisClickedではなかったら表示
   if ($.cookie('cookieBtn') != 'isClicked') {
      $('.cookie_container').show();
   }
   $('.cookie_btn').on('click', function(){
      $('.cookie_container').hide();
      var date = new Date();
      // cookieにBtnという名前でisClickedをセット 期限は365日間
      $.cookie('cookieBtn', 'isClicked', { expires: 365, path:'/' });
   });
</script>

