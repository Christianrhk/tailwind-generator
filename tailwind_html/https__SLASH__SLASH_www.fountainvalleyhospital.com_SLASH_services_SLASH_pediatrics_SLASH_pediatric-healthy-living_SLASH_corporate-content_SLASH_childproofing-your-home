                        <div class="w-full md:w-1/2 lg:w-1/4 mt-2 p-1" v-for="(items, index) in filterNavigationList" :key="index" v-if="isMobileDevice ? (index < limitBy) : (index <= filterNavigationList.length)">
                            <div class="text-lg font-semibold font-sans leading-none">
                                <a :href="items.url" class="text-purple-100 hover:underline hover:text-purple-200">{{items.title}}</a>
                            </div>
                        </div>


                    <div class="w-full md:w-1/2 lg:w-1/4 mt-2 p-1" v-if="isMobileDevice" v-show="filterNavigationList.length > 6">
                        <div class="text-lg font-semibold font-sans leading-none">
                            <a href="javascript:void(0);" class="text-purple-100 hover:underline hover:text-purple-200" v-on:click="NavigationToggle(defaultLimit, filterNavigationList.length)">{{ limitBy===6?'+ Expand to see more': '- Show less'}}</a>
                        </div>
                    </div>
                </div>
            </div>
