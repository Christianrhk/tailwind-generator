<script>
	function checkMore(){
		document.body.style.overflow = 'hidden';
		document.getElementById("predition-more").style.display = 'block';
	}
	function closeMore(){
		document.body.style.overflow = 'visible';
		document.getElementById("predition-more").style.display = 'none';
	}
	function showMlb(t1, t2, t3){ // t3 nba
		document.getElementsByClassName("frame-predition")[t1].style.display = "block";
		document.getElementsByClassName("frame-predition")[t2].style.display = "None";
		document.getElementsByClassName("frame-predition")[t3].style.display = "None";
		// change class 
		document.getElementsByClassName("btn-predition")[t1].classList.add("bgactive", "text-white");
		document.getElementsByClassName("btn-predition")[t2].classList.remove("bgactive", "text-white");
		document.getElementsByClassName("btn-predition")[t3].classList.remove("bgactive", "text-white");
	}
</script>
