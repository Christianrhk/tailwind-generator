    $(window).scroll(function () {
        if ($(this).scrollTop() >= 1) {
            $('#voltar-topo').fadeIn(200);
        } else {
            $('#voltar-topo').fadeOut(200);
        }
    });
    $('#voltar-topo').click(function () {
