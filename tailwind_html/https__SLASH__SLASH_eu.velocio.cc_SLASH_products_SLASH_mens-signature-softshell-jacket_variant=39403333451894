    <div class="eight columns omega medium-down--one-whole">
        <h3 class="center">Dimension Details</h3>
        <ul class="tabs">
            <li><a href="#inches" class="active" data-no-instant="true">
                    <h4>IMPERIAL</h4>
                </a></li>
            <li><a href="#cms" data-no-instant="true">
                    <h4>METRIC</h4>
                </a></li>
