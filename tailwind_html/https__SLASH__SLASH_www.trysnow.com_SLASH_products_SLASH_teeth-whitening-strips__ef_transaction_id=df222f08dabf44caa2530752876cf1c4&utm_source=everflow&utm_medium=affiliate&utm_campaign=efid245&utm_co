                <script>
                  // Todo: clean this up, add it in a more appropriate spot
                  const optionSelect = document.getElementById('SingleOptionSelector');
                  const radioOptions = document.querySelectorAll('input[name="whitening-option"');
                  radioOptions.forEach(element => {
                    element.addEventListener('change',(e)=>{
                      document.querySelector('#SingleOptionSelector [value="' + e.target.value + '"]').selected = true; 
                      optionSelect.dispatchEvent(new Event('change'));       
                    })
                  });
                </script>
            </div>
              <div class="form-bottom">
                	<div class="mb-5 text-bold text-base text-brand-255474 flex pdp-estimated-delivery" style="margin-top: 5px;"></div>

<script>

   var ArraymonthNames = 
    [
        "Jan", "Feb", "Mar", "Apr", "May", "June",
        "July", "Aug", "Sep", "Oct", "Nov", "Dec"
    ];

    fetch('https://webhooks.endrock.software/endrockapi/v1/app/snow/arrival.php?id=6702937636952')
    .then(response => response.json())
    .then(data => 
    {   
        var getMaxDate = new Date(data.delivery_max_date_at+" 00:00:00");
        var getMinDate = new Date(data.delivery_min_date_at+" 00:00:00");

        var mainText = `<div class="font-normal text-base text-black pr-2.5"> Estimated Arrival: </div>`;
        var maxText = ArraymonthNames[getMaxDate.getMonth()] + " " + getMaxDate.getDate(); 
        var minText = ArraymonthNames[getMinDate.getMonth()] + " " + getMinDate.getDate();

        if (!isNaN(getMaxDate.getDate())) {
            document.querySelector('.pdp-estimated-delivery').innerHTML= mainText + " " + minText + " - " + maxText;
        }
    });
    
</script>
                <p class="mt-4"><span class="font-semibold">Price:</span><span class="text-2xl text-brand-1e4978 font-bold" data-variant-price-display><span class=money>$49.00</span></span>  <del style="font-size: 20px; text-decoration: line-through!important;">  </del>
</p>
               
