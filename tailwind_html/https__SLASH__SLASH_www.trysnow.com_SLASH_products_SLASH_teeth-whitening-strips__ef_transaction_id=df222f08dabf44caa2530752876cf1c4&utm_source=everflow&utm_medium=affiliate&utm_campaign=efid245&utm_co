      <script>
        window.addEventListener('load', function () {
          const main_slider = new Glide('.product-slider-wrapper', {
            perView: 1,
            rewind: true
          }).mount()

          const thumb_slider = new Glide('.product-slider-thumbnails-wrapper', {
            perView: 4,
            rewind: true
          }).mount()

          document.querySelectorAll('[data-thumnail-index]').forEach((thumbnail, index) => {
            thumbnail.addEventListener('click', () => {
              main_slider.go('=' + index)
            })
          })
        })
      </script>
      

      

      
