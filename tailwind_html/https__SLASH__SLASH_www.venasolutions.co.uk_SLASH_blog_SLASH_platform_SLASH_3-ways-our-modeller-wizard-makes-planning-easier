</div>
<script type="text/javascript">
  const xhttp_6390261659851298081 = new XMLHttpRequest();
  xhttp_6390261659851298081.open("GET", '/_hcms/related-blog-posts-callback?blogPostId=54299744273&blogId=61642784400&targetBlogPostIds=54299744273&macro=default&startDate=1577836800000&limit=3&templatePath=Website%2FNewsroom%2FBlog+Single++UK.html&contentId=54299744273&requestPath=%2Fblog%2Fplatform%2F3-ways-our-modeller-wizard-makes-planning-easier&hs-expires=1691387298&hs-version=1&hs-signature=AM6Wq0Z3ml9sgbiSChWOjL0zERaP1gk5Rw');
  xhttp_6390261659851298081.onreadystatechange = function() {
    if (this.readyState == XMLHttpRequest.DONE) {
      if (this.status == 200) {
        var id = setInterval(function() {
          var formattingFunction = blog_post_formatter;
          if (formattingFunction !== undefined) {
            clearTimeout(id);
            document.getElementById('related-blog-posts-6390261659851298081').outerHTML = formattingFunction(JSON.parse(xhttp_6390261659851298081.responseText));
          }
        }, 50);

        setTimeout(function() {
          clearTimeout(id);
        }, 10000);
      } else {
        console.error('Error: ', this.statusText);
      }
    }
  };
  xhttp_6390261659851298081.send();
</script>
</span>

