<script>

    $('#product_img_gallery').owlCarousel({
        loop: false,
        margin: 20,
        nav: false,
        responsive:{
            0:{
                items:1
            },
            600:{
                items:1
            },
            1000:{
                items:1
            }
        },
        mouseDrag: true,
        touchDrag:true,
        dot: false
    })

</script>

<script>
    $('.owl-carousel a.nav-item').on('click', function (e) {
            //e.preventDefault()
            $('.owl-carousel a.nav-item').removeClass("active");
            $(this).tab('show');
        })
    $('.owl-carousel').owlCarousel({
        loop: false,
        margin: 20,
        nav: false,
        mouseDrag: true,
        touchDrag:false,
        dot: false,
        responsive: {
            0: {
                items: 2
            },
            600: {
                items: 4
            },
            1200: {
                items: 6
            }
        }
    })

</script>

