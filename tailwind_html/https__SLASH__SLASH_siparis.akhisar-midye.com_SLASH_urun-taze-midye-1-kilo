<script type="text/javascript">
  function arttir() {
    var deger= $('#qty').val();
    deger++;
    $('#qty').val(deger);

  }

  function azalt() {
    var deger= $('#qty').val();
    if(deger>1){
     deger--;
     $('#qty').val(deger);
   }
 }
</script>