  $('.slider').slick({
    autoplay: true,
    autoplaySpeed: 3000,
    infinite: true,
    dots: true,
    arrows: true,
    centerMode: true,
    centerPadding: '20%',
    responsive: [{
      breakpoint: 1400,
      settings: {
        centerPadding: '15%',
      }
    },{
      breakpoint: 1200,
      settings: {
        centerPadding: '10%',
      }
    },{
      breakpoint: 768,
      settings: {
        centerMode: false,
        centerPadding: '0%',
      }
    }],
  });
</script>
