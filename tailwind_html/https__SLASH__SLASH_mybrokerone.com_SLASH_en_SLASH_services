<script>
  
  let ComponentNavbarHeroMode =  true 

  
  function setHeroMode() {
    if (window.scrollY <= 70 && !ComponentNavbarHeroMode) {
      Action(this, 'ComponentNavbar:SetHeroMode', true)
      ComponentNavbarHeroMode = true
    } else if (window.scrollY > 70 && ComponentNavbarHeroMode) {
      Action(this, 'ComponentNavbar:SetHeroMode', false)
      ComponentNavbarHeroMode = false
    }
  }

  
  document.addEventListener('DOMContentLoaded', function(){
    setHeroMode()
    document.addEventListener('scroll', setHeroMode)
  })
</script>
