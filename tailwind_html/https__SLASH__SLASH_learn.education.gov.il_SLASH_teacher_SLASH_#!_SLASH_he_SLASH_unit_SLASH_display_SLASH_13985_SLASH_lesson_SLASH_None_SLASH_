<script type="text/javascript">
	   var axiosInstance = {};
	   var axiosInstancePhp = {}
	var getNewJwt=false;

	let createAxiosInstanceWithJwt = function () {
		axiosInstance = axios.create({
			baseURL: settings.api,
			timeout: settings.axiosTimeout,
			headers: {
				Authorization: "JWT " + localStorage.getItem('jwt')
			}
		});
		axiosInstancePhp = axios.create({
			baseURL: settings.server + '/dataManagement/json.api.php',
			timeout: settings.axiosTimeout,
			headers: {
				Authorization: "JWT " + localStorage.getItem('jwt')
			}
		});
		getNewJwt=true
		axiosInstance.interceptors.response.use(function (response) {
			// Any status code that lie within the range of 2xx cause this function to trigger
			// Do something with response data
			return response;
		}, function (error) {
			store.dispatch('responseError', error);
			// Any status codes that falls outside the range of 2xx cause this function to trigger
			// Do something with response error
			return Promise.reject(error);
		});
	}
	/*
		Create the `axiosInstance` for use in VueJS.
		In order to communicate with the Django API, the `axiosInstance` is initialized
		with the JWT taken from the localStorage. The JWT is usually set in the localStorage on
		login; BUT if we are redirected directly in to a Unit via go.php and are
		authenticated via IDM, then we need to manually retreive it here, as there
		was no place for it to be set in the `localStorage` before hitting this page.
	 */
		//localStorage.removeItem('jwt')
		window.getCookie = function(name) {
	var cookiename = name + "=";
	var ca = document.cookie.split(';');
	for(var i=0;i < ca.length;i++)
	{
		var c = ca[i];
		while (c.charAt(0)==' ') c = c.substring(1,c.length);
		if (c.indexOf(cookiename) == 0) return c.substring(cookiename.length,c.length);
	}
	return null;
}

	 	if(window.getCookie('jwt')){
			 localStorage.setItem('jwt',window.getCookie('jwt'));
		 }

		createAxiosInstanceWithJwt();

</script>
