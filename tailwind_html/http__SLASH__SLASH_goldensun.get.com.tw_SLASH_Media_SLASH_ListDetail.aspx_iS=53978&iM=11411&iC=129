	<script type="text/javascript">
		function fShowVideo() {
			//alert("瀏覽器：" + BrowserDetect.browser +"；版本：" + BrowserDetect.version);
			var rml = new RadiantML();
			var mp4H264AAC = rml.mp4H264AAC('high40');
			var mse = rml.mse();
			var m4aHEAACv2 = rml.m4aHEAACv2();

			if (mse && mp4H264AAC && m4aHEAACv2) {
				document.getElementById("player-size").style.display = 'block';
				document.getElementById("rmpPlayer").style = 'width:640px; height:360px;'; //RWD可能不需此段，由CSS控制
			} else {
				document.getElementById("vVideo").style.display = 'block';
				var sVideoUrl = "src=" + BS5.decode('L21hbmlmZXN0LmY0bQ==') +
					"&playButtonOverlay=false&autoPlay=true&streamType=recorded&bufferingOverlay=false&initialBufferTime=3";
				var sVideoUrl3 = BS5.decode('L3BsYXlsaXN0Lm0zdTg=');
				//window.document.all.sEnv.innerHTML=BrowserDetect.OS+','+BrowserDetect.browser;
				window.document.all.vVideo.innerHTML =
					"<Div id='vWeb' style='display:none;'><object  width='776' height=''><param name='movie' value='./File/rmp-dark.swf'></param><param name='flashvars' id='vWebValue' value='" +
					sVideoUrl +
					"'></param><param name='allowFullScreen' value='true'></param><param name='allowscriptaccess' value='always'></param><param name='wmode' value='opaque'></param><embed src='./File/rmp-dark.swf' type='application/x-shockwave-flash' wmode='opaque' allowscriptaccess='always' allowfullscreen='true'  width='776' height=''  flashvars='" +
					sVideoUrl +
					"'　id='vWebValue2'></embed></object></Div><Div id='vApple' style='display:none;'><video controls   width='776' height='' id='vWebValue3' src='" +
					sVideoUrl3 + "'></video></Div><div>無法順利播放？建議使用Chrome、Safari、Firefox等瀏覽器觀看！</div>";
				if (BrowserDetect.OS == "Windows") {
					document.all.vWeb.style.display = 'block';
				} else {
					document.all.vApple.style.display = 'block';
				}

			}

		} <
