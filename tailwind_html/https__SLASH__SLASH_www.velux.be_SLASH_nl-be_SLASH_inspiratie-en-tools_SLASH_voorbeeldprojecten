<!-- GlobalLocalHeadline.ascx -->

    
    <h2><span class="font-grey1">Zolder verbouwen in een vakantiewoning<br>&nbsp;</span></h2>
     


    </div>
</div>

<div class="filterenabled spot-wrapper"  >
    
    <div class="article-wrapper">
        <div class="article-content clearfix textspot-content">
            
    <div style="width: 100%; height: 0px; position: relative; padding-bottom: 56.25%;"><iframe src="https://velux.23video.com/v.ihtml/player.html?token=2bf1c92bfab4c091bf42a4ee93d9b243&amp;source=embed&amp;photo%5fid=10446684" style="width: 100%; height: 100%; position: absolute; top: 0px; left: 0px;" frameborder="0" border="0" scrolling="no" allowfullscreen="1" mozallowfullscreen="1" webkitallowfullscreen="1"></iframe></div>
    


        </div>
    </div>
