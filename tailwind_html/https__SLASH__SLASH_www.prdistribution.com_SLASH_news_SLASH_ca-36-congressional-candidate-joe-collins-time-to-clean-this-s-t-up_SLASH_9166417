<script>
                $(document).on('submit', 'form.news-search-form', function() {
                    // code
                    var inputValue = $('#visibleInput').val();
                    var newValue = inputValue + " site:prdistribution.com/news";
                    $('#searchQuery').val(newValue);
                });

                function submitGoogleForm() {
                    $(".news-search-form").submit();
                }

                var input = document.getElementById("visibleInput");

                input.addEventListener("keyup", function(event) {
                    if (event.keyCode === 13) {
                        event.preventDefault();
                        submitGoogleForm();
                    }
                });
                </script>
