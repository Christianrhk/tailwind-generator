<script>
                                        $("#sendEmailForm").submit(function(e) {
                                            const isValidKey = $("#validEmailKey").val()
                                            if ($("#sendEmailForm").valid() && isValidKey == 1) {
                                                return true;
                                            } else {
                                                e.preventDefault();
                                            }
                                        });
                                        </script>
