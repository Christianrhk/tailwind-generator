<script>
                                        function launchEmailKeyHint() {
                                            $("#emailKeyHint").removeClass('hidden')
                                        }

                                        function hideEmailKeyHint() {
                                            $("#emailKeyHint").addClass("hidden");
                                        }

                                        $(document).mouseup(function(e) {
                                            var container = $("#emailKeyHint");

                                            // If the target of the click isn't the container
                                            if (!container.is(e.target) && container.has(e.target).length ===
                                                0 && !container.hasClass('hidden')) {
                                                container.addClass('hidden')
                                            }
                                        });

                                        $("#emailKeyInput").keyup(function() {
                                            const val = $("#emailKeyInput").val();
                                            console.log(val.length)

                                            $.ajax({
                                                type: "post",
                                                url: "/spirit/check-email-key.php",
                                                data: {
                                                    to_check: val
                                                },
                                                success: function(data) {
                                                    console.log(data)
                                                    //alert(data)
                                                    if (data == 1) {
                                                        $("#validEmailKey").val(1)
                                                        $(".success-email-key").removeClass(
                                                            "hidden");
                                                        $(".hint-email-key").addClass("hidden");
                                                        $("#emailKeyInput").removeClass("error");
                                                        $("#message-error-key").addClass("hidden");
                                                    } else {
                                                        $("#validEmailKey").val(0)
                                                        $(".success-email-key").addClass(
                                                            "hidden");
                                                        $(".hint-email-key").removeClass("hidden");
                                                        $("#emailKeyInput").addClass("error");
                                                        $("#message-error-key").removeClass(
                                                            "hidden");
                                                    }
                                                }
                                            });
                                        })
                                        </script>
<script>
                                        $(document).ready(function() {
                                            $("#sendEmailForm").validate({
                                                rules: {
                                                    name: {
                                                        required: true
                                                    },
                                                    email: {
                                                        required: true,
                                                        email: true
                                                    },
                                                    subject: {
                                                        required: true
                                                    },
                                                    message: {
                                                        required: true
                                                    },
                                                }
                                            });
                                        });
