    <script>
  

  var theme_nav = $(".js-theme-nav");
  var theme_nav_menu = $(".js-theme-nav-menu");
  var theme_nav_item = $(".js-theme-nav-item");
  var theme_nav_modal = $("#theme-nav-modal");
  var theme_nav_close = $(".js-modal-close");

  (function ($) {
    theme_nav_menu.on({
      mouseenter: function () {
        $(this).show();
      }
    });

    theme_nav_item.on({
      mouseenter: function () {
        theme_nav_menu.show();
      }
    });

    
      theme_nav.on({
        mouseenter: function () {
          theme_nav_menu.show();
        },
        mouseleave: function () {
          theme_nav_menu.hide();
        }
      });

      theme_nav_menu.on({
        mouseleave: function () {
          $(this).hide();
        }
      });

      theme_nav_item.on({
        mouseleave: function () {
          if ( $(this).find(".js-theme-nav-flyout").length == 0 ) {
            theme_nav_menu.hide();
          }
        }
      })
    
  })(jQuery);

  (function ($) {
    theme_nav
      .click(function (e) {
        e.preventDefault();
        if ( Theme.screen.medium_down ) {
          show_modal();
        }
      });

    theme_nav_close
      .click(function (e) {
        e.preventDefault();
        hide_modal();
      });

    function show_modal() {
      $("body").addClass("modal");
      theme_nav_modal.show();
    }

    function hide_modal() {
      $("body").removeClass("modal");
      theme_nav_modal.hide();
    }
  })(jQuery);
</script>


    

    
      <script src="//cdn.shopify.com/s/files/1/0802/9923/t/50/assets/collection.js?v=141712091810122815331657265895" type="text/javascript"></script>
    

    
      

<script src="//cdnjs.cloudflare.com/ajax/libs/fancybox/2.1.4/jquery.fancybox.js" type="text/javascript"></script>
<script src="//cdn.shopify.com/s/files/1/0802/9923/t/50/assets/jquery.fancybox-media.js?v=66528337733094219111657265902" type="text/javascript"></script>
<script src="//cdn.shopify.com/shopifycloud/shopify/assets/themes_support/option_selection-fe6b72c2bbdd3369ac0bfefe8648e3c889efca213baefd4cfb0dd9363563831f.js" type="text/javascript"></script>

<script>

  var product_image = $("#js-product-image");
  var product_image_thumbnails = $(".js-product-image-thumbnail");
  var add_to_cart = $("#js-add-to-cart");
  var product_price = $("#js-product").find(".js-product-price");
  var product_compare_price = $("#js-product").find(".js-compare-price");
  var video_icon = $(".js-video-icon");

  (function ($) {
    product_image_thumbnails
      .click(function (e) {
        e.preventDefault();
        product_image[0].src = this.href;
        show_video(this);
        product_image_thumbnails
          .removeClass("is-active")
          .filter(this)
            .addClass("is-active");
      });

    $(".js-video-icon")
      .click(function (e) {
        e.preventDefault();
        $.fancybox.open({
          href: this.href,
          helpers: {
            media: true
          },
          youtube: {
            autoplay: 1
          },
          maxWidth: 800
        });
      });
  })(jQuery);

  window.product_variant_select_callback = function (variant, selector) {

    if ( variant ) {
      if ( variant.available ) {
        add_to_cart
          .val("Add to Cart")
          .removeClass("disabled")
          .prop("disabled", false);
      } else {
        add_to_cart
          .val("Sold Out")
          .addClass("disabled")
          .prop("disabled", true);
      }

      if ( variant.featured_image ) {
        var product_image_size = Shopify.Image.imageSize(product_image[0].src);
        product_image[0].src = Shopify.Image.getSizedImageUrl(variant.featured_image.src, product_image_size);

        var variant_image = product_image_thumbnails.filter("[data-image-id=" + variant.featured_image.id + "]")[0];
        show_video(variant_image);

        product_image_thumbnails
          .removeClass("is-active")
          .filter(variant_image)
            .addClass("is-active");
      }

      product_price.html(
        Shopify.formatMoney(variant.price, "${{amount}}")
      );

      if ( variant.compare_at_price > variant.price ) {
        product_compare_price
          .html(
            Shopify.formatMoney(variant.compare_at_price, "${{amount}}")
          )
          .show();
      } else {
        product_compare_price.hide();
      }

    } else {
      // The variant doesn't exist, disable submit button.
      // This may be an error or notice that a specific variant is not available.
      // To only show available variants, implement linked product options:
      //   - http://docs.shopify.com/manual/configuration/store-customization/advanced-navigation/linked-product-options
      add_to_cart
        .val("Unavailable")
        .addClass("disabled")
        .prop("disabled", true);
    }
  };

  function show_video(image_element) {
    video_icon
      [image_element.getAttribute("data-video-href") ? "show" : "hide"]()
      [0].href = image_element.getAttribute("data-video-href");
  }

  (function ($) {
    new Shopify.OptionSelectors("product-variants", {
      product: {"id":6535858520126,"title":"Dr. Seuss Thing 1 \u0026 2 Socks - Adult","handle":"thing-1-2-socks-adult","description":"\u003cspan data-mce-fragment=\"1\"\u003eBring a little flair to your Dr. Seuss Day celebration.  These Thing 1 \u0026amp; Thing 2 socks are just what you will need to add a little character to your costume.\u003c\/span\u003e","published_at":"2021-10-01T13:56:08-06:00","created_at":"2021-02-25T08:53:35-07:00","vendor":"Zurchers","type":"Wearables","tags":["Dr. Seuss","Wearables"],"price":1267,"price_min":1267,"price_max":1267,"available":true,"price_varies":false,"compare_at_price":null,"compare_at_price_min":0,"compare_at_price_max":0,"compare_at_price_varies":false,"variants":[{"id":39248522772542,"title":"Default Title","option1":"Default Title","option2":null,"option3":null,"sku":"1087493","requires_shipping":true,"taxable":true,"featured_image":null,"available":true,"name":"Dr. Seuss Thing 1 \u0026 2 Socks - Adult","public_title":null,"options":["Default Title"],"price":1267,"weight":0,"compare_at_price":null,"inventory_quantity":44,"inventory_management":"shopify","inventory_policy":"deny","barcode":"618480020960","requires_selling_plan":false,"selling_plan_allocations":[]}],"images":["\/\/cdn.shopify.com\/s\/files\/1\/0802\/9923\/products\/1087493.jpg?v=1614268416"],"featured_image":"\/\/cdn.shopify.com\/s\/files\/1\/0802\/9923\/products\/1087493.jpg?v=1614268416","options":["Title"],"media":[{"alt":null,"id":20169250177086,"position":1,"preview_image":{"aspect_ratio":1.0,"height":900,"width":900,"src":"https:\/\/cdn.shopify.com\/s\/files\/1\/0802\/9923\/products\/1087493.jpg?v=1614268416"},"aspect_ratio":1.0,"height":900,"media_type":"image","src":"https:\/\/cdn.shopify.com\/s\/files\/1\/0802\/9923\/products\/1087493.jpg?v=1614268416","width":900}],"requires_selling_plan":false,"selling_plan_groups":[],"content":"\u003cspan data-mce-fragment=\"1\"\u003eBring a little flair to your Dr. Seuss Day celebration.  These Thing 1 \u002