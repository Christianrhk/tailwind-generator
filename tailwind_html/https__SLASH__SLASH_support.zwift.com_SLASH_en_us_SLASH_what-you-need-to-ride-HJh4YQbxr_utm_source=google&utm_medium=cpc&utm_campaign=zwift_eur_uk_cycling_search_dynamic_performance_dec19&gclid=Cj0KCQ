    	$('i.mdi.1-button').addClass('mdi-chevron-up');
    	$('i.mdi.1-button').removeClass('mdi-chevron-down');
    } else {
      $('i.mdi.1-button').addClass('mdi-chevron-down');
    	$('i.mdi.1-button').removeClass('mdi-chevron-up');
    }
  });
  
