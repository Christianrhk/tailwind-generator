</script><script>function reqListener () {
  var test = JSON.parse(this.responseText);
  
  var status = JSON.parse(this.responseText);

	$('.status').text(status.status.description);
  
  $('#statusDesc').text(status.status.description);
  
  var k = window.location.pathname;
  if ( k == "/lang/de/" && status.status.description == "All Systems Operational" ) {
  	$('#statusDesc').text("Alle Systeme sind betriebsbereit");
  }
  if ( k == "/lang/ja/" && status.status.description == "All Systems Operational" ) {
  	$('#statusDesc').text("");
  }
  
  var indicator = status.status.indicator;
  
  switch(indicator) {
    case "critical":
      $(".rideonIcon").attr("src","https://content-cdn.zwift.com/uploads/2018/10/ride_on_red.png");
      break;
    case "major":
      $(".rideonIcon").attr("src","https://content-cdn.zwift.com/uploads/2018/10/ride_on_orange.png");
      break;
    case "minor":
      $(".rideonIcon").attr("src","https://content-cdn.zwift.com/uploads/2018/10/ride_on_yellow.png");
      break;
    case "none":
      $(".rideonIcon").attr("src","https://content-cdn.zwift.com/uploads/2018/10/ride_on.png");
      break;
    default:
      $(".rideonIcon").attr("src","https://content-cdn.zwift.com/uploads/2018/10/ride_on.png");
  }
}

$(document).ready(function(){
	var oReq = new XMLHttpRequest();
	oReq.addEventListener("load", reqListener);
	oReq.open("GET", "https://sj50pfj5p1yv.statuspage.io/api/v2/status.json");
	oReq.send();
});
