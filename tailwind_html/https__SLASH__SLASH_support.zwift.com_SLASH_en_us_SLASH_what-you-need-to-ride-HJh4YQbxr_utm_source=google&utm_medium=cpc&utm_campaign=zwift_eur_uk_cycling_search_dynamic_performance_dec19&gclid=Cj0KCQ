var OSName="Unknown OS";
if (navigator.appVersion.indexOf("Win")!=-1) OSName="Windows";
if (navigator.appVersion.indexOf("Mac")!=-1) OSName="MacOS";
if (navigator.appVersion.indexOf("X11")!=-1) OSName="UNIX";
if (navigator.appVersion.indexOf("Linux")!=-1) OSName="Linux";

// fix the drop down on Windows Chrome 
if( OSName=="Windows" && navigator.vendor.indexOf("Google")!=-1 ) {
	var style = '<style type="text/css">' + "select option { -webkit-appearance: none; background-color: #E9EFF4; color: #000; } .consentFormDiv p .form-field, .consentFormDiv p .form-base { margin-right: 10px; }" + "</style>";
  $("head").append(style);
}

