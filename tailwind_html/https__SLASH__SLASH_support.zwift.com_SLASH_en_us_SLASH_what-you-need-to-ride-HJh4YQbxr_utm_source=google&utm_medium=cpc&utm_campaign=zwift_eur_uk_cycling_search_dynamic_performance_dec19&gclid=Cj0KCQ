    s.src = "https://cdn.kustomerapp.com/chat-web/widget.js";
    s.setAttribute(
      "data-kustomer-api-key",
      "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9..6zzGMXmArq3dym4Vdh_WavnCKwNr1NAkFab19ozXaq0"
