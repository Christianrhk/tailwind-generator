   /* Footer Columns Toggle Show/Hide */
   var columns = [].slice.call(document.querySelectorAll('.nav-section-label'));
  
  function columnClick(event) {
    var col = event.currentTarget;

    if (col.nextSibling && col.lastChild) {
      col.nextSibling.classList.toggle('d-sm-none');
      col.lastChild.classList.toggle('znv-flip');
    } 
  }
  
  columns.forEach(column => column.addEventListener('click', columnClick));

