$( document ).ready(function() {
  document.addEventListener('solvvy_support_option_selected', (e) => {
    var supportOptionType = e.detail.type; // chat|ticket|phone
    var clientId = ga.getAll()[0].get('clientId');
    analytics.track('Help Solved', {
      questionCategory: supportOptionType,
      clientId: clientId
    });
  });
