  const loadKustomer = async () => {
    const assistantId = "614b814c29a45f001ab5b285";
    try {
      const options = await optionsPromise;
      const { schedule: scheduleId } = await options.json();
      Kustomer.start({ scheduleId, assistantId });
    } catch (e) {
      console.error("Failed to start Kustomer chat", e);
    }
  };

