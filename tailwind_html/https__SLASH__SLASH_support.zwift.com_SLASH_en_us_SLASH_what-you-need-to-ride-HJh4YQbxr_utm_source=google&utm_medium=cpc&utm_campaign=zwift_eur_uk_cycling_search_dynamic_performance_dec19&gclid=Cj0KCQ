});</script><script>/*
 * Replaces time string to a local time on pages like:
 *  https://support.zwift.com/en_us/zwift-worlds-and-cycling-routes-Sy2_gxTGB
 *  https://support.zwift.com/en_us/zwift-worlds-and-running-routes-HyhC6l_wH
 *  https://zwift-sandbox.kustomer.help/en_us/preview/article/dummy-article-Hki1sn8LL (sandbox, not published)
 * requires <span id="#event-time"> element to hook into
 *
 */

const options = { hour: "numeric", minute: "numeric" };
const event = new Date();

event.setUTCHours(4);
event.setUTCMinutes(00);

const time = event.toLocaleTimeString(undefined, options);

function getTime() {
  try {
    return time + " (" + Intl.DateTimeFormat().resolvedOptions().timeZone + ")";
  } catch (e) {
    return "9:00 PM (PT)";
  }
}

function replaceTime() {
  const timeElement = document.querySelector("#event-time");
  if (timeElement) {
    timeElement.innerHTML = getTime();
  }
}

$(document).ready(function() {
  replaceTime();
});</script><script>var MAX_DESCRIPTION_LENGTH = 200;

function shorten(str, maxLen, separator) {
  if (str.length <= maxLen) return str;
  return str.substr(0, str.lastIndexOf(' ', maxLen));
}

function createRemoveItemsWithTag(tag) {
	return function(element) {
		var elements = element.getElementsByTagName(tag);
        for (var i = 0; i < elements.length; i++) {
            elements[i].parentElement.removeChild(elements[i]);
        }
  }
}

function stripHtml(html){
    // Create a new div element
    var temporalDivElement = document.createElement("div");
    // Set the HTML content with the providen
    temporalDivElement.innerHTML = html;
		console.log(temporalDivElement)
    // Retrieve the text property of the element (cross-browser support)
    return temporalDivElement.textContent || temporalDivElement.innerText || "";
}

var removeItemsWithH1 = createRemoveItemsWithTag('h1');
var removeItemsWithTitle = createRemoveItemsWithTag('title');

function sanitizeArticleBody(textHtml) {
  var temporalDivElement = document.createElement("div");
  temporalDivElement.innerHTML = textHtml;
  removeItemsWithH1(temporalDivElement)
  removeItemsWithTitle(temporalDivElement)
  var innerText = temporalDivElement.textContent || temporalDivElement.innerText || "";
  
  return shorten(innerText, MAX_DESCRIPTION_LENGTH) + '...';
  
}
window.sanitizeArticleBody = sanitizeArticleBody;</script><script></script>
