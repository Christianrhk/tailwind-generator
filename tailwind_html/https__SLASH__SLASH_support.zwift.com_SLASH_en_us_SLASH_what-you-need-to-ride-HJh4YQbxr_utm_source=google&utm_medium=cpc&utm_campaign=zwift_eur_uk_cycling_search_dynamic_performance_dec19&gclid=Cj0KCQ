	var style = '<style type="text/css">' + "@media all and (-ms-high-contrast: none), (-ms-high-contrast: active) { .main-layout.categories-page .form-control-search-wrap .icon-search.mdi.mdi-magnify{ left: 1rem !important; } .categories-page .search-wrap { margin: -1.5rem auto 0px !important; } .article-item { padding-top: 4rem; } } " + "</style>";
