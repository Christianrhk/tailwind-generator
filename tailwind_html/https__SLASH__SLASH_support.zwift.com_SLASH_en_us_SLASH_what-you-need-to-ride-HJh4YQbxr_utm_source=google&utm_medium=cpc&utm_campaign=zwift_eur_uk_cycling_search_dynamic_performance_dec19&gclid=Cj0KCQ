      var currentCat = $(categories[index])[0].innerText.toLowerCase();
      var boolCat = currentCat == hash.substring(1);
      var hashMatch = hash.substring(1) + "\n";
      
      if(currentCat == hashMatch ) {
        $('.category-item-inner.' + index).toggle();
        $("i.mdi." + index + "-button").addClass('mdi-chevron-down');
    	  $("i.mdi." + index + "-button").removeClass('mdi-chevron-up');
      }
    });
    
  }
  
  $('.mdi.0-button').click( function(e) {
  	$('.category-item-inner.0').toggle();
    
    var hasClass = $('i.mdi.0-button').hasClass('mdi-chevron-down');
    if(hasClass) {
    	$('i.mdi.0-button').addClass('mdi-chevron-up');
    	$('i.mdi.0-button').removeClass('mdi-chevron-down');
    } else {
    	$('i.mdi.0-button').addClass('mdi-chevron-down');
    	$('i.mdi.0-button').removeClass('mdi-chevron-up');
    }
  });
  
