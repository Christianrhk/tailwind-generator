                                                                                .then((response) => response.text())
                                                                                .then(async (homeTemplate) => {
                                                                                    if (!sidebar) {
                                                                                        document.getElementById("middle-wrapper").className = "without-sidebar";

                                                                                    }
                                                                                    else {
