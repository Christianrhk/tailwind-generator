<script>
    function ComponentModalFiltersOpen(referer) {
    
    let query = new URLSearchParams(window.location.search)
    query.set("m.type", "filters")
    window.history.pushState(null, null, window.location.pathname + "?" + query.toString())
    
    if (window.innerWidth < 640) {
        setTimeout(() => {
            __cmpscrollposs = window.pageYOffset
            document.querySelector('body').style['overflow'] = 'hidden'
            document.querySelector('body').style['position'] = 'fixed'
        }, 500)
    }
    
    document.querySelector('#ComponentModalFilters').classList.remove('hidden')
    document.querySelector('#ComponentModalFilters').classList.add('opacity-0')
    setTimeout(() => {
        document.querySelector('#ComponentModalFilters').classList.remove('opacity-0')
        document.querySelector('#ComponentModalFilters').classList.add('opacity-100')
        document.querySelector('#ComponentModalFilters').classList.add('block')
    }, 0)
    
    Action(referer, `ComponentModalFilters:Open`, window.location.pathname)
}

function ComponentModalFiltersClose(referer) {
    if ((document.getElementById('ComponentModalPopup') && !document.getElementById('ComponentModalPopup').classList.contains("hidden"))) {
        return
    }
    
    let query = new URLSearchParams(window.location.search)
    query.delete("m.type")
    let suffix = query.toString() ? "?" + query.toString() : ""
    window.history.replaceState(null, null, window.location.pathname + suffix)
    
    if (window.innerWidth < 640) {
        document.querySelector('body').style['overflow'] = 'scroll'
        document.querySelector('body').style['position'] = 'static'
        window.scrollTo(0, __cmpscrollposs)
    }
    
    document.querySelector('#ComponentModalFilters').classList.add('opacity-0')
    
    setTimeout(() => {
        Action(referer, `ComponentModalFilters:Close`)
    }, 300)
    
}
</script>
