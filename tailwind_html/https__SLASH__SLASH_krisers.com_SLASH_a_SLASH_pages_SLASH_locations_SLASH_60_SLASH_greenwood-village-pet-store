    function initMap() {

        var bounds = new google.maps.LatLngBounds();

        var map = new google.maps.Map(document.getElementById('map'), { maxZoom: 16});

                var marker_60 = new google.maps.Marker({
                    position: {lat: 39.6260503, lng: -104.8846284},
                    map: map
                });
        bounds.extend(marker_60.position);
        
        map.fitBounds(bounds);
    }
</script>
