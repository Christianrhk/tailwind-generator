/* Rellena la variable global userConsent con el valor tcString para que luego el player de Brightcove la recupere y la pueda enviar al adserver */
        var userConsentCalled = false;
        var referrer = encodeURIComponent(window.location.href);
        var userConsent = "";
        var contador = 0;
        var intervaloConsentimiento = setInterval(function () {
            contador += 1;
            console.log(contador);
            if (contador === 600)
                clearInterval(intervaloConsentimiento);
            if (typeof window.__tcfapi !== "undefined") {
              console.log("DETECTADA __tcfapi"); 
                clearInterval(intervaloConsentimiento);
                window.__tcfapi("addEventListener", 2, function (tcData, isOk) {
                    if (isOk) {
                        console.log("TCFAPI IS LOADED OK");
                        window.__tcfapi("getTCData", 2, function(data,success){
                            userConsent=data.tcString; 
                            console.log("USERCONSENT: "+userConsent);
                            console.log("REFERRER: "+referrer);
                        });
                    }
                });
            }
            contador++;
        }, 10);
</script>

