     <h2 class="text-center my-3">FIT & SIZING</h2>
     <style>
    #product-size-chart table {
        width: 60%;
        margin: auto;
    }

    @media screen and (max-width: 769px) {
        #product-size-chart table {
            width: 100%;
        }
    }
</style>

<div id="product-size-chart" class="overflow-hidden mx-3 m-auto">
    

    
        
            
        
            
        
            
        
            
        
            
        
            
        
            
        
            
        
            
        
            
        
            
                
