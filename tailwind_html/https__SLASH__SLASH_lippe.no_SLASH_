/* 
*
* Modal
*
*/

.modal-container {
	position: fixed;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	background: #00000066;
	z-index: 10;
	display: flex;
	align-items: center;
	justify-content: center;
}

#hubspot-messages-iframe-container {
	z-index: 9 !important;
}

.modal {
	position: relative;
	display: flex;
	align-items: center;
	justify-content: center;
	flex-direction: column;
	background: white;
	width: 640px;
	border-radius: var(--border-radius);
	padding: 1em;
	transition: none;
	overflow: hidden;
	max-height: 90vh;
		
}

.modal__close {
	font-size: 1.5em;
	color: #666666;
	cursor: pointer;
	position: absolute;
	top: 0.5em;
	right: 1.5em;
	line-height: 1em;
}

.modal__content {
	padding: 1em;
	line-height: 2.5;
	display: flex;
	flex-wrap: wrap;
	flex-direction: row;
	word-break: break-word;
	justify-content: center;
	align-items: center;
	overflow-x: visible;
	overflow-y: auto;
	height: 100%;
	margin-top: auto;
}

.modal__content--center {
	text-align: center;
}

.modal__content img {
	height: 15em;
	margin: 0 auto;
}

.modal__content strong:before {
	content: " ";
	display: block;
}

.modal__content b:before {
	content: " ";
	display: block;
}

.modal__content h1 {
	color: #27292e;
	line-height: 1.5em;
	text-align: center;
}

.modal__buttons {
	display: flex;
	flex-wrap: wrap;
	align-items: center;
	justify-content: center;
	width: 100%;
	margin-top: auto;
	padding-top: 1em;
}

.modal__buttons .button {
	flex: 1 0 auto;
	margin: 0.25em;
}

/* 
*
* Animations
*
*/

@keyframes bounce-in {
	0% {
		transform: scale(0);
	}
	50% {
		transform: scale(1.05);
	}
	100% {
		transform: scale(1);
	}
}

@keyframes bounce-in-translated {
	0% {
		transform: translate(-50%, -50%) scale(0);
	}
	50% {
		transform: translate(-50%, -50%) scale(1.05);
	}
	100% {
		transform: translate(-50%, -50%) scale(1);
	}
}

.bounce-enter-active {
	animation: bounce-in 0.6s;
}

.bounce-leave-active {
	animation: bounce-in 0.6s reverse;
}

.bounce-fast-enter-active {
	animation: bounce-in 0.4s;
}

.bounce-fast-leave-active {
	animation: bounce-in 0.4s reverse;
}

.bounce-translated-enter-active {
	animation: bounce-in-translated 0.6s;
}

.bounce-translated-leave-active {
	animation: bounce-in-translated 0.6s reverse;
}

.fade-enter-active,
.fade-leave-active {
	transition: opacity 0.5s;
}

.fade-enter,
.fade-leave-to {
	opacity: 0;
}

.slide-fade-enter-active {
	transition: all 0.3s ease;
}
.slide-fade-leave-active {
	transition: all 0.3s cubic-bezier(1, 0.5, 0.8, 1);
}
.slide-fade-enter {
	transform: translateX(20px);
	opacity: 0;
}
.slide-fade-leave-to {
	transform: translateX(-20px);
	opacity: 0;
}

