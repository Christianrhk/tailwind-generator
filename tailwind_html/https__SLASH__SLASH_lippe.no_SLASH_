}

button.menu-toggle {
	border: none;
	margin: 0;
	padding: 0;
}

button.menu-toggle::before,
button.menu-toggle::after,
button.menu-toggle span::before {
	content: "";
	display: block;
	height: 2px;
	width: 18px;
	position: inherit;
	left: 0;
	margin: 0;
	transition: all, ease, 0.2s;
	border-radius: 3px;
}

.button,
button,
input[type="submit"],
.button.alt,
.wp-block-button__link, .woocommerce-breadcrumb a  {
	transition: all 0.2s ease;
