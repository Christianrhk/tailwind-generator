  .toast {
    position: fixed;
    z-index: 100;
    top: 90px;
    left: 50%;
    transform: translate(-50%, -50%);
    opacity: 0;
  }

  .toast-show {
    transform: translate(-50%, -50%);
    opacity: 0;
    animation: toast-appear 0.5s both;
    animation-direction: normal;
  }

  .toast-hide {
    transform: translate(-50%, 0);
    opacity: 1;
    animation: toast-disappear 0.5s both;
  }

  .toast[data-style=success] {
    background-color: #23cc6B;
  }

  .toast[data-style=error] {
    background-color: #eb5760;
  }

  .toast[data-style=info] {
    background-color: #def141;
  }

  @keyframes toast-appear {
    from {
      transform: translate(-50%, -50%);
      opacity: 0;
    }

    to {
      transform: translate(-50%, 0);
      opacity: 1;
    }
  }

  @keyframes toast-disappear {
    from {
      transform: translate(-50%, 0);
      opacity: 1;
    }

    to {
      transform: translate(-50%, -50%);
      opacity: 0;
    }
  }
</style>

<div
  data-controller="toast"
  data-toast-target="toast"
  class="toast flex items-center justify-center p-2 gap-3 text-black"
  data-action="showtoast@window->toast#showToast"
>
  <div data-toast-target="content" class="uppercase font-semibold text-sm tracking-wide"></div>

  <!-- dismiss button -->
  <span class="cursor-pointer" data-action="click->toast#hideToast">
    <svg width="16" height="16" viewBox="0 0 16 16" class="bi bi-x" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
      <path fill-rule="evenodd" d="M11.854 4.146a.5.5 0 0 1 0 .708l-7 7a.5.5 0 0 1-.708-.708l7-7a.5.5 0 0 1 .708 0z"/>
      <path fill-rule="evenodd" d="M4.146 4.146a.5.5 0 0 0 0 .708l7 7a.5.5 0 0 0 .708-.708l-7-7a.5.5 0 0 0-.708 0z"/>
    </svg>
  </span>
</div>

