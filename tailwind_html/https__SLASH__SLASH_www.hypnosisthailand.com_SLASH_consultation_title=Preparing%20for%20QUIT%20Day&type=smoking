<script type="text/javascript">
var revsense_cart = {};

function revsense_decode(html) {
  var txt = document.createElement("textarea");
  txt.innerHTML = html;
  return txt.value;
}

document.addEventListener('DOMContentLoaded', function() {  
  // Translations
  if (typeof revsense_translations !== 'undefined') {
    console.log("Translations available");
    var rev_keys = document.getElementsByClassName("tr");
    if (rev_keys && rev_keys.length > 0) {
      var rev_num_keys = rev_keys.length;
      for (var i = 0; i < rev_num_keys; i++) {
        var rev_key = rev_keys[i].innerHTML.trim();
        rev_key = revsense_decode(rev_key);
        rev_key = rev_key.replace("\r", "");
        rev_key = rev_key.trim();
        if (revsense_translations[rev_key] !== undefined) {
          rev_keys[i].innerHTML = revsense_translations[rev_key];
        } else {
          console.log('"' + rev_key + '"')
          console.log('==>"' + revsense_translations[rev_key] + '"');
        }
      }
    }
  }


  // Humanity
  var rev_forms = document.forms;
  if (rev_forms.length > 0) {
    // Get humanity token
    var xhttp = new XMLHttpRequest()
    xhttp.open("GET", "/api/humanity/v1")
    xhttp.onreadystatechange = function() {
      if (xhttp.readyState !== 4) return;
      if (xhttp.status === 200) {
        var rev_humanity = xhttp.responseText;

        // Throw in forms
        for (i = 0; i < rev_forms.length; i++) {
          var rev_input = document.createElement("input");
          rev_input.setAttribute("type", "hidden");
          rev_input.setAttribute("name", "humanity");
          rev_input.setAttribute("value", rev_humanity);
          rev_forms[i].appendChild(rev_input);
        }
      }
    }
    xhttp.send()
  }

  // Shopping cart -- check if cart container is available and update it.
  function revsense_sc_display_cart(cc, num) {
    if (typeof window.revsense_cart_markup !== 'undefined') {
      cc.innerHTML = window.revsense_cart_markup.replace("##CARTNUM##", num);
    } else {
      cc.innerHTML = "<a href=\"/cart\">&#x1f6d2;&nbsp;" + num + '</a>';
    }
  }

  // Setup the listener
  function revsense_sc_listen() {
    document.addEventListener('click', ev => {
      //var class_name = ev.target.getAttribute("class") || "";
      var target = ev.currentTarget.activeElement;
      var class_name = target.getAttribute('class');
      
      if (class_name.match("cart-add")) {
        // Add something to cart
        ev.preventDefault();        
        var post_id = target.getAttribute('data-id');
        if (post_id) { revsense_sc_add(post_id, 1); }
      } else if (class_name.match("cart-delete")) {
        // Remove something from the cart
        ev.preventDefault(); 
        var post_id = target.getAttribute('data-id');
        if (post_id) { revsense_sc_remove(post_id); }
        location.reload();
      } else if (class_name.match("cart-dec")) {
        // Decrement the count
        ev.preventDefault(); 
        var post_id = target.getAttribute('data-id');
        revsense_sc_dec(post_id);
        location.reload();
      } else if (class_name.match("cart-inc")) {
        // Increment the count
        ev.preventDefault(); 
        var post_id = target.getAttribute('data-id');
        revsense_sc_add(post_id, 1);
        location.reload();
      }
    });   
  }

  // Shopping cart -- load cart info
  function revsense_sc_update() {
    var cc = document.getElementById('revsense_cart');
    if (cc) {
      console.log("-- Revsense Cart 1.0 --")
      var raw_cart = _revs_cook.get('__revs_c');
      if (raw_cart && raw_cart.length > 0) {     
        revsense_cart = JSON.parse(raw_cart);
        revsense_sc_display_cart(cc, Object.keys(revsense_cart).length);
      } else {
        revsense_sc_display_cart(cc, 0);
      }
    } else {
      console.log("-- NO CART --");
    }
  }

  // Shopping cart -- add something to the cart (and increment it's count)
  function revsense_sc_add(post_id, quantity) {
    if (revsense_cart['_' + post_id]) {
      revsense_cart['_' + post_id] += quantity;
    } else {
      revsense_cart['_' + post_id] = quantity;
    }
    _revs_cook.set('__revs_c', JSON.stringify(revsense_cart), 30);
    revsense_sc_update();
  }

  // Shopping cart -- maybe remove something from the cart
  function revsense_sc_remove(post_id) {
    delete revsense_cart['_' + post_id];
    _revs_cook.set('__revs_c', JSON.stringify(revsense_cart), 30);
    revsense_sc_update();
  }

  // Shopping cart -- decrease number of something in the cart
  function revsense_sc_dec(post_id) {
    if (revsense_cart['_' + post_id] == 1) {
      delete revsense_cart['_' + post_id];
    } else if (revsense_cart['_' + post_id] > 1) {
      revsense_cart['_' + post_id] -= 1;
    }
    _revs_cook.set('__revs_c', JSON.stringify(revsense_cart), 30);
    revsense_sc_update();    
  }

  // Empty the cart completely
  function revsense_sc_empty() {
    revsense_cart = {};
  }

  // Start cart operations
  revsense_sc_update();
  revsense_sc_listen()

}, false);

</script>

</body>
