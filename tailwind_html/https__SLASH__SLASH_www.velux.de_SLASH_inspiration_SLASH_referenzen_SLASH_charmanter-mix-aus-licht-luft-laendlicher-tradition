cument.getElementById("InstallerChatClose"); var bubbleIntro=bubble.getElementsByClassName("installer-bubble-intro")[0]; var bubbleContent=bubble.getElementsByClassName("installer-bubble-conversation")[0]; var bubbleText=bubbleContent.getElementsByClassName("installer-bubble-text"); var trackEvent=function (eventAction, eventLabel){eventObject={event: "interactionevent", eventcategory: "installer assistant", eventaction: eventAction, eventlabel: eventLabel ? eventLabel : ""}; try{window.dataLayer=window.dataLayer || []; window.dataLayer.push(eventObject);}catch (error){console.warn(error);}}; var openChat=function (){if (isOpen){return;}isOpen=true; chatbot.classList.add("installer-bubble-open", "installer-bubble-loading"); setTimeout(function (){bubbleText[0].classList.add("installer-bubble-text__show"); chatbot.getElementsByClassName("typing-indicator")[0].style.display="none"; bubbleContent.style.display="grid"; chatbot.classList.remove("installer-bubble-loading"); opened=true;}, opened ? 0 : 2000);}; var closeChat=function (event){isOpen=false; chatbot.classList.remove("installer-bubble-open"); avatar.style.animation="none"; trackEvent("Close chat", locationPath); event.stopPropagation();}; var clickBubble=function (){trackEvent("Click", locationPath); openChat();}; var clickAvatar=function (){if (isOpen){return;}trackEvent("Click", locationPath); openChat();}; var clickQuestion=function (index){trackEvent(installerTexts.installerQuestions[index].text, locationPath);}; bubbleIntro.addEventListener("click", clickBubble); avatar.addEventListener("click", clickAvatar); closeChatbot.addEventListener("click", closeChat); (function (){var questionElement=bubble.getElementsByClassName("installer-bubble-question"); var questionContent=installerTexts.installerQuestions; bubble.getElementsByClassName("installer-bubble-intro")[0].innerHTML=installerTexts.findMe; bubble.getElementsByClassName("installer-bubble-text")[0].innerHTML=installerTexts.installerIntro; for (var i=0; i < questionContent.length; i++){var question=document.createElement("a"); question.innerHTML=questionContent[i].text; question.href=questionContent[i].url; question.classList.add("installer-bubble-question"); bubbleContent.append(question);}var bubbleQuestion=bubbleContent.getElementsByClassName("installer-bubble-question"); for (var i=0; i < bubbleQuestion.length; i++){bubbleQuestion[i].addEventListener("click", function (event){var index=Array.prototype.indexOf.call(bubbleQuestion, event.target); clickQuestion(index);});}})();}</script><style>#InstallerChatbot{position: fixed; bottom: 16px; right: 16px; display: flex; z-index: 999; align-items: flex-end;}#InstallerChatbot #InstallerChatBubble{background: white; max-width: 66vw; line-height: 24px; font-weight: 400; box-shadow: 0px 8px 32px rgba(0, 0, 0, 0.32); animation: bounce-in-bubble 0s ease; right: -60px; transition: all 350ms cubic-bezier(0.4, 0, 0.2, 1); max-height: 90vh;}#InstallerChatbot.installer-bubble-open #InstallerChatBubble{background: #f5f5f5; max-width: initial; width: 91vw; padding: 20px; right: 0;}#InstallerChatbot #InstallerChatBubble.installer-chat-open{cursor: default;}#InstallerChatbot .installer-bubble-intro{font-size: 16px; padding: 10px 16px;}#InstallerChatbot.installer-bubble-open .installer-bubble-intro{display: none; padding: 0;}#InstallerChatbot .installer-bubble-text{margin-bottom: 0.5em; transition: all 350ms cubic-bezier(0.4, 0, 0.2, 1); justify-self: end; width: 92%;}#InstallerChatbot .installer-bubble-text.installer-bubble-text__show{display: block;}#InstallerChatbot .installer-bubble-content{justify-items: start; gap: 20px; font-size: 14px; display: none;}#InstallerChatbot.installer-bubble-open .installer-bubble-content{display: grid; transition: all 350s ease; max-height: 70vh; overflow-y: scroll;}#InstallerChatbot .installer-bubble-content p+p{margin-top: 1em;}#InstallerChatbot .installer-bubble-shape{background: white; border-radius: 24px 24px 0 24px; box-shadow: 0px 8px 16p