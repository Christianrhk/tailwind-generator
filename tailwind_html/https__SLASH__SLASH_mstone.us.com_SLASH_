(function($){
  $('body').ready(function(){
    var $window=$(window);
    var $body=$("body");
    var $menuBtn=$(".cMenuBtn");
    var $menu=$(".cBigMenu");
    var $bigMenuClose=$(".cBigMenuClose");
    
    
    if($window.width()>980){
      $menuBtn.on("hover,click", function(){
          $menu.show();
          $body.addClass("cFixedOverflow");
      });
    }else{
      $menuBtn.on("click", function(){
          $menu.show();
          $body.addClass("cFixedOverflow");
      });       
    }
    $bigMenuClose.click(function(){
        $menu.hide();
        $body.removeClass("cFixedOverflow");
    });
  
  });
})(jQuery);</script>
