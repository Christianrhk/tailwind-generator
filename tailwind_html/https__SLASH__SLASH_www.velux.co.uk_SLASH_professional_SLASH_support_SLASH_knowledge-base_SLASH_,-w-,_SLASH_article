<script type="text/javascript">
    var xhttp = new XMLHttpRequest();
    var confElement = document.getElementById('confMsg');
    var ratingElement = document.getElementById('ratingQuestion');

    function submitRating(rating) {

        xhttp.open("GET", "/api/sitecore/Faq/SubmitRating?language=en-GB&settingsPath=/sitecore/content/Marketing/UK/Settings/EgainFaqSettings&rating=" + rating + "&articleId=" + , true);
        xhttp.send();
        if (confElement.style.display === "none")
        {
            confElement.style.display = "inline-flex";
            ratingElement.style.display = "none";
        }

        // Tracking rating answer
        var eGainRatingEvent = {
            event: "interactionevent",
            eventcategory: "eGain FAQ",
            eventaction: "ratings",
            eventlabel: rating == 1 ? "yes" : "no"
        }

        if (dataLayer) dataLayer.push(eGainRatingEvent);
    }

    // TRACKING
    (function () {
        // Related articles
        var relatedArticles = document.querySelectorAll(".faq__topics_popular_element.-related");

        if (relatedArticles && dataLayer) {
            relatedArticles = [].slice.call(relatedArticles);

            relatedArticles.forEach(function (article) {
                article.addEventListener('click', function () {
                    var name = article.querySelector('p');

                    var trackEvent = {
                        event: "interactionevent",
                        eventcategory: "eGain FAQ",
                        eventaction: "related article - click",
                        eventlabel: name ? name.innerText : ""
                    }

                    dataLayer.push(trackEvent);
                })
            });
        }
    })();
</script>
