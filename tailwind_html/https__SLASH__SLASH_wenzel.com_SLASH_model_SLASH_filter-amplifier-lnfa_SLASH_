																								<div class="model-image" x-init=" window.setInterval(() => { if (!manual) activeSlide = activeSlide === slides.length -1 ? 0 : activeSlide + 1}, 7000)" x-data="{ activeSlide: 0, manual:false, slides: [{'idx':0,'img':'https://wenzel.com/wp-content/uploads/LNFA1-300x300.png'}]}">
					<!-- Slides -->
					<template x-for="slide in slides" :key="slide.idx">
