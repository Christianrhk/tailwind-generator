      const handleActive = () => {
        const { hash } = location;
        if ( hash ) {
          document.querySelectorAll( 'a[href^="#"]' ).forEach( link => {
            link.classList.toggle( 'active', location.hash === link.getAttribute( 'href' ) )
          })
        }
      }
      handleActive()
      window.addEventListener( 'hashchange', handleActive )

      
      
      $(document).ready(function(){
        let collection_image_link;
        let image_link = "https://cdn.shopify.com/s/files/1/0567/2409/5029/files/menu.png?v=1656404450";
        $(".collection_hover_link").mouseenter(function(){
          let collection_image_link;
          if($(this).data("img") != ""){
            collection_image_link = $(this).data("img");
          }else{
            collection_image_link = image_link;
          }
          
          console.log(collection_image_link);
          $("#collection_hover_img").attr("src", collection_image_link);
        });
        
        $(".collection_hover_link").mouseleave(function(){
          $("#collection_hover_img").attr("src", image_link);
        });
      });
    </script>























