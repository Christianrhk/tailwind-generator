        <script>
        $(function () {
            $.ajax({
                type: 'GET',
                url: '/course/getcoursefortracking?id=b562c3dc-43cb-42a4-9232-8e059d5e5d5a',
                success: function (data) {
                    if (data.status) {
                        if (data.facebookpixel) {
                            fbq('track', 'ViewContent', {
                                value: data.price,
                                currency: data.currency,
                                content_type: 'product',
                                content_ids: data.courseid
                            });
                        }
                        if (data.googleanalytics) {
                            gtag('event', 'view_item', {
                                'items': [
                                    {
                                        'id': data.courseid,
                                        'name': data.coursename,
                                        'price': data.price
                                    }
                                ]
                            });
                        }
                    }
                }
            });
        });
        </script>
