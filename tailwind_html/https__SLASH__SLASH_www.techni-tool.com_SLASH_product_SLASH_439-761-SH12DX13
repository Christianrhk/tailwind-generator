<script>
    /*Toggle dropdown list*/
    /*https://gist.github.com/slavapas/593e8e50cf4cc16ac972afcbad4f70c8*/

    var navMenuDiv = document.getElementById("nav-content");
    var navMenu = document.getElementById("nav-toggle");
    var helpMenu = document.getElementById("menu-toggle");
    var innerDiv = document.getElementById("innerDiv2");
   
   
    document.onclick = check;
    function check(e) {
     
        var target = (e && e.target) || (event && event.srcElement);
        locationCheck();      
        if (!checkParent(target, navMenuDiv)) {
            if (navMenuDiv.classList.contains("hidden-custom")) {
                navMenuDiv.classList.remove("hidden-custom");
            }         
        }
        
        //Help Menu
        if (window.location.pathname === "/") {
            if (document.getElementById("btnAllProduct") != null) {
                document.getElementById("btnAllProduct").setAttribute("disabled", "disabled");
            }
           
            if (innerDiv != null) {
                if (!innerDiv.classList.contains("lg:w-4/5")) {
                    innerDiv.classList.add("lg:w-4/5","pl-4","pt-0","pr-0");
                }               
            }
                      
        }
        else {
            if (document.getElementById("btnAllProduct") != null) {
                document.getElementById("btnAllProduct").removeAttribute("disabled");
            }
           
            if (innerDiv != null) {
                if (innerDiv.classList.contains("lg:w-4/5")) {
                    innerDiv.classList.remove("lg:w-4/5", "pl-4", "pt-0", "pr-0");
                }
            }
             
        }
        
    }

    function checkParent(t, elm) {
        while (t.parentNode) {
            if (t == elm) {
                return true;
            }
            t = t.parentNode;
        }
        return false;
    }

    function locationCheck() {
        var svgArrow = document.getElementById("svgArrow");       
        if (window.location.pathname === "/") {
            if (svgArrow != null) {
                svgArrow.classList.add("hide");
            }  
        }
        else {
            if (svgArrow != null) {
                svgArrow.classList.remove("hide");
            } 
            
        }
    }
</script>



<script>
    function setAutoCompletePosition() {
        var searchForm = document.getElementById("search-form");
        var svgArrow = document.getElementById("svgArrow");
        if (!searchForm) return;
        var searchFormWidth = searchForm.offsetWidth;
        var rect = searchForm.getBoundingClientRect();
        var searchButtonWidth = document.getElementById("search-button").offsetWidth;
        var searchInputWidth = searchFormWidth - searchButtonWidth;

        var kendoListContainers = document.getElementsByClassName("k-list-container");
        if (kendoListContainers.length > 0) {
            kendoListContainers[0].style.width = searchInputWidth.toString() + "px";
            kendoListContainers[0].style.top = rect.height.toString() + "px";
        }
        if (window.innerWidth > 768) {
            var navMenuDiv = document.getElementById("nav-content");
            if (!navMenuDiv.classList.contains("hidden-custom")) {
            navMenuDiv.classList.add("hidden-custom");
            } 
        }
        
        locationCheck();
    }

    window.addEventListener('resize', setAutoCompletePosition);
    window.addEventListener('load', setAutoCompletePosition);
    window.addEventListener("click", function (event) {
        if (event === undefined) event = window.event;
        var target = 'target' in event ? event.target : event.srcElement;       
        if (window.location.pathname != "/") {
            if (target.tagName === "DIV" || target.tagName === "UL") {
                var div = document.getElementById("menuDropdownDiv");
                if (window.innerWidth > 1024 && div != null) {
                    document.getElementById("btnAllProduct").click();
                }
            }
        }
    });
</script>

