    <script>

        function closeToggleDiv(e) {
            var navtoggleMenuDiv = document.getElementsByClassName("navtoggleMenuDiv")[0];
            if (navtoggleMenuDiv != null) {  
                if (window.innerWidth < 1024) {
                    navtoggleMenuDiv.click();
                }
               
            }
        }
    </script>





