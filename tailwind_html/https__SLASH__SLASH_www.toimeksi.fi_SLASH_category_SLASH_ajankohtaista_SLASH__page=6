<script>
	/**
	 * Sivustohaku "appi"
	 * Näyttää hakutuloksia heti haun valmistuessa.
	 * 
	 * Ajax kutsuu kyselyn basicsearch.php:ssa
	 */

    function basicSearch() {
        return { 
            postTypes: [],
            posts: null,
			keywords: null,
			checkedCount: 0,
			showResults: false,
			results: 'Missing post types',
			isOpen: false,
			width: window.innerWidth,
			formSubmitError: false,
			closeErrorMessage(ms) {
				setTimeout( () => {
					this.formSubmitError = false;
				}, ms)
			},
			tryFormSubmit(event){
				if(!this.keywords || this.keywords.length < 1){
					this.formSubmitError = true;
					this.closeErrorMessage(3500);
				}
				else{
					this.$refs.basicSearchForm.submit();
				}
			},
			setWidth (width) {
				this.width = width
			},
            getResults() {
				if(this.checkedCount < 1){
					return this.results = 'Missing post types'
				}

                var data = new FormData(this.$refs.basicSearchForm);
                data.append('action', 'basicSearch');
                fetch(ajax_url,{
                    method: 'post',
                    body: data,
                })
                .then(response => response.json())
                .then(json => {
					this.results = json;
                });
			},
        }
    }
</script>
		
