<script>

$(document).ready(function(){
  $('.header-tab__domestic').find('.header-tab__link').addClass('header-tab__link--active');
  $('.btn-calender').click(function() {
    $( "#tabs" ).tabs( "option", "active", 0 );
    return false;
    var speed = 500;
    //リンク元を取得
    var href= $(this).attr("href");
    //リンク先を取得
    var target = $(href == "#" || href == "" ? 'html' : href);
    //リンク先までの距離を取得
    var position = target.offset().top;
    //スムーススクロール
    $("html, body").animate({scrollTop:position}, speed, "swing");
    return false;
  });
  $( "#tabs" ).tabs();
  $('.tour-slider').slick({
    slidesToShow: 1,
    slidesToScroll: 1,
    arrows: true,
   	dots:true,
    autoplay: true,
    autoplaySpeed: 8000,
    speed: 1000,
  });

});
  

 //console.log(tourDatas);
 $('#calendar').clndr({
  showAdjacentMonths: false,
  template: clndrTemplateTravel,
  daysOfTheWeek:['日', '月', '火', '水', '木', '金', '土'],
  events: tourDatas
});


</script>

