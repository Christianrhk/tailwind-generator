<script language="javascript">      Chart.defaults.font.family = "Prompt";
      let ctx = document.getElementById("chart1").getContext("2d");
      let chart1;

      function appAllDayDailyIRComponent() {
        return {
          isCreateChart: false,
          modelSelectStation: "",
          selectedStation: {},
          listStation: [],
          async init() {
            await this.getStation();
            this.getAllDayDailyIR();
            setInterval(() => {
              this.getAllDayDailyIR();
            }, 60000);
          },
          async setStation() {
            const data = await getData();

            const result = data.filter(
              (element) => element.station_id == this.modelSelectStation
            );
            this.selectedStation = result[0];
            this.getAllDayDailyIR();
          },
          async getStation() {
            const data = await getData();
            let listStation = [];
            data.forEach((element) => {
              const item = {
                id: element.station_id,
                name: element.station_name,
              };
              listStation.push(item);
            });
            this.listStation = [...listStation];
            this.modelSelectStation = listStation[0].id;
            this.setStation();
          },
          getAllDayDailyIR() {
            const station = this.selectedStation.station_code;
            
           

            const url = 'http://103.95.228.113:9901/v1.1/IRR-DEDE/storages/IRR-DEDE.dede_' + station + '/objects?api_key=MTkzZWIzYmYtNzQ5NC00OWNiLWJkNWItNTA5ZTljMjdjYmIw&last=4000';
        
            let labels = [];
            let irs = [];

            axios
              .get(url)
              .then((result) => {
                if (result.data.status.code == 200) {
                  let data = result.data.data;
                  if (data != null) {
                    data.forEach((element) => {
                      // หาวันที่ปัจจุบัน

                      const currentDate = new Date();
                      const currentDay = currentDate.getDate();

                      ts = new Date(element["data"]["sensors"]["ts"]);

                      const itemDate = ts.getDate();
                      const itemHour = ts.getHours();

                      if (currentDay == itemDate && itemHour >= 5) {
                        ir = parseFloat(element["data"]["sensors"]["ir"]);

                        if (isNaN(ir)) {
                          ir = 0;
                        }
                        labels.push(ts);
                        irs.push(ir);
                      }
                    });

                    if (!this.isCreateChart) {
                      const config = {
                        type: "line",
                        data: {
                          labels: labels,
                          datasets: [
                            {
                              data: irs,
                              fill: false,
                              borderColor: "rgb(75, 192, 192)",
                              tension: 0.1,
                              pointRadius: 0,
                            },
                          ],
                        },
                        options: {
                          responsive: true,
                          scales: {
                            x: {
                              type: "time",
                              time: {
                                unit: "hour",
                                stepSize: 0.5,
                                displayFormats: {
                                  hour: "HH:mm",
                                },
                              },
                              title: {
                                display: true,
                                color: "#a33000",
                                text: "เวลา (hrs)",
                                padding: {
                                  top: 20,
                                  left: 0,
                                  right: 0,
                                  bottom: 0,
                                },
                              },
                            },
                            y: {
                              min: 0,
                              display: true,

                              title: {
                                display: true,
                                color: "#a33000",
                                text: "วัตต์ / ตร.ม.",
                                padding: {
                                  top: 30,
                                  left: 0,
                                  right: 0,
                                  bottom: 0,
                                },
                              },
                            },
                          },
                          plugins: {
                            legend: {
                              display: false,
                              position: "top",
                            },
                          },
                        },
                      };
                      this.isCreateChart = true;
                      chart1 = new Chart(ctx, config);
                    } else {
                      chart1.data.labels = [...labels];
                      chart1.data.datasets[0].data = [...irs];
                      chart1.update();
                    }
                  }
                }
              })
              .catch((e) => {
                console.log(e);
              });
          },
        };
      }

      flatpickr("#startDate", {
        enableTime: false,
        disableMobile: true,
        dateFormat: "d-m-Y",
        locale: "th",
      });
      flatpickr("#endDate", {
        enableTime: false,
        disableMobile: true,
        minDate: "today",
        maxDate: "today",
        dateFormat: "d-m-Y",
        locale: "th",
      });
      
      
      
    axios
      .get(
        "http://www.irradiancedata.dede.go.th:8888/geoserver/dede/ows?service=WFS&version=1.0.0&request=GetFeature&typeName=dede%3Aagg_01_bkk&outputFormat=text%2Fxml%3B%20subtype%3Dgml%2F2.1.2&CQL_FILTER=%22ds%22%20between%20%272021-12-26%27%20AND%20%272021-12-27%27",
        {
          headers: {
            "Access-Control-Allow-Origin": "*",
            "Access-Control-Allow-Methods": "GET,PUT,POST,DELETE,PATCH,OPTIONS",
          },
        }
      )
      .then(function (response) {
        // handle success
  
      })
      .catch(function (error) {
        // handle error
        console.log(error);
      })
      .then(function () {
        // always executed
      });</script></body>
</html>