<script language="javascript" type="text/javascript" src="https://unpkg.com/dayjs@1.8.21/dayjs.min.js"></script>
<script language="javascript" type="text/javascript" src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>
<script language="javascript" type="text/javascript" src="/js/appDropdownComponent.js"></script>
