<script>
    
    const openCalculate = document.getElementById('open-calculator');
    const closeCalculate = document.getElementById('calculator-top');
    const mobile = document.getElementById('mobile-calculator');

    if(mobile)
    mobile.addEventListener('click', function(){
        document.getElementById('calc').classList.toggle('hidden')
    });

    if(openCalculate)
    openCalculate.addEventListener('click', function(){
        document.getElementById('calculator-top').classList.toggle('lg:hidden')
    })

    if(closeCalculate)
    document.getElementById('close-calculator').addEventListener('click', function(){
        document.getElementById('calculator-top').classList.toggle('lg:hidden')
    })
    
</script>

	

