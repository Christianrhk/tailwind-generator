                                                            visiblecategories[j]["children"][k]["slug"] = themeCategorySlug;
                                                        }
                                                    }
                                                }


                                                console.log("visible-categories:", visiblecategories);

                                                await fetch("templates/header.html?v=" + BLOG_VER)
