                                            .then((response) => response.json())
                                            .then(async (visiblecategories) => {
                                                visiblecategories = visiblecategories;
                                                for (var j = 0; j < visiblecategories.length; j++) {
                                                    categories_slug = visiblecategories[j]["slug"];
