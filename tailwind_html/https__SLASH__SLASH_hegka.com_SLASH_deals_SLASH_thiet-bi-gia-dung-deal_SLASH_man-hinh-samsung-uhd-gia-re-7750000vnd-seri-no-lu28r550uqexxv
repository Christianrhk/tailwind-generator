    $("#editDraftArticle").on("hidden.bs.modal", function () {
        checkEditorChange = 0;
        Livewire.emitTo('user.articles.draft.edit', 'cancel')
        $('#editDraftArticle .image-edit-article-delete').addClass('d-none')
        $('#editDraftArticle .image-edit-article-change').removeClass('d-none')
        $('#editDraftArticle .image-input-wrapper-preview').css('background-image', '')
        editorDraftArticleEdit.setData('')
        $('#editDraftArticle .form-select').val(null).trigger('change');
        $('#editDraftArticle').remove('full-screen');
        $('#editDraftArticle .ck.ck-toolbar__items .editor-zoom-in').remove();
        $('#editDraftArticle .ck.ck-toolbar__items .editor-zoom-out').remove();
        $('#editDraftArticle .ck.ck-toolbar__items').append('<button class="ck ck-button ck-off editor-zoom-out" type="button" tabindex="-1">' +
            '<!--begin::Svg Icon | path: assets/media/icons/duotone/Design/ZoomMinus.svg-->\n' +
