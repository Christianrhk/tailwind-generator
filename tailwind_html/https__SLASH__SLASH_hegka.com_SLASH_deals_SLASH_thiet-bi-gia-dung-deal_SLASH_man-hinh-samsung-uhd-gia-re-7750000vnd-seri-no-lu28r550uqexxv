        $('#author-editor #nav-article .form-select').val(null).trigger('change');
        $('#author-editor #form-create-post-article .image-input-wrapper').css('background-image','');
        $('#author-editor #nav-article #form-create-post-article #article-text-title').val('');
        $('#author-editor').remove('full-screen');
        $('#author-editor .ck.ck-toolbar__items .editor-zoom-in').remove();
        $('#author-editor .ck.ck-toolbar__items .editor-zoom-out').remove();
