    $(document).on('change.select2', '#author-editor #form-create-post-article .tags select.select2-tags', function () {
        Livewire.emitTo('user.articles.create-article','setTags', $(this).val())
    })
    $(document).on('change.select2', '#editArticle .tags .select-tags', function () {
        Livewire.emitTo('user.articles.edit-article','setTags', $(this).val())
    })
    $(document).on('change.select2', '#editDraftArticle .tags .select-tags', function () {
        Livewire.emitTo('user.articles.draft.edit','setTags', $(this).val())
    })
</script>
