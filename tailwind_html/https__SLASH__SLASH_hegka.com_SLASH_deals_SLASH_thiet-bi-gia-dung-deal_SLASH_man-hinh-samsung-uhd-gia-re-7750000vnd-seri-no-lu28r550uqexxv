            slidesPerView: 6,
            spaceBetween: 8,
            freeMode: true,
            watchSlidesProgress: true,
        });
        let couponImgSwiper = new Swiper(".coupon-images-swiper", {
            spaceBetween: 8,
            navigation: {
                nextEl: ".swiper-button-next",
