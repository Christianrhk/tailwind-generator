function getMetaContentByName(name,content){
  var content = (content==null)?'content':content;
  return document.querySelector("meta[property='"+name+"']").getAttribute(content);
}
var shareData = {
	title: getMetaContentByName('og:title'),
	link: getMetaContentByName('og:url'),
	imgUrl: getMetaContentByName('og:image'),
	desc: getMetaContentByName('og:description'),
	type: 'link',
	success: function() {},
	cancel: function() {}
}

wx.ready(function(){
  wx.updateTimelineShareData(shareData);
  wx.updateAppMessageShareData(shareData);

  wx.onMenuShareQQ(shareData);
  wx.onMenuShareWeibo(shareData);
  wx.onMenuShareQZone(shareData);
  wx.onMenuShareAppMessage(shareData);
  wx.onMenuShareTimeline(shareData);
});

</script>


