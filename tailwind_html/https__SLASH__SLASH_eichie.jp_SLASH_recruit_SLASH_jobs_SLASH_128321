			links.forEach(function(e) {
				e.addEventListener('click', function() {
					path = this.getAttribute('href') || this.form.action || 'unknown';
					keyword = this.form && this.form.elements['keyword'] !== undefined ? this.form.elements['keyword'].value : '';
