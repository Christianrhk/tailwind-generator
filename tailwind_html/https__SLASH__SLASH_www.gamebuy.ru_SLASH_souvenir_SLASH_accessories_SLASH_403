<script>
      let isLoadingUrl = false;
      function changeLocation(src) {
        const path = src.dataset.path;
        if(!path || path === window.location.href || isLoadingUrl) return;
        isLoadingUrl=true;
        src.classList.add('opacity-25');
        window.location.href = path;
        return false;
      }
</script>


