<script>
    $(document).ready(function(){
        $('#learning-objectives-like li').each(function(){
            var li = $(this);
            var activity = $('#learning-objectives-like').data('activity');
            var likeicon = $('<i></i>').attr({'aria-hidden':'true'}).addClass('fa fa-thumbs-up');
            $('<button></button>').attr({'type':'button','data-toggle':'button','aria-pressed':'false','autocomplete':'off'}).addClass('btn btn-like').append(likeicon).prependTo(li).click(function(){
                if(!$(this).hasClass('active')){
                    console.log('click');
                    ga('send', {hitType: 'event', eventCategory: 'learning objective', eventAction: 'like', eventLabel: '/cemidday/' + activity + '/' + li.text()});
                }

                $(this).toggleClass('active');
            });
        });

        $('.card a').each(function(){
            $(this).attr('target', '_blank');
        });
    });
</script>

