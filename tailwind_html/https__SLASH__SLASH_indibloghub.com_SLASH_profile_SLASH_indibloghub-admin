<script src="https://indibloghub.com/public/web/js/simplebar.js"></script>

<script src="https://indibloghub.com/public/web/js/custom.js"></script>

<script src="https://indibloghub.com/public/web/js/bootstrap-select.min.js"></script>

<script type="module" src="https://unpkg.com/ionicons@5.2.3/dist/ionicons/ionicons.esm.js"></script>
<script nomodule="" src="https://unpkg.com/ionicons@5.2.3/dist/ionicons/ionicons.js"></script>


<script src="https://indibloghub.com/public/web/js/jquery.jscroll.js"></script>

<script type="text/javascript">
    $('.pagination').hide();
    $(function() {
       $('.infinite-scroll').jscroll({
        autoTrigger: true,
        loadingHtml: '<img class="center-block" src="https://indibloghub.com/public/images/spinner.gif" alt="Loading..." />',
        padding: 0,
        nextSelector: '.uk-pagination li.active + li a',
        contentSelector: 'div.infinite-scroll',
        callback: function() {
            $('.pagination').remove();
        }
    });

   });
</script>

<script type="text/javascript">

    $(document).ready(function() {

     $(document).on("click",".read_more_content",function(e){
        $(this).hide();
        $('.show_more_content').show();
    });

     $(document).on("click",".share_popup",function(e){
        e.preventDefault();
        window.open($(this).attr('href'), 'fbShareWindow', 'height=450, width=550, top=' + ($(window).height() / 2 - 275) + ', left=' + ($(window).width() / 2 - 225) + ', toolbar=0, location=0, menubar=0, directories=0, scrollbars=0');
        return false;
    });

     $(document).on("click",".post_comment_like",function(){ 

        var id  = $(this).attr("id");
        $.ajax({
            type: "POST", 
            url: "https://indibloghub.com/post_comment_like", 
            data : "id="+id,
            success: function(response){
                $('.feed_post_comment_like_'+id).html(response);
            }
        });
    });

 });

    $('.no_follow_content a').attr("rel", 'nofollow').attr("target", '_blank');

    $('.comment_desc').each(function(){
        var str = $(this).html();
        var regex = /(\b(https?|ftp|file):\/\/[-A-Z0-9+&@#\/%?=~_|!:,.;]*[-A-Z0-9+&@#\/%=~_|])/ig;;
        var replaced_text = str.replace(regex, "<a class='text-blue-500' rel='noindex' href='$1' target='_blank'>$1</a>");
        $(this).html(replaced_text);
    });

    $('.username_href').each(function(){
        var username = $(this).data("username");
        var profile_url = "https://indibloghub.com/profile";
        $(this).attr("href",profile_url+"/"+username).attr("class","text-blue-500 font-semibold");
    });

    $(document).on("click",".reply_comment",function(){ 
      var id  = $(this).attr("id");
      $('#comment_reply .uname').text("@"+$(this).attr("data-username")+" ");
      $('#rep_id').val(id);
  });

    $(document).on("click",".post_like",function(){ 

        var id  = $(this).attr("id");
        $.ajax({
            type: "POST", 
            url: "https://indibloghub.com/post_like", 
            data : "id="+id,
            success: function(response){
                $('.feed_post_like_'+id).html(response);
            }
        });
    });

    $(document).on("click",".user_follow",function(){   

        var k = $(this);
        var id   = k.attr("id");
        $.ajax({
            type: "POST", 
            url: "https://indibloghub.com/user_follow", 
            data : "id="+id,
            success: function(response){
                k.html(response.link_txt);
            }
        });
    });

    $(document).on("click",".topic_follow",function(){   

        var id  = $(this).attr("id");
        $.ajax({
            type: "POST", 
            url: "https://indibloghub.com/topic_follow", 
            data : "id="+id,
            success: function(response){
                $('.t_'+id).html(response);
            }
        });
    });

    if (window.location.href.indexOf("cid") > -1) {
        var res = $(location).attr('href').split("#cid");

        if(res[1] > 0)
        {
            var target = $('#cid'+res[1]);
            $('html,body').animate({
                scrollTop: target.offset().top-130
            }, 1000);
            $('#cid'+res[1]).addClass('p-5').css("background-color","rgb(248 233 236)");
        }
    }

    $(document).on("click",".bookmark",function(){  

        var id  = $(this).attr("id");
        $.ajax({
            type: "POST", 
            url: "https://indibloghub.com/bookmark", 
            data : "id="+id,
            success: function(response){
                $('.post_bookmarked_'+id).html(response);
            }
        });
    });

    $('.callConfirm').click(function()
    {
        var confirm_default = "Are you sure you want to delete it!";
        var r = confirm(confirm_default);
        if (r == true) {
            return true;
        } 
        else
        {
            return false;
        }

    });

    $('.form_sub').submit(function(event) {

        $('.hide_btn').hide();

    });

    
    $('.sorting-select').on('change', function() { 
        var url = $(this).val(); 
        if(url)
        {
            $(this).val("");
            window.location = url;
        }
    });

</script>



<!-- For Night mode -->
<script src="https://indibloghub.com/public/web/js/night-mode.js"></script>

</body>

</html>