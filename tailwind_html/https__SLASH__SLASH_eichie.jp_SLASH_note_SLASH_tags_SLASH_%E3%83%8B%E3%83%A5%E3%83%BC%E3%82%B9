<script>
  $(window).on('load', function() {

    const redirectUrl = location.href || '/'      
  
    $.getJSON('/note/users/origin_proxy/redirect_url', {redirect_url: redirectUrl }, function(data){

      if(!data.url){
        return
      }
      
      if(data.url == 'https://eichie.jp/login'){
        location.reload()
        return
      }

      location.href = data.url
    
    })
  })  
</script>




