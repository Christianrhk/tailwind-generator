<script>
    function OTPInput() {
          const inputs = document.querySelectorAll('#otp > *[id]');
          for (let i = 0; i < inputs.length; i++) {
            inputs[i].addEventListener('keydown', function(event) {
              if (event.key === "Backspace") {
                inputs[i].value = '';
                if (i !== 0)
                  inputs[i - 1].focus();
              } else {
                if (i === inputs.length - 1 && inputs[i].value !== '') {
                  return true;
                } else if (event.keyCode > 47 && event.keyCode < 58) {
                  inputs[i].value = event.key;
                  if (i !== inputs.length - 1)
                    inputs[i + 1].focus();
                  event.preventDefault();
                } else if (event.keyCode > 64 && event.keyCode < 91) {
                  inputs[i].value = String.fromCharCode(event.keyCode);
                  if (i !== inputs.length - 1)
                    inputs[i + 1].focus();
                  event.preventDefault();
                }
              }
            });
          }
        }
    function MOTPInput() {
          const inputs = document.querySelectorAll('#Motp > *[id]');
          for (let i = 0; i < inputs.length; i++) {
            inputs[i].addEventListener('keydown', function(event) {
              if (event.key === "Backspace") {
                inputs[i].value = '';
                if (i !== 0)
                  inputs[i - 1].focus();
              } else {
                if (i === inputs.length - 1 && inputs[i].value !== '') {
                  return true;
                } else if (event.keyCode > 47 && event.keyCode < 58) {
                  inputs[i].value = event.key;
                  if (i !== inputs.length - 1)
                    inputs[i + 1].focus();
                  event.preventDefault();
                } else if (event.keyCode > 64 && event.keyCode < 91) {
                  inputs[i].value = String.fromCharCode(event.keyCode);
                  if (i !== inputs.length - 1)
                    inputs[i + 1].focus();
                  event.preventDefault();
                }
              }
            });
          }
        }
    OTPInput();
    MOTPInput();
    $(document).ready(function () {
      
        $(window).keydown(function(event){
            if( (event.keyCode == 13) && ($('#form').valid() == false) ) {
                event.preventDefault();
                return false;
            }
        });

        $(document).on('change', '#country', function(){
            $('#mcode').val($('option:selected', this).attr('data-code'));
        });

        $('#form').validate({ // initialize the plugin
            rules: {
                radio: {
                    required: true
                },
                firstname: {
                    required: true
                },
                surname: {
                    required: true
                },
                email: {
                    required: true,
                    validemail: true,
                    remote: {
                      url: "https://cobuy.novyy.com/validate-email",
                      type: "post",
                      data: {
                        _token: function() {
                          return "CNoJXpzGwjSueU8WPg1fd3D5sqGtTNfMktzdDxcZ"
                        }
                      },
                    }
                },
                joiningcode: {
                    remote: {
                      url: "https://cobuy.novyy.com/validate-joining-code",
                      type: "post",
                      data: {
                        _token: function() {
                          return "CNoJXpzGwjSueU8WPg1fd3D5sqGtTNfMktzdDxcZ"
                        }
                      },
                    }
                },
                telephone: {
                    required: true,
                    digits: true,
                    remote: {
                      url: "https://cobuy.novyy.com/validate-phone",
                      type: "post",
                      data: {
                        _token: function() {
                          return "CNoJXpzGwjSueU8WPg1fd3D5sqGtTNfMktzdDxcZ"
                        },
                        _phone: function() {
                          return $('input#mcode').val()+'-'+$('input#telephone').val()
                        }
                      },
                    }
                },
                country: {
                    required: true,
                },
            },
            messages: {
            	radio: {
                    required: 'Please select one of the 3 investing options from above'
                },
                firstname: {
                    required: 'Please enter your First Name'
                },
                surname: {
                    required: 'Please enter your Surname'
                },
                email: {
                    required: 'Please enter your Email',
                    email: 'Please enter a valid email address',
                    remote: 'This email address is already registered with us. Please use a different email to set up a new account or proceed to Login.'
                },
                joiningcode: {
                    remote: 'You entered an invalid code'
                },
                telephone: {
                    required: 'Please enter your Mobile Number',
                    remote: 'This phone number is already registered with us. Please use a different phone number to set up a new account or proceed to Login.'
                },
                country: {
                    required: 'Please select Country of Tax Residence ',
                },
                check: {
                	required: 'Please accept Novyy\'s Privacy Policy & Terms to proceed',
                },
                check1: {
                	required: 'Please accept that you are a qualified investor',
                }
            },
            submitHandler: function(form) {

              $.ajax({
                  url: "https://cobuy.novyy.com/registerform",
                  type:'POST',
                  data: $(form).serialize(),
                  cache: false,
                  processData: false,
                  beforeSend: function() {
                    
                      $('.serv-box1').html('<i class="fa fa-spinner fa-spin"></i>');
                  },
                  success: function(data) {
                      if(data.status == 1) {
                          servClicked();
                          $('#id').val(data.msg);
                          $('#first').focus();
                      } else if(data.status == 2) {
                          $('.serv-box1').html('Continue');
                          $.each( data.msg, function( key, value ) {
                              $('#'+key+'-error').html(value).show();
                          });
                      } else {
                      }
                  }
              });
              return false;
            }
        });

        $.validator.addMethod(
          "validemail",
          function(value, element) {
            return /^[a-zA-Z0-9.!#$%&'*+\/=?^_`{|}~-]+@[a-zA-Z0-9][a-zA-Z0-9-]{0,61}[a-zA-Z0-9]\.[a-zA-Z0-9]{1,61}(?:\.[a-zA-Z0-9](?:[a-zA-Z0-9-]{0,61}[a-zA-Z0-9])?)*$/.test(value);
          },
          "Please enter a valid email address"
        );

        $(document).on('blur','#emailotp, #mobileotp',function(){
          if($(this).val() != '') {
            $(this).addClass('valid');
          } else {
            $(this).removeClass('valid');
          }
        });

        $(document).on('click','#veriftotp', function() {

            if ($('input#first').val() == '' || $('input#second').val() == '' || $('input#third').val() == '' || $('input#fourth').val() == '' || $('input#fifth').val() == '' || $('input#sixth').val() == '') {
                var emailotp = '';
            }else {
                var emailotp = $('input#first').val() + $('input#second').val() + $('input#third').val() + $('input#fourth').val() + $('input#fifth').val() + $('input#sixth').val();
            }

            if ($('input#first').val() == '' || $('input#second').val() == '' || $('input#third').val() == '' || $('input#fourth').val() == '' || $('input#fifth').val() == '' || $('input#sixth').val() == '') {
                var mobileotp = '';
            }else {
                var mobileotp = $('input#Mfirst').val() + $('input#Msecond').val() + $('input#Mthird').val() + $('input#Mfourth').val() + $('input#Mfifth').val() + $('input#Msixth').val();
            }

            //var emailotp = $('#emailotp').val().trim();
            //var mobileotp = $('#mobileotp').val().trim();
            var id = $('#id').val().trim();

            if(emailotp == '' && mobileotp == '') {
                $('#emailotp-error').html('Please enter the One Time Passcode to proceed.').show();
                $('#mobileotp-error').html('Please enter the One Time Passcode to proceed.').show();
                return false;
            }
            else if(emailotp == '') {
                $('#emailotp-error').html('Please enter the One Time Passcode to proceed.').show();
                return false;
            }
            else if(mobileotp == '') {
                $('#mobileotp-error').html('Please enter the One Time Passcode to proceed.').show();
                return false;
            }else {
                $('#mobileotp-error, #emailotp-error').hide();
            }

            $.ajax({
                url: "https://cobuy.novyy.com/verify-registration-otp",
                type:'POST',
                data: {_token:'CNoJXpzGwjSueU8WPg1fd3D5sqGtTNfMktzdDxcZ', id:id, emailotp:emailotp, mobileotp:mobileotp},
                beforeSend: function() {
                    $(".preloader").fadeIn("100");
                },
                success: function(data) {
                    if(data.status != 1 && data.status != 3) {
                        $(".preloader").fadeOut("100");
                    }
                    $('#otp-error').html('').hide();
                    if(data.status == 1) {
                        window.location.replace("https://cobuy.novyy.com/register-process?token="+data.msg);
                    }
                    else if(data.status == 2) {
                        $.each( data.msg, function( key, value ) {
                            $('#'+key+'-error').html(value).show();
                        });
                    }
                    else {
                        $('#otp-error').html(data.msg).show();
                        console.log('here');
                    }
                }
            });
        });
    });
