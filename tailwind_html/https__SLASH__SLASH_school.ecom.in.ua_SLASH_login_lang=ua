        <script>
        $(function() {

            
            var frm1 = $('#regFormModal');
            frm1.submit(function (ev) {
                $.ajax({
                    type: frm1.attr('method'),
                    url: frm1.attr('action'),
                    data: frm1.serialize(),
                    success: function (data) {
                        $('#registrationModal').trigger('reveal:close');
                        
                        $('#regSuccessModal').find('p').text(data.message);
                        setTimeout(function(){
                            $('#regSuccessModal').reveal('open');
                        }, 1000);
                    }
                });

                ev.preventDefault();
            });


        });

    </script>

    
    
