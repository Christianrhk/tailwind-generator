<script>
  $(function() {
    var container = $("#filter-wrapper"),
        pagination = $("#pagination");

    var ftInstance = $.filtrify("filter-wrapper", "placeHolder", {
      callback : function(query, match, mismatch) {
        destroyPagination();
        setPagination();
        handleResetBtn(query, match, mismatch);
        handleViewingLabel(query, match, mismatch);
      }
    });

    // function declaration
    setLazyLoad();
    setPagination();
    handleResetFilter();
    handleFilterLabel();

    function handleViewingLabel(query, match, mismatch) {
      queryTypeText();
      queryTopicText();
      setViewingText();

      function queryTypeText() {
        window.typeText = '';

        if (query.type.length != 0) {
          query.type.forEach(function(type, index) {
            if (index > 0) {
              window.typeText += (', ' + type) ;
            } else {
              window.typeText += type ;
            }
          })
        }
      }

      function queryTopicText() {
        window.topicText = '';

        if (query.topic.length != 0) {
          query.topic.forEach(function(topic, index) {
            if (index > 0) {
              window.topicText += (', ' + topic) ;
            } else {
              window.topicText += topic ;
            }
          })
        }
      }

      function setViewingText() {
        var viewingText = `Now viewing "${window.typeText}" about "${window.topicText}"`;

        if (query.type.length != 0 && query.topic.length == 0) {
          viewingText = `Now viewing "${window.typeText}"`;
        };

        if (query.type.length == 0 && query.topic.length != 0) {
          viewingText = `Now viewing all about "${window.topicText}"`;
        };

        $('#viewing-text').html(viewingText);
      }
    }

    function setLazyLoad () {
      container.find("img").lazyload({
        event : "turnPage",
        effect : "fadeIn"
      });
    };

    function setPagination () {
      pagination.jPages({
        containerID : "filter-wrapper",
        perPage : 9,
        direction : "auto",
        animation : "fadeIn",
        callback : function( pages, items ){
        items.showing.find("img").trigger("turnPage");
        items.oncoming.find("img").trigger("turnPage");
      }
                        });
    };

    function destroyPagination () {
      pagination.jPages("destroy");
    };

    function handleResetBtn(query, match, mismatch) {
      if ( mismatch.length ) {
        $("#btn-ft-reset, #viewing-text").show();
        $("#heading-latest").hide();
      } 
      else {
        $("#heading-latest").show();
        $("#btn-ft-reset, #viewing-text").hide();
      } 
    }

    function handleResetFilter() {
      $(document).on('click', '#btn-ft-reset', function() {
        ftInstance.reset();
      })
    }

    function handleFilterLabel() {
      $('.ft-label').each(function() {
        if ($(this).html() == 'topic') {
          $(this).html('By Topic');
                       } else {
                       $(this).html('By Type');
                                    }
                                    })
        }

      });
</script>


