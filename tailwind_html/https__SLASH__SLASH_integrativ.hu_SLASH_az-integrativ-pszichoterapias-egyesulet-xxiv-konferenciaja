<script>
function jelTypeChanged(item){
  $(".jeltype").removeClass("active");
  $(item).addClass("active");
  $("#jeltypeinput").val($(item).text());

  if($(item).attr("data-iseloado")=="1"){
    $("#jeltypeeloado").show();
  }
  else{
    $("#jeltypeeloado").hide();
    $("#jeltypeinputeloado1").val('nem');
    $("#jeltypeinputeloado2").val('nem');
    $("#jeltypeinputeloado1cb").prop("checked", false);
    $("#jeltypeinputeloado2cb").prop("checked", false);
  }

}
function jelinputchanged(input, copyid){
  if($("#jel_szamla_same").is(":checked")){
    $("#"+copyid).val($(input).val());
  }
}
function jelSubmit(e){
  var text='';
  $('input[type=text],input[type=email]', '#jelform').each(function() {
    var label = $(this).attr("placeholder");
    if($(this).attr("data-pluss")){
      label=label+$(this).attr("data-pluss");
    }
    text=text+label+": "+$(this).val()+"\n";
  });
  $("#jeldata").val(text);
  console.log(text);
  return true;
}
$(document).ready(function(){

  $("#jel_szamla_same").on("change", function(){
    if($(this).is(":checked")){
      $("#jel_szamla").hide();
      $(".copyable").each(function(){
        $(this).trigger("change");
      });
    }
    else{
      $("#jel_szamla").show();
    }
  });

});
</script>

