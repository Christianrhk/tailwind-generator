<script>
        $(document).ready(function () {

            $("#platform").on('change', function () {
                var el = $(this);
                if (el.val() === "Anchor Protocol") {
                    $("#alert_type").find('option').remove().end();
                    $("#alert_type").append('<option>Loan Liquidation</option>');
                
                }
                if (el.val() == "TerraSwap") {
                    $("#alert_type").find('option').remove().end();
                    $("#alert_type").append('<option>Price Alert</option>');
                }
        
            
            })
        });
