<script>
  window.onscroll = function() {scrollDown()};
  var header      = document.getElementById("myHeader");
  var cat         = document.getElementById("name-cat-content");
  var catmob      = document.getElementById("name-cat-content-mobile");
  var sticky      = header.offsetTop;

  function scrollDown() {
    if (window.pageYOffset > sticky)
    {
      header.classList.add("sticky");
    }
    else
    {
      header.classList.remove("sticky");
    }
  }

  jQuery(document).ready(function () {
    if( window.location.href.indexOf("categorie-produit") > -1 || window.location.href.indexOf("produits") > -1 || window.location.href.indexOf("boutique") > -1)
    {
      function changeDot() {
          var scrollTop = jQuery('html, body').scrollTop(),
              section   = jQuery('.title-category');
          var cat       = document.getElementById("name-cat-content");
          var catmob    = document.getElementById("name-cat-content-mobile");

          section.each(function (i, elem)
          {
            var that = jQuery(elem),
                sectionScroll = that.offset().top,
                sectionHeigth = that.outerHeight(),
                distance = sectionScroll - scrollTop,
                procent = (-distance * 100) / (sectionHeigth);

            if (procent >= -10 && procent <= 90) {
              cat.innerHTML = elem.getAttribute("data-name");
              catmob.innerHTML = elem.getAttribute("data-name");
            }
          }) //.each
      } //.fn
      jQuery(document).on("scroll", changeDot);
    }
  });
</script>

<style>
  .sticky {
    position: fixed;
    top: 0;
    width: 100%;
    left: 0;
    background-color: #fff;
    z-index: 3;
    transition: background 0.5s;
  }
  .sticky + .content {
    padding-top: 102px;
  }
</style>

		
				

