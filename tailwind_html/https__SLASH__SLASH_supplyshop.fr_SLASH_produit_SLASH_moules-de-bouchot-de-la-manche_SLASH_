        <script>
            jQuery(document).ready(function($) {
                jQuery('body').on('added_to_cart', function() {
                    setTimeout(function() {
                        var byc_cart_total_price_with_symbol = jQuery("#byconsolewooodt_cart_total_price").text();
                        var byc_cart_total_price = byc_cart_total_price_with_symbol.replace(/[^\d\.]/g, '');
                        var added_total_price = {
                            'action': 'byconsolewoodt_cart_total_action',
                            'byc_cart_total_price_val': byc_cart_total_price // We pass php values differently!
                        };
                        // We can also pass the url value separately from ajaxurl for front end AJAX implementations
                        var ajaxurl = "https://supplyshop.fr/wp-admin/admin-ajax.php";

                        jQuery.post(ajaxurl, added_total_price, function(response) {
                            jQuery("#text_content").text(response);
                        });
                    }, 1000); // for 1 second delay
                });
            });
        </script>
    
        <script>
            setTimeout(function() {
                var byc_cart_total_price_with_symbol = jQuery("#byconsolewooodt_cart_total_price").text();
                var byc_cart_total_price = byc_cart_total_price_with_symbol.replace(/[^\d\.]/g, '');
                var added_total_price = {
                    'action': 'byconsolewoodt_cart_total_action',
                    'byc_cart_total_price_val': byc_cart_total_price // We pass php values differently!
                };

                // We can also pass the url value separately from ajaxurl for front end AJAX implementations
                var ajaxurl = "https://supplyshop.fr/wp-admin/admin-ajax.php";

                jQuery.post(ajaxurl, added_total_price, function(response) {
                    jQuery("#text_content").text(response);
                });
            }, 1000); // for 1 second delay
        </script>
    
    <a class="cart-contents fi-shopping-cart" id="byconsolewooodt_cart_total_price" href="#" title="price" style="display:none;"></a>
    <script>
        document.addEventListener("keydown", function(e) {

                    });

        function allowNumbersOnly(e, except_char) {
            var code = (e.which) ? e.which : e.keyCode;

            if (code > 31 && (code < 48 || code > 57)) {
                if (code.toString() != except_char) {
                    e.preventDefault();
                }
            }
        }

        function validateEmail(email) {
            var regex = /^(?:[a-z0-9!#$%&amp;'*+/=?^_`{|}~-]+(?:\.[a-z0-9!#$%&amp;'*+/=?^_`{|}~-]+)*|"(?:[\x01-\x08\x0b\x0c\x0e-\x1f\x21\x23-\x5b\x5d-\x7f]|\\[\x01-\x09\x0b\x0c\x0e-\x7f])*")@(?:(?:[a-z0-9](?:[a-z0-9-]*[a-z0-9])?\.)+[a-z0-9](?:[a-z0-9-]*[a-z0-9])?|\[(?:(?:25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.){3}(?:25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?|[a-z0-9-]*[a-z0-9]:(?:[\x01-\x08\x0b\x0c\x0e-\x1f\x21-\x5a\x53-\x7f]|\\[\x01-\x09\x0b\x0c\x0e-\x7f])+)\])$/;
            return regex.test(email);
        }

        jQuery(document).ready(function() {
            jQuery("#woocommerce-bar-checkout").css("width", "50%");
            jQuery(".woocommerce-billing-fields__field-wrapper").css("display", "block");
            jQuery(".shipping_address").slideUp();
            jQuery(".woocommerce-shipping-fields").css("display", "block");
            jQuery(".woocommerce-additional-fields__field-wrapper").css("display", "block");
            jQuery(".woocommerce-billing-fields h3").css("display", "block");


            //jQuery( "<div class='tooltip'><i class='fas fa-question-circle'></i>?<span class='tooltiptext'>Après la validation de votre commande, vous recevrez un SMS de confirmation sur ce numéro. Si cette commande est un cadeau, nous vous invitons à renseigner ici votre propre numéro de téléphone pour préserver la surprise. Veuillez indiquer le numéro de téléphone à joindre lors de la livraison dans l'espace 'NOTES DE COMMANDE'.</span></div>" ).insertAfter( "#billing_phone_field label" );

            jQuery("#billing_phone_field label").append("<div class='tooltip'><i class='fas fa-question-circle' style='vertical-align: middle;'></i><span class='tooltiptext'>Après la validation de votre commande, vous recevrez un SMS de confirmation sur ce numéro. Si cette commande est un cadeau, nous vous invitons à renseigner ici votre propre numéro de téléphone pour préserver la surprise. Veuillez indiquer le numéro de téléphone à joindre lors de la livraison dans l'espace 'NOTES DE COMMANDE'.</span></div>");


            
            jQuery("#byc_next_form_of_checkout_page").click(function() {
                jQuery("html, body").animate({
                    scrollTop: 0
                }, "slow");
                jQuery("#woocommerce-bar-checkout").css("width", "50%");
                document.getElementById("woocommerce-text-step").innerHTML = "ÉTAPE 1";

                jQuery(".woocommerce-billing-fields__field-wrapper").css("display", "block");
                jQuery(".woocommerce-shipping-fields").css("display", "block");
                jQuery(".woocommerce-additional-fields__field-wrapper").css("display", "block");
                jQuery(".woocommerce-billing-fields h3").css("display", "block");

                document.getElementById("byconsolewooodt_checkout_field").style.display = "none";
                document.getElementById("byc_prev_form_of_checkout_page_for_date_time_plugin").style.display = "none";
                document.getElementById("byc_next_form_of_checkout_page_for_payment").style.display = "block";
                document.getElementById("go-to-cart").style.display = "block";
                document.getElementById("byc_next_form_of_checkout_page").style.display = "none";
            });

            jQuery("#byc_prev_form_of_checkout_page_for_date_time_plugin").click(function() {
                jQuery("html, body").animate({
                    scrollTop: 0
                }, "slow");
                jQuery("#woocommerce-bar-checkout").css("width", "20%");
                document.getElementById("woocommerce-text-step").innerHTML = "ÉTAPE 1";

                jQuery(".woocommerce-billing-fields__field-wrapper").css("display", "none");
                jQuery(".woocommerce-shipping-fields").css("display", "none");
                jQuery(".woocommerce-additional-fields__field-wrapper").css("display", "none");
                jQuery(".woocommerce-billing-fields h3").css("display", "none");

                document.getElementById("byconsolewooodt_checkout_field").style.display = "block";
                document.getElementById("byc_prev_form_of_checkout_page_for_date_time_plugin").style.display = "none";
                document.getElementById("byc_next_form_of_checkout_page_for_payment").style.display = "none";
                document.getElementById("go-to-cart").style.display = "none";
                document.getElementById("byc_next_form_of_checkout_page").style.display = "block";
            });

            jQuery("#billing_phone").keypress(function(event) {
                allowNumbersOnly(event, '43');
            });

            jQuery("#billing_postcode").keypress(function(event) {
                allowNumbersOnly(event);
            });

            jQuery("#billing_phone").keyup(function(e) {
                if (jQuery(this).val().length >= 13) {
                    jQuery(this).val(jQuery(this).val().substr(0, 13));
                }
            });

            jQuery("#billing_postcode").keyup(function(e) {
                if (jQuery(this).val().length >= 5) {
                    jQuery(this).val(jQuery(this).val().substr(0, 5));

                                    }
            });

            jQuery("#byc_next_form_of_checkout_page_for_payment").click(function() {
                var ischeck = false;
                var prenom = document.getElementById('billing_first_name').value;
                var nom = document.getElementById('billing_last_name').value;
                var addr = document.getElementById('billing_address_1').value;
                var codepostal = document.getElementById('billing_postcode').value;
                var ville = document.getElementById('billing_city').value;
                var telephone = document.getElementById('billing_phone').value;
                var email = document.getElementById('billing_email').value;

                if (prenom == "") {
                    alert('Veuillez entrer votre prénom');
                    ischeck = false;
                    return;
                } else if (nom == "") {
                    alert('Veuillez entrer votre nom');
                    ischeck = false;
                    return;
                } else if (jQuery("#select2-billing_country-container").val() == null) {
                    alert('Veuillez selectionner un pays');
                    ischeck = false;
                    return;
                } else if (addr == "") {
                    alert('Veuillez entrer une adresse postal');
                    ischeck = false;
                    return;
                } else if (codepostal == "") {
                    alert('Veuillez entrer un code postal');
                    ischeck = false;
                    return;
                } else if (ville == "") {
                    alert('Veuillez entrer une ville');
                    ischeck = false;
                    return;
                } else if (telephone == "") {
                    alert('Veuillez entrer votre numéro de téléphone');
                    ischeck = false;
                    return;
                } else if (email == "") {
                    alert('Veuillez entrer une adresse email');
                    ischeck = false;
                    return;
                } else if (validateEmail(email) == false) {
                    alert("Veuillez entrer une adresse mail valide");
                    ischeck = false;
                    return;
                } else {
                    ischeck = true;
                }

                if (ischeck == true) {
                    jQuery("html, body").animate({
                        scrollTop: 0
                    }, "slow");
                    jQuery("#woocommerce-bar-checkout").css("width", "100%");
                    document.getElementById("woocommerce-text-step").innerHTML = "ÉTAPE 2";

                    jQuery(".woocommerce-billing-fields__field-wrapper").css("display", "none");
                    jQuery(".woocommerce-shipping-fields").css("display", "none");
                    jQuery(".woocommerce-additional-fields__field-wrapper").css("display", "none");
                    jQuery(".woocommerce-billing-fields h3").css("display", "none");

                    document.getElementById("order_review").style.display = "block";
                                        document.getElementById("order_review_heading").style.display = "block";
                    document.getElementById("byconsolewooodt_checkout_field").style.display = "none";
                    document.getElementById("byc_prev_form_of_checkout_page_for_date_time_plugin").style.display = "none";
                    document.getElementById("byc_next_form_of_checkout_page_for_payment").style.display = "none";
                    document.getElementById("go-to-cart").style.display = "none";
                    document.getElementById("byc_prev_form_of_checkout_page_for_billing_and_shipping_details").style.display = "block";
                }
            });

            jQuery("#byc_prev_form_of_checkout_page_for_billing_and_shipping_details").click(function() {
                jQuery("html, body").animate({
                    scrollTop: 0
                }, "slow");
                jQuery("#woocommerce-bar-checkout").css("width", "50%");
                document.getElementById("woocommerce-text-step").innerHTML = "ÉTAPE 2";
                jQuery(".woocommerce-billing-fields__field-wrapper").css("display", "block");
                jQuery(".woocommerce-shipping-fields").css("display", "block");
                jQuery(".woocommerce-additional-fields__field-wrapper").css("display", "block");
                jQuery(".woocommerce-billing-fields h3").css("display", "block");

                document.getElementById("order_review").style.display = "none";
                document.getElementById("order_review_heading").style.display = "none";
                document.getElementById("byc_prev_form_of_checkout_page_for_date_time_plugin").style.display = "none";
                document.getElementById("byc_next_form_of_checkout_page_for_payment").style.display = "block";
                document.getElementById("go-to-cart").style.display = "block";
                document.getElementById("byc_prev_form_of_checkout_page_for_billing_and_shipping_details").style.display = "none";
            });
            jQuery("#byconsolewooodt_delivery_date").prop("readonly", true);
        });
    </script>

