		<script>
			function myFunction(e,id) {
				let val = document.getElementById("input-add-"+id).value;
				let qty = 1;
				if( val==null || parseInt(val)<=0 ) {
					qty = 1;
				} else {
					qty = val;
				}
				let btn = document.getElementById("button-add-"+id);
				btn.setAttribute("data-quantity", qty);
			}
		</script>
