				gtag( 'event', 'add_to_cart', { 'event_category': 'ecommerce', 'event_label': '#23586', 'items': [ {'id': '#23586','name': 'Filet de saumon Julien Marée','quantity': $( 'input.qty' ).val() ? $( 'input.qty' ).val() : '1'} ] } );
			});
		


