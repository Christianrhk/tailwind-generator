  <script type="text/javascript" src="https://bravofleet.com/wp-content/themes/cambrian/js/jquery.js"></script>
  <script type="text/javascript" src="https://bravofleet.com/wp-content/themes/cambrian/js/jquery.easing-sooper.js"></script>
  <script type="text/javascript" src="https://bravofleet.com/wp-content/themes/cambrian/js/jquery.sooperfish.js"></script>
  <script type="text/javascript">
    $(document).ready(function() {
      $('ul.sf-menu').sooperfish();
    });
  </script>
  <script>
  var $btns = $('.filter-nav-item').click(function() {
    if (this.id == 'all') {
        $('#parent > div').fadeIn(450);
    } else {
        var $el = $('.' + this.id).fadeIn(450);
        $('#parent > div').not($el).hide();
    }
    $btns.removeClass('active');
    $(this).addClass('active');
  })
  </script>
</body>
</html>
