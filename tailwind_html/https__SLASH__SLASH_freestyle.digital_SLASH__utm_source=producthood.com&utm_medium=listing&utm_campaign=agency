<script type="module">
import {SplitText} from "/wp-content/themes/web-designer-southampton/js/SplitText.js";
gsap.registerPlugin(SplitText);



gsap.utils.toArray(".fs-heading").forEach(function(elem) {
  const splitTimeline = gsap.timeline({scrollTrigger: {
    trigger: elem,
    start: "top 80%",
    end: "bottom 10%",
    onToggle: self => gsap.to(elem, {opacity: 1}),
    toggleActions: "play none",
    //toggleActions: "restart pause restart none", // repeat
    //markers: true
    }
  });

  const splitTitle = new SplitText(elem);
    splitTimeline.from(splitTitle.chars, {duration: 0.2, opacity: 0, y: 10, stagger: {amount: 1}, ease: "back.out(1)", overwrite: "auto"});
});

gsap.utils.toArray(".fs-button").forEach(function(elem2) {
  const splitTimeline2 = gsap.timeline({scrollTrigger: {
    trigger: elem2,
    start: "top 70%",
    end: "bottom 10%",
    onToggle: self => gsap.to(elem2, {opacity: 1}),
    toggleActions: "play none",
    //toggleActions: "restart pause restart none", // repeat
    //markers: true
    }
  });

    splitTimeline2.from(elem2, {duration: 0.5, opacity: 0,  y: 40, stagger: {amount: 1}, ease: "back.out(1)", overwrite: "auto"});
});


gsap.utils.toArray(".fs-grid .grid-post").forEach(function(elem3) {
