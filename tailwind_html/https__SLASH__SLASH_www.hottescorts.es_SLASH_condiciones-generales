<!-- Cookie Modal -->
<script src="https://cdn.jsdelivr.net/npm/js-cookie@3.0.1/dist/js.cookie.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.29.3/moment.min.js"></script>



<script type="text/javascript">
        $(document).ready(function () {

            if(!Cookies.get("legales")){
                $("#cookiemodal").modal('show');
            }
            else{
                var cookie = Cookies.get("legales");
                var now = moment().format('YYYY-MM-DD');
                var cookiedt = moment(Cookies.get("legales")).format('YYYY-MM-DD');
                var diff = moment(cookiedt).diff(moment(now), 'days');
                if (diff >= 7){
                    $("#cookiemodal").modal('show');
                    Cookies.remove("legales");
                }
            }
            
            $('body').on('click', '.acceptCookie', function () {
                $("#cookiemodal").modal('hide');
                Cookies.set("legales", moment().format("YYYY-MM-DD"), { expires: 7 });
                // console.log(Terms);
            });
            $('body').on('click', '.closeCookie', function () {
                Cookies.remove("legales");
                location.reload();
            })

            var opt = {
                mouseover: function (o) {
                    o.stop();
                    o.css({cursor: 'pointer'});
                },
                mouseout: function (o) {
                    o.start();
                }
            }
            $('#scroll_horz').mscroller({behavior: 'circle',
                direction: 'right',
                speed: 50,
                delay: 1000
            });

            $('#scroll_horz_d1').mscroller({behavior: 'circle',
                direction: 'right',
                speed: 20,
                delay: 1000,
                scrollamount: 5
            });

            $('#scroll_horz_d2').mscroller({behavior: 'circle',
                direction: 'right',
                speed: 20,
                delay: 1000,
            });


            /*
             $(".btn-contact").click(function () {
             $('.ok-mensaje-main').show();
             $('.form-newsletter').hide();
             return false;
             });
             */

            $(".btn-servicio").on("click", function () {
                var el = $(this);
                if (el.text() == el.data("text-swap")) {
                    el.text(el.data("text-original"));
                } else {
                    el.data("text-original", el.text());
                    el.text(el.data("text-swap"));
                }
                $('.mas-servicio').toggle();
                return false;
            });
            $(".btn-caract").on("click", function () {
                var el = $(this);
                if (el.text() == el.data("text-swap")) {
                    el.text(el.data("text-original"));
                } else {
                    el.data("text-original", el.text());
                    el.text(el.data("text-swap"));
                }
                $('.mas-caract').toggle();
                return false;
            });

            $(".entradilla").jScrollPane(
                    {
                        showArrows: true,
                    }
            );
            $(window).resize(function () {
                $(".entradilla").jScrollPane(
                        {
                            showArrows: true,
                        }
                );
            });
        })
</script>
