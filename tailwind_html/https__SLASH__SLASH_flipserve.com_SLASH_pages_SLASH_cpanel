<script>
  if (!Array.prototype.find) {
    Object.defineProperty(Array.prototype, 'find', {
      value: function(predicate) {
        if (this == null) {
          throw TypeError('"this" is null or not defined');
        }

        var o = Object(this);

        var len = o.length >>> 0;

        if (typeof predicate !== 'function') {
          throw TypeError('predicate must be a function');
        }

        var thisArg = arguments[1];

        var k = 0;

        while (k < len) {
          var kValue = o[k];
          if (predicate.call(thisArg, kValue, k, o)) {
            return kValue;
          }
          k++;
        }

        return undefined;
      },
      configurable: true,
      writable: true
    });
  }
</script>



<link href="//cdn.shopify.com/s/files/1/0577/5900/6891/t/3/assets/translation-lab-language-switcher-floating.css?v=11317905096383239861626515015" rel="stylesheet" type="text/css" media="all" />

<style>

  .translation-lab-language-switcher {
