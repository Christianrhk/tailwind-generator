    top: 0px;
    bottom: auto;
    left: 0px;
    right: auto;
  }

  .translation-lab-language-switcher.bottom-left {
    position: fixed !important;
    top: auto !important;
    bottom: 0px !important;
    left: 0px !important;
    right: auto !important;
  }

  .translation-lab-language-switcher.bottom-right {
