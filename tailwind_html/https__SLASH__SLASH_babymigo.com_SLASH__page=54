<script type="text/javascript">
            // https://www.jqueryscript.net/slider/Touch-Swipe-Bootstrap-Carousel.html

            $('.my-carousel').carousel().swipeCarousel({
                // low, medium or high
                sensitivity:'high'
            });

            $('.bm-tabs').each(function(index, tabs) {

                const elem = $(this);

                // Toggle tab method
                function toggleTab (event) {
                    let index = 0;
                    const headers = $(tabs).find('.bm-tabs-header');
                    const contents = $(tabs).find('.bm-tabs-content');
                    
                    const tabStyle =  elem.hasClass('secondary') ? 'secondary' : 'primary';
                    const activeClass = 'border-r border-b';
                    const inactiveClass = 'bg-' + tabStyle + '-lighter';

                    // resolve index
                    if (event && event.target) {
                        const eventElem = $(event.target);
                        index = eventElem.find('span').length ? Number(eventElem.attr('tab-index')) : Number(eventElem.parent().attr('tab-index'));
                    }
                    
                    headers.children().each(function(idx, header) {
                        const content = contents.children().eq(idx);
                        if (idx === index) {
                            $(header).addClass(activeClass).removeClass(inactiveClass);
                            if (content) $(content).show();
                        } else {
                            $(header).removeClass(activeClass).addClass(inactiveClass);
                            if (content) $(content).hide();
                        }
                    });
                }

                // Process tab headers
                $(tabs).find('.bm-tabs-header').children().each(function(index, tab) {
                    const header = $(tab);
                    header.attr('tab-index', index);
                    header.click(toggleTab);
                })

                // Process init
                toggleTab();
            });
        </script>
