                                            :class="errors[4].custom_mmllantare == 'correct' || errors[4].custom_mmllantare == ''  ?  '' : 'bg-red-100' "
                                        />
                                    </div>                              
                                    <button class="bgMax || self-center mt-1  h-8 w-10 mx-2 px-2 pb-1  rounded-md  text-white text-3xl  font-black  focus:outline-none  "
                                        type="button" @click="form.custom_mmllantare += 1,verifyInputButton('custom_mmllantare',5)">
                                        <p class=" -mt-1">+</p> 
                                    </button>
                                </div>
                                <p class="errorField || block text-center " v-if="errors[4].custom_mmllantare !== 'correct'">{{errors[4].custom_mmllantare}}</p>
                            </div>
                        </section>
                        <!-- Contact -->
                        <section v-show="tabSelected === 6" class="mb-12">
                            <p class="mt-14 b-2 text-center  text-xl"> <b>Dejanos saber de ti</b>  </p>
                            <div class="flex justify-center">
                                <p class="colorText || mb-4 text-center w-9/12 sm:w-full text-base sm:text-lg">Esto nos ayudara a darte unos mejores resultados</p>
                            </div>
                            <div class="mx-4 md:mx-0 flex flex-col items-center">
                                <div class="w-full mx-2  sm:w-1/2 lg:w-4/12">
                                    <label for="name" class="mt-4  text-center text-xl   block"> <b>Nombre*</b> </label>
                                    <input  required type="text" name="name" id="name" v-model="form.name"  
                                    @blur="verifyInput($event, 6)"
                                      class="mt-1 pl-2 h-8  block w-full shadow-sm lg:text-sm border border-gray-300 rounded-md focus:outline-none text-center "
                                      :class="errors[5].name == 'correct' || errors[5].name == ''  ?  '' : 'bg-red-100' "
                                    />
                                    <p class="errorField || block  text-center " v-if="errors[5].name !== 'correct'">{{errors[5].name}}</p>
                                </div>
                                <div class="w-full mx-2  sm:w-1/2 lg:w-4/12">
                                    <label for="custom_apellido" class="mt-4  text-center text-xl   block"> <b>apellido*</b> </label>
                                    <input  required type="text" name="custom_apellido" id="custom_apellido" v-model="form.custom_apellido"  
                                    @blur="verifyInput($event, 6)"
                                      class="mt-1 pl-2 h-8  block w-full shadow-sm lg:text-sm border border-gray-300 rounded-md focus:outline-none text-center "
                                      :class="errors[5].custom_apellido == 'correct' || errors[5].custom_apellido == ''  ?  '' : 'bg-red-100' "
                                    />
                                    <p class="errorField || block  text-center " v-if="errors[5].custom_apellido !== 'correct'">{{errors[5].custom_apellido}}</p>
                                </div>
                                <div class="w-full mx-2  sm:w-1/2 lg:w-4/12">
                                    <label for="custom_phone" class="mt-4  text-center text-xl   block"> <b>Celular*</b> </label>
                                    <input  required type="number" name="custom_phone" id="custom_phone" v-model.number="form.custom_phone"  
                                    @blur="verifyInput($event, 6)"
                                      class="mt-1 pl-2 h-8  block w-full shadow-sm lg:text-sm border border-gray-300 rounded-md focus:outline-none text-center "
                                      :class="errors[5].custom_phone == 'correct' || errors[5].custom_phone == ''  ?  '' : 'bg-red-100' "
                                    />
                                    <p class="errorField || block  text-center " v-if="errors[5].custom_phone !== 'correct'">{{errors[5].custom_phone}}</p>
                                </div>
                                <div class="w-full mx-2  sm:w-1/2 lg:w-4/12">
                                    <label for="email" class="mt-4  text-center text-xl   block"> <b>email*</b> </label>
                                    <input  required type="email" name="email" id="email" v-model="form.email"  
                                    @blur="verifyInput($event, 6)"
                                      class="mt-1 pl-2 h-8  block w-full shadow-sm lg:text-sm border border-gray-300 rounded-md focus:outline-none text-center "
                                      :class="errors[5].email == 'correct' || errors[5].email == ''  ?  '' : 'bg-red-100' "
                                    />
                                    <p class="errorField || block  text-center " v-if="errors[5].email !== 'correct'">{{errors[5].email}}</p>
                                </div>
                                 <!-- List token -->
                                    <!-- Get the token at: https://app.getresponse.com/campaign_list.html -->
                                    <input type="hidden" name="campaign_token" value="otROD" />
                            </div>
                           
     
                         </section>
                         <!-- Extra data to getResponse page -->
                         <input class="hidden" type="text" name="custom_software" id="custom_software">  
                         <input class="hidden" type="text" name="custom_tipooperacion" id="custom_tipooperacion">  
                         <input class="hidden"  type="text" name="custom_calculos" id="custom_calculos"></input>
                         <input class="hidden" type="text" name="custom_totalahoptimizado" id="custom_totalahoptimizado">  
                         <input class="hidden" type="text" name="custom_totalahllantasn" id="custom_totalahllantasn">  
                         <input class="hidden" type="text" name="custom_totalahllantasr" id="custom_totalahllantasr">  
                         <input class="hidden" type="text" name="custom_roiservicio" id="custom_roiservicio">  
                         <input class="hidden" type="text" name="custom_rtotalahoptimizado" id="custom_rtotalahoptimizado">  
                         <input class="hidden" type="text" name="custom_rtotalahllantasn" id="custom_rtotalahllantasn">  
                         <input class="hidden" type="text" name="custom_rtotalahllantasr" id="custom_rtotalahllantasr">  
                         <input class="hidden" type="text" name="custom_rroiservicio" id="custom_rroiservicio">  
                         <input class="hidden" name="custom_phone" type="text" id="custom_phone" /><br/>
                    </main>
                    <!-- buttons Form -->
                    <footer class="flex justify-center p-2 ">
                        <!-- button atras -->
                        <button type="button" v-if=" tabSelected == 2|| tabSelected == 3 || tabSelected == 4 || tabSelected == 5 || tabSelected == 6  || tabSelected == 7 " @Click='verifyTab(tabSelected-1)' class="flex  || focus:outline-none mr-3 mb-12"> 
                            <img class="mt-2"  src="https://ruedata.com/wp-content/uploads/2021/03/Group.svg" alt="">
                            <b v-if="tabSelected === 2 || tabSelected === 7" class="ml-2 mt-2 text-xl">Atrás</b> 
                        </button>
                        <!-- button Siguiente -->
                        <button type="button" v-if="tabSelected == 3 || tabSelected == 4 || tabSelected == 5  " @Click='verifyTab(tabSelected+1)'  class="buttonForm || ml-2    focus:outline-none text-xl px-8 h-12  ">
                            <b class="">Siguiente</b>
                        </button>
                        <!-- Button calcular -->
                        <a v-if="tabSelected == 6" onclick="return finishVerification(7)" id="calcular" href="#" target="_self" rel="noopener"><button type="button" class="buttonForm || ml-2    focus:outline-none text-xl px-8 h-12  ">
                            <b class="">Calcular</b>
                        </button></a>
                    </footer>
                </form>
            </div>
        </div>
    </div>

<!-- Develop with vueJS -->
<script src="https://cdn.jsdelivr.net/npm/vue@2/dist/vue.js"></script>
<!-- <script src="./vue.min.js"></script> -->

<!-- Script that contains the calculator operation-->
<script>
        /**
        * @fileoverview Calculadora de llantas
        * @author Daniel Fernando Gomez Ortiz
        *
        * Develop with vueJS, No borrar la referencia a este para el correcto funcionamiento
        * 
        * Datos a tener en cuenta
        * WordPress tiene dificultades con los operadores && ,<, >, por lo que se deben usar otros metodos que cumplan su mismo funcionamiento
        * WordPress cambia algunos estilos, asi que es posible que unos estilos no se reflejen o no aparezcan en el mismo wordpress.
        */

    var calculadora = new Vue({
    el: '#calculadora',
    // When page starts add the first item from coin and operation
    mounted(){       
         this.form.custom_tipooperacion = this.custom_tipooperaciones[0].nombre;
         this.form.custom_moneda = this.precios[0].custom_moneda;
    },
    // Data Calculator   
    data:{
        urlResultados:"https://ruedata.com/resultados/",
        // Variable info to Calculator. You can change it or add new items keeping the same structure
        posibleAumento:[
            {siTiene: 50, puedeCrecer:0},
            {siTiene: 45, puedeCrecer:5},
            {siTiene: 40, puedeCrecer:5},
            {siTiene: 35, puedeCrecer:5},
            {siTiene: 30, puedeCrecer:10},
            {siTiene: 20, puedeCrecer:20},
            {siTiene: 10, puedeCrecer:30},
            {siTiene: 5, puedeCrecer:35}
        ],
        precios:[
                {custom_moneda:'COP - Peso Colombiano',precio:1500},
                {custom_moneda:'USD - Dolar Americano',precio:0.5},
                {custom_moneda:'MXN - Peso Mexicano',precio:10},
                {custom_moneda:'CLP - Peso Chileno',precio:360}
            ],
        custom_tipooperaciones: [
                {nombre: 'Bus Urbano',software:15, noSoftware:22,src: 'https://ruedata.com/wp-content/uploads/2021/03/Ruedata-59.png'},
                {nombre: 'Bus Regional',software:15, noSoftware:22, src:'https://ruedata.com/wp-content/uploads/2021/03/Ruedata-60.png'},
                {nombre: 'Carga',software:15, noSoftware:22, src:'https://ruedata.com/wp-content/uploads/2021/03/Ruedata-61.png'},
                {nombre: 'Infraestructura',software:15, noSoftware:22, src:'https://ruedata.com/wp-content/uploads/2021/03/Ruedata-62.png'}
                ,{nombre: 'Automóvil',software:10, noSoftware:17,src:'https://ruedata.com/wp-content/uploads/2021/03/Ruedata-63.png'}
             ],
        // Internal data to proper functioning
        tabSelected: 1,
        finish: false,
        errors : [
            // Each object correspond to a specific tab in the form. if you want to add a new tab you have to creaate a new object
            {
                software:''
            }
            ,{
                custom_tipooperaciones:''
            },
            {
                custom_cantvehiculos :'',
            },
            {
                custom_tipovehiculo: '',
                custom_cantllantasvehiculo : '',
                custom_kmrecorridoflotames:'',
                custom_porcentajere:'',
            },
            {
                custom_llantasninyectadas :'',
                custom_valorllantan : '',
                custom_mmllantan : '',
                custom_llantasreinyectada :'',
                custom_valorllantare : '',
                custom_mmllantare : '',
            },
            {
                name:'',
                custom_apellido:'',
                custom_phone:'',
                email:''

            }
        ],
        form:{
            software: 1, 
            custom_tipooperacion : '', 
            custom_moneda: '',
            custom_tipovehiculo : '',
            custom_cantvehiculos: '',
            custom_cantllantasvehiculo:'',
            custom_kmrecorridoflotames:'',
            custom_porcentajere:'',
            custom_llantasninyectadas:'',
            custom_valorllantan:'',
            custom_mmllantan:'',
            custom_llantasreinyectada:'',
            custom_valorllantare:'',
            custom_mmllantare:'', 
            name:'',
            custom_apellido:'',
            custom_phone: '',
            email:'',
            custom_calculos:""
        },
        // data generated by operations
        datosGenerales : {
                valorServicio : '',
                totalLlantasMes: '',
                totalInvMes: '',
            },
        calculoAhorro : {
                totalAhOptimizado: '',
                totalAhLlantasN: '',
                totalAhLlantasR: '',
                roiServicio: ''
            },
        calculoAhorroConRencauche : {
            totalAhOptimizado: '',
            totalAhLlantasN: '',
            totalAhLlantasR: '',
            roiServicio: '',
            incrementocustom_porcentajere:''
}
    },
    methods:{
            submitToGetResponse(){
                document.formCalculadora.custom_software.value = this.form.software;
                document.formCalculadora.custom_tipooperacion.value = this.form.custom_tipooperacion;
                document.formCalculadora.custom_totalahoptimizado.value = this.calculoAhorro.totalAhOptimizado;
                document.formCalculadora.custom_totalahllantasn.value = this.calculoAhorro.totalAhLlantasN;
                document.formCalculadora.custom_totalahllantasr.value = this.calculoAhorro.totalAhLlantasR;
                document.formCalculadora.custom_roiservicio.value = this.calculoAhorro.roiServicio;
                document.formCalculadora.custom_rtotalahoptimizado.value = this.calculoAhorroConRencauche.totalAhOptimizado;
                document.formCalculadora.custom_rtotalahllantasn.value = this.calculoAhorroConRencauche.totalAhLlantasN;
                document.formCalculadora.custom_rtotalahllantasr.value = this.calculoAhorroConRencauche.totalAhLlantasR;
                document.formCalculadora.custom_rroiservicio.value = this.calculoAhorroConRencauche.roiServicio;       
                
                document.formCalculadora.custom_phone.value = this.form.custom_phone;       
    
                document.formCalculadora.custom_calculos.value = 'valorServicio:'+ this.datosGenerales.valorServicio +
                    '\n totalLlantasMes:'+ this.datosGenerales.totalLlantasMes + '\n totalInvMes:'+ this.datosGenerales.totalInvMes + 
                    '\n incrementoPorcentajeRE:'+ this.calculoAhorroConRencauche.incrementoPorcentajeRE ;
                    document.formCalculadora.submit();
                    dataResultados = "gT="+ this.datosGenerales.totalInvMes + "&"

                    dataResultados += "lN="+ this.calculoAhorro.totalAhLlantasN + "&"
                    dataResultados += "lR="+ this.calculoAhorro.totalAhLlantasR + "&"
                    dataResultados += "vS="+ this.datosGenerales.valorServicio + "&"
                    dataResultados += "rS="+ this.calculoAhorro.roiServicio + "&"
                    dataResultados += "tO="+ this.calculoAhorro.totalAhOptimizado + "&"

                    dataResultados += "lNr="+ this.calculoAhorroConRencauche.totalAhLlantasN + "&"
                    dataResultados += "lRr="+ this.calculoAhorroConRencauche.totalAhLlantasR + "&"
                    dataResultados += "vSr="+ this.datosGenerales.valorServicio + "&"
                    dataResultados += "rSr="+ this.calculoAhorroConRencauche.roiServicio + "&"
                    dataResultados += "tOr="+ this.calculoAhorroConRencauche.totalAhOptimizado + "&"
                    dataResultados += "iP="+ this.calculoAhorroConRencauche.incrementocustom_porcentajere         
                    document.getElementById("calcular").href = this.urlResultados + "?"+dataResultados;
            },
            submit(event){
                let varResultado = true;
                for(let i =1; i<7 ;i++){
                    if(!this.verifyTab(i)){
                        varResultado = false;
                        alert('No ha completado todo el formulario, por favor regrese y revise los datos faltantes');
                        return false;
                    }
                }
                if(varResultado){
                    this.calculadoraVirtual(this.form);
                    this.submitToGetResponse();
                    this.finish = true;
                    return true;
                }
            },
            // When the button - or  + is pressed checks if the value is the correct
            verifyInputButton(field, indexTab){
                    if(this.form[field] == '1' ){this.form[field] = 1 }
                    if(this.form[field] <= 0){
                        this.errors[indexTab-1][field] = 'El número tiene que ser mayor a 0';
                    }
                    else if(field ===  'custom_porcentajere' ){
                        if(this.form[field] >= 100){
                            this.errors[indexTab-1][field] = 'El porcentaje no puede ser mayor a 100';
                        }else{
                            this.errors[indexTab-1][field] = 'correct';
                        }
                    }
                    else{
                        this.errors[indexTab-1][field] = 'correct';
                        }
            },
            // automatically checks if the value of the input by the user is correct
            verifyInput(event,indexTab){
                    if(event.target.name ===  'email'){
                        
                        if (/^([\wÀ-ÿ\u00f1\u00d1\.\-\/]+@[a-zA-Z0-9]+(\.[a-zA-Z]{2,3}){1,3})$/.test(event.target.value))
                        {
                            this.errors[indexTab-1][event.target.name] = 'correct';
                        }
                        else{
                            this.errors[indexTab-1][event.target.name] = 'Ingrese un email valido';
                        }    
                    }
                    else if(!event.target.value){
                        this.errors[indexTab-1][event.target.name] = 'El campo es obligatorio';
                    }
                    else if(event.target.value <= 0){
                        this.errors[indexTab-1][event.target.name] = 'El número tiene que ser mayor a 0';
                    }
                    else if(event.target.name ===  'custom_porcentajere' ){
                        if(event.target.value >= 100){
                            this.errors[indexTab-1][event.target.name] = 'El porcentaje no puede ser mayor a 100';
                        }else{
                            this.errors[indexTab-1][event.target.name] = 'correct';
                        }
                    }
                    else{
                        this.errors[indexTab-1][event.target.name] = 'correct';
                        }
             },
            // Checks if all the inputs in a tab are corrects  
            verifyTab(indexTab, event){
                varResultado = true;
                if(indexTab >= this.tabSelected){
                    for(key in this.errors[this.tabSelected-1]) {
                        var option = this.errors[this.tabSelected-1][key];
                        if(option !== 'correct') {
                            if(option === ''){
                                this.errors[this.tabSelected-1][key] = 'El campo es obligatorio'
                            }
                            varResultado = false;
                        }
                    }
                }
                if(!varResultado){
                    if(indexTab == 7){
                            return false;
                        }
                }
                if(varResultado){
                        if(indexTab == 7){
                            return this.submit(event);
                        }
                        else{
                            this.tabSelected = indexTab;
                            this.finish = false;
                        }
                    }
                return varResultado;   
            },
            // Methods to calculate the result of the tires
            calculadoraVirtual(form) {
                canLLantasRodando = form.custom_cantvehiculos * form.custom_cantllantasvehiculo
                kmRecorridollantasMes = form.custom_kmrecorridoflotames * canLLantasRodando;
                mmInyectadosN = form.custom_mmllantan* form.custom_llantasninyectadas;
                mmInyectadosRe = form.custom_mmllantare * form.custom_llantasreinyectada;
                costoMilimetroN = form.custom_valorllantan/ form.custom_mmllantan;
                costoMilimetroRe = form.custom_valorllantare/ form.custom_mmllantare;

                const optimizar = this.getOptimizar(form.software, form.custom_tipooperacion);

                KmMmOptimizado = (kmRecorridollantasMes/(mmInyectadosN+mmInyectadosRe)) * (1+(optimizar/100));
                mmGastadosOptimizados = kmRecorridollantasMes / KmMmOptimizado;

                llantasReMes =  mmInyectadosRe * costoMilimetroRe;
                llantasNuevasMes =   mmInyectadosN * costoMilimetroN;
                this.datosGenerales.valorServicio = this.getvalorServicio(canLLantasRodando, form.custom_moneda);
                this.datosGenerales.totalLlantasMes = form.custom_llantasninyectadas + form.custom_llantasreinyectada;
                this.datosGenerales.totalInvMes = llantasReMes + llantasNuevasMes;

                this.calcularAhorro(form, this.datosGenerales.totalInvMes,mmGastadosOptimizados,mmInyectadosN,mmInyectadosRe, this.datosGenerales.valorServicio);
                this.calcularAhorroIncrementadoRe(form, this.datosGenerales.totalInvMes,mmGastadosOptimizados,mmInyectadosN,mmInyectadosRe,this.datosGenerales.valorServicio, costoMilimetroRe);

                this.formatNumber();            
            },
            getOptimizar(software,custom_tipooperacion){
              for(let i =0; i< this.custom_tipooperaciones.length; i++){
                    if(this.custom_tipooperaciones[i].nombre === custom_tipooperacion){
                        if(software === 'Si'){ return this.custom_tipooperaciones[i].software }
                        else{ return this.custom_tipooperaciones[i].noSoftware}
                    }
              }
            },
            getvalorServicio(canLLantasRodando, tipocustom_moneda){
                for(let i =0; i< this.precios.length; i++){
                    if(this.precios[i].custom_moneda === tipocustom_moneda){
                       return canLLantasRodando*this.precios[i].precio; 
                    }
              }
            },
            calcularAhorro(form, totalInvMes, mmGastadosOptimizados,mmInyectadosN,mmInyectadosRe, valorServicio) {

                custom_mmllantanoptimizada = mmGastadosOptimizados * (1-(form.custom_porcentajere/100));
                custom_mmllantareOptimizada = mmGastadosOptimizados - custom_mmllantanoptimizada;

                valorInvertidoLlantaN = (custom_mmllantanoptimizada/form.custom_mmllantan)*form.custom_valorllantan;
                valorInvertidoLlantasRe = (custom_mmllantareOptimizada/form.custom_mmllantare)*form.custom_valorllantare;

                this.calculoAhorro.totalAhOptimizado = totalInvMes-(valorInvertidoLlantasRe + valorInvertidoLlantaN)
                this.calculoAhorro.totalAhLlantasN = (custom_mmllantanoptimizada-mmInyectadosN)/form.custom_mmllantan;
                this.calculoAhorro.totalAhLlantasR = (custom_mmllantareOptimizada-mmInyectadosRe)/form.custom_mmllantare;
                this.calculoAhorro.roiServicio =  this.calculoAhorro.totalAhOptimizado/  valorServicio;
            },
            calcularAhorroIncrementadoRe(form, totalInvMes,mmGastadosOptimizados,mmInyectadosN,mmInyectadosRe,valorServicio, costoMilimetroRe) {
                this.calculoAhorroConRencauche.incrementocustom_porcentajere = this.getincrementocustom_porcentajere(form.custom_porcentajere);
               
                custom_mmllantareIncre = mmGastadosOptimizados*(form.custom_porcentajere+ this.calculoAhorroConRencauche.incrementocustom_porcentajere)/100;
                custom_mmllantanincre = mmGastadosOptimizados-custom_mmllantareIncre;
                custom_valorllantanincre =(mmGastadosOptimizados-custom_mmllantareIncre)*costoMilimetroN;
                custom_valorllantareincre = custom_mmllantareIncre*costoMilimetroRe;

                this.calculoAhorroConRencauche.totalAhOptimizado = totalInvMes-(custom_valorllantareincre +custom_valorllantanincre);
                this.calculoAhorroConRencauche.totalAhLlantasN = (custom_mmllantanincre-mmInyectadosN)/form.custom_mmllantan;
                this.calculoAhorroConRencauche.totalAhLlantasR = (custom_mmllantareIncre-mmInyectadosRe)/form.custom_mmllantare;
                this.calculoAhorroConRencauche.roiServicio = this.calculoAhorroConRencauche.totalAhOptimizado/valorServicio;
            },
            getincrementocustom_porcentajere(custom_porcentajere){
                for(let i =0; i< this.posibleAumento.length; i++){
                    if(this.posibleAumento[i].siTiene <=  custom_porcentajere){
                        return this.posibleAumento[i].puedeCrecer;
                    }
              }
              return this.posibleAumento[this.posibleAumento.length-1].puedeCrecer;
            },
            // transform decimals to maximum two digits
            formatNumber(){
                this.datosGenerales.valorServicio = new Intl.NumberFormat('en-US',{maximumFractionDigits:2}).format(this.datosGenerales.valorServicio);
                this.datosGenerales.totalInvMes = new Intl.NumberFormat('en-US',{maximumFractionDigits:2}).format(this.datosGenerales.totalInvMes);
                this.calculoAhorro.totalAhOptimizado = new Intl.NumberFormat('en-US',{maximumFractionDigits:2}).format(this.calculoAhorro.totalAhOptimizado);
                this.calculoAhorro.totalAhLlantasN = new Intl.NumberFormat('en-US',{maximumFractionDigits:2}).format(this.calculoAhorro.totalAhLlantasN);
                this.calculoAhorro.totalAhLlantasR = new Intl.NumberFormat('en-US',{maximumFractionDigits:2}).format(this.calculoAhorro.totalAhLlantasR);
                this.calculoAhorro.roiServicio = new Intl.NumberFormat('en-US',{maximumFractionDigits:2}).format(this.calculoAhorro.roiServicio);
                this.calculoAhorroConRencauche.totalAhOptimizado = new Intl.NumberFormat('en-US',{maximumFractionDigits:2}).format(this.calculoAhorroConRencauche.totalAhOptimizado);
                this.calculoAhorroConRencauche.totalAhLlantasN = new Intl.NumberFormat('en-US',{maximumFractionDigits:2}).format(this.calculoAhorroConRencauche.totalAhLlantasN);
                this.calculoAhorroConRencauche.totalAhLlantasR = new Intl.NumberFormat('en-US',{maximumFractionDigits:2}).format(this.calculoAhorroConRencauche.totalAhLlantasR);
                this.calculoAhorroConRencauche.roiServicio = new Intl.NumberFormat('en-US',{maximumFractionDigits:2}).format(this.calculoAhorroConRencauche.roiServicio);
            }
        }
});

function finishVerification(results){
    return calculadora.verifyTab(results)
}
</script>
