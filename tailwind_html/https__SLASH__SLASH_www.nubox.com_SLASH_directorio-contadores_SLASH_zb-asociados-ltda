    messagingSenderId: "372013360943",
    appId: "1:372013360943:web:8050ac98d7d5521ee4e3da"
  };
  
	// Initialize Firebase
	firebase.initializeApp(firebaseConfig);

	// FirebaseUI config.
	var uiConfig = {
		signInSuccessUrl: '/directorio-contadores/zb-asociados-ltda#valoracion',
		signInOptions: [
  		firebase.auth.GoogleAuthProvider.PROVIDER_ID,
			firebase.auth.EmailAuthProvider.PROVIDER_ID,
		],
		tosUrl: '#',
		privacyPolicyUrl: function() {
			window.location.assign('#');
		}
	};

  var userLoggedIn = false;
	firebase.auth().onAuthStateChanged(function(user) {
		if (user) {
			// User is signed in.
// 			var displayName = user.displayName;
// 			var email = user.email;
// 			var emailVerified = user.emailVerified;
// 			var photoURL = user.photoURL;
// 			var isAnonymous = user.isAnonymous;
// 			var uid = user.uid;
// 			var providerData = user.providerData;
			displayName = user.displayName;
      emailLogged = user.email;
      useruid = user.uid;

//			console.log(user);

			if(displayName){
				$("#user-name").html(displayName + " ("+emailLogged+")");
			}else{
				$("#user-name").html(emailLogged);
			}

			$("#user-name").append("<button class='bg-orange-500 hover:bg-orange-700 ml-2 text-white font-bold py-2 px-2 rounded text-xs' type='button' onclick='signOut()'>Salir</button>");
			$("#firebaseui-auth-container").html("");
      $(".nologgedin").css("display", "none");
      $(".loggedin").css("display", "block");
      userLoggedIn = true;
      document.getElementById("overlay-modal").style.display = "block";
      document.getElementById("overlay-modal-window").style.display = "block";
      
		} else {
      $(".nologgedin").css("display", "block");
      $(".loggedin").css("display", "none");
			var ui = firebaseui.auth.AuthUI.getInstance() || new firebaseui.auth.AuthUI(firebase.auth());
			ui.start('#firebaseui-auth-container', uiConfig);
      userLoggedIn = false;
      document.getElementById("overlay-modal").style.display = "none";
      document.getElementById("overlay-modal-window").style.display = "none";
		}
	});

	function register(){
		var email = $("#correo").val();
		var password = $("#clave").val();
		firebase.auth().createUserWithEmailAndPassword(email, password).catch(function(error) {
			var errorCode = error.code;
			var errorMessage = error.message;
			alert("Error code: "+errorCode+" - Error message: "+errorMessage);
		});
	}

	function signIn(){
		var email = $("#correo").val();
		var password = $("#clave").val();
		firebase.auth().signInWithEmailAndPassword(email, password).catch(function(error) {
			var errorCode = error.code;
			var errorMessage = error.message;
			alert("Error code: "+errorCode+" - Error message: "+errorMessage);
		});
	}


	function signOut(){
		firebase.auth().signOut().then(function() {
			$("#user-name").html("");
			// Initialize the FirebaseUI Widget using Firebase.
			window.location.href = '/directorio-contadores/zb-asociados-ltda'
//			var ui = new firebaseui.auth.AuthUI(firebase.auth());
			// The start method will wait until the DOM is loaded.
//			ui.start('#firebaseui-auth-container', uiConfig);
		}).catch(function(error) {
			alert("Error on signOut");
		});
	}
  

</script>

