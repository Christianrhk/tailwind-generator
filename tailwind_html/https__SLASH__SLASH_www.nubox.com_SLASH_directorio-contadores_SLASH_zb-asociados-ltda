  function modalData() {
    return {
      showModal: false,
      openModal() { 
        this.showModal = true; 
      },
      closeModal() { 
        this.showModal = false; 
      },
      isOpenModal() { return this.showModal === true },
      userLoggedIn() { isLoggedInFirebase()
