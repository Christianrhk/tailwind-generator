<script>
  var emailLogged = '';
  var useruid = '';
  var displayName = '';

	function dejarcomentario(){
    if($("#empresa").val() == '' || $("#rol").val() == '' || $("#comentario").val() == ''){
      alert("Debe completar todos los datos.");
      return false;
    }

    
    var url = "https://api.hubapi.com/hubdb/api/v2/tables/2614750/rows?hapikey=bac85a4b-6b47-486c-b53c-eb997a375891";

    const recTitulo = $("#titulo").val();
    const recComentario = $("#comentario").val();
    const recPuntuacion = parseInt($("#valoracion").val());
    const recFecha = (new Date().getTime());
    const recEmpresa = $("#empresa").val();
    const recRol = $("#rol").val();  
  
    
 
    
    
    var dataRequest = {
      "values": {
        "2": recTitulo,
        "4": emailLogged,
        "3": recComentario,
        "9": recPuntuacion,
        "10": recFecha,
        "12": useruid,
        "5": displayName,
        "6": recEmpresa,
        "7": recRol,
        "14": zb-asociados-ltda,
      }
    }; 
   
    $.ajax({
      type: "POST",
      contentType: "application/json",
      url: url,
      data: JSON.stringify(dataRequest),
      success: function(response){
        console.log(response);
        $("#empresa").val("");
        $("#rol").val("");
        $("#comentario").val("");
        $("#titulo").val("");
        alert("Gracias por su comentario.");
      }
    });
  
    /* publicar en el formulario de hubspot que definamos */
    //const formGuid = "b7b444f5-5c50-4105-a962-e8e97f8f785a";
    const formURL = "https://api.hsforms.com/submissions/v3/integration/submit/2081075/6add1153-5098-4f27-800e-4e9712feb136";
    const formRequest = {
      "fields": [
        {
          "name": "email",
          "value": emailLogged
        },
        {
          "name": "puntuacion",
          "value": recPuntuacion
        },
        {
          "name": "acp_recomendacion_titulo",
          "value": recTitulo
        },
        {
          "name": "acp_recomendacion_comentario",
          "value": recComentario
        },
        {
          "name": "company",
          "value": recEmpresa
        },
        {
          "name": "jobtitle",
          "value": recRol
        }
      ]
    };

    $.ajax({
      type: "POST",
      contentType: "application/json",
      url: formURL,
      data: JSON.stringify(formRequest),
      success: function(response){
        console.log(response);
      }
    });
  } /* end function dejarComentario */
  
  var firebaseConfig = {
    apiKey: "AIzaSyDinWMIcHT-p6BpxjySFKNBKUTrp3cs2T0",
    authDomain: "nubox-e3827.firebaseapp.com",
