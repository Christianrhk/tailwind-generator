    function addSpecialty(){
      var seleccionados = '';
      $("#specialty > option:selected").each(function(i){
        seleccionados = seleccionados=='' ? $(this).val() : seleccionados + ', '+$(this).val();
      });

      $('#specialty_hidden').val(seleccionados);
      $("#filtro").submit();
