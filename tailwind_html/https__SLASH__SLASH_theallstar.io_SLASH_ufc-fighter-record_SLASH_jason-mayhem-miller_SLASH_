<script>
	var firebaseConfig = {
			apiKey: "AIzaSyB7Clq1lZm929HVIeL1Yz1eLF7-X7Ekd0M",
			authDomain: "fanatics-285408.firebaseapp.com",
			databaseURL: "https://fanatics-285408.firebaseio.com",
			projectId: "fanatics-285408",
			storageBucket: "fanatics-285408.appspot.com",
			messagingSenderId: "991231794691",
			appId: "1:991231794691:web:d02ad8a42ab2e9f60eb836",
			measurementId: "G-ZMTHJ7MNFS"
		  };
		  // Initialize Firebase
		  window.fanaticsApp = firebase.initializeApp(firebaseConfig, "fanatics");
		  //firebase.analytics(window.fanaticsApp);
	jQuery(document).ready(function(){
		jQuery(".convert_local_sm").each(function(){
			var d = new Date(jQuery(this).text());
			var converted = d.toLocaleString().split();
			
			var days = ["Sun", "Mon", "Tue", "Wed", "Thu", "Fri", "Sat"];
			var dayName = days[d.getDay()];
			
			var months = ["January", "February", "March", "April", "May", "June", "July", "August", "September", "October", "November", "December"];
			var monthName = months[d.getMonth()];
			
			jQuery(this).text(dayName + ' ' + d.getDate() + ' ' + monthName + ' ' + 2022);
		})
	});
		

	function getShortName(fighter){
		if(fighter.name != fighter.last_name){
			return fighter.name[0] + ". " + fighter.last_name;
		}
		return fighter.name;
	}

	function get_result_text(fight){
		var res = [];
		if(fight.status == "ended"){
			if(fight.winner){
				res.push("<strong>" + getShortName(fight.winner) + "</strong>");
				res.push("DEF");
				if(fight.winner.id == fight.red_corner.id){
					res.push(getShortName(fight.blue_corner) );
				} else {
					res.push(getShortName(fight.red_corner));
				}
				res.push(fight.result_method);
				res.push("R" + fight.result_round_ended);
				res.push(fight.result_round_end_time);
			} else {
				res.push( getShortName(fight.blue_corner)  + " vs " + getShortName(fight.red_corner) + " ends in DRAW");
				// res.push(fight.result_method);
				// res.push("R" + fight.result_round_ended);
				// res.push(fight.result_round_end_time);
			}
			
			
		} else {
			
		}
		return res.join(" ");
	}
	jQuery(document).ready(function($){
		$(".fightcard-firebase").each(function(i, el){
			var id = $(el).data("fightcard");
			
			window.fanaticsApp
				.firestore()
				.collection("fight_cards")
				.doc("fightcard-" + id)
				.onSnapshot((documentSnapshot) => {
					var fights = [];
					var fightcard = documentSnapshot.data();
					var lastSection = "";
					fightcard.fightsOnCard.forEach(function(fight){
						var result = get_result_text(fight);
						if(false && fights.length == 0){
							fights.push(
								"<li class='fight-section'>" + 
								"<div class='section-title'>" + 
								'Main card' + 
								"</div>" + 
								"</li>"
							);
						}
						if(fight.status != "cancelled" && fight.card_section && lastSection != fight.card_section){
								lastSection = fight.card_section;
								fights.push(
									"<li class='fight-section'>" + 
										"<div class='section-title'>" + 
										fight.card_section + 
										"</div>" + 
									"</li>"
								);
						}
						if(result.length){
							fights.push(
								"<li class='fight-row'>" + 
									"<div class='matchup-result'>" + 
										result + 
									"</div>" + 
								"</li>"
							)
						} else {
							var cancelled = "";
							if(fight.status != "cancelled"){
								fights.push(
									"<li class='fight-row status-" + fight.status + "'>" + cancelled + 
										"<div class='matchup'><div class='red-corner-fighter'>" + 
										getShortName(fight.red_corner) + 
										"</div><div class='vs'> vs </div><div class='blue-corner-fighter'>" + 
										getShortName(fight.blue_corner)+
										"</div></div>" + 
									"</li>"
								);
							}
							
						}
						
					});
				
					$(el).html("<ul>" + fights.join("") + "</ul>");
				});
		});
	});
</script></div><div id="allstar_rankings_index-2" class="widget widget_allstar_rankings_index"><div class="widget-title block-head block-head-ac block-head block-head-ac block-head-g is-left has-style"><h5 class="heading">UFC Rankings</h5></div><ul class='list-sep'>
