                <h3 class="m-4 mb-0 text-sm">Preis</h3>
                <price-range class="flex flex-wrap justify-between p-4">
                  <label class="block">
                    <span class="text-sm">Von</span>
                    <input
                      class="mt-1 block w-full border-black"
                      name="filter.v.price.gte"
                      id="Mobile-Filter-Preis-GTE"type="number"
                      placeholder="0"
                      min="0"
                      inputmode="decimal"max="470.00"
                    >
                  </label>
                  <label class="block">
                    <span class="text-sm">Bis</span>
                    <input
                      class="mt-1 block w-full border-black"
