<script>  
  (function(){
  
  let oldValue = 0;
  let newValue = 0;
   
    window.addEventListener("load", function(){
      const stickyElem = document.querySelector(".facets-container");
      const footerEle = document.querySelector('#shopify-section-newsletter') || document.querySelector('#shopify-section-app') || document.querySelector('#shopify-section-footer');
      if (!stickyElem) return;
      const headerEl = document.querySelector('#shopify-section-header');

      let footerOffset;

      if (footerEle) footerOffset = footerEle.getBoundingClientRect().top + window.pageYOffset;
      let headerOffset = 0;
      if (headerEl) headerOffset = headerEl.clientHeight;
      
      const currStickyPos = stickyElem.getBoundingClientRect().top + window.pageYOffset;
      
      window.addEventListener("scroll", function(e){
	  newValue = window.pageYOffset;
      if (oldValue > newValue) {
		if(window.pageYOffset > currStickyPos && window.pageYOffset < footerOffset ) {
          stickyElem.classList.add('sticky-facet');
          stickyElem.style.top = headerOffset - 1 + 'px';
        } else {
          stickyElem.classList.remove('sticky-facet');
          stickyElem.style.top = 'unset';
        }

      } else {
        
        stickyElem.classList.remove('sticky-facet');
        stickyElem.style.top = 'unset';
        
      }
        
      oldValue = newValue;
    })
    
  });

  })();
</script>

