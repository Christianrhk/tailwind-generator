     var dFunction = "oLoad" + Math.floor(Math.random() * 1000);

        window[dFunction] = function (iid) {
            var script = document.getElementById(iid);
            var herobannerconfig = script.getAttribute('data-herobannerconfig');
            var uid = script.getAttribute('data-uid');
            HeroBannerInitVue(uid, herobannerconfig);
        }
    if (window.personalizationManager) {
        window.personalizationManager.addPersonalizedContentLoaded(function () {
            var scriptpresent = document.getElementById('5ecb723c-ca1b-441b-83b7-f7cba2146f63');
            if (scriptpresent) {
                return;
            }
            var pScript = document.createElement('script');
            pScript.setAttribute('src', "/assets/js/dist/herobanner/herobanner-min.js");
            pScript.setAttribute('onload', dFunction + "('5ecb723c-ca1b-441b-83b7-f7cba2146f63')");
            pScript.setAttribute('id', '5ecb723c-ca1b-441b-83b7-f7cba2146f63');
