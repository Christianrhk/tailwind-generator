<script>
    function debounce(func, timeout = 200) {
        let timer
        return (...args) => {
            clearTimeout(timer)
            timer = setTimeout(() => { func.apply(this, args); }, timeout)
        };
    }

    const processChange = debounce((referer) => Action(referer, `Search`))
    const processBlur = debounce((referer) => Action(referer, `CloseDropdown`), 200)
</script>
