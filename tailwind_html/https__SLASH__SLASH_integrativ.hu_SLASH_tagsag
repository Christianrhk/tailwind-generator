<script>
function cloneTagsagAlapdiploma(){
  var template = $(".tagsag_alapd").first().clone();
  $(".tagsag_alapd").last().after(template);
  $('input[type=text]', $(".tagsag_alapd").last()).each(function(){
    $(this).val('');
  });
  return false;
}
function cloneTagsagSzakvizsga(){
  var template = $(".tagsag_szakv").first().clone();
  $(".tagsag_szakv").last().after(template);
  $('input[type=text]', $(".tagsag_szakv").last()).each(function(){
    $(this).val('');
  });
  return false;
}
function tagsaginputchanged(input, copyid){
  if($("#tagsag_szamla_same").is(":checked")){
    $("#"+copyid).val($(input).val());
  }
}
function tagsagSubmit(e){
  var text='';
  $('input[type=text],input[type=email]', '#tagsagform').each(function() {
    var label = $(this).attr("placeholder");
    if($(this).attr("data-pluss")){
      label=label+$(this).attr("data-pluss");
    }
    text=text+label+": "+$(this).val()+"\n";
  });
  $("#tagsagdata").val(text);
  console.log(text);
  return true;
}
$(document).ready(function(){

  $("#tagsag_szamla_same").on("change", function(){
    if($(this).is(":checked")){
      $("#tagsag_szamla").hide();
      $(".copyable").each(function(){
        $(this).trigger("change");
      });
    }
    else{
      $("#tagsag_szamla").show();
    }
  });

});
</script>
<!-- tagsag modal end -->


