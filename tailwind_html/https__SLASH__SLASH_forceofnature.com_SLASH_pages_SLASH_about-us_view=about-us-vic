        $(".swipe").css("display", "none");
        $(".swipeleft").css("display", "block");
      });
      $(".swipeleft").click(function () {
        $(".homebuffalomobilenew").animate({ "left": "+0%" }, "slow");
        $(".swipe").css("display", "block");
        $(".swipeleft").css("display", "none");
      });
  
