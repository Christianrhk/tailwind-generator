            $('body, #header').removeClass('open__search');
            $('body, #header').toggleClass('open__login');
            return false;
        });
        $('.btn_search').on('click', function(){
            $('body, #header').removeClass('open__login');
            $('body, #header').toggleClass('open__search');
        });
