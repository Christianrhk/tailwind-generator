    /// Tạo bài viết
    $(document).ready(function () {
        window.livewire.on('createArticleModalHide', () => {
            $('#author-editor').modal('hide');
        });
        window.livewire.on('updateArticleModalHide', () => {
            $('#editArticle').modal('hide');
        });
        window.livewire.on('updateDraftArticleModalHide', () => {
            $('#editDraftArticle').modal('hide');
        });
    });

    $("#editArticle").on("hidden.bs.modal", function () {
        Livewire.emitTo('user.articles.create-article', 'cancel')
        $('#editArticle .image-edit-article-delete').addClass('d-none')
        $('#editArticle .image-edit-article-change').removeClass('d-none')
        $('#editArticle .image-input-wrapper-preview').css('background-image', '')
        editorArticleEdit.setData('')
        $('#editArticle .form-select').val(null).trigger('change');
        $('#editArticle').remove('full-screen');
        $('#editArticle .ck.ck-toolbar__items .editor-zoom-in').remove();
        $('#editArticle .ck.ck-toolbar__items .editor-zoom-out').remove();
        $('#editArticle .ck.ck-toolbar__items').append('<button class="ck ck-button ck-off editor-zoom-out" type="button" tabindex="-1">' +
            '<!--begin::Svg Icon | path: assets/media/icons/duotone/Design/ZoomMinus.svg-->\n' +
            '<span class="svg-icon svg-icon-dark svg-icon-1"><svg xmlns="http://www.w3.org/2000/svg" width="24px" height="24px" viewBox="0 0 24 24" version="1.1">\n' +
            '        <path d="M14.2928932,16.7071068 C13.9023689,16.3165825 13.9023689,15.6834175 14.2928932,15.2928932 C14.6834175,14.9023689 15.3165825,14.9023689 15.7071068,15.2928932 L19.7071068,19.2928932 C20.0976311,19.6834175 20.0976311,20.3165825 19.7071068,20.7071068 C19.3165825,21.0976311 18.6834175,21.0976311 18.2928932,20.7071068 L14.2928932,16.7071068 Z" fill="#000000" fill-rule="nonzero" opacity="0.3"/>\n' +
