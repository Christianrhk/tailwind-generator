<script data-cfasync="false" src="/cdn-cgi/scripts/5c5dd728/cloudflare-static/email-decode.min.js"></script><script>
  window.addEventListener("parallel-start", ({ detail: application }) => {
    function bool(value) {
      if (typeof value === "string") {
        return value === "true";
      }
      return !!value;
    }

    application.register("NewNavBar", class extends WalletConnectedStimulusController {
      static targets = ["navbar", "profile", "hamburger", "mobileItems", "profileItems", "walletAddress"];

      renderNoWallet() {
        this.navbarTarget.dataset.hasWallet = false;
        for (const target of this.walletAddressTargets) {
          target.innerText = "";
        }
      }

      renderWallet(event) {
        this.navbarTarget.dataset.hasWallet = true;
        for (const target of this.walletAddressTargets) {
          target.innerText = event.detail.ethAddress.slice(0, 6) + "..." + event.detail.ethAddress.slice(-4);
        }
      }

      expandHamburger() {
        this.navbarTarget.dataset.hamburgerExpanded = !bool(this.navbarTarget.dataset.hamburgerExpanded);
      }

      expandProfile() {
        this.navbarTarget.dataset.profileExpanded = !bool(this.navbarTarget.dataset.profileExpanded);
      }

      dismiss(event) {
        if (!this.mobileItemsTarget.contains(event.target) && !this.hamburgerTarget.contains(event.target)) {
          this.navbarTarget.dataset.hamburgerExpanded = false;
        }
        if (!this.profileItemsTarget.contains(event.target) && !this.profileTarget.contains(event.target)) {
          this.navbarTarget.dataset.profileExpanded = false;
        }
      }
    });

    application.register("NewNavBarGroup", class extends Stimulus.Controller {
      static targets = ["group"];

      expand() {
        this.groupTarget.dataset.expanded = !bool(this.groupTarget.dataset.expanded);
      }

      dismiss(event) {
        if (!this.groupTarget.contains(event.target)) {
          this.groupTarget.dataset.expanded = false;
        }
      }
    });
  });
</script>

  

<style>
  .toast {
    position: fixed;
    z-index: 100;
    top: 90px;
    left: 50%;
    transform: translate(-50%, -50%);
    opacity: 0;
  }

  .toast-show {
    transform: translate(-50%, -50%);
    opacity: 0;
    animation: toast-appear 0.5s both;
    animation-direction: normal;
  }

  .toast-hide {
    transform: translate(-50%, 0);
    opacity: 1;
    animation: toast-disappear 0.5s both;
  }

  .toast[data-style=success] {
    background-color: #23cc6B;
  }

  .toast[data-style=error] {
    background-color: #eb5760;
  }

  .toast[data-style=info] {
    background-color: #def141;
  }

  @keyframes toast-appear {
    from {
      transform: translate(-50%, -50%);
      opacity: 0;
    }

    to {
      transform: translate(-50%, 0);
      opacity: 1;
    }
  }

  @keyframes toast-disappear {
    from {
      transform: translate(-50%, 0);
      opacity: 1;
    }

    to {
      transform: translate(-50%, -50%);
      opacity: 0;
    }
  }
</style>

