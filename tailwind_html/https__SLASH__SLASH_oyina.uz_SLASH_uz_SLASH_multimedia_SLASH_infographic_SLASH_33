    <script>



        // Media filter
        var btnContainer_prez = document.getElementById("prezbtns");
        var btns_prez = btnContainer_prez.getElementsByClassName("prezbtn");
        for (var i = 0; i < btns_prez.length; i++) {
            btns_prez[i].addEventListener("click", function() {
                var current_prez = document.getElementsByClassName("active_prez");
                current_prez[0].className = current_prez[0].className.replace("active_prez", "");
                this.className += " active_prez";
            });
        }




        function filterprez(e) {
            const prez = document.querySelectorAll(".filterprez"); // select all animal divs
            let filter_prez = e.target.dataset.filter; // grab the value in the event target's data-filter attribute

            prez.forEach(pre => {
                pre.classList.contains(filter_prez) // does the vid have the filter in its class list?
                    ?  pre.classList.remove('hidden') // if yes, make sure .hidden is not applied
                    : pre.classList.add('hidden'); // if no, apply .hidden
            });
        };
    </script>








