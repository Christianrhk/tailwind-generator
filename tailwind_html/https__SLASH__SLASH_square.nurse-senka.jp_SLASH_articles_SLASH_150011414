</div><script>let page = 0;let bullets;let maxPage;$(function() {const imgs = $(".entry-content .slide-img").find("img");const imgWidth = parseInt(imgs.css("width").replace(/px/, ""));const imgHeight = parseInt(imgs.css("height").replace(/px/, ""));const entryContentWidth = $(".entry-content")[0].offsetWidth;const rate = entryContentWidth/imgWidth;$(".swiper").css("width", (imgWidth*rate)+"px");$(".swiper").css("height", (imgHeight*rate+90)+"px");for(img of imgs) {const imgHtml = '<div class="swiper-slide"><img src="'+img.src+'" /></div>';$(".swiper-wrapper").append(imgHtml);}const swiper = new Swiper(".swiper", {direction: "horizontal",loop: false,pagination: {el: ".swiper-pagination",clickable: true}});bullets = swiper.pagination.bullets;for(index in bullets) { if($(bullets[index]).hasClass("swiper-pagination-bullet-active")) page=parseInt(index)+1 }maxPage = swiper.pagination.bullets.length;$(".swiper-pagination-text p").text(page+"/"+maxPage+"ページ");swiper.on("slideChange", () => {for(index in bullets) { if($(bullets[index]).hasClass("swiper-pagination-bullet-active")) page=parseInt(index)+1 }$(".swiper-pagination-text p").text(page+"/"+maxPage+"ページ");if(page > 1) $(".prev-button").removeClass("prev-button-deactive");if(page == maxPage) $(".next-button").addClass("next-button-deactive");});$(".next-button a").on("click", (e)=>{e.preventDefault();swiper.slideNext();for(index in bullets) { if($(bullets[index]).hasClass("swiper-pagination-bullet-active")) page=parseInt(index)+1 }if(page > 1) $(".prev-button").removeClass("prev-button-deactive");if(page == maxPage) $(".next-button").addClass("next-button-deactive");});$(".prev-button a").on("click", (e)=>{e.preventDefault();swiper.slidePrev();for(index in bullets) { if($(bullets[index]).hasClass("swiper-pagination-bullet-active")) page=parseInt(index)+1 }if(page == 1) $(".prev-button").addClass("prev-button-deactive");if(page < maxPage) $(".next-button").removeClass("next-button-deactive");});});</script><div class="