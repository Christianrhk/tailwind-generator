        <script>
		  var input = document.querySelector("#phone");
		  var ii = window.intlTelInput(input, {
			// any initialisation options go here
			separateDialCode:true
		  });
		  
		  
		 
		$('#phone').on("countrychange", function() {
			// var iti = intlTelInput(input);
			// iti.destroy();
			// var extension = iti.getExtension();
			// var number = iti.getNumber();
		  	// console.log(iti);
			
			console.log( ii.getSelectedCountryData().dialCode );
			//$(this).val('+'+ii.getSelectedCountryData().dialCode);
			$(this).val('');
		
		});
		</script>
		<script>
			jQuery(document).ready(function() {
				$('#phone').mask('(000) 000-0000');
				
			});
            jQuery(document).ready(function() {
                
                var email    = '';
                var phone    = '';
                var pass     = '';
                var cpass    = '';
                var fname    = '';
                var lname    = '';
                var errMsg   = '';
                var errCount = 0;
                var fullNumber = '';

                var RegisterBtn = $('#register-btn');
                var errorBox = $('#errorBox');
                var errorMsg = $('#errorMsg');

                var page      = '';
                
                RegisterBtn.on('click', function() {

                    $(this).attr('disabled', true);

                    RegisterBtn.text('Checking credentials...');

                    errorBox.fadeOut(1); 

                    errMsg   = '';
                    errCount = 0;

                    if( $('#fname').val() == '' ) { errCount++; errMsg += 'Enter first name. <br />'; }
                    else { fname = $('#fname').val(); }

                    if( $('#lname').val() == '' ) { errCount++; errMsg += 'Enter last name. <br />'; }
                    else { lname = $('#lname').val(); }

                    if( $('#email').val() == '' ) { errCount++; errMsg += 'Enter email address. <br />'; }
                    else if( ! validateEmail( $('#email').val() ) ) { errCount++; errMsg += 'Enter a valid email. <br />'; }
                    else { email = $('#email').val(); }
					
					if( $('#phone').val() == '' ) { errCount++; errMsg += 'Enter phone number. <br />'; }
                    else { 
							code = ii.getSelectedCountryData().dialCode;
							phone = $('#phone').val(); 
							fullNumber	=code+' '+phone; 
						}

                    if( $('#password').val() == '' ) { errCount++; errMsg += 'Enter password. <br />'; }
                    else { pass = $('#password').val(); }

                    if( $('#cpassword').val() == '' ) { errCount++; errMsg += 'Enter confirm password. <br />'; }
                    else { cpass = $('#cpassword').val(); }
                    
                    if( errCount == 0 ) {
                        $.ajax({
                            url: 'https://go.snapps.ai/ajax/register/step-one',
                            method: 'POST',
                            data: { first_name: fname, last_name: lname, email_address: email, phone:'+'+fullNumber, password: pass, password_confirmation: cpass, _token: "KMYm52vckjXel2cmqyHO0txWniirzhSqlgM4Dccy" },
                            success: function(data) {
                                var data = $.parseJSON( data );
                                //Redirect User
                                if( data.success == 1 ) { 
                                    if( page == 'purchase' ) {
                                        window.location.href = "https://go.snapps.ai/settings/purchase-plan";
                                    } else {
                                        window.location.href = "https://go.snapps.ai/dashboard?t=1";
                                    }
                                   
                                } else {
                                    var err = '';
                                    for( var i = 0; i < data.msg.length; i++ ) {
                                        if( data.msg[i] == 'The email has already been taken.' ) {
                                            err += 'It looks like you already have a snapps account with this email. <br />';
                                        } else {
                                            err += data.msg[i]+' <br />';
                                        }
                                        
                                    }

                                    errorBox.fadeIn(1);
                                    errorMsg.html( err );
                                    
                                    RegisterBtn.attr('disabled', false); 

                                    RegisterBtn.text('Sign up');
                                }
                                
                            }
                        });
                    } else {
                        
                        errorBox.fadeIn(1);
                        errorMsg.html( errMsg );

                        RegisterBtn.attr('disabled', false);
                    }

                });

            });

            function validateEmail(email) {
                const re = /^(([^<>()[\]\\.,;:\s@"]+(\.[^<>()[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
                return re.test(String(email).toLowerCase());
            }
        
        </script>
