                                <div id="searchbar" style="position: absolute; top: 5rem; left: 0; width: 100%" class="bg-black pb-6 px-6">
                        <form name="search" data-simplesearch-form>
        <div class="form-field">
            <div class="form-label">Rechercher...</div>
            <div class="form-data">
                <div class="form-input-wrapper">
                    <input
                        name="searchfield"
                        class="search-input pl-4 py-4 pr-11"
                        aria-label="search"
                        type="text"
                         data-min="3"                         required
                        value=""
                        data-search-invalid="Veuillez ajouter au moins 3 caractères"
                        data-search-separator=":"
                        data-search-input="/fr/search/query"
                    />
                                        <div style="position: absolute; right: 0.6rem; top: 0.8rem" class="button-style-icon hide light">
                        <i class="mdi mdi-close text-lg"></i>
                      </div>
                </div>
            </div>
        </div>
    </form>

<script>
    {
      let field;
      let input;
      let label;
      let clear;
    
      if(document.querySelector('input[name="searchfield"]')){
        input = document.querySelector('input[name="searchfield"]')
        clear = input.parentElement.children[1]
        field = input.parentElement.parentElement.parentElement
        label = field.children[0]
    
        if(document.activeElement === input){
          field.classList.add('focused')
        }
        if(input.value){
          field.classList.add('filled')
          clear.classList.remove('hide')
        }
    
        if(clear){
          clear.addEventListener('click', () => {
            if(input.value) {
              input.value = ""
              input.focus()
              clear.classList.add('hide')
              field.classList.remove('filled')
            }
          })
    
          input.addEventListener('keyup', () => {
            if(input.value) {
              clear.classList.remove('hide')
            }
            else {
              clear.classList.add('hide')
            }
          })
        }
    
        input.addEventListener('focus', () => {
          field.classList.add('focused')
    
          if(input.value){
            field.classList.add('filled')
          }
        })
    
        input.addEventListener('blur', () => {
          field.classList.remove('focused')
    
          if(!input.value){
            field.classList.remove('filled')
          }
          else {
            field.classList.add('filled')
          }
        })
      }
    }
    </script>
    
    <style>
      .form-field {
        position: relative;
      }
      .form-field .form-label {
        transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1);
        position: absolute;
        left: 1rem;
        top: 1.1rem;
        pointer-events: none;
        width: calc(100% - 2rem);
      }
      .form-field.focused .form-label, .form-field.filled .form-label {
        font-size: 0.7rem;
        top: 0.07rem
      }
      .form-field.focused .form-label, .form-field.focused .button-style-icon {
        color: rgb(0, 119, 255);
      }
      .form-field input, .form-field textarea {
        background: none;
        border: 0.1rem solid white;
        width: 100%;
        outline: 0;
      }
      .form-field input, .section-light .form-field textarea {
        border: 0.1rem solid white;
      }
      .section-light .form-field input, .section-light .form-field textarea {
        border: 0.1rem solid black;
      }
      .section-light .form-field {
        background: white;
      }
      .section-dark .form-field {
        background: black;
      }
      .form-field textarea {
        padding: 1rem;
        height: 20rem
    
      }
      .form-field input:focus-visible, .form-field textarea:focus-visible {
        border: 0.1rem solid rgb(0, 119, 255);
      }
      .form-field textarea {
        min-height: 3.65rem
      }
      .button-style-icon.hide {
        opacity: 0;
        pointer-events: none;
      }
    </style>
    
